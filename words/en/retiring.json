    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0f3da55f-b415-3e8b-879f-6d598cd6fd86",
                    "sense_definition": "Present participle and gerund of <w>retire</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My father is <w>retiring</w> from his job at the end of this month.",
                            "target_lang_translations": {
                                "ru": "Мой отец <w>уходит на пенсию</w> в конце этого месяца."
                            }
                        },
                        {
                            "text": "She plans on <w>retiring</w> to a small cottage near the sea.",
                            "target_lang_translations": {
                                "ru": "Она планирует <w>удалиться</w> в маленький коттедж у моря."
                            }
                        }
                    ],
                    "common_phrases": [
                        "thinking of retiring",
                        "retiring soon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола retire (уходить на пенсию, удаляться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выход на пенсию",
                                "target_lang_sense_clarification": "процесс завершения трудовой деятельности"
                            },
                            {
                                "target_lang_word": "уходящий на пенсию",
                                "target_lang_sense_clarification": "о человеке, завершающем карьеру"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2ec3345d-7bb5-3680-afff-6651c66a9415",
                    "sense_definition": "Shy, introverted, and preferring to avoid social contact or attention.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite his <w>retiring</w> nature, he was a brilliant and effective leader.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свой <w>замкнутый</w> характер, он был блестящим и эффективным лидером."
                            }
                        },
                        {
                            "text": "She is a quiet and <w>retiring</w> person who rarely speaks in large groups.",
                            "target_lang_translations": {
                                "ru": "Она тихий и <w>застенчивый</w> человек, который редко говорит в больших группах."
                            }
                        }
                    ],
                    "synonyms": [
                        "shy",
                        "reserved",
                        "introverted",
                        "withdrawn"
                    ],
                    "antonyms": [
                        "outgoing",
                        "sociable",
                        "extroverted"
                    ],
                    "common_phrases": [
                        "shy and retiring",
                        "retiring disposition",
                        "retiring nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застенчивый, интровертированный, предпочитающий избегать социального контакта или внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застенчивый",
                                "target_lang_sense_clarification": "робкий, стеснительный"
                            },
                            {
                                "target_lang_word": "скромный",
                                "target_lang_sense_clarification": "не любящий привлекать к себе внимание"
                            },
                            {
                                "target_lang_word": "замкнутый",
                                "target_lang_sense_clarification": "необщительный, скрытный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b3180d6-855f-35e5-aa60-a0019a5b80e0",
                    "sense_definition": "The act of leaving one's job or withdrawing to a private place.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>retiring</w> of the jury to consider their verdict took longer than expected.",
                            "target_lang_translations": {
                                "ru": "<w>Удаление</w> присяжных для вынесения вердикта заняло больше времени, чем ожидалось."
                            }
                        },
                        {
                            "text": "His sudden <w>retiring</w> from public life surprised everyone.",
                            "target_lang_translations": {
                                "ru": "Его внезапный <w>уход</w> из общественной жизни удивил всех."
                            }
                        }
                    ],
                    "synonyms": [
                        "retirement",
                        "withdrawal",
                        "departure"
                    ],
                    "target_lang_definitions": {

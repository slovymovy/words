    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "c1be15bc-3df2-3378-8667-dd0b9cfed7a4",
                    "sense_definition": "Used to refer to two or more people or things that have been mentioned previously or are easily identified.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "referential_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have two dogs, and <w>they</w> love to play in the park."
                        },
                        {
                            "text": "Where are the keys? <w>They</w> were on the table a minute ago."
                        },
                        {
                            "text": "My parents are coming to visit, and <w>they</w> will arrive at noon."
                        }
                    ],
                    "antonyms": [
                        "we"
                    ]
                },
                {
                    "sense_id": "8f873bbf-b953-3733-84fa-923a01193ea8",
                    "sense_definition": "Used to refer to a single person whose gender is not specified, not known, or who identifies as non-binary.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "referential_singular",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If a student needs help, <w>they</w> should ask the teacher immediately."
                        },
                        {
                            "text": "Someone left their umbrella here; I hope <w>they</w> come back for it."
                        },
                        {
                            "text": "Sam uses gender-neutral pronouns, so <w>they</w> will join us later."
                        }
                    ]
                },
                {
                    "sense_id": "9a60c777-9a92-3b12-9c0d-ff3a4c49c69a",
                    "sense_definition": "Used to refer to people in general, often when stating a common belief, rumor, or indefinite opinion.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "indefinite_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>They</w> say that breakfast is the most important meal of the day."
                        },
                        {
                            "text": "I don't care what <w>they</w> think about my new haircut."
                        },
                        {
                            "text": "<w>They</w> don't make cars like that anymore."
                        }
                    ],
                    "common_phrases": [
                        "they say",
                        "so they say"
                    ]
                },
                {
                    "sense_id": "66c58c32-ebcb-3620-af54-da1568f27e15",
                    "sense_definition": "Used to refer to people in authority, such as the government, employers, or the police.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "indefinite_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>They</w> are going to raise taxes again next year."
                        },
                        {
                            "text": "Why don't <w>they</w> fix the potholes in this street?"
                        },
                        {
                            "text": "<w>They</w> should increase our wages if they want us to work harder."
                        }
                    ],
                    "common_phrases": [
                        "what they want",
                        "they will"
                    ]
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "77e82452-cf91-31f5-8a6b-9b7109c2c562",
                    "sense_definition": "Those; used to point out specific people or things.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dialectal_determiner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Look at <w>they</w> birds flying over the field."
                        },
                        {
                            "text": "I never did like <w>they</w> shoes you wear."
                        }
                    ],
                    "synonyms": [
                        "those"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Used in some regional dialects instead of 'those'."
                        }
                    ]
                },
                {
                    "sense_id": "0561834a-39d9-3021-83cc-450e61976ebd",
                    "sense_definition": "Belonging to or associated with them.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dialectal_possessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children forgot <w>they</w> books at school today."
                        },
                        {
                            "text": "People should mind <w>they</w> own business."
                        }
                    ],
                    "synonyms": [
                        "their"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Used in AAVE and some dialects instead of 'their'."
                        }
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "62ee2df1-78a0-3f43-ae04-935609dc3ce7",
                    "sense_definition": "To refer to a person using the pronoun 'they'.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I accidentally <w>theyed</w> him because I forgot his preferred pronouns."
                        },
                        {
                            "text": "She prefers to be <w>theyed</w> rather than referred to as 'she'."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Neologism used in discussions about gender pronouns."

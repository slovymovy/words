                {
                    "sense_id": "f41bf442-9a99-3644-ac83-0111d2b4ca03",
                    "sense_definition": "De voormalige munteenheid van Nederland (tot 2002) en de huidige munteenheid van Curaçao en Sint Maarten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "currency_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Nederland wordt niet meer met de <w>gulden</w> betaald.",
                            "target_lang_translations": {
                                "en": "In the Netherlands, people no longer pay with the <w>guilder</w>."
                            }
                        },
                        {
                            "text": "De toegangsprijs voor het museum was destijds vijf <w>gulden</w>.",
                            "target_lang_translations": {
                                "en": "The entrance fee for the museum was five <w>guilders</w> at the time."
                            }
                        }
                    ],
                    "synonyms": [
                        "florijn",
                        "fl",
                        "ƒ"
                    ],
                    "common_phrases": [
                        "op de gulden af"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "historical in Netherlands, current in parts of Caribbean"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The former currency of the Netherlands (until 2002) and the current currency of Curaçao and Sint Maarten."
                    },
                    "translations": {

                {
                    "sense_id": "7af0207f-7534-3874-9188-3780e8a40059",
                    "sense_definition": "Logiczny wywód wykazujący prawdziwość twierdzenia naukowego, zwłaszcza w matematyce.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "logic_proof",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia o przeprowadzenie <dowodu> twierdzenia Pitagorasa przy tablicy.",

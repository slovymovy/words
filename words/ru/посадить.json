                {
                    "sense_id": "db961a13-d151-370d-8661-f1e6aa17a540",
                    "sense_definition": "Попросить или заставить кого-либо сесть; предложить место для сидения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "seat_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель <посадил> учеников за парты.",
                            "target_lang_translations": {
                                "en": "The teacher <seated> the students at their desks."
                            }
                        },
                        {
                            "text": "Мама <посадила> ребёнка к себе на колени.",
                            "target_lang_translations": {
                                "en": "Mom <sat> the child on her lap."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посадить за стол",
                        "посадить на стул",
                        "посадить на колени"
                    ],
                    "target_lang_definitions": {
                        "en": "To ask or force someone to sit; to offer a seat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seat",
                                "target_lang_sense_clarification": "to cause to sit"
                            },
                            {
                                "target_lang_word": "sit down",
                                "target_lang_sense_clarification": "causative; to make someone sit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb0224bd-29f9-399f-8cf3-786a1d8f603e",
                    "sense_definition": "Поместить семена, корни или саженцы в землю для выращивания.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plant_agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Весной мы <посадили> в саду яблоню.",
                            "target_lang_translations": {
                                "en": "In the spring, we <planted> an apple tree in the garden."
                            }
                        },
                        {
                            "text": "Бабушка <посадила> цветы на клумбе перед домом.",
                            "target_lang_translations": {
                                "en": "Grandma <planted> flowers in the flowerbed in front of the house."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посадить дерево",
                        "посадить цветы",
                        "посадить картошку"
                    ],
                    "target_lang_definitions": {
                        "en": "To place seeds, roots, or seedlings in the ground for growing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plant",
                                "target_lang_sense_clarification": "agriculture/gardening"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cf4416f-545c-3912-8c61-aefbc468daaf",
                    "sense_definition": "Лишить свободы, отправив в тюрьму в качестве наказания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "imprison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Преступника поймали и <посадили> в тюрьму.",
                            "target_lang_translations": {
                                "en": "The criminal was caught and <jailed>."
                            }
                        },
                        {
                            "text": "Его могут <посадить> за кражу на несколько лет.",
                            "target_lang_translations": {
                                "en": "They could <imprison> him for theft for several years."
                            }
                        }
                    ],
                    "synonyms": [
                        "упечь",
                        "закрыть"
                    ],
                    "common_phrases": [
                        "посадить в тюрьму",
                        "посадить за решётку"
                    ],
                    "target_lang_definitions": {
                        "en": "To deprive of liberty by sending to prison as punishment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jail",
                                "target_lang_sense_clarification": "common term"
                            },
                            {
                                "target_lang_word": "imprison",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "put away",
                                "target_lang_sense_clarification": "slang/idiomatic"
                            },
                            {
                                "target_lang_word": "lock up",
                                "target_lang_sense_clarification": "phrasal verb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d4771d0-47d8-3682-a8d5-a53673b08a5c",
                    "sense_definition": "Управляя летательным аппаратом, опустить его на землю.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "land_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пилот мастерски <посадил> самолёт в густом тумане.",
                            "target_lang_translations": {
                                "en": "The pilot skillfully <landed> the plane in the thick fog."
                            }
                        },
                        {
                            "text": "Нам удалось <посадить> вертолёт на лесной поляне.",
                            "target_lang_translations": {
                                "en": "We managed to <land> the helicopter in a forest clearing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посадить самолёт",
                        "экстренно посадить",
                        "мягко посадить"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring an aircraft down to the ground while piloting it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "land",
                                "target_lang_sense_clarification": "aviation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5aa7ad0-d1ee-3ed0-b140-486f0e9b844a",
                    "sense_definition": "Заставить кого-либо соблюдать определённый режим или условия.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "restrict_regime",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач <посадил> пациента на строгую диету.",
                            "target_lang_translations": {
                                "en": "The doctor <put> the patient on a strict diet."
                            }
                        },
                        {
                            "text": "Родители <посадили> сына под домашний арест за плохие оценки.",
                            "target_lang_translations": {
                                "en": "The parents <placed> their son under house arrest for bad grades."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посадить на диету",
                        "посадить на карантин",
                        "посадить на цепь"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to observe a specific regime or conditions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "on a diet/regime"
                            },
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "under arrest/restriction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00b0f68f-fe50-3818-bfc0-7582b9dd9622",
                    "sense_definition": "Испортить или истощить ресурс чего-либо (зрения, голоса, батареи) чрезмерным использованием.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "damage_drain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты можешь <посадить> зрение, если будешь читать в темноте.",
                            "target_lang_translations": {
                                "en": "You can <ruin> your eyesight if you read in the dark."
                            }
                        },
                        {
                            "text": "Мы <посадили> аккумулятор в машине, забыв выключить фары.",
                            "target_lang_translations": {
                                "en": "We <drained> the car battery by forgetting to turn off the headlights."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посадить зрение",
                        "посадить аккумулятор",
                        "посадить голос",
                        "посадить батарейку"
                    ],
                    "target_lang_definitions": {
                        "en": "To spoil or exhaust a resource (vision, voice, battery) through excessive use."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "eyesight/health"
                            },
                            {
                                "target_lang_word": "drain",
                                "target_lang_sense_clarification": "battery/accumulator"
                            },
                            {
                                "target_lang_word": "strain",
                                "target_lang_sense_clarification": "voice/eyes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9c1a2d6-806f-3657-8798-054c07e408cd",

{
    "word_family": [
        "revolutionair",
        "revolutioneren",
        "contrarevolutie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "25fe9e6a-d181-3648-8b8f-d1ce75ff601c",
                    "sense_definition": "Een plotselinge en ingrijpende verandering in de politieke machtsverhoudingen, vaak gepaard gaand met geweld.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "drastic_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Castro kwam aan de macht door middel van een <w>revolutie</w>.",
                            "target_lang_translations": {
                                "ru": "Кастро пришел к власти в результате <w>революции</w>."
                            }
                        },
                        {
                            "text": "Na de <w>revolutie</w> werd er een nieuwe regering gevormd.",
                            "target_lang_translations": {
                                "ru": "После <w>революции</w> было сформировано новое правительство."
                            }
                        }
                    ],
                    "synonyms": [
                        "omwenteling"
                    ],
                    "antonyms": [
                        "evolutie"
                    ],
                    "common_phrases": [
                        "de Franse Revolutie",
                        "een bloedige revolutie",
                        "socialistische revolutie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное и радикальное изменение политического строя, часто сопровождающееся насилием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "революция",
                                "target_lang_sense_clarification": "политическая"
                            },
                            {
                                "target_lang_word": "переворот",
                                "target_lang_sense_clarification": "смена власти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "062cc475-a08c-390b-bb1c-fbb82fc0a650",
                    "sense_definition": "Een grote en radicale vernieuwing op een bepaald gebied, zoals in de wetenschap of technologie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "drastic_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het internet heeft voor een <w>revolutie</w> gezorgd in de manier waarop we communiceren.",
                            "target_lang_translations": {
                                "ru": "Интернет произвел <w>революцию</w> в том, как мы общаемся."
                            }
                        },
                        {
                            "text": "De industriële <w>revolutie</w> veranderde de samenleving voorgoed.",
                            "target_lang_translations": {
                                "ru": "Промышленная <w>революция</w> навсегда изменила общество."
                            }
                        }
                    ],
                    "synonyms": [
                        "omwenteling"
                    ],
                    "antonyms": [
                        "evolutie"
                    ],
                    "common_phrases": [
                        "industriële revolutie",
                        "digitale revolutie",
                        "culturele revolutie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное и радикальное обновление в определенной области, например, в науке или технике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "революция",
                                "target_lang_sense_clarification": "радикальное обновление"
                            },
                            {
                                "target_lang_word": "переворот",
                                "target_lang_sense_clarification": "резкий скачок в развитии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f79dc868-15a4-3956-9ffe-63bca9c5e6a6",
                    "sense_definition": "Een volledige draaiing van een hemellichaam om een ander hemellichaam of om zijn eigen as.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aarde maakt elk jaar een volledige <w>revolutie</w> om de zon.",
                            "target_lang_translations": {
                                "ru": "Каждый год Земля совершает полный <w>оборот</w> вокруг Солнца."
                            }
                        },
                        {
                            "text": "In de sterrenkunde spreekt men soms van een <w>revolutie</w> als men een omloop bedoelt.",
                            "target_lang_translations": {
                                "ru": "В астрономии иногда используют термин <w>революция</w>, подразумевая оборот."
                            }
                        }
                    ],
                    "synonyms": [
                        "omwenteling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный оборот небесного тела вокруг другого небесного тела или вокруг собственной оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборот",
                                "target_lang_sense_clarification": "полный круг"
                            },
                            {
                                "target_lang_word": "обращение",
                                "target_lang_sense_clarification": "движение по орбите"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
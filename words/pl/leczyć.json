            "pos": "verb",
            "senses": [
                {
                    "sense_id": "06e6b655-8562-3d4f-b060-186407113be7",
                    "sense_definition": "Stosować środki medyczne, terapię lub zabiegi wobec kogoś, aby przywrócić mu zdrowie lub usunąć chorobę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "zdrowie_i_medycyna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony lekarz skutecznie <leczy> pacjentów w szpitalu.",
                            "target_lang_translations": {
                                "ru": "Опытный врач эффективно <w>лечит</w> пациентов в больнице."
                            }
                        },
                        {
                            "text": "Moja babcia zawsze <leczyła> przeziębienie herbatką z lipy.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда <w>лечила</w> простуду липовым чаем."
                            }
                        }
                    ],
                    "synonyms": [
                        "kurować",
                        "uzdrawiać"
                    ],
                    "common_phrases": [
                        "leczyć ludzi",
                        "leczyć zęby",
                        "leczyć grypę",
                        "leczyć rany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Применять медицинские средства, терапию или процедуры для восстановления здоровья или устранения болезни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лечить",
                                "target_lang_sense_clarification": "оказывать медицинскую помощь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bae3e708-871e-3027-bc9e-d3ac2c0ed1dc",
                    "sense_definition": "Poddawać się terapii, przyjmować leki lub stosować domowe sposoby w celu odzyskania zdrowia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "zdrowie_i_medycyna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek od wielu lat <leczy się> na nadciśnienie.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка уже много лет <w>лечится</w> от гипертонии."
                            }
                        },
                        {
                            "text": "Zamiast iść do przychodni, woli <leczyć się> domowymi sposobami.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы идти в поликлинику, он предпочитает <w>лечиться</w> домашними средствами."
                            }
                        }
                    ],
                    "synonyms": [
                        "kurować się"
                    ],
                    "common_phrases": [
                        "leczyć się na grypę",
                        "leczyć się u specjalisty",
                        "leczyć się samemu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить лечение, принимать лекарства или использовать домашние средства для выздоровления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лечиться",
                                "target_lang_sense_clarification": "проходить курс лечения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

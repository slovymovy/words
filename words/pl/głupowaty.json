                {
                    "sense_id": "129ee464-0556-31d2-8459-c3b07619190f",
                    "sense_definition": "Wyrażający brak inteligencji, bezmyślność lub rozbawienie (zwykle o wyrazie twarzy lub uśmiechu).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "appearance_stupidity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miał przyklejony do twarzy ten swój stały, <w>głupowaty</w> uśmiech.",
                            "target_lang_translations": {
                                "ru": "У него на лице застыла эта его постоянная <w>глуповатая</w> улыбка."
                            }
                        },
                        {
                            "text": "Zrobił <w>głupowatą</w> minę, kiedy nauczyciel zapytał go o zadanie domowe.",
                            "target_lang_translations": {
                                "ru": "Он состроил <w>придурковатую</w> мину, когда учитель спросил его о домашнем задании."
                            }
                        }
                    ],
                    "synonyms": [
                        "głupkowaty",
                        "głupawy"
                    ],
                    "antonyms": [
                        "poważny"
                    ],
                    "common_phrases": [
                        "głupowaty uśmiech",
                        "głupowata mina",
                        "głupowaty wyraz twarzy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий отсутствие ума, бессмысленность или веселье (обычно о выражении лица или улыбке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глуповатый",
                                "target_lang_sense_clarification": "о выражении лица"
                            },
                            {
                                "target_lang_word": "придурковатый",
                                "target_lang_sense_clarification": "выглядящий глупо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94ad93ea-f3ab-39e0-8a01-02d0c283d064",
                    "sense_definition": "O człowieku: niezbyt mądry, ograniczony umysłowo lub zachowujący się w sposób mało inteligentny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_stupidity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź taki <w>głupowaty</w> i zacznij wreszcie zachowywać się jak dorosły.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>придурковатым</w> и начни наконец вести себя как взрослый."
                            }
                        },
                        {
                            "text": "Ten <w>głupowaty</w> chłopak znowu zepsuł nam zabawę swoim zachowaniem.",
                            "target_lang_translations": {
                                "ru": "Этот <w>глуповатый</w> парень снова испортил нам веселье своим поведением."
                            }
                        }
                    ],
                    "synonyms": [
                        "przygłupi",

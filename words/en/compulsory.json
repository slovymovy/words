{
    "word_family": [
        "compel",
        "compulsion",
        "compulsive",
        "compulsively"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bcab0d5d-7d31-3295-ab87-79d4d39f1bac",
                    "sense_definition": "Required by law, rule, or authority; not optional.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Attendance at the morning meeting is <w>compulsory</w> for all staff members.",
                            "target_lang_translations": {
                                "ru": "Присутствие на утреннем собрании <w>обязательно</w> для всех сотрудников."
                            }
                        },
                        {
                            "text": "In many countries, <w>compulsory</w> education continues until the age of sixteen.",
                            "target_lang_translations": {
                                "ru": "Во многих странах <w>обязательное</w> образование продолжается до шестнадцати лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "mandatory",
                        "obligatory",
                        "required",
                        "binding",
                        "imperative"
                    ],
                    "antonyms": [
                        "optional",
                        "voluntary"
                    ],
                    "common_phrases": [
                        "compulsory education",
                        "compulsory attendance",
                        "compulsory subject",
                        "compulsory military service"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требуемый по закону или правилам; не являющийся добровольным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обяза́тельный",
                                "target_lang_sense_clarification": "требуемый правилами или законом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6223e2a-a6fc-3a1c-b399-7041d71d0b0d",
                    "sense_definition": "Having the power to force or constrain someone to do something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government introduced <w>compulsory</w> measures to limit the spread of the virus.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело <w>принудительные</w> меры для ограничения распространения вируса."
                            }
                        },
                        {
                            "text": "The committee was granted <w>compulsory</w> powers to obtain the necessary documents.",
                            "target_lang_translations": {
                                "ru": "Комитет получил <w>принудительные</w> полномочия для получения необходимых документов."
                            }
                        }
                    ],
                    "synonyms": [
                        "forced",
                        "binding"
                    ],
                    "antonyms": [
                        "voluntary"
                    ],
                    "common_phrases": [
                        "compulsory measures",
                        "compulsory powers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий силой принуждать или заставлять кого-либо делать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принуди́тельный",
                                "target_lang_sense_clarification": "осуществляемый по принуждению"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "11f8c977-62e5-3dda-9382-9d00cefa24dd",
                    "sense_definition": "Something that must be done or performed, often referring to required exercises in sports like figure skating.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The skaters spent hours practicing their <w>compulsories</w> before the main competition.",
                            "target_lang_translations": {
                                "ru": "Фигуристы часами отрабатывали <w>обязательную программу</w> перед основными соревнованиями."
                            }
                        },
                        {
                            "text": "Although he scored poorly in the <w>compulsories</w>, he excelled in the freestyle event.",
                            "target_lang_translations": {
                                "ru": "Хотя он получил низкие оценки за <w>обязательную программу</w>, он преуспел в произвольной программе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "technical compulsories",
                        "skating compulsories"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обязательные упражнения или элементы программы, которые необходимо выполнить (обычно в спорте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обязательная программа",
                                "target_lang_sense_clarification": "в спорте"
                            },
                            {
                                "target_lang_word": "обязательные упражнения",
                                "target_lang_sense_clarification": "элементы соревнований"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
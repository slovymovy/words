    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f3bda9c-312b-3dda-9688-9618579f0ba4",
                    "sense_definition": "A writer who writes short items or paragraphs, often for a newspaper.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "writer_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>paragrapher</w> wrote a witty comment about local politics in the daily column.",
                            "target_lang_translations": {
                                "ru": "<w>Автор заметок</w> написал остроумный комментарий о местной политике в ежедневной колонке."
                            }
                        },
                        {
                            "text": "He worked as a <w>paragrapher</w> for a small town newspaper before writing novels.",
                            "target_lang_translations": {
                                "ru": "До того как начать писать романы, он работал <w>хроникером</w> в газете небольшого городка."
                            }
                        }
                    ],
                    "synonyms": [
                        "paragraphist",
                        "columnist"
                    ],
                    "common_phrases": [
                        "newspaper paragrapher",
                        "witty paragrapher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Писатель или журналист, который пишет короткие заметки, небольшие статьи или абзацы, часто для газет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "автор заметок",
                                "target_lang_sense_clarification": "журналист"
                            },
                            {
                                "target_lang_word": "хроникер",
                                "target_lang_sense_clarification": "репортер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a420bb6-23ec-339c-9eb2-f508fa213bc2",
                    "sense_definition": "A text or document that has a specific number or type of paragraphs.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The assignment was a simple three-<w>paragrapher</w> describing our favorite hobby.",
                            "target_lang_translations": {
                                "ru": "Задание представляло собой простое <w>сочинение из трех абзацев</w> с описанием нашего любимого хобби."
                            }
                        },
                        {
                            "text": "She wrote a quick <w>paragrapher</w> to explain the new office rules.",
                            "target_lang_translations": {
                                "ru": "Она написала небольшую <w>заметку</w>, чтобы объяснить новые правила офиса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "three-paragrapher",
                        "short paragrapher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Текст или документ, состоящий из определенного количества абзацев (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "текст",
                                "target_lang_sense_clarification": "документ"
                            },
                            {
                                "target_lang_word": "сочинение",
                                "target_lang_sense_clarification": "учебное задание"
                            },
                            {
                                "target_lang_word": "заметка",
                                "target_lang_sense_clarification": "короткая запись"
                            }
                        ]
                    }
                }
            ]
        }
    ]

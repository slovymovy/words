                            {
                                "target_lang_word": "могущественный",
                                "target_lang_sense_clarification": "о власти или силе"
                            },
                            {
                                "target_lang_word": "великий",
                                "target_lang_sense_clarification": "о делах или свершениях"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "retain",
        "retainer",
        "retentive",
        "retentiveness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62dc0e64-6f28-3685-a132-47ea9d870a90",
                    "sense_definition": "The action of keeping something or someone rather than losing them, removing them, or letting them go.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "keeping_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is focusing on the <w>retention</w> of its most skilled employees.",
                            "target_lang_translations": {
                                "ru": "Компания сосредоточена на <w>удержании</w> своих самых квалифицированных сотрудников."
                            }
                        },
                        {
                            "text": "Good insulation ensures the <w>retention</w> of heat within the building during winter.",
                            "target_lang_translations": {
                                "ru": "Хорошая изоляция обеспечивает <w>сохранение</w> тепла внутри здания зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "preservation",
                        "maintenance",
                        "keeping",
                        "conservation",
                        "holding"
                    ],
                    "antonyms": [
                        "loss",
                        "release",
                        "disposal",
                        "surrender"
                    ],
                    "common_phrases": [
                        "customer retention",
                        "employee retention",
                        "data retention",
                        "heat retention",
                        "staff retention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по сохранению или удержанию кого-либо или чего-либо, предотвращение потери или ухода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удержание",
                                "target_lang_sense_clarification": "сотрудников, клиентов, денег"
                            },
                            {
                                "target_lang_word": "сохранение",
                                "target_lang_sense_clarification": "тепла, влаги, данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05f94454-9d36-32c1-b336-02f4ef835f99",
                    "sense_definition": "The ability to remember facts, information, or experiences and keep them in the mind.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "keeping_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Visual aids can significantly improve the students' <w>retention</w> of the lesson material.",
                            "target_lang_translations": {
                                "ru": "Наглядные пособия могут значительно улучшить <w>усвоение</w> студентами учебного материала."
                            }
                        },
                        {
                            "text": "His <w>retention</w> of details from the meeting was impressive.",
                            "target_lang_translations": {
                                "ru": "Его <w>запоминание</w> деталей встречи было впечатляющим."
                            }
                        }
                    ],
                    "synonyms": [
                        "memory",
                        "recall",
                        "remembrance"
                    ],
                    "antonyms": [
                        "forgetfulness"
                    ],
                    "common_phrases": [
                        "knowledge retention",
                        "information retention",
                        "memory retention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность запоминать факты, информацию или опыт и удерживать их в памяти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запоминание",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "усвоение",
                                "target_lang_sense_clarification": "материала, информации"
                            },
                            {
                                "target_lang_word": "сохранение в памяти",
                                "target_lang_sense_clarification": "способность помнить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb6e80dd-4ff6-3008-ba5e-c481bffc3ad3",
                    "sense_definition": "A medical condition where the body involuntarily holds back a substance, such as urine or fluid, instead of releasing it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "keeping_medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor treated the patient for acute urinary <w>retention</w>.",
                            "target_lang_translations": {
                                "ru": "Врач лечил пациента от острой <w>задержки</w> мочи."
                            }
                        },
                        {
                            "text": "Consuming too much salt can lead to water <w>retention</w> and swelling.",
                            "target_lang_translations": {
                                "ru": "Употребление слишком большого количества соли может привести к <w>задержке</w> воды и отекам."
                            }
                        }
                    ],
                    "antonyms": [
                        "incontinence"
                    ],
                    "common_phrases": [
                        "fluid retention",
                        "urinary retention",
                        "water retention"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинское состояние, при котором организм непроизвольно задерживает вещества (например, жидкость) вместо их выведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задержка",
                                "target_lang_sense_clarification": "жидкости, мочи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae721368-b07d-310e-9863-25623f7117bd",
                    "sense_definition": "The legal right to keep possession of another person's property until a debt owed by that person is paid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "keeping_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The landlord exercised a right of <w>retention</w> over the goods until the rent was paid.",
                            "target_lang_translations": {
                                "ru": "Арендодатель воспользовался правом <w>удержания</w> товаров, пока не была выплачена арендная плата."
                            }
                        },
                        {
                            "text": "Legal <w>retention</w> allows the mechanic to hold the car until the repair bill is settled.",
                            "target_lang_translations": {
                                "ru": "Законное <w>удержание</w> позволяет механику удерживать автомобиль до оплаты счета за ремонт."
                            }
                        }
                    ],
                    "synonyms": [
                        "lien"
                    ],
                    "common_phrases": [
                        "right of retention"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридическое право удерживать имущество другого лица до тех пор, пока долг этого лица не будет выплачен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удержание",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перенаправлять вывод данных в несколько мест назначения, например, в файл и на экран одновременно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дублировать",

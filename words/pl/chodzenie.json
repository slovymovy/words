                },
                {
                    "sense_id": "1f18bed3-3d40-3b8b-ba14-202817e559e6",
                    "sense_definition": "noszenie na sobie określonego ubrania lub obuwia",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciągłe <w>chodzenie</w> w wysokich obcasach może być męczące dla stóp.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>хождение</w> на высоких каблуках может утомлять ноги."
                            }

{
    "word_family": [
        "wheeler",
        "wheelbarrow",
        "wheelchair",
        "flywheel",
        "freewheel",
        "pinwheel",
        "cogwheel",
        "paddlewheel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4058edca-04a6-3bdd-b340-c3e684632dc7",
                    "sense_definition": "A circular object that rotates on an axle and is used to help vehicles move or machines work.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car needs a new <w>wheel</w> because the old one is damaged.",
                            "target_lang_translations": {
                                "ru": "Машине нужно новое <w>колесо</w>, потому что старое повреждено."
                            }
                        },
                        {
                            "text": "The bus has a large back <w>wheel</w> on each side to support the weight.",
                            "target_lang_translations": {
                                "ru": "У автобуса есть большое заднее <w>колесо</w> с каждой стороны, чтобы выдерживать вес."
                            }
                        }
                    ],
                    "common_phrases": [
                        "front wheel",
                        "back wheel",
                        "cog wheel",
                        "spinning wheel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглый объект, который вращается на оси и используется для движения транспортных средств или работы механизмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колесо",
                                "target_lang_sense_clarification": "часть механизма или транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2ecb231-7992-385d-a242-dd97f3c908cb",
                    "sense_definition": "A circular device used for steering a vehicle or a ship.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat behind the <w>wheel</w> and started the engine.",
                            "target_lang_translations": {
                                "ru": "Он сел за <w>руль</w> и завел двигатель."
                            }
                        },
                        {
                            "text": "Keep both hands on the <w>wheel</w> while you are driving on the highway.",
                            "target_lang_translations": {
                                "ru": "Держи обе руки на <w>руле</w>, пока едешь по шоссе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "behind the wheel",
                        "take the wheel",
                        "steering wheel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглое устройство, используемое для управления транспортным средством или судном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руль",
                                "target_lang_sense_clarification": "автомобиля"
                            },
                            {
                                "target_lang_word": "штурвал",
                                "target_lang_sense_clarification": "судна или самолета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e18fc0d-0dc9-34bb-aa1b-4fc1e5bb9482",
                    "sense_definition": "A large, round block of hard cheese.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "food_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought a whole <w>wheel</w> of cheese for the party.",
                            "target_lang_translations": {
                                "ru": "Мы купили целую <w>головку</w> сыра для вечеринки."
                            }
                        },
                        {
                            "text": "The shop displayed a giant <w>wheel</w> of parmesan in the window.",
                            "target_lang_translations": {
                                "ru": "В витрине магазина была выставлена гигантская <w>головка</w> пармезана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wheel of cheese",
                        "wheel of brie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой круглый кусок твердого сыра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "головка",
                                "target_lang_sense_clarification": "сыра"
                            },
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "сыра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1abc5525-bb5a-3418-babe-c600c8288386",
                    "sense_definition": "A rotating horizontal disk used by a potter to shape clay.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist shaped the wet clay on the potter's <w>wheel</w>.",
                            "target_lang_translations": {
                                "ru": "Художник придавал форму мокрой глине на гончарном <w>круге</w>."
                            }
                        },
                        {
                            "text": "She learned how to use a <w>wheel</w> to make bowls in her art class.",
                            "target_lang_translations": {
                                "ru": "Она училась использовать <w>гончарный круг</w>, чтобы делать чаши на уроке искусства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "potter's wheel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вращающийся горизонтальный диск, используемый гончаром для придания формы глине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гончарный круг",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a28790f-97a8-3e8a-8faa-f079dd26f96a",
                    "sense_definition": "A recurring cycle of events or changes, often thought of as rotating.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_cycle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>wheel</w> of fortune turns for everyone eventually, bringing both good and bad luck.",
                            "target_lang_translations": {
                                "ru": "<w>Колесо</w> фортуны в конце концов поворачивается для каждого, принося и удачу, и неудачу."
                            }
                        },
                        {
                            "text": "We are all bound to the turning <w>wheel</w> of time and fate.",
                            "target_lang_translations": {
                                "ru": "Мы все привязаны к вращающемуся <w>колесу</w> времени и судьбы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wheel of fortune",
                        "wheel of life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторяющийся цикл событий или изменений, часто представляемый как вращение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колесо",
                                "target_lang_sense_clarification": "цикл событий, судьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "610be9d2-8180-377f-a8f5-1e1ac9c3e63e",
                    "sense_definition": "A bicycle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He rode his <w>wheel</w> to the village every morning.",
                            "target_lang_translations": {
                                "ru": "Он ездил на своем <w>велосипеде</w> в деревню каждое утро."
                            }
                        },
                        {
                            "text": "It is faster to go by <w>wheel</w> than to walk all that way.",
                            "target_lang_translations": {
                                "ru": "Быстрее поехать на <w>велосипеде</w>, чем идти пешком весь этот путь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "common in the late 19th and early 20th centuries"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Велосипед."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велосипед",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fd0a79d-810f-31a2-b866-0b8cec62242a",
                    "sense_definition": "A very important or influential person in an organization.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He acts like a big <w>wheel</w> in the company, giving orders to everyone.",
                            "target_lang_translations": {
                                "ru": "Он ведет себя как важная <w>шишка</w> в компании, раздавая всем приказы."
                            }
                        },
                        {
                            "text": "Only a big <w>wheel</w> could make such a major decision for the firm.",
                            "target_lang_translations": {
                                "ru": "Только важная <w>шишка</w> могла принять такое серьезное решение для фирмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "big wheel"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень важный или влиятельный человек в организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шишка",
                                "target_lang_sense_clarification": "важная персона (разг.)"
                            },
                            {
                                "target_lang_word": "воротила",
                                "target_lang_sense_clarification": "влиятельный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68fdf704-b0dd-33f6-a762-714476728433",
                    "sense_definition": "An old instrument of torture used to break the bones of victims.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoner was punished on the breaking <w>wheel</w> for his crimes.",
                            "target_lang_translations": {
                                "ru": "Заключенного наказали на <w>колесе</w> за его преступления."
                            }
                        },
                        {
                            "text": "History books describe the <w>wheel</w> as a cruel device used in the Middle Ages.",
                            "target_lang_translations": {
                                "ru": "Учебники истории описывают <w>колесо</w> как жестокое устройство, использовавшееся в Средние века."
                            }
                        }
                    ],
                    "synonyms": [
                        "breaking wheel"
                    ],
                    "common_phrases": [
                        "breaking wheel"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное орудие пытки, использовавшееся для ломания костей жертв."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колесо",
                                "target_lang_sense_clarification": "орудие пытки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c541bf02-9d19-352f-b839-3e4cc0e5b1db",
                    "sense_definition": "To push or move something or someone that is on wheels.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_move_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>wheel</w> the heavy cart into the kitchen.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>закати</w> тяжелую тележку на кухню."
                            }
                        },
                        {
                            "text": "The nurse <w>wheeled</w> the patient down the hallway to the exam room.",
                            "target_lang_translations": {
                                "ru": "Медсестра <w>повезла</w> пациента по коридору в смотровую."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wheel a cart",
                        "wheel in",
                        "wheel out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Толкать или перемещать что-то или кого-то, находящегося на колесах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "катить",
                                "target_lang_sense_clarification": "перемещать на колесах"
                            },
                            {
                                "target_lang_word": "везти",
                                "target_lang_sense_clarification": "транспортировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58201b3b-5275-3d99-a416-78d1a4c8e64c",
                    "sense_definition": "To turn around quickly or change direction suddenly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_move_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>wheeled</w> around to see who was calling her name.",
                            "target_lang_translations": {
                                "ru": "Она <w>резко обернулась</w>, чтобы увидеть, кто зовет ее по имени."
                            }
                        },
                        {
                            "text": "The soldiers <w>wheeled</w> to the left in perfect unison.",
                            "target_lang_translations": {
                                "ru": "Солдаты <w>развернулись</w> влево в полном унисоне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wheel around",
                        "wheel about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро повернуться или внезапно изменить направление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резко повернуться",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "развернуться",
                                "target_lang_sense_clarification": "сменить направление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de6e7acf-f7bc-33b7-9410-19fe46674630",
                    "sense_definition": "To fly in a wide circle or curve.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_move_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Seagulls <w>wheeled</w> overhead looking for food in the water.",
                            "target_lang_translations": {
                                "ru": "Чайки <w>кружили</w> над головой в поисках еды в воде."
                            }
                        },
                        {
                            "text": "The eagle <w>wheeled</w> high in the sky before diving down.",
                            "target_lang_translations": {
                                "ru": "Орел <w>описывал круги</w> высоко в небе, прежде чем нырнуть вниз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wheel overhead",
                        "wheel in the sky"
                    ],
                    "target_lang_definitions": {
                        "ru": "Летать широким кругом или по дуге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружить",
                                "target_lang_sense_clarification": "в воздухе"
                            },
                            {
                                "target_lang_word": "описывать круги",
                                "target_lang_sense_clarification": "о полете"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6152b415-14c8-3467-a421-53fb3662d351",
                    "sense_definition": "Een vrouwelijke kat, of in het algemeen een huiskat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <poes> ligt heerlijk te slapen in haar mandje.",
                            "target_lang_translations": {
                                "ru": "<w>Кошка</w> сладко спит в своей корзинке."
                            }
                        },
                        {
                            "text": "Onze buren hebben sinds kort een nieuwe <poes> in huis.",
                            "target_lang_translations": {
                                "ru": "У наших соседей недавно появилась новая <w>кошка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kat",
                        "huiskat",
                        "kattin"
                    ],
                    "common_phrases": [
                        "lieve poes",
                        "zachte poes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самка кошки или, в более общем смысле, домашняя кошка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошка",
                                "target_lang_sense_clarification": "домашнее животное; самка"
                            },
                            {
                                "target_lang_word": "киса",
                                "target_lang_sense_clarification": "ласковое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec3c9e14-6b88-36b5-8e8a-1984ae047ecc",
                    "sense_definition": "Het vrouwelijk geslachtsdeel.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij maakte een zeer ongepaste grap over haar <poes>.",
                            "target_lang_translations": {
                                "ru": "Он отпустил очень неуместную шутку про её <w>киску</w>."
                            }
                        },
                        {
                            "text": "In platte taal wordt dat lichaamsdeel soms een <poes> genoemd.",
                            "target_lang_translations": {
                                "ru": "На сленге эту часть тела иногда называют <w>киской</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "natte poes"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женский половой орган."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киска",
                                "target_lang_sense_clarification": "сленг, метафорическое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d58b67f9-7bce-38d9-b847-bb50da1161ed",
                    "sense_definition": "Een aantrekkelijke vrouw of meisje, vaak met een seksuele bijklank.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mannen in het café keken om toen de hete <poes> binnenkwam.",
                            "target_lang_translations": {
                                "ru": "Мужчины в кафе обернулись, когда вошла горячая <w>киса</w>."
                            }
                        },
                        {
                            "text": "Hij doet altijd heel stoer als er een mooie <poes> in de buurt is.",
                            "target_lang_translations": {
                                "ru": "Он всегда строит из себя крутого, когда рядом красивая <w>киса</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hete poes"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекательная женщина или девушка, часто с сексуальным подтекстом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киса",
                                "target_lang_sense_clarification": "о привлекательной девушке"
                            },
                            {
                                "target_lang_word": "цыпочка",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]

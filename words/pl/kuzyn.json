{
    "word_family": [
        "kuzynka",
        "kuzynostwo",
        "kuzynowski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "74369d61-5229-3274-95d5-c4e9ff58d225",
                    "sense_definition": "Syn brata lub siostry jednego z rodziców (syn wujka lub cioci).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>kuzyn</w> mieszka w Warszawie i często nas odwiedza.",
                            "target_lang_translations": {
                                "ru": "Мой <w>двоюродный брат</w> живёт в Варшаве и часто нас навещает."
                            }
                        },
                        {
                            "text": "Jutro przyjeżdża do nas <w>kuzyn</w> ze strony ojca.",
                            "target_lang_translations": {
                                "ru": "Завтра к нам приезжает <w>двоюродный брат</w> со стороны отца."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. pozn. kuzaj"
                    ],
                    "common_phrases": [
                        "mój kuzyn",
                        "starszy kuzyn",
                        "kuzyn ze strony matki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сын брата или сестры одного из родителей (сын дяди или тёти)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двоюродный брат",
                                "target_lang_sense_clarification": "сын дяди или тёти"
                            },
                            {
                                "target_lang_word": "кузен",
                                "target_lang_sense_clarification": "сын дяди или тёти (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4554b5f7-4659-3b1f-9a13-61213f5a0c2b",
                    "sense_definition": "Dalszy krewny; członek rodziny niebędący najbliższym krewnym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "family_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To miał być cichy ślub, ale w końcu zaprosiliśmy też wszystkich naszych <w>kuzynów</w>.",
                            "target_lang_translations": {
                                "ru": "Это должна была быть тихая свадьба, но в итоге мы пригласили всех наших <w>кузенов</w>."
                            }
                        },
                        {
                            "text": "Na zjeździe rodzinnym spotkałem wielu dalekich <w>kuzynów</w>.",
                            "target_lang_translations": {
                                "ru": "На семейной встрече я встретил много дальних <w>родственников</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "daleki kuzyn",
                        "wszyscy kuzyni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дальний родственник; член семьи, не являющийся ближайшим родственником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальний родственник",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "кузен",
                                "target_lang_sense_clarification": "дальний кровный родственник"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "mniszka",
        "mniszy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2760b3f1-ecda-323d-8e98-18106bcfb82a",
                    "sense_definition": "Mężczyzna należący do zakonu religijnego, żyjący we wspólnocie klasztornej i przestrzegający określonej reguły zakonnej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym klasztorze na wzgórzu mieszkał samotny <w>mnich</w>, który pisał ikony.",
                            "target_lang_translations": {
                                "ru": "В старом монастыре на холме жил одинокий <w>монах</w>, который писал иконы."
                            }
                        },
                        {
                            "text": "Każdy <w>mnich</w> w tym zgromadzeniu składa śluby ubóstwa, czystości i posłuszeństwa.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>монах</w> в этой общине дает обеты бедности, целомудрия и послушания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mnich cysterski",
                        "ubogi mnich",
                        "zakonny mnich",
                        "sędziwy mnich"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, принадлежащий к религиозному ордену, живущий в монастырской общине и соблюдающий определенный устав."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монах",
                                "target_lang_sense_clarification": "член религиозной общины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1e9a84f-bb16-39ad-b55b-9bb4689238fd",
                    "sense_definition": "Duchowny lub członek wspólnoty klasztornej w religiach niechrześcijańskich, na przykład w buddyzmie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas podróży do Tajlandii widzieliśmy, jak buddyjski <w>mnich</w> zbierał dary od wiernych.",
                            "target_lang_translations": {
                                "ru": "Во время путешествия в Таиланд мы видели, как буддийский <w>монах</w> собирал подношения от верующих."
                            }
                        },
                        {
                            "text": "Tybetański <w>mnich</w> spędził wiele lat na medytacji w górskiej jaskini.",
                            "target_lang_translations": {
                                "ru": "Тибетский <w>монах</w> провел много лет в медитации в горной пещере."
                            }
                        }
                    ],
                    "synonyms": [
                        "bhiksu",
                        "bhikszu",
                        "śramana"
                    ],
                    "common_phrases": [
                        "mnich buddyjski",
                        "mnich tybetański",
                        "mnich szaolin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Духовное лицо или член монастырской общины в нехристианских религиях, например, в буддизме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монах",
                                "target_lang_sense_clarification": "в буддизме и других религиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b71ac3fc-fad7-3a3a-911d-fb0e1d86ebf8",
                    "sense_definition": "Figura w szachach poruszająca się po przekątnych o dowolną liczbę pól; goniec.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gracz wykonał sprytny ruch i jego <w>mnich</w> zagroził królowej przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Игрок сделал хитрый ход, и его <w>слон</w> угрожал королеве противника."
                            }
                        },
                        {
                            "text": "Na szachownicy pozostał mi tylko jeden <w>mnich</w> i dwa pionki.",
                            "target_lang_translations": {
                                "ru": "На шахматной доске у меня остался только один <w>слон</w> и две пешки."
                            }
                        }
                    ],
                    "synonyms": [
                        "goniec",
                        "laufer"
                    ],
                    "common_phrases": [
                        "czarny mnich",
                        "biały mnich"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard term is 'goniec'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шахматная фигура, ходящая по диагонали на любое количество полей; слон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слон",
                                "target_lang_sense_clarification": "шахматная фигура"
                            },
                            {
                                "target_lang_word": "офицер",
                                "target_lang_sense_clarification": "разговорное название фигуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da59dfce-6765-35c6-afb4-22c1abf57734",
                    "sense_definition": "Wypukła dachówka o kształcie półokrągłego korytka, układana na wklęsłej dachówce zwanej mniszką.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zabytkowy dach kościoła został pokryty tradycyjną dachówką typu <w>mnich</w> i mniszka.",
                            "target_lang_translations": {
                                "ru": "Старинная крыша церкви была покрыта традиционной черепицей типа <w>монах</w> и монашка."
                            }
                        },
                        {
                            "text": "Podczas renowacji musieliśmy wymienić każdy uszkodzony <w>mnich</w> na nowy.",
                            "target_lang_translations": {
                                "ru": "Во время реставрации нам пришлось заменить каждый поврежденный <w>монах</w> на новый."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krycie mnich-mniszka",
                        "dachówka mnich"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical architectural term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпуклая черепица в форме полукруглого желоба, укладываемая на вогнутую черепицу, называемую «монашкой»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монах",
                                "target_lang_sense_clarification": "тип черепицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b7eee48-97c9-3574-9da9-1fa247049fcc",
                    "sense_definition": "Urządzenie hydrotechniczne służące do regulowania poziomu wody w stawach rybnych i jej spuszczania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hydrology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rybacy otworzyli <w>mnicha</w>, aby powoli spuścić wodę ze stawu przed odłowem.",
                            "target_lang_translations": {
                                "ru": "Рыбаки открыли <w>монах</w>, чтобы медленно спустить воду из пруда перед отловом."
                            }
                        },
                        {
                            "text": "Betonowy <w>mnich</w> przy grobli wymagał natychmiastowej naprawy po zimie.",
                            "target_lang_translations": {
                                "ru": "Бетонный <w>монах</w> у плотины требовал немедленного ремонта после зимы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mnich spustowy",
                        "betonowy mnich"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical term in fishery/hydrology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гидротехническое сооружение, служащее для регулирования уровня воды в рыбоводных прудах и ее спуска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монах",
                                "target_lang_sense_clarification": "водоспускное устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "016ae737-d329-3b01-8952-13a636e9ff6c",
                    "sense_definition": "Gatunek foki z rodziny mniszkowatych, występujący w ciepłych wodach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "zoology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzadki <w>mnich</w> śródziemnomorski jest gatunkiem krytycznie zagrożonym wyginięciem.",
                            "target_lang_translations": {
                                "ru": "Редкий средиземноморский <w>тюлень-монах</w> является видом, находящимся под критической угрозой исчезновения."
                            }
                        },
                        {
                            "text": "Na plaży wylegiwał się wielki <w>mnich</w>, nie zwracając uwagi na turystów.",
                            "target_lang_translations": {
                                "ru": "На пляже вылеживался огромный <w>тюлень-монах</w>, не обращая внимания на туристов."
                            }
                        }
                    ],
                    "synonyms": [
                        "mniszka śródziemnomorska"
                    ],
                    "common_phrases": [
                        "mnich śródziemnomorski",
                        "mnich hawajski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид тюленя из семейства настоящих тюленей, обитающий в теплых водах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюлень-монах",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e06f01f8-e464-33d0-9549-da3a213eba43",
                    "sense_definition": "Pospolita roślina polna o żółtych kwiatach i charakterystycznych nasionach z puchem; mniszek.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wiosnę cała łąka była żółta, bo zakwitł na niej <w>mnich</w>.",
                            "target_lang_translations": {
                                "ru": "Весной весь луг был желтым, потому что на нем зацвел <w>одуванчик</w>."
                            }
                        },
                        {
                            "text": "Dzieci zbierały kwiaty, które potocznie nazywano mleczem lub <w>mnichem</w>.",
                            "target_lang_translations": {
                                "ru": "Дети собирали цветы, которые в просторечии называли молочаем или <w>одуванчиком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mniszek"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "or colloquial"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обычное полевое растение с желтыми цветами и характерными семенами с пухом; одуванчик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одуванчик",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3edc9df8-41cf-3293-af21-cb2f4ba4c315",
                    "sense_definition": "Charakterystyczny, spiczasty szczyt górski w polskich Tatrach Wysokich, położony nad Morskim Okiem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geography",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Widok na <w>Mnicha</w> znad tafli jeziora jest jednym z symboli Tatr.",
                            "target_lang_translations": {
                                "ru": "Вид на <w>Мних</w> с поверхности озера является одним из символов Татр."
                            }
                        },
                        {
                            "text": "Wspinaczka na <w>Mnicha</w> jest marzeniem wielu początkujących taterników.",
                            "target_lang_translations": {
                                "ru": "Восхождение на <w>Мних</w> — мечта многих начинающих альпинистов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szczyt Mnich",
                        "ściana Mnicha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерная остроконечная горная вершина в польских Высоких Татрах, расположенная над озером Морское Око."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мних",
                                "target_lang_sense_clarification": "горная вершина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f484bb97-d9ae-344c-aa65-86ed8c9dfb4d",
                    "sense_definition": "Polskie nazwisko.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Mnich</w> pracuje w naszym dziale księgowości od pięciu lat.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Мних</w> работает в нашем отделе бухгалтерии пять лет."
                            }
                        },
                        {
                            "text": "Na liście obecności zauważyłem nazwisko <w>Mnich</w>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих я заметил фамилию <w>Мних</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мних",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
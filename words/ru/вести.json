    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e9308914-2582-3d66-a60b-fb9f3da9a876",
                    "sense_definition": "Направлять движение, указывать дорогу, помогать кому-либо идти или двигаться в определённом направлении.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_guidance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Экскурсовод <вёл> группу туристов по узким улицам старого города.",
                            "target_lang_translations": {
                                "en": "The tour guide <led> the group of tourists through the narrow streets of the old city."
                            }
                        },
                        {
                            "text": "Мама крепко держала сына за руку и <вела> его в детский сад.",
                            "target_lang_translations": {
                                "en": "Mom held her son's hand tightly and <led> him to kindergarten."
                            }
                        }
                    ],
                    "synonyms": [
                        "направлять",
                        "указывать"
                    ],
                    "common_phrases": [
                        "вести за руку",
                        "вести домой",
                        "вести группу"
                    ],
                    "target_lang_definitions": {
                        "en": "To guide movement, show the way, or help someone walk or move in a certain direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "guide physically"
                            },
                            {
                                "target_lang_word": "guide",
                                "target_lang_sense_clarification": "show the way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95ed84fc-60b2-3db7-92fb-65195a1cb827",
                    "sense_definition": "Соблюдать определённые нормы поведения, поступать каким-либо образом (обычно с возвратным местоимением «себя»).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители попросили детей <вести> себя прилично в гостях у бабушки.",
                            "target_lang_translations": {
                                "en": "The parents asked the children to <behave> decently while visiting grandma."
                            }
                        },
                        {
                            "text": "Почему ты так странно <ведёшь> себя сегодня вечером?",
                            "target_lang_translations": {
                                "en": "Why are you <behaving> so strangely tonight?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "вести себя хорошо",
                        "вести себя плохо",
                        "вести себя тихо"
                    ],
                    "target_lang_definitions": {
                        "en": "To observe certain norms of behavior, to act in a specific way (usually with the reflexive pronoun \"oneself\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "behave",
                                "target_lang_sense_clarification": "conduct oneself"
                            },
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "in a specific way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "245350dd-1d93-3fad-ac64-dd436ede22ca",
                    "sense_definition": "Управлять транспортным средством, находясь за рулём.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "driving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она очень боялась <вести> машину в первый раз после получения прав.",
                            "target_lang_translations": {
                                "en": "She was very afraid to <drive> the car for the first time after getting her license."
                            }
                        },
                        {
                            "text": "Мой отец любит <вести> автомобиль медленно и аккуратно.",
                            "target_lang_translations": {
                                "en": "My father likes to <drive> the car slowly and carefully."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вести машину",
                        "вести автомобиль",
                        "вести автобус"
                    ],
                    "target_lang_definitions": {
                        "en": "To control a vehicle while being at the wheel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "a vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6767e05f-b03c-37db-9624-bdd20142d4c1",
                    "sense_definition": "Иметь определённое направление, простираться в пространстве до какого-либо места (о дороге, тропинке).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта извилистая лесная тропинка <ведёт> прямо к нашему загородному дому.",
                            "target_lang_translations": {
                                "en": "This winding forest path <leads> straight to our country house."
                            }
                        },
                        {
                            "text": "Широкая асфальтированная дорога <ведёт> к главному входу в парк.",
                            "target_lang_translations": {
                                "en": "A wide paved road <leads> to the main entrance of the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "приводить"
                    ],
                    "common_phrases": [
                        "дорога ведёт",
                        "тропинка ведёт",
                        "дверь ведёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a certain direction, to extend in space to a specific place (regarding a road or path)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "direction/destination"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "extend to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bceb49d-19f3-35c9-ba0b-0fa3e1f4f1f4",
                    "sense_definition": "Руководить чем-либо, управлять ходом дела, осуществлять какую-либо деятельность или процесс.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор фирмы лично <ведёт> все важные переговоры с иностранными партнёрами.",
                            "target_lang_translations": {
                                "en": "The company director personally <conducts> all important negotiations with foreign partners."
                            }
                        },
                        {
                            "text": "Учитель продолжает <вести> урок, несмотря на шум в коридоре.",
                            "target_lang_translations": {
                                "en": "The teacher continues to <conduct> the lesson despite the noise in the corridor."
                            }
                        }
                    ],
                    "synonyms": [
                        "руководить",
                        "проводить"
                    ],
                    "common_phrases": [
                        "вести переговоры",
                        "вести собрание",
                        "вести урок",
                        "вести войну"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct something, manage the course of affairs, or carry out an activity or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conduct",
                                "target_lang_sense_clarification": "negotiations, lessons"
                            },
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "manage or direct"
                            },
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "control affairs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dba3f51f-e508-301e-8bb8-2718cac065a3",
                    "sense_definition": "Иметь что-либо своим результатом, вызывать какие-либо последствия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Постоянные споры и конфликты в семье ни к чему хорошему не <ведут>.",
                            "target_lang_translations": {
                                "en": "Constant arguments and conflicts in the family <lead> to nothing good."
                            }
                        },
                        {
                            "text": "Упорный труд и дисциплина всегда <ведут> к достижению поставленных целей.",
                            "target_lang_translations": {
                                "en": "Hard work and discipline always <lead> to the achievement of set goals."
                            }
                        }
                    ],
                    "synonyms": [
                        "приводить"
                    ],
                    "common_phrases": [
                        "вести к успеху",
                        "вести к победе",
                        "вести к ошибкам"
                    ],
                    "target_lang_definitions": {
                        "en": "To have something as a result, to cause consequences."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "result in"
                            },
                            {
                                "target_lang_word": "result in",
                                "target_lang_sense_clarification": "cause consequences"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fec2a6bc-de95-3cd9-977a-06788e868ff1",
                    "sense_definition": "Иметь преимущество в счёте во время спортивного состязания или игры.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша сборная уверенно <вела> в счёте на протяжении всего первого тайма.",
                            "target_lang_translations": {
                                "en": "Our national team was confidently <leading> in the score throughout the entire first half."
                            }
                        },
                        {
                            "text": "К концу матча команда гостей <вела> с разницей в два мяча.",
                            "target_lang_translations": {
                                "en": "By the end of the match, the visiting team was <leading> by a margin of two goals."
                            }
                        }
                    ],
                    "synonyms": [
                        "иметь преимущество"
                    ],
                    "antonyms": [
                        "проигрывать"
                    ],
                    "common_phrases": [
                        "вести в счёте",
                        "команда ведёт",
                        "вести игру"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "в контексте спорта"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To have the advantage in the score during a sports competition or game."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "be ahead in score"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aab2ceec-d81b-3c72-83f3-f2cc2fe2605e",
                    "sense_definition": "Двигать предметом по поверхности чего-либо или прокладывать линию.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Музыкант плавно <вёл> смычком по струнам своей старинной скрипки.",
                            "target_lang_translations": {
                                "en": "The musician smoothly <drew> the bow across the strings of his antique violin."
                            }
                        },
                        {
                            "text": "Архитектор начал <вести> прямую линию на чертеже простым карандашом.",
                            "target_lang_translations": {
                                "en": "The architect began to <draw> a straight line on the blueprint with a simple pencil."
                            }
                        }
                    ],
                    "synonyms": [
                        "двигать",
                        "проводить"
                    ],
                    "common_phrases": [
                        "вести пальцем",
                        "вести смычком",
                        "вести линию"
                    ],
                    "target_lang_definitions": {
                        "en": "To move an object along a surface or to trace a line."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "a line or bow"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "an object over a surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e45c6ce2-af7b-3d37-b969-d28d2ab92d16",
                    "sense_definition": "Тайком следовать за кем-либо, наблюдая за его действиями и передвижениями.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surveillance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Частный детектив <вёл> подозреваемого от самого офиса до его дома.",
                            "target_lang_translations": {
                                "en": "The private detective <tailed> the suspect from the office to his home."
                            }
                        },
                        {
                            "text": "Опытный охотник долго <вёл> зверя по свежим следам на снегу.",
                            "target_lang_translations": {
                                "en": "The experienced hunter <tracked> the beast for a long time by fresh tracks in the snow."
                            }
                        }
                    ],
                    "synonyms": [
                        "следить"
                    ],
                    "common_phrases": [
                        "вести наблюдение",
                        "вести цель"
                    ],
                    "target_lang_definitions": {
                        "en": "To secretly follow someone, observing their actions and movements."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "track",
                                "target_lang_sense_clarification": "follow traces"
                            },
                            {
                                "target_lang_word": "tail",
                                "target_lang_sense_clarification": "follow secretly"
                            },
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "observe movement"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c1e452a1-d086-34b4-b683-6833a586b6b7",
                    "sense_definition": "Форма множественного числа существительного <w>весть</w> (новости, сообщения).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "news",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С фронта пришли тревожные <вести>, которые всех очень расстроили.",
                            "target_lang_translations": {
                                "en": "Disturbing <news> came from the front, which upset everyone very much."
                            }
                        },
                        {
                            "text": "Мы уже давно не получали от него никакие <вести>.",
                            "target_lang_translations": {
                                "en": "We haven't received any <news> from him for a long time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "благие вести",
                        "ждать вестей",
                        "дурные вести"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural form of the noun \"весть\" (news, messages)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "news",
                                "target_lang_sense_clarification": "information"
                            },
                            {
                                "target_lang_word": "tidings",
                                "target_lang_sense_clarification": "literary/dated"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "95181fdc-26f4-392f-a28a-3b57e3c02788",
                    "sense_definition": "Временная остановка или перерыв в речи, каком-либо действии или процессе.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pause_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Оратор сделал небольшую <паузу>, чтобы выпить воды.",
                            "target_lang_translations": {
                                "en": "The speaker made a short <pause> to drink some water."
                            }
                        },
                        {
                            "text": "В нашем разговоре возникла неловкая <пауза>.",
                            "target_lang_translations": {
                                "en": "An awkward <pause> arose in our conversation."
                            }
                        }
                    ],
                    "synonyms": [
                        "перерыв"
                    ],
                    "antonyms": [
                        "продолжение"
                    ],
                    "common_phrases": [
                        "сделать паузу",
                        "небольшая пауза",
                        "пауза в разговоре",
                        "театральная пауза"
                    ],
                    "target_lang_definitions": {
                        "en": "A temporary stop or break in speech, an action, or a process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pause",
                                "target_lang_sense_clarification": "temporary stop"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "interruption"
                            }
                        ]
                    }
                },
                {

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "fad005ce-a990-33cf-b99c-a2784c94feb4",
                    "sense_definition": "Een uitroep om een plotselinge emotie uit te drukken, zoals verbazing, schrik, teleurstelling, pijn of begrip.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>O</w>, dat wist ik helemaal niet!",
                            "target_lang_translations": {
                                "en": "<w>Oh</w>, I didn't know that at all!",
                                "ru": "<w>О</w>, я этого совсем не знал!"
                            }
                        },
                        {
                            "text": "<w>O</w>, wat heb ik me vergist in de tijd.",
                            "target_lang_translations": {
                                "en": "<w>Oh</w>, how wrong I was about the time.",
                                "ru": "<w>О</w>, как я ошибся со временем."
                            }
                        }
                    ],
                    "synonyms": [
                        "oh",
                        "ach",
                        "hé"
                    ],
                    "common_phrases": [
                        "o ja",
                        "o nee",
                        "o god"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used to express a sudden emotion, such as surprise, shock, disappointment, pain, or understanding.",
                        "ru": "Междометие для выражения внезапной эмоции, такой как удивление, испуг, разочарование, боль или понимание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "interjection expressing emotion"
                            },
                            {
                                "target_lang_word": "ah",
                                "target_lang_sense_clarification": "interjection of understanding or realization"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "о",
                                "target_lang_sense_clarification": "междометие, выражающее эмоцию"
                            },
                            {
                                "target_lang_word": "ой",
                                "target_lang_sense_clarification": "междометие испуга или удивления"
                            },
                            {
                                "target_lang_word": "ах",
                                "target_lang_sense_clarification": "междометие сильного чувства"
                            }
                        ]

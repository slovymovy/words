                },
                {
                    "sense_id": "1c021b04-773b-39a5-a33f-ca334ed8bf8f",
                    "sense_definition": "Говорить что-то глупое, бессмысленное или ложное",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speech",
                    "name_type": "no",
                    "examples": [

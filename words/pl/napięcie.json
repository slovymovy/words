            "senses": [
                {
                    "sense_id": "7f15b490-e96c-32b5-ac0b-57cf6ce6d4f4",
                    "sense_definition": "Różnica potencjałów elektrycznych między dwoma punktami obwodu elektrycznego lub pola elektrycznego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W domowym gniazdku elektrycznym zazwyczaj występuje <w>napięcie</w> 230 woltów.",
                            "target_lang_translations": {
                                "ru": "В бытовой электрической розетке обычно присутствует <w>напряжение</w> 230 вольт."
                            }
                        },
                        {
                            "text": "Tabliczka ostrzegawcza informuje o wysokim <w>napięciu</w> w tej strefie.",
                            "target_lang_translations": {
                                "ru": "Предупреждающая табличка сообщает о высоком <w>напряжении</w> в этой зоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "woltaż"
                    ],
                    "common_phrases": [
                        "wysokie napięcie",
                        "napięcie elektryczne",
                        "spadek napięcia",
                        "niskie napięcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разность электрических потенциалов между двумя точками электрической цепи или электрического поля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напряжение",
                                "target_lang_sense_clarification": "электрическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33faaac5-e45f-3863-affd-66c1594bbb1e",
                    "sense_definition": "Stan psychiczny charakteryzujący się silnym niepokojem, zdenerwowaniem lub stresem w oczekiwaniu na coś.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "psychology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed ważnym egzaminem czuł ogromne <w>napięcie</w> i nie mógł spać.",
                            "target_lang_translations": {
                                "ru": "Перед важным экзаменом он чувствовал огромное <w>напряжение</w> и не мог спать."
                            }
                        },
                        {
                            "text": "Długi spacer pomógł jej rozładować <w>napięcie</w> po ciężkim dniu w pracy.",
                            "target_lang_translations": {
                                "ru": "Длительная прогулка помогла ей снять <w>напряжение</w> после тяжелого рабочего дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "stres",
                        "niepokój",
                        "zdenerwowanie",
                        "obawa",
                        "strach",
                        "emocje"
                    ],
                    "antonyms": [

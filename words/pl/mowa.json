                },
                {
                    "sense_id": "2783049f-d431-3e8b-b09e-1790d51bc35b",
                    "sense_definition": "Sposób relacjonowania czyichś słów w gramatyce.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel wytłumaczył nam, jak zamienić <w>mowę</w> niezależną na zależną.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил нам, как перевести прямую <w>речь</w> в косвенную."
                            }
                        },
                        {
                            "text": "W artykule użyto <w>mowy</w> zależnej do przytoczenia wypowiedzi świadków.",
                            "target_lang_translations": {
                                "ru": "В статье использовали косвенную <w>речь</w> для цитирования показаний свидетелей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mowa zależna",
                        "mowa niezależna"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "grammatical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ передачи чьих-либо слов в грамматике."

                            {
                                "target_lang_word": "опросный лист",
                                "target_lang_sense_clarification": "список вопросов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56a9bc53-b3af-307a-a0da-32976cf169b7",
                    "sense_definition": "Metoda badawcza polegająca na zbieraniu opinii lub danych od grupy osób za pomocą pytań.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "badanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza firma przeprowadziła <w>ankietę</w> dotyczącą zadowolenia klientów.",
                            "target_lang_translations": {
                                "ru": "Наша компания провела <w>опрос</w> касательно удовлетворённости клиентов."
                            }
                        },
                        {
                            "text": "Wyniki ostatniej <w>ankiety</w> wskazują na wzrost zainteresowania ekologią.",
                            "target_lang_translations": {
                                "ru": "Результаты последнего <w>опроса</w> указывают на рост интереса к экологии."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8d52d952-dfdb-3ca5-95b4-ad9dd7263254",
                    "sense_definition": "Een stuk gereedschap met een breed blad aan een steel, gebruikt om grond te verplaatsen of te graven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tuinman pakte een <w>schop</w> uit de schuur om een gat te graven.",
                            "target_lang_translations": {
                                "en": "The gardener took a <w>shovel</w> from the shed to dig a hole.",
                                "ru": "Садовник взял <w>лопату</w> из сарая, чтобы выкопать яму."
                            }
                        },
                        {
                            "text": "Het veld met de <w>schop</w> omspitten is een heel zwaar karwei.",
                            "target_lang_translations": {
                                "en": "Digging over the field with a <w>spade</w> is a very heavy job.",
                                "ru": "Вскапывать поле <w>лопатой</w> — очень тяжелая работа."
                            }
                        }
                    ],
                    "synonyms": [
                        "schep",
                        "spade",
                        "spa"
                    ],
                    "common_phrases": [
                        "op de schop"
                    ],
                    "target_lang_definitions": {
                        "en": "A tool with a broad blade on a handle, used for moving soil or digging.",
                        "ru": "Инструмент с широким лезвием на рукоятке, используемый для перемещения грунта или копания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shovel",
                                "target_lang_sense_clarification": "tool for moving loose material"
                            },
                            {
                                "target_lang_word": "spade",
                                "target_lang_sense_clarification": "tool for digging"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лопата",
                                "target_lang_sense_clarification": "инструмент для копания"
                            },
                            {
                                "target_lang_word": "заступ",
                                "target_lang_sense_clarification": "разновидность лопаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a1135c4-53f6-3c6e-b390-5be58879ba83",
                    "sense_definition": "Een harde stoot of slag die met de voet wordt gegeven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boze speler gaf uit frustratie een harde <w>schop</w> tegen de muur.",
                            "target_lang_translations": {
                                "en": "The angry player gave the wall a hard <w>kick</w> out of frustration.",
                                "ru": "Рассерженный игрок от разочарования со всей силы <w>пнул</w> стену."
                            }
                        },
                        {
                            "text": "Ik heb hem daarop een grote <w>schop</w> verkocht.",
                            "target_lang_translations": {
                                "en": "I gave him a big <w>kick</w> after that.",
                                "ru": "Я отвесил ему за это смачный <w>пинок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trap"
                    ],
                    "common_phrases": [
                        "een schop onder de kont"
                    ],
                    "target_lang_definitions": {
                        "en": "A hard thrust or blow given with the foot.",
                        "ru": "Сильный удар или толчок, наносимый ногой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kick",
                                "target_lang_sense_clarification": "blow with the foot"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пинок",
                                "target_lang_sense_clarification": "удар ногой"
                            },
                            {
                                "target_lang_word": "удар ногой",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5538490b-81f6-3a61-9804-ab0148015e8d",
                    "sense_definition": "Een van de vier kleuren in het kaartspel, gekenmerkt door een zwart symbool in de vorm van een omgekeerd hart met een steel (♠).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "game_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je <w>schoppen</w> aas hebt, maak je een goede kans om de slag te winnen.",
                            "target_lang_translations": {
                                "en": "If you have the ace of <w>spades</w>, you have a good chance of winning the trick.",
                                "ru": "Если у тебя есть туз <w>пик</w>, у тебя хорошие шансы выиграть взятку."
                            }
                        },
                        {
                            "text": "Ik kon gelukkig op die ingetroefde slag mijn vuile <w>schopje</w> kwijt.",
                            "target_lang_translations": {
                                "en": "Luckily I could get rid of my little <w>spade</w> on that trumped trick.",
                                "ru": "К счастью, я смог сбросить свою мелкую <w>пику</w> на эту убитую козырем взятку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schoppen aas",
                        "schoppen heer"
                    ],
                    "target_lang_definitions": {
                        "en": "One of the four suits in a deck of cards, marked by a black symbol (♠).",
                        "ru": "Одна из четырех мастей в игральных картах, обозначаемая черным символом в форме перевернутого сердца с ножкой (♠)."
                    },
                    "translations": {

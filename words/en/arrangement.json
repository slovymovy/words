{
    "word_family": [
        "arrange",
        "arranger",
        "rearrange",
        "rearrangement"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3b24e424-34b3-3358-8879-e2ea34e89566",
                    "sense_definition": "Preparations or plans made for a future event or undertaking.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to make <w>arrangements</w> for the upcoming conference.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сделать <w>приготовления</w> к предстоящей конференции."
                            }
                        },
                        {
                            "text": "The travel <w>arrangements</w> include flights and hotel bookings.",
                            "target_lang_translations": {
                                "ru": "<w>Организация</w> поездки включает перелеты и бронирование отелей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "make arrangements",
                        "travel arrangements",
                        "funeral arrangements",
                        "living arrangements"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготовка или планы, сделанные для будущего события или мероприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приготовле́ния",
                                "target_lang_sense_clarification": "подготовка к чему-либо"
                            },
                            {
                                "target_lang_word": "организа́ция",
                                "target_lang_sense_clarification": "процесс устройства или планирования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9bcdf63-ff3e-3b9d-8fc3-d27044c16305",
                    "sense_definition": "An agreement or settlement reached between two or more parties.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They came to an <w>arrangement</w> regarding the payment schedule.",
                            "target_lang_translations": {
                                "ru": "Они пришли к <w>соглашению</w> касательно графика платежей."
                            }
                        },
                        {
                            "text": "We have a special <w>arrangement</w> with the supplier for bulk orders.",
                            "target_lang_translations": {
                                "ru": "У нас есть особая <w>договорённость</w> с поставщиком для оптовых заказов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "come to an arrangement",
                        "special arrangement",
                        "mutual arrangement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашение или договоренность, достигнутая между двумя или более сторонами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "договорённость",
                                "target_lang_sense_clarification": "взаимное согласие"
                            },
                            {
                                "target_lang_word": "соглаше́ние",
                                "target_lang_sense_clarification": "официальный договор или согласие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d454795e-29f1-33fb-a819-06918babf09e",
                    "sense_definition": "The particular way in which items are organized, positioned, or structured.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>arrangement</w> of furniture in the living room makes it feel spacious.",
                            "target_lang_translations": {
                                "ru": "<w>Расстановка</w> мебели в гостиной делает её просторной."
                            }
                        },
                        {
                            "text": "Most cameras mimic the <w>arrangement</w> of the mammalian eye.",
                            "target_lang_translations": {
                                "ru": "Большинство камер имитируют <w>устройство</w> глаза млекопитающих."
                            }
                        }
                    ],
                    "synonyms": [
                        "configuration",
                        "composition"
                    ],
                    "common_phrases": [
                        "seating arrangement",
                        "spatial arrangement",
                        "alphabetical arrangement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ, которым предметы организованы, расположены или структурированы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстано́вка",
                                "target_lang_sense_clarification": "порядок размещения предметов"
                            },
                            {
                                "target_lang_word": "расположе́ние",
                                "target_lang_sense_clarification": "местонахождение или позиция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45249820-bfd7-3041-b6fc-df3d8540427a",
                    "sense_definition": "A creative collection of objects, especially flowers, put together for display.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She received a beautiful floral <w>arrangement</w> for her birthday.",
                            "target_lang_translations": {
                                "ru": "Она получила красивую цветочную <w>композицию</w> на свой день рождения."
                            }
                        },
                        {
                            "text": "The centerpiece was a stunning <w>arrangement</w> of dried leaves and berries.",
                            "target_lang_translations": {
                                "ru": "Центральным украшением была потрясающая <w>композиция</w> из сухих листьев и ягод."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flower arrangement",
                        "floral arrangement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Художественная коллекция предметов, особенно цветов, собранных вместе для демонстрации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компози́ция",
                                "target_lang_sense_clarification": "сочетание элементов, например, цветов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c915eb31-a0a1-3bda-9c61-ca3f6677ddc5",
                    "sense_definition": "A piece of music that has been adapted for a different instrument or style than the original.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The orchestra played a new <w>arrangement</w> of a popular folk song.",
                            "target_lang_translations": {
                                "ru": "Оркестр сыграл новую <w>аранжировку</w> популярной народной песни."
                            }
                        },
                        {
                            "text": "He is working on a jazz <w>arrangement</w> for the school band.",
                            "target_lang_translations": {
                                "ru": "Он работает над джазовой <w>аранжировкой</w> для школьного ансамбля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "musical arrangement",
                        "orchestral arrangement",
                        "string arrangement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное произведение, адаптированное для другого инструмента или стиля, отличного от оригинала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аранжиро́вка",
                                "target_lang_sense_clarification": "музыкальная обработка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
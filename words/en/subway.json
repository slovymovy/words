            "senses": [
                {
                    "sense_id": "23cc163c-cf37-3810-9e85-c7401b059a8d",
                    "sense_definition": "An underground railway system used for mass transit in urban areas.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He takes the <w>subway</w> to work every morning to avoid traffic.",
                            "target_lang_translations": {
                                "ru": "Он ездит на <w>метро</w> на работу каждое утро, чтобы избежать пробок."
                            }
                        },
                        {
                            "text": "The new <w>subway</w> station is located right next to the museum.",
                            "target_lang_translations": {
                                "ru": "Новая станция <w>метро</w> находится прямо рядом с музеем."
                            }
                        }
                    ],
                    "synonyms": [
                        "metro",
                        "underground",
                        "tube",
                        "rapid transit"
                    ],
                    "common_phrases": [
                        "take the subway",
                        "subway station",
                        "subway line",
                        "on the subway",
                        "subway map"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Система подземного железнодорожного транспорта для массовых перевозок в городских районах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метро́",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8b233bd9-68b1-334e-9429-5ea19993c43d",
                    "sense_definition": "To show that something is true or correct by providing evidence or arguments.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer presented new evidence to <w>prove</w> the innocence of his client.",
                            "target_lang_translations": {
                                "ru": "Адвокат представил новые улики, чтобы <w>доказать</w> невиновность своего клиента."
                            }
                        },
                        {
                            "text": "You must <w>prove</w> your identity before you can enter the secure building.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>подтвердить</w> свою личность, прежде чем сможете войти в охраняемое здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "demonstrate",
                        "verify",
                        "confirm",
                        "substantiate",
                        "validate"
                    ],
                    "antonyms": [
                        "disprove",
                        "refute"
                    ],
                    "common_phrases": [
                        "prove a point",
                        "prove someone wrong",
                        "prove yourself",
                        "prove it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показать, что что-то является истинным или правильным, предоставив доказательства или аргументы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доказывать",
                                "target_lang_sense_clarification": "приводить доводы (несов. вид)"
                            },
                            {
                                "target_lang_word": "доказать",
                                "target_lang_sense_clarification": "подтвердить истинность (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08e0eb91-cc5c-34c0-b3ba-57a0c8b6b5ed",
                    "sense_definition": "To be found to be something or to have a particular quality after a period of time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "turn_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new marketing strategy <w>proved</w> to be very effective for the company.",
                            "target_lang_translations": {
                                "ru": "Новая маркетинговая стратегия <w>оказалась</w> очень эффективной для компании."
                            }
                        },
                        {
                            "text": "His advice <w>proved</w> useful when we encountered problems with the project.",
                            "target_lang_translations": {
                                "ru": "Его совет <w>оказался</w> полезным, когда мы столкнулись с проблемами в проекте."
                            }
                        }
                    ],
                    "synonyms": [
                        "turn out",
                        "end up"
                    ],
                    "common_phrases": [
                        "prove successful",
                        "prove useful",
                        "prove fatal",
                        "prove to be"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаруживать определенные качества или свойства спустя какое-то время; оказываться кем-то или чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказываться",
                                "target_lang_sense_clarification": "выявляться (несов. вид)"
                            },
                            {
                                "target_lang_word": "оказаться",
                                "target_lang_sense_clarification": "стать явным (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1255d2a8-198d-3638-9df9-39c8066ac33e",
                    "sense_definition": "To allow dough to rise due to the fermentation of yeast before baking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "baking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should cover the bowl and let the dough <w>prove</w> in a warm place.",
                            "target_lang_translations": {
                                "ru": "Вам следует накрыть миску и дать тесту <w>расстояться</w> в теплом месте."
                            }
                        },
                        {
                            "text": "If you let the bread <w>prove</w> for too long, it might collapse in the oven.",
                            "target_lang_translations": {
                                "ru": "Если дать хлебу <w>расстаиваться</w> слишком долго, он может опасть в духовке."
                            }
                        }
                    ],
                    "synonyms": [
                        "rise",
                        "proof"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in British English; US English often uses 'proof'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать тесту подняться перед выпечкой в результате брожения дрожжей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстаиваться",
                                "target_lang_sense_clarification": "о тесте"
                            },
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "увеличиваться в объеме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1d4d024-1072-3fe1-83c9-098f94fa44c1",
                    "sense_definition": "To test the quality or performance of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "test",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The harsh winter conditions will <w>prove</w> the durability of the new shelter.",
                            "target_lang_translations": {
                                "ru": "Суровые зимние условия <w>проверят</w> прочность нового укрытия."
                            }
                        },
                        {
                            "text": "This old saying suggests that the exception <w>proves</w> the rule.",
                            "target_lang_translations": {
                                "ru": "Эта старая поговорка гласит, что исключение <w>подтверждает</w> правило."
                            }
                        }
                    ],
                    "synonyms": [
                        "test",
                        "trial"
                    ],
                    "common_phrases": [
                        "exception proves the rule",
                        "proving ground"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly survives in specific idioms or technical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять качество или эффективность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испытывать",
                                "target_lang_sense_clarification": "проверять на прочность"
                            },
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "тестировать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bf4e0fe-233e-3bb7-aeb4-54a1f6a7cced",
                    "sense_definition": "The process or period of time in which dough is allowed to rise.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "baking_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bread needs a second <w>prove</w> to develop a lighter texture.",
                            "target_lang_translations": {
                                "ru": "Хлебу нужна вторая <w>расстойка</w>, чтобы текстура стала более воздушной."
                            }
                        },
                        {
                            "text": "Check the dough after the first <w>prove</w> to see if it has doubled in size.",
                            "target_lang_translations": {
                                "ru": "Проверьте тесто после первой <w>расстойки</w>, чтобы увидеть, увеличилось ли оно вдвое."
                            }
                        }
                    ],
                    "synonyms": [
                        "proofing",
                        "rise"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in British English; US English often uses 'proofing'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс или период времени, в течение которого тесту дают подняться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстойка",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                }
            ]
        }

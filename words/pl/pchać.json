    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c9ff02bc-9fac-3933-b307-ff67a7d9453e",
                    "sense_definition": "Przesuwać kogoś lub coś przed sobą, używając siły fizycznej",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy <pchać> zepsuty samochód przez dwa kilometry do najbliższego warsztatu.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>толкать</w> сломанную машину два километра до ближайшей мастерской."
                            }
                        },
                        {
                            "text": "Nie musisz <pchać> tych drzwi, ponieważ otwierają się one automatycznie.",
                            "target_lang_translations": {
                                "ru": "Тебе не нужно <w>толкать</w> эту дверь, так как она открывается автоматически."
                            }
                        }
                    ],
                    "synonyms": [
                        "popychać",
                        "przesuwać"
                    ],
                    "antonyms": [
                        "ciągnąć"
                    ],
                    "common_phrases": [
                        "pchać wózek",
                        "pchać samochód",
                        "pchać drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать кого-то или что-то перед собой, применяя физическую силу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толкать",
                                "target_lang_sense_clarification": "двигать от себя"
                            },
                            {
                                "target_lang_word": "двигать",
                                "target_lang_sense_clarification": "перемещать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd2db2a8-4c77-3e2e-9508-86b8799ffe06",
                    "sense_definition": "Tłoczyć się i napierać na innych ludzi, próbując przejść lub dostać się gdzieś (używane z „się”)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludzie zaczęli się <pchać> do wyjścia, gdy tylko ogłoszono koniec koncertu.",
                            "target_lang_translations": {
                                "ru": "Люди начали <w>толкаться</w> к выходу, как только объявили об окончании концерта."
                            }
                        },
                        {
                            "text": "Nie lubię jeździć autobusem w godzinach szczytu, bo wszyscy pasażerowie strasznie się <pchają>.",
                            "target_lang_translations": {
                                "ru": "Я не люблю ездить на автобусе в час пик, потому что все пассажиры ужасно <w>толкаются</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cisnąć się",
                        "przepychać się"
                    ],
                    "common_phrases": [
                        "pchać się do kolejki",
                        "pchać się drzwiami i oknami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тесниться и давить на других людей, пытаясь пройти или попасть куда-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толкаться",
                                "target_lang_sense_clarification": "в толпе"
                            },
                            {
                                "target_lang_word": "лезть",
                                "target_lang_sense_clarification": "пробираться без очереди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5afb82f-98b5-3c22-a09b-443dcb312d49",
                    "sense_definition": "Wkładać coś do środka czegoś w sposób niedbały, w pośpiechu lub na siłę",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "placing_inside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego próbujesz <pchać> te wszystkie brudne ubrania do jednej małej pralki?",
                            "target_lang_translations": {
                                "ru": "Зачем ты пытаешься <w>пихать</w> всю эту грязную одежду в одну маленькую стиральную машину?"
                            }
                        },
                        {
                            "text": "Nie powinieneś <pchać> rąk do kieszeni, kiedy rozmawiasz z nauczycielem.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>совать</w> руки в карманы, когда разговариваешь с учителем."
                            }
                        }
                    ],
                    "synonyms": [
                        "wpychać",
                        "wtykać",
                        "wkładać"
                    ],
                    "antonyms": [
                        "ciągnąć"
                    ],
                    "common_phrases": [
                        "pchać ręce do kieszeni",
                        "pchać wszystko do worka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Класть что-то внутрь чего-то небрежно, в спешке или с силой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пихать",
                                "target_lang_sense_clarification": "засовывать силой или небрежно"
                            },
                            {
                                "target_lang_word": "совать",
                                "target_lang_sense_clarification": "класть поспешно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a8393e0-0709-3ec1-9b95-6babd46674ad",
                    "sense_definition": "Wywierać wpływ na kogoś, aby podjął określone działanie, często ryzykowne lub negatywne",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bieda i brak perspektyw często <pchają> młodych ludzi do przestępczości.",
                            "target_lang_translations": {
                                "ru": "Бедность и отсутствие перспектив часто <w>толкают</w> молодых людей на преступления."
                            }
                        },
                        {
                            "text": "To niezdrowa ambicja <pcha> go do podejmowania tak ryzykownych decyzji finansowych.",
                            "target_lang_translations": {
                                "ru": "Это нездоровая амбиция <w>толкает</w> его на принятие таких рискованных финансовых решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "popychać",
                        "dźgać"
                    ],
                    "antonyms": [
                        "powstrzymywać"
                    ],
                    "common_phrases": [
                        "pchać do złego",
                        "pchać w ramiona"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать влияние на кого-то, побуждая к определенным действиям, часто рискованным или негативным"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толкать",
                                "target_lang_sense_clarification": "побуждать к действию"
                            },
                            {
                                "target_lang_word": "подталкивать",
                                "target_lang_sense_clarification": "склонять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d737ba5-f751-3b2d-926a-ed8069a0ba3b",
                    "sense_definition": "Przeznaczać duże środki finansowe na jakiś cel, często bez gwarancji zysku",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie ma sensu dalej <pchać> pieniędzy w ten stary, rozpadający się dom.",
                            "target_lang_translations": {
                                "ru": "Нет смысла дальше <w>вкладывать</w> деньги в этот старый, разваливающийся дом."
                            }
                        },
                        {
                            "text": "Inwestorzy przestali <pchać> kapitał w ryzykowne projekty, które nie przynoszą zysków.",
                            "target_lang_translations": {
                                "ru": "Инвесторы перестали <w>вкладывать</w> капитал в рискованные проекты, которые не приносят прибыли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pchać pieniądze w błoto",
                        "pchać kasę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used regarding money"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тратить большие финансовые средства на какую-то цель, часто без гарантии прибыли"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкладывать",
                                "target_lang_sense_clarification": "инвестировать"
                            },
                            {
                                "target_lang_word": "вбухивать",
                                "target_lang_sense_clarification": "разг. тратить много денег"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a96f882e-8416-3ebd-b8e3-7814c715df0c",
                    "sense_definition": "Zadawać cios ostrym narzędziem, wykonując szybki ruch ręką",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik próbował <pchać> ofiarę nożem, ale na szczęście został szybko obezwładniony.",
                            "target_lang_translations": {
                                "ru": "Нападавший пытался <w>ударить</w> жертву ножом, но, к счастью, был быстро обезврежен."
                            }
                        },
                        {
                            "text": "W dawnych technikach walki uczono żołnierzy, jak skutecznie <pchać> włócznią w stronę wroga.",
                            "target_lang_translations": {
                                "ru": "В древних техниках боя солдат учили, как эффективно <w>колоть</w> копьем в сторону врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "dźgać",
                        "kłuć"
                    ],
                    "common_phrases": [
                        "pchać nożem",
                        "pchać bagnetem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить удар острым предметом, делая быстрое движение рукой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колоть",
                                "target_lang_sense_clarification": "наносить удары колющим оружием"
                            },
                            {
                                "target_lang_word": "ударять",
                                "target_lang_sense_clarification": "бить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0227e7dd-57ca-3ef2-b989-a0dacb2c78c3",
                    "sense_definition": "Usilnie i natrętnie starać się o osiągnięcie jakiegoś stanowiska lub pozycji (używane z „się”)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ambition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy w firmie wiedzą, że on od dawna <pcha> się na stanowisko dyrektora.",
                            "target_lang_translations": {
                                "ru": "Все в компании знают, что он давно <w>рвется</w> на должность директора."
                            }
                        },
                        {
                            "text": "Nie rozumiem, dlaczego ona tak bardzo <pcha> się do polityki, skoro się na tym nie zna.",
                            "target_lang_translations": {
                                "ru": "Я не понимаю, почему она так сильно <w>лезет</w> в политику, если она в этом не разбирается."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pchać się na afisz",
                        "pchać się do władzy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво и навязчиво стремиться занять какую-то должность или положение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лезть",
                                "target_lang_sense_clarification": "навязчиво стремиться"
                            },
                            {
                                "target_lang_word": "рваться",
                                "target_lang_sense_clarification": "сильно стремиться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
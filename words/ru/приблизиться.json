    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "88ef12c3-50a1-308a-8668-cff2bfdd376f",
                    "sense_definition": "Переместиться в пространстве и стать ближе к кому-либо или чему-либо; сократить расстояние.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лодка медленно <приблизилась> к берегу реки.",
                            "target_lang_translations": {
                                "en": "The boat slowly <w>approached</w> the river bank."
                            }
                        },
                        {
                            "text": "Я боюсь <приблизиться> к этой большой собаке.",
                            "target_lang_translations": {
                                "en": "I am afraid to <w>get closer</w> to that big dog."
                            }
                        }
                    ],
                    "synonyms": [
                        "подойти",
                        "подступить"
                    ],
                    "antonyms": [
                        "удалиться"
                    ],
                    "common_phrases": [
                        "приблизиться к дому",
                        "приблизиться к цели",
                        "приблизиться вплотную"
                    ],
                    "target_lang_definitions": {
                        "en": "To move through space and become closer to someone or something; to reduce the distance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "movement"
                            },
                            {
                                "target_lang_word": "come closer",
                                "target_lang_sense_clarification": "movement"
                            },
                            {
                                "target_lang_word": "get closer",
                                "target_lang_sense_clarification": "movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "619708c3-c1f5-3298-b82d-daa32bc6c789",
                    "sense_definition": "Наступить или стать скорым, близким по времени (о событиях, сроках, временах года).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Незаметно <приблизилась> холодная и дождливая осень.",
                            "target_lang_translations": {
                                "en": "Cold and rainy autumn <w>approached</w> unnoticed."
                            }
                        },
                        {
                            "text": "Срок сдачи важного проекта стремительно <приблизился>.",
                            "target_lang_translations": {
                                "en": "The deadline for the important project rapidly <w>approached</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "настать",
                        "подойти"
                    ],
                    "common_phrases": [
                        "приблизился вечер",
                        "приблизилась зима",
                        "приблизился конец"
                    ],
                    "target_lang_definitions": {
                        "en": "To happen or become imminent; to become close in time (regarding events, deadlines, or seasons)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "time"
                            },
                            {
                                "target_lang_word": "draw near",
                                "target_lang_sense_clarification": "time, literary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "077a1873-62c5-368a-a878-5da9c3d6a4fd",
                    "sense_definition": "Стать более похожим на что-либо или почти достичь какого-либо состояния, качества или результата.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перевод текста максимально <приблизился> к оригиналу по стилю.",
                            "target_lang_translations": {
                                "en": "The text translation <w>approached</w> the original as much as possible in style."
                            }
                        },
                        {
                            "text": "Ученые <приблизились> к разгадке древней тайны.",
                            "target_lang_translations": {
                                "en": "Scientists have <w>come close</w> to solving the ancient mystery."
                            }
                        }
                    ],
                    "antonyms": [
                        "удалиться"
                    ],
                    "common_phrases": [
                        "приблизиться к истине",
                        "приблизиться к идеалу",
                        "приблизиться к решению"
                    ],
                    "target_lang_definitions": {
                        "en": "To become more similar to something or almost reach a certain state, quality, or result."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come close to",
                                "target_lang_sense_clarification": "result or similarity"
                            },
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "abstract"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d81af372-686c-3287-affe-3666b62327fe",
                    "sense_definition": "Установить более тесные, доверительные отношения с кем-либо; войти в чей-либо круг общения.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "relationships",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему удалось <приблизиться> к известному писателю и взять у него интервью.",
                            "target_lang_translations": {
                                "en": "He managed to <w>get closer</w> to the famous writer and interview him."
                            }
                        },
                        {
                            "text": "Она хотела <приблизиться> к кругу избранных, но её не приняли.",
                            "target_lang_translations": {
                                "en": "She wanted to <w>get closer</w> to the chosen circle, but she was not accepted."
                            }
                        }
                    ],
                    "synonyms": [
                        "сблизиться",
                        "сойтись"
                    ],
                    "antonyms": [
                        "удалиться"
                    ],
                    "common_phrases": [
                        "приблизиться к человеку",
                        "приблизиться к власти"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish closer, more trusting relationships with someone; to enter someone's social circle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get closer to",
                                "target_lang_sense_clarification": "relationships"
                            },
                            {
                                "target_lang_word": "become close to",
                                "target_lang_sense_clarification": "relationships"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
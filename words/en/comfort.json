{
    "word_family": [
        "comfortable",
        "comforter",
        "discomfort",
        "uncomfortable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac4ac4b0-e022-3d91-8455-c7ff0b521dc6",
                    "sense_definition": "A state of physical ease and freedom from pain, constraint, or worry.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She can finally live in <w>comfort</w> after years of hard work.",
                            "target_lang_translations": {
                                "ru": "Она наконец-то может жить в <w>комфорте</w> после долгих лет тяжелой работы."
                            }
                        },
                        {
                            "text": "The new sofa provides excellent <w>comfort</w> for your back.",
                            "target_lang_translations": {
                                "ru": "Новый диван обеспечивает превосходное <w>удобство</w> для вашей спины."
                            }
                        }
                    ],
                    "synonyms": [
                        "ease",
                        "relief"
                    ],
                    "antonyms": [
                        "austerity"
                    ],
                    "common_phrases": [
                        "live in comfort",
                        "sense of comfort",
                        "for comfort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние физического покоя, благополучия и отсутствия боли или тревоги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комфорт",
                                "target_lang_sense_clarification": "общая обстановка, благоустроенность"
                            },
                            {
                                "target_lang_word": "удобство",
                                "target_lang_sense_clarification": "эргономичность, приспособленность для легкого использования"
                            },
                            {
                                "target_lang_word": "уют",
                                "target_lang_sense_clarification": "приятная, домашняя обстановка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fff75c8-aad7-339a-8a89-7abdd8d5f16c",
                    "sense_definition": "A person or thing that helps to alleviate sorrow, distress, or worry.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_consolation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a great <w>comfort</w> to know that he was safe.",
                            "target_lang_translations": {
                                "ru": "Было большим <w>утешением</w> знать, что он в безопасности."
                            }
                        },
                        {
                            "text": "Her kind words were a <w>comfort</w> to the grieving family.",
                            "target_lang_translations": {
                                "ru": "Её добрые слова стали <w>утешением</w> для скорбящей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "consolation",
                        "solace",
                        "relief"
                    ],
                    "common_phrases": [
                        "be a comfort",
                        "take comfort",
                        "bring comfort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, приносящий облегчение в горе или тревоге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утешение",
                                "target_lang_sense_clarification": "то, что облегчает страдание или горе"
                            },
                            {
                                "target_lang_word": "отрада",
                                "target_lang_sense_clarification": "источник радости и успокоения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f578e6e7-61cf-32a1-98de-b35ad9726c04",
                    "sense_definition": "Things that contribute to physical ease and well-being.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_amenity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He missed the <w>comforts</w> of home while traveling abroad.",
                            "target_lang_translations": {
                                "ru": "Путешествуя за границей, он скучал по домашнему <w>уюту</w>."
                            }
                        },
                        {
                            "text": "The hotel offers all the modern <w>comforts</w> you would expect.",
                            "target_lang_translations": {
                                "ru": "Отель предлагает все современные <w>удобства</w>, которые можно ожидать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "creature comforts",
                        "home comforts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Условия или предметы, создающие физическое удобство и благополучие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удобства",
                                "target_lang_sense_clarification": "бытовые приспособления, условия проживания"
                            },
                            {
                                "target_lang_word": "уют",
                                "target_lang_sense_clarification": "домашнее тепло и спокойствие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8c22aaab-c094-390e-90e2-bd2a2a9996bd",
                    "sense_definition": "To make someone feel less unhappy, worried, or upset.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_soothe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tried to <w>comfort</w> the crying child with a soft toy.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <w>утешить</w> плачущего ребенка мягкой игрушкой."
                            }
                        },
                        {
                            "text": "Friends came to <w>comfort</w> him after he received the bad news.",
                            "target_lang_translations": {
                                "ru": "Друзья пришли <w>утешить</w> его после того, как он получил плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "console",
                        "soothe",
                        "cheer",
                        "solace"
                    ],
                    "antonyms": [
                        "hurt",
                        "vex"
                    ],
                    "common_phrases": [
                        "comfort a friend",
                        "comfort the victim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успокаивать кого-либо, облегчать чье-то горе или страдание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утешать",
                                "target_lang_sense_clarification": "облегчать горе словами или участием"
                            },
                            {
                                "target_lang_word": "успокаивать",
                                "target_lang_sense_clarification": "приводить в состояние покоя, снимать тревогу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d41f1b2e-7dd0-3657-93bc-7bc1465e07d0",
                    "sense_definition": "To make strong, invigorate, or fortify.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_strengthen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient texts were written to <w>comfort</w> and strengthen the spirit.",
                            "target_lang_translations": {
                                "ru": "Древние тексты были написаны, чтобы <w>укреплять</w> и поддерживать дух."
                            }
                        },
                        {
                            "text": "He felt <w>comforted</w> by the support of his allies in battle.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>укреплённым</w> поддержкой своих союзников в битве."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Original meaning related to strength, now mostly found in older texts."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать силы, укреплять дух (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укреплять",
                                "target_lang_sense_clarification": "делать сильным, придавать стойкость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6530a3f8-cb66-36fb-b707-ad1474976cbc",
                    "sense_definition": "A female given name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My aunt <w>Comfort</w> is visiting us next week.",
                            "target_lang_translations": {
                                "ru": "Моя тетя <w>Комфорт</w> навестит нас на следующей неделе."
                            }
                        },
                        {
                            "text": "The name <w>Comfort</w> was popular among Puritans.",
                            "target_lang_translations": {
                                "ru": "Имя <w>Комфорт</w> было популярно среди пуритан."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское личное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Комфорт",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99e4298e-0707-31af-b0ef-7bc1f6c45317",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Comfort</w> wrote several books on the subject.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Комфорт</w> написал несколько книг на эту тему."
                            }
                        },
                        {
                            "text": "We are looking for a family named <w>Comfort</w> in the directory.",
                            "target_lang_translations": {
                                "ru": "Мы ищем семью по фамилии <w>Комфорт</w> в справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Комфорт",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5663b2e7-3618-37ee-b90b-c8d158880c21",
                    "sense_definition": "A place name used for various locations in the United States.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They drove through the small town of <w>Comfort</w> on their road trip.",
                            "target_lang_translations": {
                                "ru": "Во время поездки они проехали через маленький городок <w>Комфорт</w>."
                            }
                        },
                        {
                            "text": "<w>Comfort</w> is located in the southern part of the state.",
                            "target_lang_translations": {
                                "ru": "<w>Комфорт</w> расположен в южной части штата."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название населенного пункта в США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Комфорт",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
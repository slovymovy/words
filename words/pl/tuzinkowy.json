                    "sense_id": "4bd9f708-aa22-32e7-9285-24f9ece80b69",
                    "sense_definition": "Taki, który nie wyróżnia się niczym szczególnym, jest pospolity, przeciętny i spotykany w dużej liczbie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality_originality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Film okazał się niestety bardzo <w>tuzinkowy</w> i powielał wszystkie znane schematy gatunku.",
                            "target_lang_translations": {
                                "ru": "Фильм, к сожалению, оказался очень <w>заурядным</w> и повторял все известные жанровые клише."
                            }
                        },
                        {
                            "text": "Nie chciała wieść <w>tuzinkowego</w> życia, dlatego rzuciła pracę w korporacji i wyjechała w podróż.",
                            "target_lang_translations": {
                                "ru": "Она не хотела вести <w>обыденную</w> жизнь, поэтому бросила работу в корпорации и отправилась в путешествие."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieoryginalny",
                        "banalny",
                        "konwencjonalny"
                    ],
                    "antonyms": [
                        "nietuzinkowy",
                        "niebanalny",
                        "niekonwencjonalny",
                        "nieschematyczny",
                        "niestereotypowy",
                        "nieszablonowy",
                        "niesztampowy"
                    ],
                    "common_phrases": [
                        "tuzinkowa uroda",
                        "tuzinkowy scenariusz",
                        "tuzinkowy pomysł",
                        "tuzinkowe rozwiązanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который ничем особенным не выделяется, обычный, посредственный и часто встречающийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заурядный",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d643bb2a-1006-3fbb-8679-42640b6ca1cd",
                    "sense_definition": "Gromadzić pieniądze lub zużywać zasoby w małych ilościach, aby starczyło ich na dłużej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym miesiącu musimy bardziej <w>oszczędzać</w> wodę i prąd.",
                            "target_lang_translations": {
                                "ru": "В этом месяце мы должны больше <w>экономить</w> воду и электричество."
                            }
                        },
                        {
                            "text": "Moi rodzice potrafią <w>oszczędzać</w> pieniądze na czarną godzinę.",
                            "target_lang_translations": {
                                "ru": "Мои родители умеют <w>откладывать</w> деньги на чёрный день."
                            }
                        }
                    ],
                    "synonyms": [
                        "odkładać",
                        "zaoszczędzać",
                        "zaciskać pasa",
                        "gw. (Górny Śląsk) szporować"
                    ],
                    "antonyms": [
                        "trwonić"
                    ],
                    "common_phrases": [
                        "oszczędzać pieniądze",
                        "oszczędzać czas",
                        "oszczędzać na mieszkanie",
                        "oszczędzać energię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тратить деньги или ресурсы экономно, а также накапливать их на будущее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экономить",
                                "target_lang_sense_clarification": "расходовать бережно"
                            },
                            {
                                "target_lang_word": "копить",
                                "target_lang_sense_clarification": "собирать деньги"
                            },
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "сберегать в запас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44662961-c347-3e55-843d-4711c696e8c3",
                    "sense_definition": "Unikać nadmiernego wysiłku fizycznego lub stresu w trosce o swoje zdrowie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił mi, abym po operacji zaczął się <w>oszczędzać</w>.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал мне <w>беречь себя</w> после операции."
                            }
                        },
                        {
                            "text": "Marek nigdy nie lubił się <w>oszczędzać</w> podczas treningów.",
                            "target_lang_translations": {
                                "ru": "Марек никогда не любил <w>жалеть себя</w> на тренировках."
                            }
                        }
                    ],
                    "common_phrases": [
                        "musisz się oszczędzać",
                        "oszczędzać się w pracy",
                        "oszczędzać siły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избегать чрезмерных усилий или стресса, заботясь о своем здоровье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беречь себя",
                                "target_lang_sense_clarification": "заботиться о здоровье"
                            },
                            {
                                "target_lang_word": "щадить себя",
                                "target_lang_sense_clarification": "не перенапрягаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ed24b61-01d6-380d-a225-6f325dd1a4a2",
                    "sense_definition": "Chronić kogoś przed przykrościami, bólem, wstydem lub trudnymi przeżyciami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym <w>oszczędzić</w> ci tego przykrego widoku.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>избавить</w> тебя от этого неприятного зрелища."
                            }
                        },
                        {
                            "text": "Proszę, <w>oszczędź</w> mi swoich złośliwych komentarzy.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>избавь</w> меня от своих злобных комментариев."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaoszczędzać"
                    ],
                    "common_phrases": [
                        "oszczędzać komuś wstydu",
                        "oszczędzać nerwy",
                        "oszczędzać szczegółów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избавлять кого-то от неприятных переживаний, боли или стыда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избавлять",
                                "target_lang_sense_clarification": "освобождать от чего-то неприятного"
                            },
                            {
                                "target_lang_word": "уберегать",
                                "target_lang_sense_clarification": "защищать от переживаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b4898de-efc0-321c-ba94-aa2de9ba9e90",
                    "sense_definition": "Darować komuś życie lub powstrzymać się od zniszczenia czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "protection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dowódca rozkazał żołnierzom <w>oszczędzać</w> ludność cywilną.",
                            "target_lang_translations": {
                                "ru": "Командир приказал солдатам <w>щадить</w> мирное население."
                            }
                        },
                        {
                            "text": "Wielki pożar lasu cudem <w>oszczędził</w> nasz dom.",
                            "target_lang_translations": {
                                "ru": "Сильный лесной пожар чудом <w>пощадил</w> наш дом."
                            }
                        }
                    ],
                    "common_phrases": [

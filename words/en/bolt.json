    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "113a635a-3183-358a-99b0-6b4410e6e3a5",
                    "sense_definition": "A metal fastener consisting of a threaded cylindrical body with a head, designed to be used with a nut.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fastener_lock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tightened the <w>bolt</w> with a wrench to secure the wheel.",
                            "target_lang_translations": {
                                "ru": "Он затянул <w>болт</w> гаечным ключом, чтобы закрепить колесо."
                            }
                        },
                        {
                            "text": "The machine is held together by nuts and <w>bolts</w>.",
                            "target_lang_translations": {
                                "ru": "Машина держится на гайках и <w>болтах</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "screw",
                        "fastener"
                    ],
                    "common_phrases": [
                        "nuts and bolts",
                        "bolt cutter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический крепеж, состоящий из цилиндрического стержня с резьбой и головкой, предназначенный для использования с гайкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болт",
                                "target_lang_sense_clarification": "крепежная деталь"
                            },
                            {
                                "target_lang_word": "винт",
                                "target_lang_sense_clarification": "крепеж с резьбой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "255be4c2-797c-30d5-b379-8b4889a5aab4",
                    "sense_definition": "A sliding metal bar or pin in a lock mechanism used to fasten a door or window.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fastener_lock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She slid the <w>bolt</w> across to lock the door for the night.",
                            "target_lang_translations": {
                                "ru": "Она задвинула <w>засов</w>, чтобы запереть дверь на ночь."
                            }
                        },
                        {
                            "text": "The rusty <w>bolt</w> stuck and would not move easily.",
                            "target_lang_translations": {
                                "ru": "Ржавый <w>засов</w> застрял и не двигался."
                            }
                        }
                    ],
                    "synonyms": [
                        "latch",
                        "bar",
                        "lock"
                    ],
                    "common_phrases": [
                        "slide the bolt",
                        "dead bolt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скользящий металлический стержень в замковом механизме, используемый для запирания двери или окна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засов",
                                "target_lang_sense_clarification": "запирающее устройство"
                            },
                            {
                                "target_lang_word": "задвижка",
                                "target_lang_sense_clarification": "небольшой засов"
                            },
                            {
                                "target_lang_word": "щеколда",
                                "target_lang_sense_clarification": "простой запор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d699dd69-cd5a-355f-839a-b3f5ca17d44e",
                    "sense_definition": "A flash of lightning.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sudden_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A bright <w>bolt</w> of lightning struck the old oak tree.",
                            "target_lang_translations": {
                                "ru": "Яркая <w>молния</w> ударила в старый дуб."
                            }
                        },
                        {
                            "text": "We saw a sudden <w>bolt</w> flash across the dark sky.",
                            "target_lang_translations": {
                                "ru": "Мы увидели, как внезапная <w>молния</w> сверкнула в темном небе."
                            }
                        }
                    ],
                    "synonyms": [
                        "flash",
                        "stroke"
                    ],
                    "common_phrases": [
                        "lightning bolt",
                        "bolt of lightning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспышка молнии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молния",
                                "target_lang_sense_clarification": "атмосферное явление"
                            },
                            {
                                "target_lang_word": "разряд молнии",
                                "target_lang_sense_clarification": "электрическая вспышка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e9d57af-ce18-3eb2-8969-54d35d390be8",
                    "sense_definition": "A sudden movement or attempt to escape.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sudden_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thief made a sudden <w>bolt</w> for the open window.",
                            "target_lang_translations": {
                                "ru": "Вор сделал внезапный <w>рывок</w> к открытому окну."
                            }
                        },
                        {
                            "text": "With a <w>bolt</w> of energy, the horse jumped over the fence.",
                            "target_lang_translations": {
                                "ru": "С <w>приливом</w> энергии лошадь перепрыгнула через забор."
                            }
                        }
                    ],
                    "synonyms": [
                        "dash",
                        "sprint",
                        "rush"
                    ],
                    "common_phrases": [
                        "make a bolt for",
                        "bolt from the blue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное движение или попытка убежать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое движение"
                            },
                            {
                                "target_lang_word": "бегство",
                                "target_lang_sense_clarification": "побег"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c98f5ca-939d-33a2-a7ec-3bf3187683f9",
                    "sense_definition": "A large roll of fabric, canvas, or similar material.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tailor purchased a <w>bolt</w> of silk to make the wedding dress.",
                            "target_lang_translations": {
                                "ru": "Портной купил <w>рулон</w> шёлка, чтобы сшить свадебное платье."
                            }
                        },
                        {
                            "text": "She unrolled the <w>bolt</w> of cloth on the cutting table.",
                            "target_lang_translations": {
                                "ru": "Она развернула <w>рулон</w> ткани на раскройном столе."
                            }
                        }
                    ],
                    "synonyms": [
                        "roll"
                    ],
                    "common_phrases": [
                        "bolt of cloth",
                        "bolt of fabric"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой рулон ткани, холста или подобного материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рулон",
                                "target_lang_sense_clarification": "сверток ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f868091-06da-3725-8a53-f49094ffda7b",

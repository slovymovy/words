    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f83a62fb-aa40-31cc-90b3-2978815e7020",
                    "sense_definition": "Переместить кого-либо или что-либо вниз, в более низкое положение.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грузчики аккуратно <w>опустили</w> тяжёлый диван на пол.",
                            "target_lang_translations": {
                                "en": "The movers carefully <w>lowered</w> the heavy sofa onto the floor."
                            }
                        },
                        {
                            "text": "Вам нужно <w>опустить</w> письмо в синий почтовый ящик на углу.",
                            "target_lang_translations": {
                                "en": "You need to <w>drop</w> the letter into the blue mailbox on the corner."
                            }
                        }
                    ],
                    "synonyms": [
                        "спустить"
                    ],
                    "common_phrases": [
                        "опустить письмо",
                        "опустить штору",
                        "опустить стекло"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something downwards, to a lower position."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lower",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "put down",
                                "target_lang_sense_clarification": "to place something on a lower surface"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "to let something fall (e.g., a letter)"
                            },
                            {
                                "target_lang_word": "let down",
                                "target_lang_sense_clarification": "to allow to descend (e.g., on a rope)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac0f7b3b-95cd-347e-9f21-d4fb5e4b8fcd",
                    "sense_definition": "Склонить или направить вниз (обычно о голове, глазах или руках).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девушка смущённо <w>опустила</w> глаза и замолчала.",
                            "target_lang_translations": {
                                "en": "The girl shyly <w>lowered</w> her eyes and fell silent."
                            }
                        },
                        {
                            "text": "От бессилия он <w>опустил</w> руки и перестал спорить.",
                            "target_lang_translations": {
                                "en": "Out of powerlessness, he <w>dropped</w> his hands and stopped arguing."
                            }
                        }
                    ],
                    "synonyms": [
                        "потупить",
                        "склонить"
                    ],
                    "common_phrases": [
                        "опустить глаза",
                        "опустить голову",
                        "опустить руки",
                        "опустить взгляд"
                    ],
                    "target_lang_definitions": {
                        "en": "To incline or direct downwards (usually about head, eyes, or hands)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lower",
                                "target_lang_sense_clarification": "eyes, head, or gaze"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "hands or gaze"
                            },
                            {
                                "target_lang_word": "cast down",
                                "target_lang_sense_clarification": "literary, usually eyes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d06e42cd-e402-3b71-b579-545abc55f8b9",
                    "sense_definition": "Не включить во что-либо, пропустить в тексте или речи (слова, подробности, главы).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор решил <w>опустить</w> скучные технические подробности в своей книге.",
                            "target_lang_translations": {
                                "en": "The author decided to <w>omit</w> the boring technical details in his book."
                            }
                        },
                        {
                            "text": "Давайте <w>опустим</w> вступление и перейдём сразу к делу.",
                            "target_lang_translations": {
                                "en": "Let's <w>skip</w> the introduction and get straight to the point."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропустить",
                        "исключить"
                    ],
                    "common_phrases": [
                        "опустить подробности",
                        "опустить детали"
                    ],
                    "target_lang_definitions": {
                        "en": "To not include in something, to skip in text or speech (words, details, chapters)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "omit",
                                "target_lang_sense_clarification": "formal, to exclude"
                            },
                            {
                                "target_lang_word": "skip",
                                "target_lang_sense_clarification": "informal, to pass over"
                            },
                            {
                                "target_lang_word": "leave out",
                                "target_lang_sense_clarification": "phrasal verb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8d0ef96-d488-3edd-9f17-63062e6eca9c",
                    "sense_definition": "Откинуть вниз, придав лежачее или висячее положение (о воротнике, капюшоне, спинке сиденья).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В помещении стало жарко, и он <w>опустил</w> воротник пальто.",
                            "target_lang_translations": {
                                "en": "It got hot in the room, and he <w>turned down</w> his coat collar."
                            }
                        },
                        {
                            "text": "Пассажир попросил разрешения <w>опустить</w> спинку кресла, чтобы поспать.",
                            "target_lang_translations": {
                                "en": "The passenger asked for permission to <w>recline</w> the seat back to sleep."
                            }
                        }
                    ],
                    "synonyms": [
                        "откинуть"
                    ],
                    "common_phrases": [
                        "опустить воротник",
                        "опустить капюшон"
                    ],
                    "target_lang_definitions": {
                        "en": "To fold or move downwards to a lying or hanging position (about a collar, hood, seat back)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn down",
                                "target_lang_sense_clarification": "collar or brim"
                            },
                            {
                                "target_lang_word": "lower",
                                "target_lang_sense_clarification": "hood or general mechanism"
                            },
                            {
                                "target_lang_word": "recline",
                                "target_lang_sense_clarification": "seat back"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84a8eebc-d221-36e0-a6a5-b9290af1d1ed",
                    "sense_definition": "Унизить кого-либо, публично подорвать чей-либо авторитет или репутацию.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его злая шутка была попыткой <w>опустить</w> конкурента в глазах коллег.",
                            "target_lang_translations": {
                                "en": "His mean joke was an attempt to <w>put down</w> the competitor in the eyes of colleagues."
                            }
                        },
                        {
                            "text": "Не стоит пытаться <w>опустить</w> других, чтобы казаться лучше.",
                            "target_lang_translations": {
                                "en": "It's not worth trying to <w>put down</w> others to seem better."
                            }
                        }
                    ],
                    "synonyms": [
                        "дискредитировать"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To humiliate someone, to publicly undermine someone's authority or reputation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put down",
                                "target_lang_sense_clarification": "idiomatic, to criticize or humiliate"
                            },
                            {
                                "target_lang_word": "humiliate",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "belittle",
                                "target_lang_sense_clarification": "to make someone seem unimportant"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b04d49ec-b75c-39d1-8cb2-d701a2049dd1",
                    "sense_definition": "Совершить насилие над заключённым, переведя его в низшую касту тюремной иерархии.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В криминальных кругах угрожали <w>опустить</w> должника за неуплату.",
                            "target_lang_translations": {
                                "en": "In criminal circles, they threatened to <w>degrade</w> the debtor for non-payment."
                            }
                        },
                        {
                            "text": "Этот жестокий ритуал призван <w>опустить</w> человека на самое дно иерархии.",
                            "target_lang_translations": {
                                "en": "This cruel ritual is intended to <w>lower</w> a person to the very bottom of the hierarchy."
                            }
                        }
                    ],
                    "synonyms": [
                        "опетушить",
                        "зафоршма́чить"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "тюремный жаргон"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To commit sexual violence against a prisoner, transferring them to the lowest caste of the prison hierarchy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "degrade",
                                "target_lang_sense_clarification": "to lower in status/character"
                            },
                            {
                                "target_lang_word": "punk",
                                "target_lang_sense_clarification": "prison slang"
                            },
                            {
                                "target_lang_word": "violate",
                                "target_lang_sense_clarification": "sexual violence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
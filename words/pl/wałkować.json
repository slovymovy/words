    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c92e5164-9224-3680-a6fd-95fd10ebda54",
                    "sense_definition": "Rozpłaszczać ciasto lub inną plastyczną masę na płaski płat przy użyciu wałka.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "culinary_actions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mocno <wałkować> ciasto, aby było idealnie cienkie na pierogi.",
                            "target_lang_translations": {
                                "ru": "Ты должен сильно <раскатывать> тесто, чтобы оно было идеально тонким для вареников."
                            }
                        },
                        {
                            "text": "Mama nauczyła mnie <wałkować> masę na pizzę, żeby była równa.",
                            "target_lang_translations": {
                                "ru": "Мама научила меня <раскатывать> тесто для пиццы, чтобы оно было ровным."
                            }
                        }
                    ],
                    "synonyms": [
                        "wałkonić"
                    ],
                    "common_phrases": [
                        "wałkować ciasto",
                        "wałkować na stolnicy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскатывать тесто или другую пластичную массу в плоский пласт с помощью скалки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскатывать",
                                "target_lang_sense_clarification": "тесто"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbc1f637-4429-3554-ad5a-d0cf2bce83af",
                    "sense_definition": "Omawiać jakieś zagadnienie wielokrotnie, bardzo szczegółowo i często w sposób nużący dla słuchaczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziennikarze będą <wałkować> ten skandal przez kolejne tygodnie.",
                            "target_lang_translations": {
                                "ru": "Журналисты будут <мусолить> этот скандал следующие несколько недель."
                            }
                        },
                        {
                            "text": "Nie ma sensu znowu <wałkować> tego samego problemu na zebraniu.",
                            "target_lang_translations": {
                                "ru": "Нет смысла снова <мусолить> ту же самую проблему на собрании."
                            }
                        }
                    ],
                    "synonyms": [
                        "wałkonić"
                    ],
                    "common_phrases": [
                        "wałkować temat",
                        "wałkować problem",
                        "wałkować to samo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Многократно и подробно обсуждать какую-либо тему, часто утомительно для слушателей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мусолить",
                                "target_lang_sense_clarification": "разговорное; долго обсуждать"
                            },
                            {
                                "target_lang_word": "пережёвывать",
                                "target_lang_sense_clarification": "переносное; повторять одно и то же"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05e4c488-487d-344f-ad36-1050355a71a2",
                    "sense_definition": "O tkaninie lub substancji: zwijać się w małe grudki, kuleczki lub wałeczki (używane z zaimkiem 'się').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten wełniany sweter jest słabej jakości i zaczął się <wałkować>.",
                            "target_lang_translations": {
                                "ru": "Этот шерстяной свитер плохого качества и начал <скатываться>."
                            }
                        },
                        {
                            "text": "Jeśli nałożysz zbyt grubą warstwę kremu, podkład może się <wałkować>.",
                            "target_lang_translations": {
                                "ru": "Если нанесешь слишком толстый слой крема, тональный крем может <скатываться>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "materiał się wałkuje",
                        "podkład się wałkuje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О ткани или веществе: сворачиваться в маленькие комочки или катышки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скатываться",
                                "target_lang_sense_clarification": "о ткани или косметике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe6a3bca-d641-368c-9841-d61066ae7bd0",
                    "sense_definition": "Wymawiać głoskę 'ł' jako 'w' (zjawisko fonetyczne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Współcześnie większość Polaków <wałkuje> w mowie potocznej.",
                            "target_lang_translations": {
                                "ru": "Сегодня большинство поляков <произносит «ł» как «w»> в разговорной речи."
                            }
                        },
                        {
                            "text": "Aktor musiał nauczyć się nie <wałkować>, aby brzmieć jak postać z przedwojennego filmu.",
                            "target_lang_translations": {
                                "ru": "Актер должен был научиться не <произносить «ł» как «w»>, чтобы звучать как персонаж из довоенного фильма."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносить звук «ł» как губно-губной «w» (фонетическое явление)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произносить «ł» как «w»",
                                "target_lang_sense_clarification": "фонетика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

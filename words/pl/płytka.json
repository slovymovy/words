                },
                {
                    "sense_id": "464785c5-dc92-3924-b21e-86b2ef6ed8f3",
                    "sense_definition": "Zdrobnienie od: <w>płyta</w>; potoczne określenie płyty CD lub DVD",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagrałem ci wszystkie zdjęcia z wakacji na <w>płytkę</w>.",
                            "target_lang_translations": {
                                "ru": "Я записал тебе все фотографии с отпуска на <w>диск</w>."
                            }
                        },
                        {
                            "text": "Nie mogę znaleźć tej <w>płytki</w> ze sterownikami do drukarki.",
                            "target_lang_translations": {
                                "ru": "Я не могу найти этот <w>диск</w> с драйверами для принтера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płytka CD",
                        "nagrać na płytkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie o płycie CD/DVD"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительное от \"płyta\"; разговорное название CD или DVD диска"
                    },
                    "translations": {
                        "ru": [
                            {

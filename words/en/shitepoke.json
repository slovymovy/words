            "senses": [
                {
                    "sense_id": "031d6708-9864-3a73-8b5f-9e8162b432d6",
                    "sense_definition": "The green heron, a small wading bird found in wetlands.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heron_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>shitepoke</w> flew away quickly when we approached the riverbank.",
                            "target_lang_translations": {
                                "ru": "<w>Зелёная кваква</w> быстро улетела, когда мы подошли к берегу реки."
                            }
                        },
                        {
                            "text": "My grandfather always called the green heron a <w>shitepoke</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка всегда называл зелёную квакву <w>зелёной кваквой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "green heron"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mostly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Зеленая кваква, небольшая болотная птица, обитающая в водно-болотных угодьях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зелёная кваква",
                                "target_lang_sense_clarification": "вид цапли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3faf116-59ba-3ed1-abbd-2e7e5ec86e77",
                    "sense_definition": "The black-crowned night heron, a bird known for being active at dusk and night.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heron_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>shitepoke</w> stood silently at the edge of the marsh.",
                            "target_lang_translations": {
                                "ru": "<w>Обыкновенная кваква</w> молча стояла на краю болота."
                            }
                        },
                        {
                            "text": "We spotted a <w>shitepoke</w> hunting for frogs in the twilight.",
                            "target_lang_translations": {
                                "ru": "Мы заметили <w>обыкновенную квакву</w>, охотящуюся на лягушек в сумерках."
                            }
                        }
                    ],
                    "synonyms": [
                        "black-crowned night heron",
                        "night heron"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mostly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обыкновенная кваква, птица, известная своей активностью в сумерках и ночью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обыкновенная кваква",
                                "target_lang_sense_clarification": "ночная цапля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e031de6-8b87-3494-90fb-bbeef1ad5e20",
                    "sense_definition": "The American bittern, a solitary wading bird with brown streaks.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heron_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud call of the <w>shitepoke</w> echoed across the wetlands.",
                            "target_lang_translations": {
                                "ru": "Громкий крик <w>американской выпи</w> эхом разнесся по болотам."
                            }
                        },
                        {
                            "text": "He identified the bird as a <w>shitepoke</w> by its distinctive flight.",
                            "target_lang_translations": {
                                "ru": "Он опознал птицу как <w>американскую выпь</w> по её характерному полету."
                            }
                        }
                    ],
                    "synonyms": [
                        "American bittern"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mostly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Американская выпь, одиночная болотная птица с коричневыми полосами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "американская выпь",
                                "target_lang_sense_clarification": "вид выпи"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "5d64e5cf-d0c0-36c8-8820-e6a0decf6f99",
                    "sense_definition": "Trwający bez końca, istniejący zawsze; dotyczący wieczności lub życia po śmierci.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu ludzi wierzy w życie <w>wieczne</w> po śmierci.",
                            "target_lang_translations": {
                                "ru": "Многие люди верят в <w>вечную</w> жизнь после смерти."
                            }
                        },
                        {
                            "text": "Ten pomnik jest symbolem <w>wiecznej</w> pamięci o bohaterach.",
                            "target_lang_translations": {
                                "ru": "Этот памятник является символом <w>вечной</w> памяти о героях."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieśmiertelny",
                        "wiekuisty",
                        "wieczysty",
                        "nieprzemijający",
                        "nieskończony",
                        "ponadczasowy"
                    ],
                    "antonyms": [

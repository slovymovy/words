{
    "word_family": [
        "of",
        "oftewel"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "a958247e-beb8-3982-8e27-c0e06b2bc380",
                    "sense_definition": "Geeft een keuze aan tussen twee mogelijkheden, vaak gebruikt in een constructie waarbij beide opties worden benadrukt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt <ofwel> met de trein gaan, <ofwel> de auto nemen.",
                            "target_lang_translations": {
                                "en": "You can <w>either</w> go by train <w>or</w> take the car."
                            }
                        },
                        {
                            "text": "Hij is <ofwel> erg slim, <ofwel> een enorme geluksvogel.",
                            "target_lang_translations": {
                                "en": "He is <w>either</w> very smart <w>or</w> extremely lucky."
                            }
                        }
                    ],
                    "synonyms": [
                        "hetzij",
                        "of"
                    ],
                    "common_phrases": [
                        "ofwel ... ofwel"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a choice between two possibilities, often used in a construction where both options are emphasized."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "either ... or",
                                "target_lang_sense_clarification": "used to present two alternatives"
                            },
                            {
                                "target_lang_word": "or",
                                "target_lang_sense_clarification": "indicating an alternative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3a00295-3ecb-3792-8582-43362f91e44b",
                    "sense_definition": "Introduceert een andere benaming of een verduidelijking voor iets wat net genoemd is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "clarification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De huurder, <ofwel> de pachter, is verantwoordelijk voor het onderhoud van de grond.",
                            "target_lang_translations": {
                                "en": "The tenant, <w>or</w> the leaseholder, is responsible for the maintenance of the land."
                            }
                        },
                        {
                            "text": "Dit is de Canis lupus, <ofwel> de wolf.",
                            "target_lang_translations": {
                                "en": "This is the Canis lupus, <w>aka</w> the wolf."
                            }
                        }
                    ],
                    "synonyms": [
                        "oftewel"
                    ],
                    "target_lang_definitions": {
                        "en": "Introduces another name or a clarification for something just mentioned."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in other words",
                                "target_lang_sense_clarification": "introducing a clarification"
                            },
                            {
                                "target_lang_word": "or",
                                "target_lang_sense_clarification": "introducing a synonym or explanation"
                            },
                            {
                                "target_lang_word": "aka",
                                "target_lang_sense_clarification": "also known as"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
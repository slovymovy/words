    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e943c4f1-26a2-3eab-8a52-7feb2f87b5f6",
                    "sense_definition": "A male given name.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Norman</w> lives in Canada.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Норман</w> живет в Канаде."
                            }
                        },
                        {
                            "text": "Please tell <w>Norman</w> that the meeting has been rescheduled.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай <w>Норману</w>, что встреча перенесена."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское личное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Норман",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "116d72fa-5aa0-32c1-be87-594bdf18fcc3",
                    "sense_definition": "A surname derived from a nickname for someone from Normandy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Norman</w> has been teaching history at our school for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Норман</w> преподает историю в нашей школе уже двадцать лет."
                            }
                        },
                        {
                            "text": "The <w>Norman</w> family donated a large sum to the local library.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Норман</w> пожертвовала крупную сумму местной библиотеке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от прозвища выходца из Нормандии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Норман",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07ec4813-4dad-3c93-bc6b-5b42095e6a5f",
                    "sense_definition": "A city in Oklahoma, United States; also the name of several other smaller towns.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The university is located in <w>Norman</w>, Oklahoma.",
                            "target_lang_translations": {
                                "ru": "Университет находится в городе <w>Норман</w>, штат Оклахома."
                            }
                        },
                        {
                            "text": "We drove through <w>Norman</w> on our way across the state.",
                            "target_lang_translations": {
                                "ru": "Мы проехали через <w>Норман</w> по пути через штат."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в штате Оклахома, США; также название нескольких других небольших городов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Норман",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7160bd6d-0d92-3f99-ad03-9a2e283d13b6",
                    "sense_definition": "The Romance language spoken in Normandy, closely related to French.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "language_group",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Some older residents of the Channel Islands still speak <w>Norman</w>.",
                            "target_lang_translations": {
                                "ru": "Некоторые пожилые жители Нормандских островов до сих пор говорят на <w>нормандском</w>."
                            }
                        },
                        {
                            "text": "The <w>Norman</w> language had a significant influence on the development of English.",
                            "target_lang_translations": {
                                "ru": "<w>Нормандский</w> язык оказал значительное влияние на развитие английского."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Романский язык, на котором говорят в Нормандии; тесно связан с французским."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормандский язык",
                                "target_lang_sense_clarification": "язык"
                            },
                            {
                                "target_lang_word": "нормандский",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0cd71534-989b-34e5-ae48-4c66480f8ea2",
                    "sense_definition": "Of or pertaining to Normandy (a region in France) or its inhabitants and history.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_hist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Norman</w> invasion of 1066 changed the course of English history.",
                            "target_lang_translations": {
                                "ru": "<w>Нормандское</w> завоевание 1066 года изменило ход английской истории."
                            }
                        },
                        {
                            "text": "Many English words have <w>Norman</w> French origins.",
                            "target_lang_translations": {
                                "ru": "Многие английские слова имеют <w>нормандское</w> (старофранцузское) происхождение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Norman Conquest",
                        "Norman French",
                        "Norman invasion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Нормандии (регион во Франции), её жителям или истории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормандский",
                                "target_lang_sense_clarification": "относящийся к Нормандии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b17360d-4bf7-316a-9007-aad17c9c2584",
                    "sense_definition": "Relating to the Romanesque style of architecture developed by the Normans, characterized by large arches and heavy columns.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_arch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cathedral features a magnificent <w>Norman</w> arch above the entrance.",
                            "target_lang_translations": {
                                "ru": "В соборе есть великолепная <w>нормандская</w> арка над входом."
                            }
                        },
                        {
                            "text": "We visited several <w>Norman</w> castles during our trip to the UK.",
                            "target_lang_translations": {
                                "ru": "Мы посетили несколько <w>нормандских</w> замков во время нашей поездки в Великобританию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Norman architecture",
                        "Norman arch",
                        "Norman style"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к романскому архитектурному стилю, развитому нормандцами, для которого характерны большие арки и массивные колонны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормандский",
                                "target_lang_sense_clarification": "архитектурный стиль"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8de0527f-78a8-3932-b317-c1d5bd032287",
                    "sense_definition": "A member of the people from Normandy, France, who conquered England in the 11th century.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Normans</w> built extensive stone fortifications to secure their control over the land.",
                            "target_lang_translations": {
                                "ru": "<w>Нормандцы</w> построили обширные каменные укрепления, чтобы обеспечить контроль над землей."
                            }
                        },
                        {
                            "text": "Under the rule of the <w>Normans</w>, the legal system underwent significant changes.",
                            "target_lang_translations": {
                                "ru": "При правлении <w>нормандцев</w> правовая система претерпела значительные изменения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель народа из Нормандии (Франция), завоевавшего Англию в XI веке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормандец",
                                "target_lang_sense_clarification": "исторический народ"
                            },
                            {
                                "target_lang_word": "норманн",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8480e25-85e9-3efa-8bab-d1b999fa7e44",
                    "sense_definition": "A native or inhabitant of Normandy, France.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a proud <w>Norman</w>, he enjoys the traditional cider and cheese of his region.",
                            "target_lang_translations": {
                                "ru": "Как гордый <w>нормандец</w>, он наслаждается традиционным сидром и сыром своего региона."
                            }
                        },
                        {
                            "text": "The <w>Normans</w> have a distinct cultural heritage within France.",
                            "target_lang_translations": {
                                "ru": "<w>Нормандцы</w> имеют самобытное культурное наследие в рамках Франции."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уроженец или житель Нормандии, Франция."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормандец",
                                "target_lang_sense_clarification": "житель региона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7496effb-4663-3746-8193-190a2d9f99be",
                    "sense_definition": "A normal person; someone who conforms to mainstream social standards.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He doesn't understand our internet culture because he is such a <w>norman</w>.",
                            "target_lang_translations": {
                                "ru": "Он не понимает нашу интернет-культуру, потому что он такой <w>нормис</w>."
                            }
                        },
                        {
                            "text": "Stop acting like a <w>norman</w> and try something unique for once.",
                            "target_lang_translations": {
                                "ru": "Перестань вести себя как <w>нормис</w> и попробуй хоть раз что-то уникальное."
                            }
                        }
                    ],
                    "synonyms": [
                        "normie"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обычный человек; тот, кто соответствует общепринятым социальным стандартам (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормис",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "обыватель",
                                "target_lang_sense_clarification": "нейтральное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8803b01-b120-35fa-9b7e-4db49b25e09c",
                    "sense_definition": "A wooden bar or iron pin used in nautical contexts to secure a cable or line.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor inserted the <w>norman</w> into the bitts to keep the rope from slipping.",
                            "target_lang_translations": {
                                "ru": "Матрос вставил <w>норман</w> в кнехты, чтобы трос не соскальзывал."
                            }
                        },
                        {
                            "text": "Check the <w>norman</w> pin for signs of rust or wear before we set sail.",
                            "target_lang_translations": {
                                "ru": "Проверь штырь-<w>норман</w> на наличие ржавчины или износа перед отплытием."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянный брус или железный штырь, используемый в морском деле для закрепления троса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "норман",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
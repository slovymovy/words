{
    "word_family": [
        "threat",
        "threaten",
        "threateningly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2a0de413-2a15-3c30-a20b-958e8349951d",
                    "sense_definition": "Presenting a threat, posing a likely risk of harm.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Never turn your back to someone who is displaying <w>threatening</w> behavior.",
                            "target_lang_translations": {
                                "ru": "Никогда не поворачивайтесь спиной к тому, кто демонстрирует <w>угрожающее</w> поведение."
                            }
                        },
                        {
                            "text": "The dark clouds looked very <w>threatening</w> as the storm approached the coast.",
                            "target_lang_translations": {
                                "ru": "Темные тучи выглядели очень <w>грозными</w>, когда шторм приближался к побережью."
                            }
                        }
                    ],
                    "synonyms": [
                        "menacing",
                        "baleful"
                    ],
                    "common_phrases": [
                        "threatening behavior",
                        "threatening sky",
                        "life-threatening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представляющий угрозу, предвещающий опасность или беду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угрожающий",
                                "target_lang_sense_clarification": "представляющий опасность"
                            },
                            {
                                "target_lang_word": "грозный",
                                "target_lang_sense_clarification": "о внешнем виде или стихии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13f8ccbb-cbc6-3009-89b0-374430b13460",
                    "sense_definition": "Making threats, making statements about a willingness to cause harm.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He received a <w>threatening</w> letter from an anonymous sender yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера он получил <w>угрожающее</w> письмо от анонимного отправителя."
                            }
                        },
                        {
                            "text": "The manager was fired for using <w>threatening</w> language with the employees.",
                            "target_lang_translations": {
                                "ru": "Менеджера уволили за использование <w>угрожающих</w> выражений в общении с сотрудниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "menacing"
                    ],
                    "common_phrases": [
                        "threatening letter",
                        "threatening call",
                        "threatening gesture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий или выражающий угрозы, намерение причинить вред."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угрожающий",
                                "target_lang_sense_clarification": "выражающий угрозу"
                            },
                            {
                                "target_lang_word": "с угрозами",
                                "target_lang_sense_clarification": "фраза (напр. письмо с угрозами)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd741d5d-b5ed-392b-a8d5-c8a7e4bf8df4",
                    "sense_definition": "Present participle and gerund of <w>threaten</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The union is <w>threatening</w> to strike if their demands are not met soon.",
                            "target_lang_translations": {
                                "ru": "Профсоюз <w>угрожает</w> забастовкой, если их требования не будут выполнены в ближайшее время."
                            }
                        },
                        {
                            "text": "She is <w>threatening</w> to call the police if the noise continues.",
                            "target_lang_translations": {
                                "ru": "Она <w>угрожает</w> вызвать полицию, если шум продолжится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "threatening to leave",
                        "threatening with"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола «угрожать»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угрожающий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "грозящий",
                                "target_lang_sense_clarification": "вариант причастия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1e74cce-160c-3775-90ae-613bcf24f4da",
                    "sense_definition": "An act of threatening; a threat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His constant <w>threatening</w> of the staff created a hostile work environment.",
                            "target_lang_translations": {
                                "ru": "Его постоянное <w>запугивание</w> персонала создало враждебную рабочую атмосферу."
                            }
                        },
                        {
                            "text": "The <w>threatening</w> of witnesses is a serious criminal offense in this country.",
                            "target_lang_translations": {
                                "ru": "<w>Запугивание</w> свидетелей является серьезным уголовным преступлением в этой стране."
                            }
                        }
                    ],
                    "common_phrases": [
                        "constant threatening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по выражению угроз; запугивание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запугивание",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "угрозы",
                                "target_lang_sense_clarification": "множественное число"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
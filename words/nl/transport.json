            "senses": [
                {
                    "sense_id": "0aeeb057-bac6-3f62-9484-24cb335bfdd6",
                    "sense_definition": "Het verplaatsen van mensen, goederen of energie van de ene plek naar de andere.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>transport</w> van de goederen duurt ongeveer drie dagen.",
                            "target_lang_translations": {
                                "en": "The <w>transport</w> of the goods takes about three days.",
                                "ru": "<w>Транспортировка</w> товаров занимает около трех дней."
                            }
                        },
                        {
                            "text": "Wij regelen het <w>transport</w> naar het vliegveld met een busje.",
                            "target_lang_translations": {
                                "en": "We are arranging <w>transport</w> to the airport with a van.",
                                "ru": "Мы организуем <w>транспорт</w> в аэропорт на микроавтобусе."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervoer",
                        "overbrenging"
                    ],
                    "common_phrases": [
                        "openbaar transport",
                        "internationaal transport",
                        "transport van goederen"
                    ],
                    "target_lang_definitions": {
                        "en": "The movement of people, goods, or energy from one place to another.",
                        "ru": "Перемещение людей, товаров или энергии из одного места в другое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "general movement"
                            },
                            {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "364558ed-5a5b-39d1-9cea-ae27ef3fa31a",
                    "sense_definition": "To remove something that is old, broken, or lost and put a new one in its place.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The batteries were dead, so I <w>replaced</w> them immediately."
                        },
                        {
                            "text": "I decided to <w>replace</w> my old car with a newer model."
                        }
                    ],
                    "synonyms": [
                        "exchange",
                        "swap"
                    ],
                    "common_phrases": [

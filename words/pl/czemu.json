    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "2e1b4b4a-31da-33fa-806d-024080245776",
                    "sense_definition": "Słowo pytające o przyczynę, powód lub cel jakiegoś działania; to samo co „dlaczego”.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "question_why",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Czemu</w> nie przyszedłeś wczoraj do szkoły?",
                            "target_lang_translations": {
                                "ru": "<w>Почему</w> ты не пришел вчера в школу?"
                            }
                        },
                        {
                            "text": "Nie rozumiem, <w>czemu</w> on się tak dziwnie zachowuje.",
                            "target_lang_translations": {
                                "ru": "Я не понимаю, <w>почему</w> он так странно себя ведет."
                            }
                        },
                        {
                            "text": "<w>Czemu</w> kupiłeś taki drogi samochód?",
                            "target_lang_translations": {
                                "ru": "<w>Почему</w> ты купил такую дорогую машину?"
                            }
                        }
                    ],
                    "synonyms": [
                        "dlaczego"
                    ],
                    "common_phrases": [
                        "czemu nie",
                        "a czemu",
                        "czemu by nie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительное слово, спрашивающее о причине, поводе или цели действия; то же, что «почему»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почему",
                                "target_lang_sense_clarification": "вопрос о причине"
                            },
                            {
                                "target_lang_word": "отчего",
                                "target_lang_sense_clarification": "по какой причине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f6b882a-5634-3082-bf85-878758c58913",
                    "sense_definition": "Forma celownika zaimka <w>co</w> (odpowiada na pytanie: komu? czemu?).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Czemu</w> się tak uważnie przyglądasz?",
                            "target_lang_translations": {
                                "ru": "К <w>чему</w> ты так внимательно присматриваешься?"
                            }
                        },
                        {
                            "text": "Dzięki <w>czemu</w> udało ci się to naprawić?",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>чему</w> тебе удалось это починить?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzięki czemu",
                        "przeciw czemu",
                        "ku czemu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "celownik (dative)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма дательного падежа местоимения «что» (отвечает на вопрос: чему?)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чему",

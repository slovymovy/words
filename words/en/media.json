                        {
                            "text": "The laptop has dedicated media keys for controlling volume and playback.",
                            "target_lang_translations": {
                                "ru": "У ноутбука есть специальные <w>медиа</w>-клавиши для управления громкостью и воспроизведением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "media player",
                        "media center",
                        "media file"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"мультимедийный\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медиа-",
                                "target_lang_sense_clarification": "первая часть сложных слов"
                            },
                            {
                                "target_lang_word": "мультимедийный",
                                "target_lang_sense_clarification": "полное прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {

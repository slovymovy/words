                            },
                            {
                                "target_lang_word": "тип",
                                "target_lang_sense_clarification": "разговорное, часто с негативным оттенком"
                            },
                            {
                                "target_lang_word": "человек",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]

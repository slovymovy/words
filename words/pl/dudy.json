                    }
                },
                {
                    "sense_id": "2629a1b9-8349-37f9-914f-857686dddd7f",
                    "sense_definition": "Jadalne narządy wewnętrzne zwierząt, takie jak płuca, serce czy wątroba; podroby.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnej kuchni polskiej często przyrządzano <w>dudy</w> na kwaśno.",
                            "target_lang_translations": {
                                "ru": "В старинной польской кухне часто готовили <w>потроха</w> под кислым соусом."
                            }

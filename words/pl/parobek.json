{
    "word_family": [
        "parobeczek",
        "parobas"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a36c4944-c4e3-39af-9d1c-23408f12beaa",
                    "sense_definition": "Najemny pracownik fizyczny wykonujący prace w gospodarstwie rolnym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "worker_subordinate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kilku naszych <w>parobków</w> pobiegło zaraz do pożaru.",
                            "target_lang_translations": {
                                "ru": "Несколько наших <w>батраков</w> тут же побежали на пожар."
                            }
                        },
                        {
                            "text": "Bogaty gospodarz zatrudniał wielu ludzi, a każdy <w>parobek</w> miał swoje zadania w stajni.",
                            "target_lang_translations": {
                                "ru": "Богатый хозяин нанимал много людей, и у каждого <w>батрака</w> были свои задачи в конюшне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wierny parobek",
                        "stary parobek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наемный работник, выполняющий физическую работу в сельском хозяйстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "батрак",
                                "target_lang_sense_clarification": "наемный сельскохозяйственный рабочий"
                            },
                            {
                                "target_lang_word": "работник",
                                "target_lang_sense_clarification": "человек, выполняющий работу по найму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b643d6b-1cf0-3120-a97e-5c200dd545b3",
                    "sense_definition": "Osoba bezkrytycznie wykonująca czyjeś polecenia, wysługująca się komuś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "worker_subordinate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nawet nie zauważyłeś, jak zrobił z ciebie swojego <w>parobka</w>.",
                            "target_lang_translations": {
                                "ru": "Ты даже не заметил, как он сделал из тебя своего <w>прислужника</w>."
                            }
                        },
                        {
                            "text": "Nie zamierzam być twoim <w>parobkiem</w> i robić wszystkiego za ciebie.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь быть твоим <w>слугой</w> и делать всё за тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "sługus",
                        "pachołek"
                    ],
                    "common_phrases": [
                        "traktować jak parobka",
                        "robić za parobka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, беспрекословно выполняющий чьи-то приказы, прислуживающий кому-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прислужник",
                                "target_lang_sense_clarification": "тот, кто угождает кому-либо, выполняет волю другого"
                            },
                            {
                                "target_lang_word": "холуй",
                                "target_lang_sense_clarification": "презрительное название лакея или подхалима"
                            },
                            {
                                "target_lang_word": "слуга",
                                "target_lang_sense_clarification": "человек, выполняющий чьи-то поручения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "967b0fdb-e004-3c69-90d7-f463bd47624b",
                    "sense_definition": "Młody, nieżonaty mężczyzna.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wiejskiej zabawie każdy <w>parobek</w> chciał zatańczyć z najładniejszą panną.",
                            "target_lang_translations": {
                                "ru": "На деревенском гулянье каждый <w>парень</w> хотел потанцевать с самой красивой девушкой."
                            }
                        },
                        {
                            "text": "Ten młody <w>parobek</w> wciąż szuka żony w sąsiedniej wsi.",
                            "target_lang_translations": {
                                "ru": "Этот молодой <w>парень</w> всё ещё ищет жену в соседней деревне."
                            }
                        }
                    ],
                    "synonyms": [
                        "chłopak"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "usage often found in rural dialects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодой неженатый мужчина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парень",
                                "target_lang_sense_clarification": "молодой человек"
                            },
                            {
                                "target_lang_word": "хлопец",
                                "target_lang_sense_clarification": "разговорное или региональное обозначение юноши"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "15f0c9fe-f7b8-3532-8747-49620884ca82",
                    "sense_definition": "Iemand die niet kan zien; een persoon die het gezichtsvermogen mist.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>blinde</w> gebruikte een witte stok om de weg te vinden.",
                            "target_lang_translations": {
                                "ru": "<w>Слепой</w> использовал белую трость, чтобы найти дорогу."
                            }
                        },
                        {
                            "text": "In de bibliotheek zijn speciale boeken voor <w>blinden</w> beschikbaar.",
                            "target_lang_translations": {
                                "ru": "В библиотеке доступны специальные книги для <w>слепых</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "ziende"
                    ],
                    "common_phrases": [
                        "geleidehond voor blinden",
                        "hulp aan blinden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который не может видеть; незрячий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепой",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "незрячий",
                                "target_lang_sense_clarification": "вежливая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10ac4ca2-a09a-35bd-9837-8450c132b69f",
                    "sense_definition": "Een houten of metalen luik dat voor een raam wordt geplaatst om licht of inkijk te weren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor de nacht sloten we de <w>blinden</w> van het oude huis.",
                            "target_lang_translations": {
                                "ru": "На ночь мы закрыли <w>ставни</w> старого дома."
                            }
                        },
                        {
                            "text": "De groene <w>blinden</w> beschermden de kamer tegen de felle zon.",
                            "target_lang_translations": {
                                "ru": "Зеленые <w>ставни</w> защищали комнату от яркого солнца."
                            }
                        }
                    ],
                    "synonyms": [

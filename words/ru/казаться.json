{
    "word_family": [
        "показаться",
        "оказаться"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "684cb26d-d7f7-360e-84cf-f511d8b9bb63",
                    "sense_definition": "Представляться в мыслях, возникать в сознании как предположение, мнение или ощущение.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <кажется>, что мы уже встречались раньше.",
                            "target_lang_translations": {
                                "en": "It <w>seems</w> to me that we have met before."
                            }
                        },
                        {
                            "text": "Иногда нам <кажется>, что время летит слишком быстро.",
                            "target_lang_translations": {
                                "en": "Sometimes it <w>seems</w> to us that time flies too fast."
                            }
                        }
                    ],
                    "synonyms": [
                        "представляться",
                        "сдаваться",
                        "чудиться",
                        "мерещиться"
                    ],
                    "common_phrases": [
                        "мне кажется",
                        "кажется, что",
                        "как мне кажется"
                    ],
                    "target_lang_definitions": {
                        "en": "To come to mind as a supposition, opinion, or feeling; to have the impression that something is true."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seem",
                                "target_lang_sense_clarification": "to give the impression of being; to appear to one's own mind"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to seem; to give a certain impression (often formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd31e126-d15a-3b7d-9632-d22b656cb18b",
                    "sense_definition": "Производить определённое впечатление своим видом или свойствами; выглядеть каким-либо образом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старый дом <казался> заброшенным и пустым.",
                            "target_lang_translations": {
                                "en": "This old house <w>seemed</w> abandoned and empty."
                            }
                        },
                        {
                            "text": "Задача <казалась> простой, но решить её было трудно.",
                            "target_lang_translations": {
                                "en": "The task <w>seemed</w> simple, but it was difficult to solve."
                            }
                        }
                    ],
                    "synonyms": [
                        "выглядеть",
                        "производить впечатление"
                    ],
                    "common_phrases": [
                        "казаться странным",
                        "казаться знакомым",
                        "казаться счастливым",
                        "казаться больше"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce a certain impression through appearance or qualities; to look a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seem",
                                "target_lang_sense_clarification": "to appear to be something based on general impression or evidence"
                            },
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "to have a particular appearance; to give a visual impression"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to give the impression of being"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "zwichnięcie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "294856e0-7cb7-3cec-aef5-6a34bae66e65",
                    "sense_definition": "Uszkodzić staw poprzez nienaturalne przemieszczenie kości względem siebie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas gry w piłkę nożną niefortunnie upadł i <w>zwichnął</w> kostkę.",
                            "target_lang_translations": {
                                "ru": "Во время игры в футбол он неудачно упал и <w>вывихнул</w> лодыжку."
                            }
                        },
                        {
                            "text": "Musisz uważać na oblodzonym chodniku, żeby nie <w>zwichnąć</w> nogi.",
                            "target_lang_translations": {
                                "ru": "Нужно быть осторожным на обледенелом тротуаре, чтобы не <w>вывихнуть</w> ногу."
                            }
                        }
                    ],
                    "synonyms": [
                        "wywichnąć",
                        "wybić"
                    ],
                    "common_phrases": [
                        "zwichnąć nogę",
                        "zwichnąć kostkę",
                        "zwichnąć staw",
                        "zwichnąć rękę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повредить сустав в результате смещения костей (вывих)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывихнуть",
                                "target_lang_sense_clarification": "сустав, конечность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b490e2e0-1574-3034-b735-fcd9fee5e068",
                    "sense_definition": "Spowodować załamanie się czyjejś kariery, planów życiowych lub doprowadzić do życiowej porażki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeden poważny skandal może trwale <w>zwichnąć</w> karierę polityka.",
                            "target_lang_translations": {
                                "ru": "Один серьезный скандал может навсегда <w>сломать</w> карьеру политика."
                            }
                        },
                        {
                            "text": "Nieprzemyślane decyzje finansowe mogą łatwo <w>zwichnąć</w> komuś życie.",
                            "target_lang_translations": {
                                "ru": "Необдуманные финансовые решения могут легко <w>сломать</w> кому-то жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zniszczyć",
                        "zniweczyć",
                        "wywichnąć"
                    ],
                    "common_phrases": [
                        "zwichnąć karierę",
                        "zwichnąć sobie życie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушить чью-то карьеру, жизненные планы или привести к неудаче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сломать",
                                "target_lang_sense_clarification": "жизнь, карьеру"
                            },
                            {
                                "target_lang_word": "разрушить",
                                "target_lang_sense_clarification": "планы, будущее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7df760d-e5a2-31c2-ad1b-39292b6562cf",
                    "sense_definition": "Zniekształcić coś, zmienić na gorsze lub wypaczyć (np. charakter, psychikę).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_distortion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trudne doświadczenia z dzieciństwa mogą <w>zwichnąć</w> psychikę młodego człowieka.",
                            "target_lang_translations": {
                                "ru": "Тяжелый опыт детства может <w>искалечить</w> психику молодого человека."
                            }
                        },
                        {
                            "text": "Zbyt duża władza potrafi <w>zwichnąć</w> charakter nawet uczciwej osoby.",
                            "target_lang_translations": {
                                "ru": "Слишком большая власть может <w>испортить</w> характер даже честного человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypaczyć"
                    ],
                    "common_phrases": [
                        "zwichnąć charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исказить, изменить к худшему (характер, психику)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искалечить",
                                "target_lang_sense_clarification": "психику, душу"
                            },
                            {
                                "target_lang_word": "испортить",
                                "target_lang_sense_clarification": "характер"
                            }
                        ]
                    }
                }
            ]
        }
    ]

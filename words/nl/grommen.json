{
    "word_family": [
        "gegrom",
        "grompot",
        "grommerig"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ff8a4797-6892-3e21-a6b7-5e954eaacb95",
                    "sense_definition": "Een laag, dreigend geluid maken, meestal door een hond of ander dier.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_low_threat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grote waakhond begon gevaarlijk te <w>grommen</w> toen we het hek naderden.",
                            "target_lang_translations": {
                                "ru": "Большая сторожевая собака начала угрожающе <w>рычать</w>, когда мы подошли к воротам."
                            }
                        },
                        {
                            "text": "Er werd <w>gegromd</w> en geblaft, maar tot een gevecht kwam het niet.",
                            "target_lang_translations": {
                                "ru": "Слышалось <w>рычание</w> и лай, но до драки дело не дошло."
                            }
                        }
                    ],
                    "synonyms": [
                        "brommen",
                        "knorren"
                    ],
                    "antonyms": [
                        "piepen",
                        "janken"
                    ],
                    "common_phrases": [
                        "grommen naar",
                        "gevaarlijk grommen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать низкий, угрожающий звук (обычно о собаках или других животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рычать",
                                "target_lang_sense_clarification": "о животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71adc4dc-ec49-3c17-abf7-c7c23e47bf39",
                    "sense_definition": "Op een boze, lage toon iets zeggen of mopperen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_low_threat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude man <w>gromde</w> een verwensing toen hij zijn trein miste.",
                            "target_lang_translations": {
                                "ru": "Старик <w>проворчал</w> проклятие, когда опоздал на поезд."
                            }
                        },
                        {
                            "text": "‘Noem jij dat water? Het lijkt wel slijm!’ <w>gromde</w> hij.",
                            "target_lang_translations": {
                                "ru": "«Ты называешь это водой? Это похоже на слизь!» — <w>прорычал</w> он."
                            }
                        }
                    ],
                    "synonyms": [
                        "mopperen",
                        "snauwen",
                        "brommen"
                    ],
                    "common_phrases": [
                        "iets binnensmonds grommen",
                        "een antwoord grommen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сердито ворчать или произносить что-то низким голосом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ворчать",
                                "target_lang_sense_clarification": "выражать недовольство"
                            },
                            {
                                "target_lang_word": "бурчать",
                                "target_lang_sense_clarification": "говорить неразборчиво и сердито"
                            },
                            {
                                "target_lang_word": "рычать",
                                "target_lang_sense_clarification": "грубо или злобно говорить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевести",
                                "target_lang_sense_clarification": "сотрудника или ученика"
                            },
                            {
                                "target_lang_word": "перенести",
                                "target_lang_sense_clarification": "офис или штаб-квартиру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "996c43e8-856f-3fa3-9dc5-469e4581ee46",
                    "sense_definition": "Przekazać coś (np. dane, prawa, chorobę) na inny obiekt, osobę lub nośnik",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <przenieść> wszystkie ważne pliki na dysk zewnętrzny.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <перенести> все важные файлы на внешний диск."
                            }

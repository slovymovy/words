        "ranchland"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "afe2eb19-979a-3d7a-8467-23a9f69d6de8",
                    "sense_definition": "A large plot of land used for raising cattle, sheep, or other livestock.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My uncle owns a large cattle <w>ranch</w> in Texas.",
                            "target_lang_translations": {
                                "ru": "Мой дядя владеет большим скотоводческим <w>ранчо</w> в Техасе."
                            }
                        },
                        {
                            "text": "Tourists can visit a dude <w>ranch</w> to experience the cowboy lifestyle.",
                            "target_lang_translations": {
                                "ru": "Туристы могут посетить туристическое <w>ранчо</w>, чтобы познакомиться с образом жизни ковбоев."
                            }
                        }
                    ],
                    "synonyms": [
                        "farm"
                    ],
                    "common_phrases": [
                        "cattle ranch",
                        "dude ranch",
                        "ranch hand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой участок земли, используемый для разведения крупного рогатого скота, овец или других животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранчо",
                                "target_lang_sense_clarification": "земельный участок"
                            },
                            {
                                "target_lang_word": "ферма",
                                "target_lang_sense_clarification": "скотоводческое хозяйство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dbead39-d05e-32e0-b624-233ad6b18956",
                    "sense_definition": "A creamy salad dressing made with buttermilk, garlic, onion, herbs, and spices.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She ordered a green salad with <w>ranch</w> on the side.",
                            "target_lang_translations": {
                                "ru": "Она заказала зеленый салат с <w>соусом ранч</w> отдельно."
                            }
                        },
                        {
                            "text": "I love to dip my pizza crust in <w>ranch</w>.",
                            "target_lang_translations": {
                                "ru": "Я люблю макать корочку пиццы в <w>соус ранч</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ranch dressing"
                    ],
                    "common_phrases": [
                        "side of ranch",
                        "ranch dip",
                        "chicken bacon ranch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сливочный соус-заправка для салата с добавлением чеснока, лука и трав."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соус ранч",
                                "target_lang_sense_clarification": "заправка"
                            },
                            {
                                "target_lang_word": "ранч",
                                "target_lang_sense_clarification": "соус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecd67e11-4074-3f8c-95e3-e60919d86237",
                    "sense_definition": "A house or residential property situated on a large plot of land used for livestock.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cowboys returned to the <w>ranch</w> after a long day in the fields.",
                            "target_lang_translations": {
                                "ru": "Ковбои вернулись на <w>ранчо</w> после долгого дня в полях."
                            }
                        },
                        {
                            "text": "They built a modern <w>ranch</w> overlooking the valley.",
                            "target_lang_translations": {
                                "ru": "Они построили современное <w>ранчо</w> с видом на долину."
                            }
                        }
                    ],
                    "synonyms": [
                        "ranch house"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жилой дом или усадьба, расположенные на территории скотоводческого хозяйства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранчо",
                                "target_lang_sense_clarification": "дом"
                            },
                            {
                                "target_lang_word": "усадьба",
                                "target_lang_sense_clarification": "жилое строение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57d77678-d521-3989-b59d-f9556063f77c",
                    "sense_definition": "To manage, operate, or work on a large farm raising livestock.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His family has <w>ranched</w> in this valley for three generations.",
                            "target_lang_translations": {
                                "ru": "Его семья <w>занималась скотоводством</w> в этой долине на протяжении трех поколений."
                            }
                        },
                        {
                            "text": "She <w>ranches</w> cattle for a living and enjoys the outdoor work.",
                            "target_lang_translations": {
                                "ru": "Она <w>разводит</w> скот на жизнь и любит работу на свежем воздухе."
                            }
                        }
                    ],
                    "synonyms": [
                        "farm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управлять большой фермой по разведению скота или работать на ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разводить скот",
                                "target_lang_sense_clarification": "заниматься животноводством"
                            },
                            {
                                "target_lang_word": "управлять ранчо",
                                "target_lang_sense_clarification": "вести хозяйство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

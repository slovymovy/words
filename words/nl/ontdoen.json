    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "960d6205-002e-33e3-8810-b45688007d56",
                    "sense_definition": "Iets of iemand vrijmaken van een omhulsel, toevoeging of ongewenste eigenschap.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verwijderen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De artisjokken werden eerst <w>ontdaan</w> van hun hooi.",
                            "target_lang_translations": {
                                "en": "The artichokes were first <w>stripped</w> of their choke.",
                                "ru": "Артишоки сначала <w>очистили</w> от сердцевины."
                            }
                        },
                        {
                            "text": "De gemeente wil de binnenstad <w>ontdoen</w> van zwerfafval.",
                            "target_lang_translations": {
                                "en": "The municipality wants to <w>rid</w> the city center of litter.",
                                "ru": "Муниципалитет хочет <w>очистить</w> центр города от мусора."
                            }
                        }
                    ],
                    "synonyms": [
                        "strippen",
                        "zuiveren",
                        "bevrijden"
                    ],
                    "antonyms": [
                        "voorzien",
                        "bedekken"
                    ],
                    "common_phrases": [
                        "ontdoen van"
                    ],
                    "target_lang_definitions": {
                        "en": "To free something or someone from a covering, addition, or unwanted property.",
                        "ru": "Освобождать что-либо или кого-либо от оболочки, дополнения или нежелательного свойства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rid",
                                "target_lang_sense_clarification": "to free from something unwanted"
                            },
                            {
                                "target_lang_word": "strip",
                                "target_lang_sense_clarification": "to remove a covering or layer"
                            },
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "to remove obstructions or rubbish"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "очищать",
                                "target_lang_sense_clarification": "от оболочки, грязи или лишнего"
                            },
                            {
                                "target_lang_word": "освобождать",
                                "target_lang_sense_clarification": "делать свободным от чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3260885a-cdb2-3b77-b341-7bbd798b5046",
                    "sense_definition": "Zichzelf losmaken van iets dat men draagt, bezit of als last ervaart (meestal wederkerend gebruikt).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "verwijderen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij binnenkomst <w>ontdeed</w> hij zich direct van zijn natte jas.",
                            "target_lang_translations": {
                                "en": "Upon entering, he immediately <w>divested</w> himself of his wet coat.",
                                "ru": "При входе он сразу же <w>снял</w> с себя мокрое пальто."
                            }
                        },
                        {
                            "text": "De crimineel <w>ontdeed</w> zich snel van het bewijsmateriaal toen de politie arriveerde.",
                            "target_lang_translations": {
                                "en": "The criminal quickly <w>disposed</w> of the evidence when the police arrived.",
                                "ru": "Преступник быстро <w>избавился</w> от улик, когда прибыла полиция."
                            }
                        }
                    ],
                    "synonyms": [
                        "afleggen",
                        "wegdoen",
                        "lozen"
                    ],
                    "antonyms": [
                        "aannemen",
                        "behouden"
                    ],
                    "common_phrases": [
                        "zich ontdoen van"
                    ],
                    "target_lang_definitions": {
                        "en": "To free oneself from something one is wearing, possessing, or experiencing as a burden.",
                        "ru": "Освобождаться от чего-либо, что человек носит, имеет или ощущает как бремя (обычно возвратный глагол)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dispose of",
                                "target_lang_sense_clarification": "to get rid of something possessed"
                            },
                            {
                                "target_lang_word": "divest",
                                "target_lang_sense_clarification": "reflexive: to remove clothing or a burden"
                            },
                            {
                                "target_lang_word": "get rid of",
                                "target_lang_sense_clarification": "general: to free oneself from"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "избавляться",
                                "target_lang_sense_clarification": "от чего-то ненужного, улик или бремени"
                            },
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "одежду или аксессуары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb21af99-4b94-350c-991b-df3b9156ae90",
                    "sense_definition": "Iemand afhelpen van een verkeerd idee, een illusie of een misvatting.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "verwijderen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet je helaas <w>ontdoen</w> van de illusie dat dit proces snel zal gaan.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I must <w>disabuse</w> you of the illusion that this process will be quick.",
                                "ru": "К сожалению, я должен <w>избавить</w> тебя от иллюзии, что этот процесс пройдет быстро."
                            }
                        },
                        {
                            "text": "Het artikel probeert de lezer te <w>ontdoen</w> van hardnekkige vooroordelen.",
                            "target_lang_translations": {
                                "en": "The article attempts to <w>rid</w> the reader of persistent prejudices.",
                                "ru": "Статья пытается <w>избавить</w> читателя от укоренившихся предрассудков."
                            }
                        }
                    ],
                    "synonyms": [
                        "genezen",
                        "bevrijden"
                    ],
                    "common_phrases": [
                        "ontdoen van een illusie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cure or free someone of a wrong idea, an illusion, or a misconception.",
                        "ru": "Избавлять кого-либо от ложной идеи, иллюзии или заблуждения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disabuse",
                                "target_lang_sense_clarification": "of an idea or illusion"
                            },
                            {
                                "target_lang_word": "rid",
                                "target_lang_sense_clarification": "to free from a state or belief"
                            },
                            {
                                "target_lang_word": "cure",
                                "target_lang_sense_clarification": "to heal from a misconception"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "избавлять",
                                "target_lang_sense_clarification": "от иллюзий или предрассудков"
                            },
                            {
                                "target_lang_word": "разубеждать",
                                "target_lang_sense_clarification": "в ошибочном мнении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
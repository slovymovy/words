                {
                    "sense_id": "2c243299-4132-3699-8c90-abf57f43a435",
                    "sense_definition": "De verbogen vorm van <w>groot</w> die gebruikt wordt na onbepaalde voornaamwoorden om een aanzienlijke omvang of belang aan te duiden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Of wat daar voorbijvloog een arend was of een gier weet ik niet, maar het was wel iets <w>groots</w>.",
                            "target_lang_translations": {
                                "ru": "Не знаю, был ли это орел или стервятник, но это было что-то <w>большое</w>."
                            }
                        },
                        {
                            "text": "Zij wil iets <w>groots</w> kopen voor in de nieuwe woonkamer.",
                            "target_lang_translations": {
                                "ru": "Она хочет купить что-то <w>крупное</w> для новой гостиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iets groots",
                        "niets groots",
                        "wat groots"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "partitief"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Склоняемая форма слова <w>groot</w>, используемая после неопределенных местоимений для обозначения значительного размера или важности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "большое",
                                "target_lang_sense_clarification": "о размере (обычно после «что-то»)"
                            },
                            {
                                "target_lang_word": "крупное",
                                "target_lang_sense_clarification": "значительное по величине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4e8ff0f-b525-31e0-aae0-262ef324439e",
                    "sense_definition": "Indrukwekkend, verheven of met veel ambitie en allure opgezet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan woensdag het winnende elftal <w>groots</w> onthalen en huldigen in de stad.",
                            "target_lang_translations": {
                                "ru": "В среду мы собираемся <w>грандиозно</w> встретить и чествовать победившую команду в городе."
                            }
                        },
                        {
                            "text": "Er bestaan <w>grootse</w> plannen voor de vernieuwing van het stadscentrum.",
                            "target_lang_translations": {
                                "ru": "Существуют <w>грандиозные</w> планы по обновлению центра города."
                            }
                        }
                    ],
                    "synonyms": [

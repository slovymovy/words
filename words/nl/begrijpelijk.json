                },
                {
                    "sense_id": "75b589c4-92b4-33b4-80f2-e54f50f88aed",
                    "sense_definition": "Waar men begrip voor kan hebben; logisch of acceptabel in de gegeven situatie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>begrijpelijk</w> dat je moe bent na zo'n lange werkdag.",
                            "target_lang_translations": {
                                "en": "It is <w>understandable</w> that you are tired after such a long workday.",
                                "ru": "<w>Понятно</w>, что ты устал после такого долгого рабочего дня."
                            }
                        },
                        {
                            "text": "Haar boze reactie was volkomen <w>begrijpelijk</w> gezien de omstandigheden.",
                            "target_lang_translations": {
                                "en": "Her angry reaction was completely <w>understandable</w> given the circumstances.",
                                "ru": "Её гневная реакция была вполне <w>понятной</w>, учитывая обстоятельства."
                            }
                        }
                    ],
                    "synonyms": [
                        "aannemelijk",
                        "logisch",
                        "verklaarbaar"
                    ],
                    "antonyms": [
                        "onbegrijpelijk",
                        "onlogisch",
                        "vreemd"
                    ],
                    "common_phrases": [
                        "volkomen begrijpelijk",
                        "heel begrijpelijk"
                    ],
                    "target_lang_definitions": {
                        "en": "Something one can empathize with; logical or acceptable in the given situation.",
                        "ru": "То, что можно понять и принять; логичный или оправданный в данной ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "understandable",

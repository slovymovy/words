{
    "word_family": [
        "hoog",
        "verhogen",
        "verhoging",
        "hoogtepunt",
        "hoogtevrees",
        "hoogtij"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b66d7275-4d9c-33f1-a526-5de10d4d1c70",
                    "sense_definition": "De verticale afstand van de onderkant tot de bovenkant van iets, of het niveau ten opzichte van een referentiepunt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>hoogte</w> van de berg is meer dan tweeduizend meter.",
                            "target_lang_translations": {
                                "en": "The <w>height</w> of the mountain is more than two thousand meters.",
                                "ru": "<w>Высота</w> горы составляет более двух тысяч метров."
                            }
                        },
                        {
                            "text": "Ik ben helaas niet op de <w>hoogte</w> van de laatste ontwikkelingen.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I am not up to <w>date</w> with the latest developments.",
                                "ru": "К сожалению, я не в <w>курсе</w> последних событий."
                            }
                        }
                    ],
                    "antonyms": [
                        "diepte",
                        "laagte"
                    ],
                    "common_phrases": [
                        "op de hoogte",
                        "de hoogte in",
                        "op grote hoogte"
                    ],
                    "target_lang_definitions": {
                        "en": "The vertical distance from the bottom to the top of something, or the level relative to a reference point.",
                        "ru": "Вертикальное расстояние от основания до вершины чего-либо или уровень относительно точки отсчета."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "height",
                                "target_lang_sense_clarification": "vertical distance"
                            },
                            {
                                "target_lang_word": "altitude",
                                "target_lang_sense_clarification": "height above sea level"
                            },
                            {
                                "target_lang_word": "level",
                                "target_lang_sense_clarification": "relative position or height"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "расстояние по вертикали"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79452993-a658-3a1b-ae3b-048fc234adf4",
                    "sense_definition": "Een natuurlijk verhoogd stuk land in het landschap, zoals een heuvel.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanaf de <w>hoogte</w> hadden we een prachtig uitzicht over de vallei.",
                            "target_lang_translations": {
                                "en": "From the <w>height</w> we had a beautiful view over the valley.",
                                "ru": "С <w>возвышенности</w> открывался прекрасный вид на долину."
                            }
                        },
                        {
                            "text": "Het oude kasteel stond op een strategische <w>hoogte</w>.",
                            "target_lang_translations": {
                                "en": "The old castle stood on a strategic <w>height</w>.",
                                "ru": "Старый замок стоял на стратегической <w>высоте</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "laagte"
                    ],
                    "target_lang_definitions": {
                        "en": "A naturally raised piece of land in the landscape, such as a hill.",
                        "ru": "Естественное возвышение рельефа местности, например, холм."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "height",
                                "target_lang_sense_clarification": "high point or hill"
                            },
                            {
                                "target_lang_word": "elevation",
                                "target_lang_sense_clarification": "raised piece of land"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "возвышенность",
                                "target_lang_sense_clarification": "участок местности"
                            },
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "холм, вершина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b6fae2b-aabd-3435-9023-c98e41d5f125",
                    "sense_definition": "De eigenschap van een geluid die bepaald wordt door de frequentie van de trillingen.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zangeres kon de <w>hoogte</w> van de noot niet goed vasthouden.",
                            "target_lang_translations": {
                                "en": "The singer could not hold the <w>pitch</w> of the note well.",
                                "ru": "Певица не могла хорошо удерживать <w>высоту</w> ноты."
                            }
                        },
                        {
                            "text": "Door de spanning in zijn stem veranderde de <w>hoogte</w> van zijn spraak.",
                            "target_lang_translations": {
                                "en": "Due to the tension in his voice, the <w>pitch</w> of his speech changed.",
                                "ru": "Из-за напряжения в голосе изменилась <w>высота</w> его речи."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The property of a sound determined by the frequency of the vibrations.",
                        "ru": "Характеристика звука, определяемая частотой колебаний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pitch",
                                "target_lang_sense_clarification": "of sound or voice"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "звука"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "b9a6ba94-69a9-3395-8727-8b4b44168671",
                    "sense_definition": "To remain loyal, firm, or consistent with a plan or idea.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "persist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you <w>stick</w> to your strategy, you will eventually succeed.",
                            "target_lang_translations": {
                                "ru": "Если вы будете <w>придерживаться</w> своей стратегии, то в конечном итоге добьетесь успеха."
                            }

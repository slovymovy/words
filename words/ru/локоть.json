    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0ff4d1a-90b5-3c9b-9701-e5d393adbfda",
                    "sense_definition": "Место сгиба руки, где плечевая кость соединяется с костями предплечья.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_joint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он поставил <локти> на стол и внимательно слушал собеседника.",
                            "target_lang_translations": {
                                "en": "He put his <w>elbows</w> on the table and listened attentively to his interlocutor."
                            }
                        },
                        {
                            "text": "Я случайно ударился <локтем> о дверной косяк.",
                            "target_lang_translations": {
                                "en": "I accidentally hit my <w>elbow</w> against the doorframe."
                            }
                        }
                    ],
                    "synonyms": [
                        "локтевой сустав"
                    ],
                    "common_phrases": [
                        "опереться на локоть",
                        "согнуть руку в локте",
                        "толкать локтями",
                        "кусать локти"
                    ],
                    "target_lang_definitions": {
                        "en": "The place where the arm bends, where the humerus connects to the forearm bones."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "elbow",
                                "target_lang_sense_clarification": "anatomical joint"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "132b70c7-fc15-3346-a809-d012ced18fc9",
                    "sense_definition": "Часть руки от локтевого сгиба до кисти.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мужчина вежливо взял даму под <локоть>, помогая ей выйти из машины.",
                            "target_lang_translations": {
                                "en": "The man politely took the lady by the <w>elbow</w>, helping her out of the car."
                            }
                        },
                        {
                            "text": "У него были сильные руки, заросшие волосами до самого <локтя>.",
                            "target_lang_translations": {
                                "en": "He had strong arms, covered in hair up to the <w>elbow</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "предплечье"
                    ],
                    "common_phrases": [
                        "взять под локоть",
                        "под локоть"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of the arm from the elbow bend to the hand."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "elbow",
                                "target_lang_sense_clarification": "body part reference"
                            },
                            {
                                "target_lang_word": "forearm",
                                "target_lang_sense_clarification": "anatomical region"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a7c2c68-e09b-3c0c-900a-870a6fc2c32c",
                    "sense_definition": "Часть рукава одежды, которая закрывает локтевой сустав.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На старом пиджаке совсем протёрлись <локти>.",
                            "target_lang_translations": {
                                "en": "The <w>elbows</w> on the old jacket were completely worn through."
                            }
                        },
                        {
                            "text": "Мама пришила кожаную заплатку на <локоть> свитера.",
                            "target_lang_translations": {
                                "en": "Mom sewed a leather patch onto the <w>elbow</w> of the sweater."
                            }
                        }
                    ],
                    "common_phrases": [
                        "протёртый локоть",
                        "заплата на локте",
                        "дырка на локте"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of a sleeve that covers the elbow joint."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "elbow",
                                "target_lang_sense_clarification": "part of clothing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d849323-0645-379b-9bf2-20364a8e5fc8",
                    "sense_definition": "Старинная мера длины, равная длине руки от локтевого сгиба до кончиков пальцев (примерно 45 сантиметров).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Купец отмерил три <локтя> дорогого сукна.",
                            "target_lang_translations": {
                                "en": "The merchant measured out three <w>cubits</w> of expensive cloth."
                            }
                        },
                        {
                            "text": "Глубина ямы составляла около двух <локтей>.",
                            "target_lang_translations": {
                                "en": "The depth of the pit was about two <w>cubits</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "несколько локтей"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical unit of measurement"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An archaic unit of length equal to the length of the arm from the elbow bend to the fingertips (approx. 45 cm)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cubit",
                                "target_lang_sense_clarification": "unit of measurement"
                            },
                            {
                                "target_lang_word": "ell",
                                "target_lang_sense_clarification": "unit of measurement"
                            }
                        ]
                    }
                }
            ]
        },

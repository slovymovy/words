{
    "word_family": [
        "manifold"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "a2315284-6a9c-3023-95bc-1d403eaa8c97",
                    "sense_definition": "Used before a countable noun to indicate a large, indefinite number.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_large",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are <w>many</w> books on the shelf.",
                            "target_lang_translations": {
                                "ru": "На полке <w>много</w> книг."
                            }
                        },
                        {
                            "text": "She has visited <w>many</w> different countries.",
                            "target_lang_translations": {
                                "ru": "Она посетила <w>много</w> разных стран."
                            }
                        }
                    ],
                    "synonyms": [
                        "a lot of",
                        "numerous"
                    ],
                    "antonyms": [
                        "few",
                        "a few#Determiner"
                    ],
                    "common_phrases": [
                        "how many",
                        "many people",
                        "many times",
                        "many thanks",
                        "many years"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед исчисляемым существительным для обозначения большого неопределенного количества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мно́го",
                                "target_lang_sense_clarification": "наречие, обозначающее большое количество"
                            },
                            {
                                "target_lang_word": "мно́гие",
                                "target_lang_sense_clarification": "прилагательное, часто подразумевает «многие из» или «разные»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f83c50df-3695-3288-a8fc-9b44c1ee6b2d",
                    "sense_definition": "Used to indicate, demonstrate, or compare a specific number of people or things, often with words like 'as', 'so', or 'too'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not have as <w>many</w> apples as you do.",
                            "target_lang_translations": {
                                "ru": "У меня не так <w>много</w> яблок, как у тебя."
                            }
                        },
                        {
                            "text": "There were too <w>many</w> people in the small room.",
                            "target_lang_translations": {
                                "ru": "В маленькой комнате было слишком <w>много</w> людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "as many as",
                        "so many",
                        "too many",
                        "this many"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания или сравнения количества, часто со словами 'as', 'so' или 'too'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мно́го",
                                "target_lang_sense_clarification": "в конструкциях сравнения или степени (так много, слишком много)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "54ebd1d9-812a-3306-9cd5-fead6e5bf779",
                    "sense_definition": "A large, indefinite number of people or things.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_large",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Many</w> are called, but few are chosen.",
                            "target_lang_translations": {
                                "ru": "<w>Многие</w> званы, но немногие избраны."
                            }
                        },
                        {
                            "text": "I invited all my friends, and a good <w>many</w> of them came.",
                            "target_lang_translations": {
                                "ru": "Я пригласил всех своих друзей, и <w>многие</w> из них пришли."
                            }
                        }
                    ],
                    "antonyms": [
                        "few"
                    ],
                    "common_phrases": [
                        "many of",
                        "a good many"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое неопределенное количество людей или вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мно́гие",
                                "target_lang_sense_clarification": "местоимение, обозначающее группу людей или предметов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "55f84ad7-8528-357f-b654-a1b7a8956fe6",
                    "sense_definition": "Existing in a large number; numerous.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_large",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The possibilities for our future are <w>many</w>.",
                            "target_lang_translations": {
                                "ru": "Возможности для нашего будущего <w>многочисленны</w>."
                            }
                        },
                        {
                            "text": "His talents were <w>many</w>, but he remained humble.",
                            "target_lang_translations": {
                                "ru": "Его таланты были <w>многочисленны</w>, но он оставался скромным."
                            }
                        }
                    ],
                    "synonyms": [
                        "numerous",
                        "multiple",
                        "manifold"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "predicative"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий в большом количестве; многочисленный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "многочи́сленный",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d9a569eb-b789-33d9-8d14-3608e932ae56",
                    "sense_definition": "The majority of people; the general public or masses.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "the_masses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Democracy must balance the rights of the few against the will of the <w>many</w>.",
                            "target_lang_translations": {
                                "ru": "Демократия должна уравновешивать права меньшинства и волю <w>большинства</w>."
                            }
                        },
                        {
                            "text": "This new policy is designed to benefit the <w>many</w> rather than the elite.",
                            "target_lang_translations": {
                                "ru": "Эта новая политика призвана принести пользу <w>большинству</w>, а не элите."
                            }
                        }
                    ],
                    "synonyms": [
                        "masses",
                        "multitude",
                        "crowd",
                        "populace"
                    ],
                    "common_phrases": [
                        "the many",
                        "the few and the many"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большинство людей; широкая общественность или массы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "большинство́",
                                "target_lang_sense_clarification": "основная масса людей"
                            },
                            {
                                "target_lang_word": "ма́ссы",
                                "target_lang_sense_clarification": "народ, общественность"
                            }
                        ]
                    }
                }
            ]
        },

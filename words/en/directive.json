{
    "word_family": [
        "direction",
        "director",
        "directory",
        "directness",
        "directorate"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93ab9235-d8be-3fd5-8ef5-a804e693ae55",
                    "sense_definition": "An instruction or guideline that indicates how to perform an action or reach a goal.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "instruction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager issued a clear <w>directive</w> regarding the new safety protocols.",
                            "target_lang_translations": {
                                "ru": "Менеджер дал четкое <w>указание</w> касательно новых протоколов безопасности."
                            }
                        },
                        {
                            "text": "Employees must follow the <w>directive</w> to ensure the project is completed on time.",
                            "target_lang_translations": {
                                "ru": "Сотрудники должны следовать <w>распоряжению</w>, чтобы проект был завершен вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "instruction",
                        "guideline",
                        "order",
                        "command"
                    ],
                    "common_phrases": [
                        "issue a directive",
                        "follow a directive",
                        "strict directive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструкция или руководство, указывающее, как выполнить действие или достичь цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указание",
                                "target_lang_sense_clarification": "общее наставление или инструкция"
                            },
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "приказ или требование выполнить действие"
                            },
                            {
                                "target_lang_word": "директива",
                                "target_lang_sense_clarification": "формальное руководящее указание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cce4ffe-be5f-3db1-b82e-73daab2d5c27",
                    "sense_definition": "An authoritative decision from an official body, which may or may not have binding force.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "instruction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government published a new <w>directive</w> concerning environmental protection standards.",
                            "target_lang_translations": {
                                "ru": "Правительство опубликовало новую <w>директиву</w> касательно стандартов охраны окружающей среды."
                            }
                        },
                        {
                            "text": "Hospitals received a <w>directive</w> from the ministry to prepare for the flu season.",
                            "target_lang_translations": {
                                "ru": "Больницы получили <w>директиву</w> от министерства о подготовке к сезону гриппа."
                            }
                        }
                    ],
                    "synonyms": [
                        "decree",
                        "mandate",
                        "edict"
                    ],
                    "common_phrases": [
                        "presidential directive",
                        "policy directive",
                        "binding directive"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Авторитетное решение официального органа, которое может иметь или не иметь обязательную силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директива",
                                "target_lang_sense_clarification": "официальный документ с руководящими указаниями"
                            },
                            {
                                "target_lang_word": "постановление",
                                "target_lang_sense_clarification": "решение коллегиального органа"
                            },
                            {
                                "target_lang_word": "указ",
                                "target_lang_sense_clarification": "решение главы государства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d320813-325c-3e0a-a55c-7724ac91b10e",
                    "sense_definition": "A form of legislative act addressed to the member states of the European Union, binding them to reach certain objectives in their national legislation.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "instruction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The European Union adopted a <w>directive</w> to standardize copyright laws across all member nations.",
                            "target_lang_translations": {
                                "ru": "Европейский Союз принял <w>директиву</w> для стандартизации законов об авторском праве во всех странах-членах."
                            }
                        },
                        {
                            "text": "Each country must implement the <w>directive</w> into its own legal system by the deadline.",
                            "target_lang_translations": {
                                "ru": "Каждая страна должна внедрить эту <w>директиву</w> в свою правовую систему к установленному сроку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "EU directive",
                        "implement a directive"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "European Union law"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид законодательного акта, адресованного государствам-членам Европейского Союза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директива",
                                "target_lang_sense_clarification": "законодательный акт ЕС"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c23f3ade-2f49-3106-b215-022cc33200be",
                    "sense_definition": "A construct in source code that indicates how it should be processed but is not necessarily part of the program to be run.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The programmer added a compiler <w>directive</w> to optimize the code for a specific processor.",
                            "target_lang_translations": {
                                "ru": "Программист добавил <w>директиву</w> компилятора для оптимизации кода под конкретный процессор."
                            }
                        },
                        {
                            "text": "You can use a preprocessor <w>directive</w> to include external files before compilation begins.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать <w>директиву</w> препроцессора для включения внешних файлов перед началом компиляции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "compiler directive",
                        "preprocessor directive"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конструкция в исходном коде, указывающая, как он должен быть обработан, но не обязательно являющаяся частью исполняемой программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директива",
                                "target_lang_sense_clarification": "инструкция для компилятора или препроцессора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c3c4a3a-9d6c-300c-8933-bb2aaa031753",
                    "sense_definition": "The directive case.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In some languages, the <w>directive</w> indicates movement toward a specific location.",
                            "target_lang_translations": {
                                "ru": "В некоторых языках <w>директив</w> указывает на движение к определенному месту."
                            }
                        },
                        {
                            "text": "The student studied the function of the <w>directive</w> in the ancient text.",
                            "target_lang_translations": {
                                "ru": "Студент изучал функцию <w>директива</w> в древнем тексте."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Директивный падеж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директив",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "директивный падеж",
                                "target_lang_sense_clarification": "грамматическая категория"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6cf3e1e9-108e-3ae6-b723-7a1b0c99477c",
                    "sense_definition": "Serving to direct, indicate, or guide; often implying an authoritative or controlling style.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "guiding_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The CEO is known for her <w>directive</w> leadership style, making all major decisions herself.",
                            "target_lang_translations": {
                                "ru": "Генеральный директор известна своим <w>директивным</w> стилем руководства, принимая все важные решения самостоятельно."
                            }
                        },
                        {
                            "text": "Therapists sometimes use a <w>directive</w> approach to help clients change specific behaviors quickly.",
                            "target_lang_translations": {
                                "ru": "Терапевты иногда используют <w>директивный</w> подход, чтобы помочь клиентам быстро изменить определенное поведение."
                            }
                        }
                    ],
                    "synonyms": [
                        "guiding",
                        "controlling",
                        "authoritative"
                    ],
                    "antonyms": [
                        "passive"
                    ],
                    "common_phrases": [
                        "directive leadership",
                        "directive approach",
                        "directive counseling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий для руководства или указания; часто подразумевает авторитарный или контролирующий стиль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директивный",
                                "target_lang_sense_clarification": "не допускающий возражений, властный"
                            },
                            {
                                "target_lang_word": "руководящий",
                                "target_lang_sense_clarification": "направляющий, управляющий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc3ffa26-37ac-35a3-adf7-f43ae6751619",
                    "sense_definition": "Relating to the directive case.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar_adj_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The linguist analyzed the <w>directive</w> suffix used in the local dialect.",
                            "target_lang_translations": {
                                "ru": "Лингвист проанализировал <w>директивный</w> суффикс, используемый в местном диалекте."
                            }
                        },
                        {
                            "text": "This language lacks a distinct <w>directive</w> form for nouns.",
                            "target_lang_translations": {
                                "ru": "В этом языке отсутствует отдельная <w>директивная</w> форма для существительных."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к директивному падежу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директивный",
                                "target_lang_sense_clarification": "относящийся к грамматической категории"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
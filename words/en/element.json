                    ],
                    "synonyms": [
                        "chemical element",
                        "elementary substance"
                    ],
                    "antonyms": [
                        "compound"
                    ],
                    "common_phrases": [
                        "chemical element",
                        "trace element",
                        "periodic table"
                    ],
                    "target_lang_definitions": {

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ae03af5-d118-309c-a2a1-7c166e4a14a3",
                    "sense_definition": "The number of people who attend an event or participate in an activity, especially an election.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>turnout</w> for the general election was higher than predicted.",
                            "target_lang_translations": {
                                "ru": "<w>Явка</w> на всеобщих выборах была выше прогнозируемой."
                            }
                        },
                        {
                            "text": "Despite the rain, there was a good <w>turnout</w> at the outdoor concert.",
                            "target_lang_translations": {
                                "ru": "Несмотря на дождь, на концерте под открытым небом была хорошая <w>посещаемость</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lay-by"
                    ],
                    "common_phrases": [
                        "voter turnout",
                        "high turnout",
                        "low turnout",
                        "poor turnout",
                        "election turnout"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество людей, посетивших мероприятие или принявших участие в деятельности, особенно в выборах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "я́вка",
                                "target_lang_sense_clarification": "на выборы, собрание"
                            },
                            {
                                "target_lang_word": "посеща́емость",
                                "target_lang_sense_clarification": "мероприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a3f3329-b2c1-3b57-b920-f5f668fdb050",
                    "sense_definition": "A widened area on the side of a road where vehicles can stop or let others pass.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We pulled into a <w>turnout</w> to take photos of the scenic valley.",
                            "target_lang_translations": {
                                "ru": "Мы заехали в <w>карман</w>, чтобы сфотографировать живописную долину."
                            }
                        },
                        {
                            "text": "Slow vehicles should use the <w>turnout</w> to allow faster traffic to pass.",
                            "target_lang_translations": {
                                "ru": "Медленные транспортные средства должны использовать <w>карман</w>, чтобы пропустить более быстрый поток."
                            }
                        }
                    ],
                    "synonyms": [
                        "lay-by"
                    ],
                    "common_phrases": [
                        "scenic turnout",
                        "pull into a turnout"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "especially North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Расширенный участок дороги, где транспортные средства могут остановиться или пропустить других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карма́н",
                                "target_lang_sense_clarification": "на дороге"
                            },
                            {
                                "target_lang_word": "разъе́зд",
                                "target_lang_sense_clarification": "место для разъезда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2e469fc-8a0b-3686-a9ae-d9c081ee1a16",
                    "sense_definition": "A mechanical installation on a railway track that allows trains to switch from one track to another.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train slowed down significantly as it approached the <w>turnout</w>.",
                            "target_lang_translations": {
                                "ru": "Поезд значительно замедлил ход при приближении к <w>стрелке</w>."
                            }
                        },
                        {
                            "text": "Engineers are repairing the <w>turnout</w> to ensure smooth switching between lines.",
                            "target_lang_translations": {
                                "ru": "Инженеры ремонтируют <w>стрелочный перевод</w>, чтобы обеспечить плавное переключение между линиями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "railway turnout",
                        "high-speed turnout"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Механическое устройство на железнодорожных путях, позволяющее поездам переходить с одного пути на другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стре́лочный перево́д",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "стре́лка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7edcfbc7-0257-32c3-a107-7efac5d62834",
                    "sense_definition": "The outward rotation of the legs from the hips, used as a fundamental technique in ballet.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dancers must work on their <w>turnout</w> to improve their form and stability.",
                            "target_lang_translations": {
                                "ru": "Танцоры должны работать над своей <w>выворотностью</w>, чтобы улучшить форму и устойчивость."
                            }
                        },
                        {
                            "text": "A perfect <w>turnout</w> requires flexibility in the hips rather than the knees.",
                            "target_lang_translations": {
                                "ru": "Идеальная <w>выворотность</w> требует гибкости в бедрах, а не в коленях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "improve turnout",
                        "good turnout"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "ballet"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разворот ног наружу от бедра, используемый как базовая техника в балете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы́воротность",
                                "target_lang_sense_clarification": "в балете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04ba2b20-ee9b-3db9-9689-93932b712048",
                    "sense_definition": "The act of releasing horses into a pasture or field for exercise and grazing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stable manager is responsible for the daily <w>turnout</w> of the horses.",
                            "target_lang_translations": {
                                "ru": "Управляющий конюшней отвечает за ежедневный <w>выгул</w> лошадей."
                            }
                        },
                        {
                            "text": "Winter <w>turnout</w> can be challenging when the fields are muddy.",
                            "target_lang_translations": {
                                "ru": "Зимний <w>выгул</w> может быть сложным, когда поля грязные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "daily turnout",
                        "morning turnout"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "equestrianism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпуск лошадей на пастбище или в поле для упражнений и выпаса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы́гул",
                                "target_lang_sense_clarification": "лошадей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
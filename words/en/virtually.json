    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8562c707-d227-3583-a389-518fd82ef695",
                    "sense_definition": "Almost entirely or very nearly; lacking only a negligible amount.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>virtually</w> impossible to predict exactly when the earthquake will occur."
                        },
                        {
                            "text": "The two distinct species appear <w>virtually</w> identical to the untrained eye."
                        }
                    ],
                    "synonyms": [
                        "nearly",
                        "practically",
                        "more or less"
                    ],
                    "common_phrases": [
                        "virtually impossible",
                        "virtually certain",
                        "virtually unknown",
                        "virtually identical"
                    ]
                },
                {
                    "sense_id": "8bf5cbd7-bd88-33d6-90e0-2ea4a934c8c5",
                    "sense_definition": "By means of a computer or through a digital network rather than physically.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Since the office is closed, the team will meet <w>virtually</w> to discuss the project."
                        },
                        {
                            "text": "Students can attend the lecture <w>virtually</w> from anywhere in the world."
                        }
                    ],
                    "synonyms": [
                        "digitally"
                    ],
                    "common_phrases": [
                        "meet virtually",
                        "work virtually",
                        "attend virtually"
                    ]
                },
                {
                    "sense_id": "4ab215d8-141f-3641-b7ba-6cbf9b3955cb",
                    "sense_definition": "In effect or for all practical purposes, though not officially or strictly true.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy snow meant that we were <w>virtually</w> prisoners in our own home."
                        },
                        {
                            "text": "With that final goal, the match was <w>virtually</w> over before the whistle blew."
                        }
                    ],
                    "synonyms": [
                        "effectively",
                        "practically",
                        "as good as"
                    ],
                    "common_phrases": [
                        "virtually the same",
                        "virtually a prisoner"
                    ]
                },
                {
                    "sense_id": "75e31d97-1fde-3394-a19a-fe16171806a4",
                    "sense_definition": "Used in mathematics to describe a group or space that has a specific property within a subgroup of finite index.",

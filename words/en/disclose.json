            "senses": [
                {
                    "sense_id": "14589000-f233-3f5c-b53f-10602b931731",
                    "sense_definition": "To make secret or new information known to people.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company refused to <w>disclose</w> the details of the contract to the public.",
                            "target_lang_translations": {
                                "ru": "Компания отказалась <w>раскрывать</w> подробности контракта общественности."
                            }
                        },
                        {
                            "text": "You must <w>disclose</w> information about any medical conditions before joining the sports team.",
                            "target_lang_translations": {
                                "ru": "Вы обязаны <w>раскрыть</w> информацию о любых медицинских противопоказаниях перед вступлением в спортивную команду."
                            }
                        }
                    ],
                    "synonyms": [

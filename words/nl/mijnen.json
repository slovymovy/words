        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ae7d545e-a931-3d53-a6b5-a04e68067eca",
                    "sense_definition": "Delfstoffen uit de grond halen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mining_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit gebied <w>mijnen</w> ze al jaren naar goud en koper.",
                            "target_lang_translations": {
                                "ru": "В этом районе уже много лет <w>добывают</w> золото и медь.",
                                "en": "In this area, they have been <w>mining</w> for gold and copper for years."
                            }
                        },
                        {
                            "text": "Het bedrijf wil lithium gaan <w>mijnen</w> voor de productie van batterijen.",
                            "target_lang_translations": {
                                "ru": "Компания хочет начать <w>добывать</w> литий для производства аккумуляторов.",
                                "en": "The company wants to start <w>mining</w> lithium for the production of batteries."
                            }
                        }
                    ],
                    "synonyms": [
                        "delven",
                        "verwerven",
                        "mineren"
                    ],
                    "common_phrases": [
                        "goud mijnen",
                        "grondstoffen mijnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добывать полезные ископаемые из недр земли.",
                        "en": "To extract resources from the ground."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добывать",
                                "target_lang_sense_clarification": "извлекать ископаемые"
                            },
                            {
                                "target_lang_word": "разрабатывать",
                                "target_lang_sense_clarification": "месторождение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to mine",
                                "target_lang_sense_clarification": "extract resources"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26d2abc8-b583-3d35-a6da-eec1aae0dcc5",
                    "sense_definition": "Verzwakken door er gangen onder te graven en die in te laten storten of met explosieven te laten ontploffen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten probeerden de vestingmuren te <w>mijnen</w> om een bres te slaan.",
                            "target_lang_translations": {
                                "ru": "Солдаты пытались <w>подкопать</w> крепостные стены, чтобы пробить брешь.",
                                "en": "The soldiers tried to <w>undermine</w> the fortress walls to create a breach."
                            }
                        },
                        {
                            "text": "Het moreel van de troepen werd <w>gemijnd</w> door de voortdurende aanvallen.",
                            "target_lang_translations": {
                                "ru": "Боевой дух войск был <w>подорван</w> постоянными атаками.",
                                "en": "The troops' morale was <w>undermined</w> by the constant attacks."
                            }
                        }
                    ],
                    "synonyms": [
                        "ondergraven",
                        "mineren"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "military context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ослаблять путем создания подкопов или использования взрывчатки; подрывать (прям. и перен.).",
                        "en": "To weaken by digging tunnels underneath or using explosives; to undermine."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подрывать",
                                "target_lang_sense_clarification": "разрушать или ослаблять"
                            },
                            {
                                "target_lang_word": "делать подкоп",
                                "target_lang_sense_clarification": "военная тактика"
                            },
                            {
                                "target_lang_word": "минировать",
                                "target_lang_sense_clarification": "закладывать мины"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to undermine",
                                "target_lang_sense_clarification": "weaken or sabotage"
                            },
                            {
                                "target_lang_word": "to sap",
                                "target_lang_sense_clarification": "military engineering"
                            }
                        ]
                    }
                }
            ]
        },

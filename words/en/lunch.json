    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d94ded60-0623-397d-999e-ba086e899962",
                    "sense_definition": "A light meal usually eaten around midday, notably when not as main meal of the day.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meal_midday",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We usually have <w>lunch</w> together at the cafeteria.",
                            "target_lang_translations": {
                                "ru": "Мы обычно <w>обедаем</w> вместе в кафетерии."
                            }
                        },
                        {
                            "text": "She packed a healthy <w>lunch</w> to take to work.",
                            "target_lang_translations": {
                                "ru": "Она собрала полезный <w>обед</w> с собой на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "luncheon"
                    ],
                    "common_phrases": [
                        "have lunch",
                        "eat lunch",
                        "lunch break",
                        "go for lunch",
                        "packed lunch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прием пищи в середине дня"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обед",
                                "target_lang_sense_clarification": "основной прием пищи в середине дня"
                            },
                            {
                                "target_lang_word": "ланч",
                                "target_lang_sense_clarification": "второй завтрак или легкий обед"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baaa2a15-bf37-3466-bed1-c30570d4c734",
                    "sense_definition": "Any small meal, especially one eaten at a social gathering.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "meal_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the meeting, there was a light <w>lunch</w> for the attendees.",
                            "target_lang_translations": {
                                "ru": "После собрания для присутствующих был легкий <w>обед</w>."
                            }
                        },
                        {
                            "text": "They served a cold <w>lunch</w> in the garden.",
                            "target_lang_translations": {
                                "ru": "В саду подали холодный <w>обед</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "light lunch",
                        "buffet lunch"
                    ],
                    "target_lang_definitions": {

                },
                {
                    "sense_id": "aaf3cb5c-c928-38b5-81a7-be8f2a2923ee",
                    "sense_definition": "The general mood or feeling that exists in a place or situation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_mood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant has a warm and inviting <w>atmosphere</w> that guests love.",

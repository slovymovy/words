    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7fc11852-eb5d-336b-910e-62854a8279b8",
                    "sense_definition": "Bardzo wysoka temperatura powietrza lub otoczenia; wielkie gorąco.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W południe <w>żar</w> lał się z nieba, więc zostaliśmy w domu.",
                            "target_lang_translations": {
                                "ru": "В полдень с неба лился <w>зной</w>, поэтому мы остались дома."
                            }
                        },
                        {
                            "text": "Podróżni byli zmęczeni pustynnym <w>żarem</w> i brakiem wody.",
                            "target_lang_translations": {
                                "ru": "Путники устали от пустынного <w>зноя</w> и нехватки воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "upał",
                        "skwar",
                        "spiekota",
                        "gorąco"
                    ],
                    "antonyms": [
                        "mróz"
                    ],
                    "common_phrases": [
                        "żar tropików",
                        "żar leje się z nieba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень высокая температура воздуха или окружающей среды; сильная жара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зной",
                                "target_lang_sense_clarification": "сильная, изнуряющая жара"
                            },
                            {
                                "target_lang_word": "жара",
                                "target_lang_sense_clarification": "высокая температура воздуха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24d84aec-395d-353a-ae8d-5441c4fe217c",
                    "sense_definition": "Tlące się, rozgrzane do czerwoności resztki spalonego drewna lub węgla.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Harcerze piekli ziemniaki w gorącym <w>żarze</w> ogniska.",
                            "target_lang_translations": {
                                "ru": "Скауты пекли картошку в горячих <w>углях</w> костра."
                            }
                        },
                        {
                            "text": "Po wielkim ognisku pozostał tylko stygnący <w>żar</w>.",
                            "target_lang_translations": {
                                "ru": "После большого костра остались только остывающие <w>угли</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zagrzebać w żarze",
                        "czerwony żar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тлеющие, раскаленные докрасна остатки сгоревшего дерева или угля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угли",
                                "target_lang_sense_clarification": "раскаленные остатки костра"
                            },
                            {
                                "target_lang_word": "жар",
                                "target_lang_sense_clarification": "горячие угли, сильное тепло от них"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ec5f678-f098-3ca7-95b0-4b2a70b885f3",
                    "sense_definition": "Silne, gwałtowne emocje lub uczucia, takie jak miłość, nienawiść lub zapał do działania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody poeta recytował swoje wiersze z wielkim <w>żarem</w>.",
                            "target_lang_translations": {
                                "ru": "Молодой поэт читал свои стихи с большим <w>жаром</w>."
                            }
                        },
                        {
                            "text": "W jej oczach widać było <w>żar</w> prawdziwej pasji.",
                            "target_lang_translations": {
                                "ru": "В её глазах был виден <w>жар</w> истинной страсти."
                            }
                        }
                    ],
                    "synonyms": [
                        "spiekota",
                        "ukrop",
                        "gorąco"
                    ],
                    "common_phrases": [
                        "żar miłości",
                        "żar uczuć",
                        "mówić z żarem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильные, бурные эмоции или чувства, такие как любовь, ненависть или рвение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пыл",
                                "target_lang_sense_clarification": "душевный подъем, страстность"
                            },
                            {
                                "target_lang_word": "жар",
                                "target_lang_sense_clarification": "сильное чувство, страсть"
                            }
                        ]
                    }
                }
            ]
        }
    ]

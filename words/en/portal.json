        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2847545-12c5-3323-b9d1-db81dbc1d00f",
                    "sense_definition": "To travel instantly between locations using a magical or technological doorway.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "digital_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player <w>portaled</w> into the enemy base to launch a surprise attack.",
                            "target_lang_translations": {
                                "ru": "Игрок <w>телепортировался</w> на вражескую базу, чтобы начать внезапную атаку."
                            }

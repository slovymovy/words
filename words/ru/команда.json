                    ],
                    "common_phrases": [
                        "дать команду",
                        "по команде",
                        "ждать команды",
                        "выполнять команду"
                    ],
                    "target_lang_definitions": {
                        "en": "A verbal order or signal to start an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "command",
                                "target_lang_sense_clarification": "order or signal"
                            },
                            {
                                "target_lang_word": "order",
                                "target_lang_sense_clarification": "instruction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb6e48b6-4a46-3f5b-9145-8a94d718e5ea",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "14ddb3d0-993c-34ca-bb00-e747bfcef20d",
                    "sense_definition": "Лёгкая женская одежда для верхней части тела, обычно с короткими рукавами или без них.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом она любит носить лёгкий белый <w>топ</w> и шорты.",
                            "target_lang_translations": {
                                "en": "In summer she likes to wear a light white <w>top</w> and shorts."
                            }
                        },
                        {
                            "text": "Для вечеринки она выбрала блестящий <w>топ</w>.",
                            "target_lang_translations": {
                                "en": "She chose a shiny <w>top</w> for the party."
                            }
                        }
                    ],
                    "synonyms": [
                        "топик"
                    ],
                    "common_phrases": [
                        "короткий топ",
                        "летний топ",
                        "надеть топ"
                    ],
                    "target_lang_definitions": {
                        "en": "Light women's clothing for the upper body, usually with short sleeves or sleeveless."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "top",
                                "target_lang_sense_clarification": "clothing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d975fc41-3708-3555-85af-6c34ca8d1473",
                    "sense_definition": "Список лучших, наиболее популярных или важных объектов в какой-либо категории; верхняя позиция в таком списке.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ranking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта новая песня сразу попала в <w>топ</w> музыкального чарта.",
                            "target_lang_translations": {
                                "en": "This new song immediately hit the <w>top</w> of the music chart."
                            }
                        },
                        {
                            "text": "Мы составили <w>топ</w> самых интересных книг этого года.",
                            "target_lang_translations": {
                                "en": "We compiled a <w>top list</w> of the most interesting books of this year."
                            }
                        }
                    ],
                    "common_phrases": [
                        "в топе",
                        "попасть в топ",
                        "топ лучших"
                    ],
                    "target_lang_definitions": {
                        "en": "A list of the best, most popular, or important items in a category; the highest position in such a list."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "top",
                                "target_lang_sense_clarification": "ranking/position"
                            },
                            {
                                "target_lang_word": "top list",
                                "target_lang_sense_clarification": "list of best items"
                            },
                            {
                                "target_lang_word": "charts",
                                "target_lang_sense_clarification": "music ranking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "228d99e0-1751-3c34-8f98-1113f9c8843a",
                    "sense_definition": "Руководитель высшего звена в компании или организации (сокращение от «топ-менеджер»).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На совещании присутствовали все <w>топы</w> нашей фирмы.",
                            "target_lang_translations": {
                                "en": "All the <w>top managers</w> of our firm were present at the meeting."
                            }
                        },
                        {
                            "text": "Зарплаты у <w>топов</w> в этой индустрии очень высокие.",
                            "target_lang_translations": {
                                "en": "Salaries for <w>top executives</w> in this industry are very high."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for топ-менеджер"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A senior executive in a company or organization."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "top manager",
                                "target_lang_sense_clarification": "person"
                            },
                            {
                                "target_lang_word": "top executive",
                                "target_lang_sense_clarification": "person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63491c24-2977-36ef-9262-1f3bc4c9f954",

            "senses": [
                {
                    "sense_id": "6d99ec97-3dfe-3966-9148-7f2d1a6ce9c5",
                    "sense_definition": "wybór lub rozwiązanie, na które można się zdecydować w miejsce innego",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "choice_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rower to doskonała <w>alternatywa</w> dla samochodu w zatłoczonym mieście.",
                            "target_lang_translations": {
                                "ru": "Велосипед — это отличная <w>альтернатива</w> автомобилю в переполненном городе."
                            }
                        },
                        {
                            "text": "Nie podoba mi się ten plan, ale niestety nie mam żadnej sensownej <w>alternatywy</w>.",
                            "target_lang_translations": {
                                "ru": "Мне не нравится этот план, но, к сожалению, у меня нет никакой разумной <w>альтернативы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "opcja",
                        "możliwość",
                        "wybór"
                    ],
                    "common_phrases": [
                        "brak alternatywy",
                        "szukać alternatywy",
                        "jedyna alternatywa",
                        "realna alternatywa"
                    ],
                    "target_lang_definitions": {
                        "ru": "вариант или решение, которое можно выбрать вместо другого"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альтернатива",
                                "target_lang_sense_clarification": "другой вариант"
                            },
                            {
                                "target_lang_word": "вариант",
                                "target_lang_sense_clarification": "опция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2c0a484-be34-336d-8019-9efb787e8123",
                    "sense_definition": "sytuacja konieczności wyboru między dwiema wykluczającymi się możliwościami",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "choice_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stanąłem przed trudną <w>alternatywą</w>: wyjazd za granicę lub pozostanie w kraju.",
                            "target_lang_translations": {
                                "ru": "Я оказался перед сложной <w>альтернативой</w>: уехать за границу или остаться в стране."
                            }
                        },
                        {
                            "text": "Ta przykra <w>alternatywa</w> spędzała mu sen z powiek przez wiele nocy.",
                            "target_lang_translations": {
                                "ru": "Эта неприятная <w>альтернатива</w> не давала ему спать много ночей."
                            }
                        }
                    ],
                    "synonyms": [
                        "dylemat",
                        "wybór"
                    ],
                    "common_phrases": [
                        "stanąć przed alternatywą",
                        "trudna alternatywa",
                        "przykra alternatywa"
                    ],
                    "target_lang_definitions": {
                        "ru": "ситуация необходимости выбора между двумя исключающими друг друга возможностями"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альтернатива",
                                "target_lang_sense_clarification": "необходимость выбора"
                            },
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "между двумя возможностями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c407988-06f6-3be0-a42b-daf219700c59",
                    "sense_definition": "suma logiczna; zdanie złożone, które jest prawdziwe, gdy przynajmniej jedno ze zdań składowych jest prawdziwe",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logic_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W logice matematycznej <w>alternatywa</w> jest fałszywa tylko wtedy, gdy oba zdania są fałszywe.",
                            "target_lang_translations": {
                                "ru": "В математической логике <w>дизъюнкция</w> ложна только тогда, когда оба высказывания ложны."
                            }
                        },
                        {
                            "text": "Nauczyciel wyjaśnił studentom, czym różni się koniunkcja od <w>alternatywy</w>.",
                            "target_lang_translations": {
                                "ru": "Преподаватель объяснил студентам, чем конъюнкция отличается от <w>дизъюнкции</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "suma logiczna",
                        "dysjunkcja"
                    ],
                    "common_phrases": [
                        "alternatywa rozłączna",
                        "alternatywa zwykła",
                        "prawo alternatywy"
                    ],
                    "target_lang_definitions": {
                        "ru": "логическое сложение; сложное высказывание, которое истинно, когда истинно хотя бы одно из составляющих высказываний"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дизъюнкция",

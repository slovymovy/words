    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "102f8ddb-74e4-3747-b2bd-bb66cd74af85",
                    "sense_definition": "Op de onderste verdieping van een huis of gebouw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locatie_laag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn ouders slapen <w>beneden</w> en ik slaap boven.",
                            "target_lang_translations": {
                                "ru": "Мои родители спят <w>внизу</w>, а я сплю наверху."
                            }
                        },
                        {
                            "text": "Is er iemand <w>beneden</w> in de keuken?",
                            "target_lang_translations": {
                                "ru": "Есть кто-нибудь <w>внизу</w> на кухне?"
                            }
                        }
                    ],
                    "antonyms": [
                        "boven"
                    ],
                    "common_phrases": [
                        "naar beneden",
                        "hier beneden"
                    ],
                    "target_lang_definitions": {
                        "ru": "На нижнем этаже дома или здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внизу",
                                "target_lang_sense_clarification": "на нижнем этаже"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae99dffa-17ae-32ba-94df-525790cdf236",
                    "sense_definition": "In neerwaartse richting of op een lagere plaats.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "locatie_laag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water stroomt snel naar <w>beneden</w>.",
                            "target_lang_translations": {
                                "ru": "Вода быстро течет <w>вниз</w>."
                            }
                        },
                        {
                            "text": "Kijk niet naar <w>beneden</w> als je op de ladder staat.",
                            "target_lang_translations": {
                                "ru": "Не смотри <w>вниз</w>, когда стоишь на лестнице."
                            }
                        }
                    ],
                    "antonyms": [
                        "boven"
                    ],
                    "common_phrases": [
                        "van boven naar beneden",
                        "naar beneden gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении вниз или в более низком месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вниз",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "внизу",
                                "target_lang_sense_clarification": "местоположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d3972ae-a1a7-37d8-bccd-0693148445b7",
                    "sense_definition": "In het zuidelijke deel van het land (vooral ten zuiden van de grote rivieren in Nederland).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "locatie_zuid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij vieren elk jaar carnaval <w>beneden</w> de rivieren.",
                            "target_lang_translations": {
                                "ru": "Они каждый год празднуют карнавал <w>к югу</w> от рек."
                            }
                        },
                        {
                            "text": "Mijn familie woont <w>beneden</w> de Moerdijk.",
                            "target_lang_translations": {
                                "ru": "Моя семья живет <w>к югу</w> от Мурдейка."
                            }
                        }
                    ],
                    "antonyms": [
                        "boven"
                    ],
                    "common_phrases": [
                        "beneden de rivieren"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in Netherlands context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В южной части страны (особенно к югу от больших рек в Нидерландах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к югу",
                                "target_lang_sense_clarification": "географическое направление"
                            },
                            {
                                "target_lang_word": "ниже",
                                "target_lang_sense_clarification": "географически ниже"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "8638a4c4-f61a-3fff-8326-463998f67f53",
                    "sense_definition": "Minder dan een bepaalde hoeveelheid, leeftijd of niveau.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "voorzetsel_waarde",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kinderen <w>beneden</w> de twaalf jaar mogen gratis naar binnen.",
                            "target_lang_translations": {
                                "ru": "Дети <w>младше</w> двенадцати лет могут войти бесплатно."
                            }
                        },
                        {
                            "text": "Zijn prestaties bleven ver <w>beneden</w> de verwachtingen.",
                            "target_lang_translations": {
                                "ru": "Его результаты оставались значительно <w>ниже</w> ожиданий."
                            }
                        }
                    ],
                    "antonyms": [
                        "boven"
                    ],
                    "common_phrases": [
                        "beneden de maat",
                        "beneden alle peil",
                        "beneden de achttien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Меньше определенного количества, возраста или уровня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниже",
                                "target_lang_sense_clarification": "уровень или количество"
                            },
                            {
                                "target_lang_word": "младше",
                                "target_lang_sense_clarification": "возраст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c2054b3-2a70-325b-83cc-3705d4073f83",
                    "sense_definition": "Fysiek lager gelegen dan een bepaald punt of object.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "voorzetsel_fysiek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een groot deel van Nederland ligt <w>beneden</w> de zeespiegel.",
                            "target_lang_translations": {
                                "ru": "Большая часть Нидерландов находится <w>ниже</w> уровня моря."
                            }
                        },
                        {
                            "text": "Het vliegtuig vloog ver <w>beneden</w> de wolken.",
                            "target_lang_translations": {
                                "ru": "Самолет летел далеко <w>ниже</w> облаков."
                            }
                        }
                    ],
                    "antonyms": [
                        "boven"
                    ],
                    "common_phrases": [
                        "beneden zeeniveau",
                        "beneden het vriespunt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически ниже определенной точки или объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниже",
                                "target_lang_sense_clarification": "относительно объекта"
                            }
                        ]
                    }
                }
            ]
        },
        {

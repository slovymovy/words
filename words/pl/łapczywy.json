{
    "word_family": [
        "łapczywość",
        "łapczywie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d0525284-8365-386a-8eb8-77d582aa7c2d",
                    "sense_definition": "Pragnący jedzenia lub picia w sposób gwałtowny i nieumiarkowany; świadczący o wielkim łakomstwie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głodny pies rzucał się na miskę i brał wielkie, <w>łapczywe</w> kęsy mięsa.",
                            "target_lang_translations": {
                                "ru": "Голодный пес бросался на миску и хватал большие, <w>жадные</w> куски мяса."
                            }
                        },
                        {
                            "text": "Goście weselni rzucali <w>łapczywe</w> spojrzenia na wnoszony na salę tort.",
                            "target_lang_translations": {
                                "ru": "Свадебные гости бросали <w>жадные</w> взгляды на вносимый в зал торт."
                            }
                        }
                    ],
                    "synonyms": [
                        "łakomy",
                        "zachłanny"
                    ],
                    "antonyms": [
                        "wstrzemięźliwy",
                        "umiarkowany"
                    ],
                    "common_phrases": [
                        "łapczywe kęsy",
                        "łapczywe picie",
                        "łapczywe spojrzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Желающий еды или питья резким и неумеренным образом; свидетельствующий о большом обжорстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жадный",
                                "target_lang_sense_clarification": "до еды, ненасытный"
                            },
                            {
                                "target_lang_word": "прожорливый",
                                "target_lang_sense_clarification": "склонный к обжорству"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "662badaa-ea60-338c-b234-71650f79eaf7",
                    "sense_definition": "Pragnący zdobyć i posiadać jak najwięcej dóbr materialnych; wyrażający chciwość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "greed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>łapczywy</w> charakter sprawił, że dla pieniędzy był gotów poświęcić przyjaźń.",
                            "target_lang_translations": {
                                "ru": "Его <w>алчный</w> характер привел к тому, что ради денег он был готов пожертвовать дружбой."
                            }
                        },
                        {
                            "text": "Kupiec miał <w>łapczywy</w> wzrok, gdy oglądał drogocenne kamienie.",
                            "target_lang_translations": {
                                "ru": "У купца был <w>жадный</w> взгляд, когда он рассматривал драгоценные камни."
                            }
                        }
                    ],
                    "synonyms": [
                        "chciwy",
                        "zachłanny",
                        "pożądliwy"
                    ],
                    "antonyms": [
                        "hojny",
                        "bezinteresowny"
                    ],
                    "common_phrases": [
                        "łapczywy na pieniądze",
                        "łapczywy na zysk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремящийся получить и иметь как можно больше материальных благ; выражающий корыстолюбие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алчный",
                                "target_lang_sense_clarification": "стремящийся к наживе"
                            },
                            {
                                "target_lang_word": "жадный",
                                "target_lang_sense_clarification": "скупой, стремящийся к обогащению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35ab52c8-61b3-39e2-b248-ed6b0c0665c9",
                    "sense_definition": "Wykonywany pospiesznie i gwałtownie, wynikający z nagłej, silnej potrzeby (często o oddechu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wybiegnięciu na powierzchnię nurek wziął pierwszy, <w>łapczywy</w> oddech.",
                            "target_lang_translations": {
                                "ru": "Вынырнув на поверхность, дайвер сделал первый <w>жадный</w> вдох."
                            }
                        },
                        {
                            "text": "Dziewczyna piła wodę, robiąc przy tym głośne, <w>łapczywe</w> łyki.",
                            "target_lang_translations": {
                                "ru": "Девушка пила воду, делая при этом громкие, <w>жадные</w> глотки."
                            }
                        }
                    ],
                    "antonyms": [
                        "spokojny",
                        "powolny"
                    ],
                    "common_phrases": [
                        "łapczywy oddech",
                        "łapczywy haust"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершаемый поспешно и резко, вызванный внезапной, сильной потребностью (часто о дыхании)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жадный",
                                "target_lang_sense_clarification": "о дыхании или глотках"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
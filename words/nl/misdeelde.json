    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ff447e23-1287-39a7-9f22-7e4d127446bc",
                    "sense_definition": "Verbogen vorm van de stellende trap van <w>misdeeld</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>misdeelde</w> kinderen kregen extra ondersteuning op school.",
                            "target_lang_translations": {
                                "ru": "<w>Обездоленные</w> дети получали дополнительную поддержку в школе.",
                                "en": "The <w>underprivileged</w> children received extra support at school."
                            }
                        },
                        {
                            "text": "Zij zette zich jarenlang in voor de <w>misdeelde</w> bevolkingsgroepen in de stad.",
                            "target_lang_translations": {
                                "ru": "Она годами работала ради <w>обездоленных</w> слоёв населения в городе.",
                                "en": "She dedicated herself for years to the <w>disadvantaged</w> population groups in the city."
                            }
                        }
                    ],
                    "common_phrases": [
                        "misdeelde kinderen",
                        "misdeelde regio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склоняемая форма прилагательного misdeeld (обездоленный, неимущий).",
                        "en": "Inflected form of the adjective 'misdeeld' (disadvantaged, underprivileged)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обездоленный",
                                "target_lang_sense_clarification": "лишённый жизненных благ"
                            },
                            {
                                "target_lang_word": "обделённый",
                                "target_lang_sense_clarification": "получивший меньше, чем другие"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "disadvantaged",
                                "target_lang_sense_clarification": "lacking social or economic opportunities"
                            },
                            {
                                "target_lang_word": "underprivileged",
                                "target_lang_sense_clarification": "having less money or fewer opportunities than others"
                            },
                            {
                                "target_lang_word": "deprived",
                                "target_lang_sense_clarification": "suffering a lack of a specified benefit"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6657e994-899f-3f59-9abe-e9f821cfca81",
                    "sense_definition": "Iemand die in het leven weinig kansen, bezittingen of geluk heeft gehad.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stichting zamelt geld in om de <w>misdeelden</w> in de samenleving te helpen.",
                            "target_lang_translations": {
                                "ru": "Фонд собирает деньги, чтобы помочь <w>обездоленным</w> в обществе.",
                                "en": "The foundation collects money to help the <w>underprivileged</w> in society."
                            }
                        },
                        {
                            "text": "Hij voelde zich vaak een <w>misdeelde</w> omdat hij nooit de kans kreeg om te studeren.",
                            "target_lang_translations": {
                                "ru": "Он часто чувствовал себя <w>обделённым</w>, так как у него никогда не было возможности учиться.",
                                "en": "He often felt like a <w>disadvantaged person</w> because he never got the chance to study."
                            }
                        }
                    ],
                    "synonyms": [
                        "minderbedeelde",
                        "verschoppeling",
                        "arme"
                    ],
                    "antonyms": [
                        "bevoorrechte",
                        "rijkaard"
                    ],
                    "common_phrases": [
                        "lot der misdeelden",
                        "hulp aan misdeelden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который имел мало шансов, имущества или счастья в жизни.",
                        "en": "Someone who has had few opportunities, possessions, or happiness in life."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обездоленный",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            },
                            {
                                "target_lang_word": "бедняк",
                                "target_lang_sense_clarification": "неимущий человек"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "underprivileged person",
                                "target_lang_sense_clarification": "someone lacking societal advantages"
                            },
                            {
                                "target_lang_word": "have-not",
                                "target_lang_sense_clarification": "a poor person (informal)"
                            },
                            {
                                "target_lang_word": "unfortunate",
                                "target_lang_sense_clarification": "an unlucky or unhappy person"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "df4562c8-7f76-3fb6-ad4c-ca73e517fbb1",
                    "sense_definition": "Enkelvoud verleden tijd van <w>misdelen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vader <w>misdeelde</w> zijn oudste zoon in het testament.",
                            "target_lang_translations": {
                                "ru": "Отец <w>обделил</w> своего старшего сына в завещании.",
                                "en": "The father <w>shortchanged</w> his eldest son in the will."
                            }
                        },
                        {
                            "text": "Hoewel hij rijk was, <w>misdeelde</w> hij niemand die om hulp vroeg.",
                            "target_lang_translations": {
                                "ru": "Хотя он был богат, он не <w>обделял</w> никого, кто просил о помощи.",
                                "en": "Although he was rich, he <w>shortchanged</w> no one who asked for help."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время единственного числа от глагола misdelen (обделять).",
                        "en": "Singular past tense of 'misdelen' (to shortchange, to deprive)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обделил",
                                "target_lang_sense_clarification": "дал меньше положенного (совершенный вид)"
                            },
                            {
                                "target_lang_word": "обделял",
                                "target_lang_sense_clarification": "давал меньше положенного (несовершенный вид)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "shortchanged",
                                "target_lang_sense_clarification": "gave less than was due or fair"
                            },
                            {
                                "target_lang_word": "deprived",
                                "target_lang_sense_clarification": "denied the possession or use of something"
                            },
                            {
                                "target_lang_word": "disinherited",
                                "target_lang_sense_clarification": "excluded from inheritance (specific context)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f060046-732d-3206-9259-88d4bdd0a4b5",
                    "sense_definition": "Verbogen vorm van <w>misdeeld</w>, voltooid deelwoord van <w>misdelen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De door de natuur <w>misdeelde</w> man zocht zijn toevlucht in de kunst.",
                            "target_lang_translations": {
                                "ru": "<w>Обделённый</w> природой человек искал утешения в искусстве.",
                                "en": "The man <w>ill-favored</w> by nature sought refuge in art."
                            }
                        },
                        {
                            "text": "Het was een pijnlijk besef voor de <w>misdeelde</w> erfgenamen.",
                            "target_lang_translations": {
                                "ru": "Это было болезненное осознание для <w>обделённых</w> наследников.",
                                "en": "It was a painful realization for the <w>shortchanged</w> heirs."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Склоняемая форма причастия прошедшего времени от misdelen (обделённый).",
                        "en": "Inflected form of 'misdeeld', past participle of 'misdelen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обделённый",
                                "target_lang_sense_clarification": "которому чего-то не досталось"
                            },
                            {
                                "target_lang_word": "обиженный",
                                "target_lang_sense_clarification": "в контексте судьбы или природы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "shortchanged",
                                "target_lang_sense_clarification": "treated unfairly regarding distribution"
                            },
                            {
                                "target_lang_word": "ill-favored",
                                "target_lang_sense_clarification": "lacking beauty or natural gifts (often with 'by nature')"
                            },
                            {
                                "target_lang_word": "deprived",
                                "target_lang_sense_clarification": "kept from possessing something"
                            }
                        ]
                    }
                }
            ]
        }
    ]

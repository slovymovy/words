{
    "word_family": [
        "arrangement",
        "arranger",
        "rearrange",
        "prearrange",
        "rearrangement"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "10811b9f-b60b-345b-9fb0-239c21234b95",
                    "sense_definition": "To put objects or details into a particular order, position, or sequence.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She likes to <w>arrange</w> fresh flowers in a vase on the table.",
                            "target_lang_translations": {
                                "ru": "Она любит <w>расставлять</w> свежие цветы в вазе на столе."
                            }
                        },
                        {
                            "text": "Please <w>arrange</w> the books on the shelf in alphabetical order.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>расставь</w> книги на полке в алфавитном порядке."
                            }
                        }
                    ],
                    "synonyms": [
                        "order",
                        "organize",
                        "array",
                        "set up",
                        "range",
                        "classify"
                    ],
                    "common_phrases": [
                        "arrange in order",
                        "arrange the furniture",
                        "arrange alphabetically"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размещать объекты или детали в определенном порядке, положении или последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расставлять",
                                "target_lang_sense_clarification": "размещать предметы в определенном порядке"
                            },
                            {
                                "target_lang_word": "располагать",
                                "target_lang_sense_clarification": "размещать в пространстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "739afc1d-62bc-37ca-9b68-6685150b44b9",
                    "sense_definition": "To plan or prepare for an event, meeting, or activity in advance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>arrange</w> a meeting with the director for next week.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>организовать</w> встречу с директором на следующей неделе."
                            }
                        },
                        {
                            "text": "They managed to <w>arrange</w> for a taxi to pick us up at the airport.",
                            "target_lang_translations": {
                                "ru": "Им удалось <w>организовать</w> такси, чтобы нас забрали из аэропорта."
                            }
                        }
                    ],
                    "synonyms": [
                        "organize",
                        "set up"
                    ],
                    "common_phrases": [
                        "arrange a meeting",
                        "arrange a time",
                        "arrange for",
                        "arrange to meet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Планировать или подготавливать событие, встречу или мероприятие заранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "организовывать",
                                "target_lang_sense_clarification": "подготавливать и осуществлять"
                            },
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "налаживать или организовывать событие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fe3f491-b044-3f8b-831b-bf5b0c5ac9ff",
                    "sense_definition": "To adapt a musical composition for a specific instrument, voice, or performance style.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The composer was asked to <w>arrange</w> the folk song for a jazz orchestra.",
                            "target_lang_translations": {
                                "ru": "Композитора попросили <w>аранжировать</w> народную песню для джазового оркестра."
                            }
                        },
                        {
                            "text": "He can <w>arrange</w> the melody for the piano and violin.",
                            "target_lang_translations": {
                                "ru": "Он может <w>аранжировать</w> мелодию для фортепиано и скрипки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "arrange for piano",
                        "arrange a song"
                    ],
                    "target_lang_definitions": {
                        "ru": "Адаптировать музыкальное произведение для конкретного инструмента, голоса или стиля исполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аранжировать",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a1f9a281-90d7-35d6-8665-b2a9ad115f6e",
                    "sense_definition": "A rearranged version or rendition of a musical song.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band played a new <w>arrange</w> of their classic hit during the concert.",
                            "target_lang_translations": {
                                "ru": "Группа сыграла новую <w>аранжировку</w> своего классического хита во время концерта."
                            }
                        },
                        {
                            "text": "This <w>arrange</w> gives the track a completely different atmosphere.",
                            "target_lang_translations": {
                                "ru": "Эта <w>аранжировка</w> придает треку совершенно иную атмосферу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переработанная версия или исполнение музыкальной композиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аранжировка",
                                "target_lang_sense_clarification": "музыкальная обработка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "synonyms": [
                        "trudny",
                        "ciężki"
                    ],
                    "common_phrases": [
                        "stan krytyczny",
                        "sytuacja krytyczna",
                        "w stanie krytycznym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень трудный, тяжелый, часто угрожающий жизни или безопасности."

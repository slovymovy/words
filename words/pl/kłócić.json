    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15d4725c-fcb6-3d6b-8947-1e598cac47f7",
                    "sense_definition": "Wymieniać z kimś ostre uwagi, być w sporze z powodu różnicy zdań; czasownik w tym znaczeniu wymaga użycia zaimka zwrotnego „się”",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conflict_verbal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi sąsiedzi często <kłócą> się o głośną muzykę wieczorami.",
                            "target_lang_translations": {
                                "ru": "Мои соседи часто <ссорятся> из-за громкой музыки по вечерам."
                            }
                        },
                        {
                            "text": "Nie warto <kłócić> się z rodziną o politykę przy świątecznym stole.",
                            "target_lang_translations": {
                                "ru": "Не стоит <ругаться> с семьей о политике за праздничным столом."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprzeczać się",
                        "awanturować się",
                        "czubić się",
                        "drzeć koty"
                    ],
                    "antonyms": [
                        "godzić się",
                        "zgadzać się"
                    ],
                    "common_phrases": [
                        "kłócić się o drobiazgi",
                        "kłócić się jak pies z kotem",
                        "kłócić się o wszystko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спорить, вступать в конфликт, обмениваться резкими замечаниями (требует возвратного местоимения «się»)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ссориться",
                                "target_lang_sense_clarification": "вступать в ссору, конфликтовать"
                            },
                            {
                                "target_lang_word": "ругаться",
                                "target_lang_sense_clarification": "браниться, выяснять отношения на повышенных тонах"
                            },
                            {
                                "target_lang_word": "спорить",
                                "target_lang_sense_clarification": "выражать несогласие, дискутировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2878914f-fef0-3e30-a204-2eae6f6273e4",
                    "sense_definition": "Nie pasować do siebie, pozostawać w sprzeczności lub dysharmonii (o kolorach, faktach, poglądach); używane z zaimkiem zwrotnym „się”",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten jaskrawy kolor ścian <kłóci> się z twoimi spokojnymi meblami.",
                            "target_lang_translations": {
                                "ru": "Этот яркий цвет стен <диссонирует> с твоей спокойной мебелью."
                            }
                        },
                        {
                            "text": "Jego wersja wydarzeń wyraźnie <kłóci> się z dowodami zebranymi przez policję.",
                            "target_lang_translations": {
                                "ru": "Его версия событий явно <противоречит> доказательствам, собранным полицией."
                            }
                        }
                    ],
                    "synonyms": [
                        "gryźć się",
                        "sprzeczać się",
                        "sprzeczać się"
                    ],
                    "antonyms": [
                        "współgrać",
                        "pasować",
                        "harmonizować"
                    ],
                    "common_phrases": [
                        "kłócić się z faktami",
                        "kłócić się z logiką",
                        "kłócić się z poczuciem estetyki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не подходить друг другу, находиться в противоречии или дисгармонии (о цветах, фактах, взглядах)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противоречить",
                                "target_lang_sense_clarification": "о фактах, мнениях, логике"
                            },
                            {
                                "target_lang_word": "диссонировать",
                                "target_lang_sense_clarification": "о цветах, звуках, стиле"
                            },
                            {
                                "target_lang_word": "не сочетаться",
                                "target_lang_sense_clarification": "не подходить друг другу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4476064-e684-34b1-86f2-8d3b2cbfac52",
                    "sense_definition": "Doprowadzać do niezgody między ludźmi, nastawiać kogoś wrogo przeciwko innej osobie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conflict_causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Plotki i pomówienia potrafią skutecznie <kłócić> nawet najlepszych przyjaciół.",
                            "target_lang_translations": {
                                "ru": "Сплетни и наговоры могут эффективно <ссорить> даже лучших друзей."
                            }
                        },
                        {
                            "text": "Nie powinieneś <kłócić> braci ze sobą, opowiadając im kłamstwa.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <ссорить> братьев друг с другом, рассказывая им ложь."
                            }
                        }
                    ],
                    "synonyms": [
                        "skłócać",
                        "drzeć koty",
                        "iść na udry"
                    ],
                    "antonyms": [
                        "godzić",
                        "jednać"
                    ],
                    "common_phrases": [
                        "kłócić ludzi ze sobą",
                        "kłócić przyjaciół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вносить разлад, настраивать людей друг против друга"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ссорить",
                                "target_lang_sense_clarification": "вызывать вражду между кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01e7ae6c-fa4b-3577-8f94-69a3e1a1451b",
                    "sense_definition": "Zakłócać spokój, wprowadzać zamieszanie lub niepokój",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "disturbance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie śmiałbym <kłócić> twojego spokoju w tak ważnej chwili.",
                            "target_lang_translations": {
                                "ru": "Я не посмел бы <тревожить> твой покой в столь важный момент."
                            }
                        },
                        {
                            "text": "Zły sen <kłócił> jego nocny odpoczynek, budząc go co godzinę.",
                            "target_lang_translations": {
                                "ru": "Дурной сон <нарушал> его ночной отдых, пробуждая его каждый час."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "użycie literackie, obecnie rzadkie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушать покой или тишину (устаревшее, книжное)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тревожить",
                                "target_lang_sense_clarification": "беспокоить"
                            },
                            {
                                "target_lang_word": "нарушать",
                                "target_lang_sense_clarification": "прерывать состояние покоя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2763aad5-be38-3ba5-b313-d98cdce4dd7d",
                    "sense_definition": "Ubijać śmietanę w celu uzyskania masła",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "regional_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni zaczęła <kłócić> masło w drewnianej maselnicy z samego rana.",
                            "target_lang_translations": {
                                "ru": "Хозяйка начала <сбивать> масло в деревянной маслобойке с самого утра."
                            }
                        },
                        {
                            "text": "Na wsi rzadko kto już potrafi ręcznie <kłócić> śmietanę.",
                            "target_lang_translations": {
                                "ru": "В деревне редко кто уже умеет вручную <сбивать> сметану."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Górny Śląsk"
                        },
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбивать сметану или сливки в масло (региональное)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивать",
                                "target_lang_sense_clarification": "делать масло"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "szczurek",
        "szczurzyca",
        "szczurzy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "efa159c0-5c61-3bc7-bff0-cca1e3289b8a",
                    "sense_definition": "Gryzoń z rodziny myszowatych, wszystkożerny, często występujący w pobliżu siedzib ludzkich i uważany za szkodnika.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W piwnicy naszego starego domu zamieszkał duży, szary <w>szczur</w>.",
                            "target_lang_translations": {
                                "ru": "В подвале нашего старого дома поселилась большая серая <w>крыса</w>."
                            }
                        },
                        {
                            "text": "Ten ciągły wyścig <w>szczurów</w> w korporacji bardzo mnie męczy.",
                            "target_lang_translations": {
                                "ru": "Эти постоянные <w>крысиные</w> бега в корпорации меня очень утомляют."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyścig szczurów",
                        "zmokły jak szczur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грызун из семейства мышиных, всеядный, часто обитающий вблизи человеческого жилья и считающийся вредителем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыса",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54c3f75b-597b-3ec2-9af6-b3f50397afe0",
                    "sense_definition": "Torebka herbaty ekspresowej ze sznurkiem i etykietą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mam czasu na parzenie liściastej herbaty, zaleję sobie <w>szczura</w>.",
                            "target_lang_translations": {
                                "ru": "У меня нет времени заваривать листовой чай, я залью себе <w>пакетик</w>."
                            }
                        },
                        {
                            "text": "W kubku na stole został tylko zimny, nasiąknięty <w>szczur</w>.",
                            "target_lang_translations": {
                                "ru": "В кружке на столе остался только холодный, размокший <w>пакетик</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zaparzyć szczura"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "potocznie o torebce herbaty"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пакетик чая с ниткой и этикеткой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чайный пакетик",
                                "target_lang_sense_clarification": "пакетированный чай"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7e58c7d7-6945-31fc-a18c-91c279eeb63a",
                    "sense_definition": "Nazwisko polskie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Szczur</w> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Щур</w> был избран председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie spotkałem nikogo o nazwisku <w>Szczur</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не встречал никого по фамилии <w>Щур</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Щур",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
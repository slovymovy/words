                        "bound"
                    ],
                    "common_phrases": [
                        "ungathered sheets",
                        "ungathered signatures"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к печатным листам, которые не были сложены в правильном порядке для переплета."
                    },
                    "translations": {
                        "ru": [

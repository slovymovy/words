            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8c015568-2060-35fb-a912-3c15799eab7f",
                    "sense_definition": "Bardzo ciężko i intensywnie pracować, często ponad siły",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nowej firmie muszę <w>zapierdalać</w> po dwanaście godzin dziennie.",
                            "target_lang_translations": {
                                "ru": "В новой фирме мне приходится <w>ебашить</w> по двенадцать часов в день."
                            }
                        },
                        {
                            "text": "Mój ojciec całe życie <w>zapierdalał</w> na budowie, żeby utrzymać rodzinę.",
                            "target_lang_translations": {
                                "ru": "Мой отец всю жизнь <w>пахал</w> на стройке, чтобы содержать семью."
                            }
                        }
                    ],
                    "synonyms": [
                        "harować",
                        "tyrać",
                        "zapieprzać",
                        "zasuwać"
                    ],
                    "antonyms": [
                        "obijać się",
                        "leżeć do góry brzuchem",
                        "lenić się"
                    ],
                    "common_phrases": [
                        "zapierdalać jak wół",
                        "zapierdalać za grosze",
                        "zapierdalać na kogoś"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgaryzm"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

                },
                {
                    "sense_id": "09e9037b-552d-39fc-b4be-a8f7ec92bcc9",
                    "sense_definition": "An act of smoking tobacco.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am going outside for a quick <w>smoke</w>.",
                            "target_lang_translations": {
                                "ru": "Я выхожу на улицу на быстрый <w>перекур</w>."
                            }
                        },
                        {
                            "text": "She decided to have a <w>smoke</w> before the meeting started.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>покурить</w> перед началом встречи."
                            }
                        }
                    ],
                    "common_phrases": [

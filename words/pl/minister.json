    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac5cbf0e-db52-33aa-ae54-1c805e04c217",
                    "sense_definition": "urzędnik państwowy wysokiego szczebla, członek rządu kierujący pracą ministerstwa lub wypełniający inne zadania powierzone przez premiera",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy <w>minister</w> finansów zapowiedział obniżkę podatków dla przedsiębiorców.",
                            "target_lang_translations": {
                                "ru": "Новый <w>министр</w> финансов объявил о снижении налогов для предпринимателей."
                            }
                        },
                        {
                            "text": "Premier i <w>ministrowie</w> spotkali się na nadzwyczajnym posiedzeniu rządu.",
                            "target_lang_translations": {
                                "ru": "Премьер и <w>министры</w> встретились на чрезвычайном заседании правительства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Rada Ministrów",
                        "teka ministra",
                        "dymisja ministra",
                        "minister spraw zagranicznych"
                    ],
                    "target_lang_definitions": {
                        "ru": "высокопоставленный государственный чиновник, член правительства, возглавляющий министерство или выполняющий другие задачи, порученные премьер-министром"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "министр",
                                "target_lang_sense_clarification": "член правительства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4bfb040-2885-367a-b91a-cc8519f08527",
                    "sense_definition": "kobieta sprawująca urząd ministra",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "political_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pani <w>minister</w> udzieliła wczoraj wywiadu telewizji publicznej.",
                            "target_lang_translations": {
                                "ru": "Госпожа <w>министр</w> вчера дала интервью общественному телевидению."
                            }
                        },
                        {
                            "text": "Jako <w>minister</w> edukacji wprowadziła wiele zmian w szkołach podstawowych.",
                            "target_lang_translations": {
                                "ru": "Как <w>министр</w> образования, она внесла много изменений в начальных школах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pani minister"
                    ],
                    "target_lang_definitions": {
                        "ru": "женщина, занимающая пост министра"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "министр",
                                "target_lang_sense_clarification": "женщина-политик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ce8693d-e2ed-322b-aa6f-2d3bcc4589fb",
                    "sense_definition": "duchowny w kościołach protestanckich uprawniony do odprawiania nabożeństw",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowy <w>minister</w> wygłosił poruszające kazanie podczas niedzielnego nabożeństwa.",
                            "target_lang_translations": {
                                "ru": "Местный <w>пастор</w> произнес трогательную проповедь во время воскресного богослужения."
                            }
                        },
                        {
                            "text": "Wspólnota wybrała nowego <w>ministra</w>, który będzie przewodził zborowi.",
                            "target_lang_translations": {
                                "ru": "Община выбрала нового <w>пастора</w>, который будет руководить собранием."
                            }
                        }
                    ],
                    "synonyms": [
                        "predykant"
                    ],
                    "common_phrases": [
                        "minister słowa bożego"
                    ],
                    "target_lang_definitions": {
                        "ru": "священнослужитель в протестантских церквях, уполномоченный проводить богослужения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пастор",
                                "target_lang_sense_clarification": "протестантский священник"
                            },
                            {
                                "target_lang_word": "служитель",
                                "target_lang_sense_clarification": "церковный чин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53e833a9-9557-3cf7-8a95-92f46397ca32",
                    "sense_definition": "w dawnej Polsce urzędnik dworski lub zarządca majątku ziemskiego",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierny <w>minister</w> dbał o finanse i zapasy w spichlerzach dworskich.",
                            "target_lang_translations": {
                                "ru": "Верный <w>управляющий</w> заботился о финансах и запасах в придворных амбарах."
                            }
                        },
                        {
                            "text": "Król wysłał swojego zaufanego <w>ministra</w>, aby doglądał budowy nowej twierdzy.",
                            "target_lang_translations": {
                                "ru": "Король послал своего доверенного <w>сановника</w>, чтобы присматривать за строительством новой крепости."
                            }
                        }
                    ],
                    "synonyms": [
                        "oficjalista",
                        "klucznik",
                        "szafarz",
                        "włodarz"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy dawnych urzędów i funkcji dworskich"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "в старой Польше придворный чиновник или управляющий земельным имением"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управляющий",
                                "target_lang_sense_clarification": "заведующий хозяйством"
                            },
                            {
                                "target_lang_word": "эконом",
                                "target_lang_sense_clarification": "хозяйственный чиновник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7314a0c2-c2f9-35e1-8196-7e5fa93c85b0",

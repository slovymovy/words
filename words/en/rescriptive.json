{
    "word_family": [
        "rescript",
        "rescribe",
        "rescription"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "52e6d982-b7ec-3486-a4c3-24f74712f70b",
                    "sense_definition": "Relating to a rescript, which is an official decree, order, or answer issued by a ruler or authority.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_decree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The emperor issued a <w>rescriptive</w> order to settle the land dispute.",
                            "target_lang_translations": {
                                "ru": "Император издал <w>рескриптный</w> указ для разрешения земельного спора."
                            }
                        },
                        {
                            "text": "They claimed a <w>rescriptive</w> right to manage the territory based on the old document.",
                            "target_lang_translations": {
                                "ru": "Они заявили о <w>рескриптном</w> праве управлять территорией на основании старого документа."
                            }
                        }
                    ],
                    "synonyms": [
                        "decretal"
                    ],
                    "common_phrases": [
                        "rescriptive right",
                        "rescriptive denunciation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к рескрипту — официальному указу, распоряжению или ответу правителя или органа власти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рескриптный",
                                "target_lang_sense_clarification": "относящийся к указам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf8f3ce4-c0dc-302e-a48b-2f2f7cc7ddd8",
                    "sense_definition": "Involving the act of rewriting, revising, or converting text into a new form.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rewriting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author's <w>rescriptive</w> process transformed the rough draft into a masterpiece.",
                            "target_lang_translations": {
                                "ru": "Авторский процесс <w>переписывания</w> превратил черновик в шедевр."
                            }
                        },
                        {
                            "text": "This software has a <w>rescriptive</w> function that automatically updates old files.",
                            "target_lang_translations": {
                                "ru": "У этого программного обеспечения есть функция <w>перезаписи</w>, которая автоматически обновляет старые файлы."
                            }
                        }
                    ],
                    "synonyms": [
                        "revisory",
                        "redactory"
                    ],
                    "common_phrases": [
                        "rescriptive force",
                        "rescriptive action"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с процессом переписывания, редактирования или преобразования текста в новую форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переписывающий",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "редакционный",
                                "target_lang_sense_clarification": "правка текста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29dfa60e-1e36-3f66-979f-cadc66f87890",
                    "sense_definition": "Pertaining to the reinterpretation of meaning, often allowing for multiple understandings of a text or concept.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "interpretation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor used a <w>rescriptive</w> method to analyze the poem's hidden themes.",
                            "target_lang_translations": {
                                "ru": "Профессор использовал метод <w>переосмысления</w> для анализа скрытых тем стихотворения."
                            }
                        },
                        {
                            "text": "Her approach is <w>rescriptive</w>, offering new meanings to traditional symbols.",
                            "target_lang_translations": {
                                "ru": "Её подход является <w>переосмысляющим</w>, предлагая новые значения традиционным символам."
                            }
                        }
                    ],
                    "synonyms": [
                        "hermeneutic",
                        "interpretive"
                    ],
                    "common_phrases": [
                        "rescriptive model",
                        "rescriptive grammar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к переосмыслению значения, часто допускающий множественное толкование текста или концепции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переосмысляющий",
                                "target_lang_sense_clarification": "интерпретация"
                            },
                            {
                                "target_lang_word": "интерпретационный",
                                "target_lang_sense_clarification": "толкование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
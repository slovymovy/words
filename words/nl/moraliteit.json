{
    "word_family": [
        "moreel",
        "moraal",
        "moralist",
        "moralisme",
        "moraliseren",
        "immoreel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ef468ab-2ea8-37ab-807b-982b752ff69d",
                    "sense_definition": "Het geheel van normen en waarden die bepalen wat goed en slecht gedrag is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ethics_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>moraliteit</w> van zijn beslissing werd door velen in twijfel getrokken.",
                            "target_lang_translations": {
                                "ru": "Многие ставили под сомнение <w>моральность</w> его решения."
                            }
                        },
                        {
                            "text": "In de politiek lijkt <w>moraliteit</w> soms minder belangrijk dan macht en invloed.",
                            "target_lang_translations": {
                                "ru": "В политике <w>мораль</w> иногда кажется менее важной, чем власть и влияние."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dubbele moraliteit",
                        "publieke moraliteit",
                        "christelijke moraliteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность норм и ценностей, определяющих хорошее и плохое поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мораль",
                                "target_lang_sense_clarification": "система норм"
                            },
                            {
                                "target_lang_word": "нравственность",
                                "target_lang_sense_clarification": "внутренние принципы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6df840d9-90bf-3dd6-af5d-c2bd24de3798",
                    "sense_definition": "Gedrag dat overeenkomt met wat als goed en deugdzaam wordt beschouwd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ethics_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gebrek aan <w>moraliteit</w> in de financiële sector zorgde voor veel publieke woede.",
                            "target_lang_translations": {
                                "ru": "Отсутствие <w>нравственности</w> в финансовом секторе вызвало большой общественный гнев."
                            }
                        },
                        {
                            "text": "Zij toonde een grote <w>moraliteit</w> door eerlijk te blijven in die lastige situatie.",
                            "target_lang_translations": {
                                "ru": "Она проявила высокую <w>нравственность</w>, оставшись честной в той сложной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "zedelijkheid"
                    ],
                    "common_phrases": [
                        "gebrek aan moraliteit",
                        "hoge moraliteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поведение, соответствующее тому, что считается хорошим и добродетельным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нравственность",
                                "target_lang_sense_clarification": "добродетельность"
                            },
                            {
                                "target_lang_word": "моральность",
                                "target_lang_sense_clarification": "соответствие морали"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c68e4b9f-31b7-3997-86da-cdd4d23951a2",
                    "sense_definition": "Een middeleeuws toneelstuk met een opvoedend karakter waarin deugden en ondeugden als personen optreden.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "theater",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elckerlijc is de bekendste <w>moraliteit</w> uit de Nederlandse literatuurgeschiedenis.",
                            "target_lang_translations": {
                                "ru": "«Элкерлик» — самое известное <w>моралите</w> в истории нидерландской литературы."
                            }
                        },
                        {
                            "text": "In een <w>moraliteit</w> wordt de mens vaak geconfronteerd met de dood en zijn zonden.",
                            "target_lang_translations": {
                                "ru": "В <w>моралите</w> человек часто сталкивается со смертью и своими грехами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Средневековая назидательная пьеса, в которой добродетели и пороки выступают как персонажи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моралите",
                                "target_lang_sense_clarification": "театральный жанр"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
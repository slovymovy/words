{
    "word_family": [
        "lot",
        "loting",
        "inloten",
        "verloten"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ce302b2-bb75-31f6-8324-2672e9274de6",
                    "sense_definition": "Door loting niet toegelaten worden tot een school of studie met een beperkt aantal plaatsen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "education_lottery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer is helaas <w>uitgeloot</w> voor de studie geneeskunde.",
                            "target_lang_translations": {
                                "ru": "К сожалению, мой брат <w>не прошел по жребию</w> на медицинский факультет."
                            }
                        },
                        {
                            "text": "Als je wordt <w>uitgeloot</w>, moet je snel op zoek naar een andere opleiding.",
                            "target_lang_translations": {
                                "ru": "Если ты <w>не пройдешь по жребию</w>, тебе придется быстро искать другое место учебы."
                            }
                        }
                    ],
                    "antonyms": [
                        "inloten"
                    ],
                    "common_phrases": [
                        "uitgeloot worden",
                        "voor een studie uitloten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не быть зачисленным в учебное заведение из-за результатов жеребьёвки при ограниченном количестве мест."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не пройти по жребию",
                                "target_lang_sense_clarification": "об учебе"
                            },
                            {
                                "target_lang_word": "отсеяться",
                                "target_lang_sense_clarification": "при распределении мест"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e068a8a-948a-319a-8262-9c0b28e0a377",
                    "sense_definition": "Geselecteerd of getrokken worden bij een loting, vaak betreffende de aflossing van obligaties.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "financial_lottery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bank heeft bekendgemaakt welke nummers dit jaar zijn <w>uitgeloot</w>.",
                            "target_lang_translations": {
                                "ru": "Банк объявил, какие номера <w>вышли в тираж</w> в этом году."
                            }
                        },
                        {
                            "text": "Wanneer een obligatie wordt <w>uitgeloot</w>, krijgt de houder zijn geld terug.",
                            "target_lang_translations": {
                                "ru": "Когда облигация <w>выходит в тираж</w>, держатель получает свои деньги назад."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een obligatie uitloten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть выбранным в ходе жеребьёвки, часто речь идет о погашении облигаций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выйти в тираж",
                                "target_lang_sense_clarification": "об облигациях"
                            },
                            {
                                "target_lang_word": "погашаться по жребию",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd7cdf23-208d-3961-8fa9-b6efdba3e4b0",
                    "sense_definition": "Тайно взять чужое имущество без разрешения владельца.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_theft",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вор попытался <украсть> дорогую картину из городского музея.",
                            "target_lang_translations": {
                                "en": "The thief tried to <steal> an expensive painting from the city museum."
                            }
                        },
                        {
                            "text": "У туриста на вокзале успели <украсть> паспорт и все деньги.",
                            "target_lang_translations": {
                                "en": "Someone managed to <steal> the tourist's passport and all his money at the train station."
                            }
                        }
                    ],
                    "synonyms": [
                        "своровать",
                        "похитить",
                        "стащить"
                    ],
                    "antonyms": [
                        "вернуть",
                        "отдать"
                    ],
                    "common_phrases": [
                        "украсть деньги",
                        "украсть кошелёк",
                        "украсть машину",
                        "украсть документы"
                    ],
                    "target_lang_definitions": {
                        "en": "To secretly take someone else's property without the owner's permission."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steal",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "swipe",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "pinch",
                                "target_lang_sense_clarification": "informal, British"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "341ba22c-0915-3dc4-8505-9d1a0c144c5a",
                    "sense_definition": "Присвоить себе что-то нематериальное (власть, славу, идею) или добиться чьего-то расположения.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_appropriation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он боялся, что конкуренты могут <украсть> его гениальную идею.",
                            "target_lang_translations": {
                                "en": "He was afraid that competitors might <steal> his brilliant idea."
                            }
                        },
                        {
                            "text": "Эта красивая песня помогла певцу <украсть> сердца миллионов слушателей.",
                            "target_lang_translations": {
                                "en": "This beautiful song helped the singer <steal> the hearts of millions of listeners."
                            }
                        }
                    ],
                    "synonyms": [
                        "присвоить"
                    ],
                    "antonyms": [
                        "уступить"
                    ],
                    "common_phrases": [
                        "украсть идею",
                        "украсть победу",
                        "украсть сердце",
                        "украсть шоу"
                    ],
                    "target_lang_definitions": {
                        "en": "To appropriate something intangible (like an idea, credit, or glory) or to win someone's affection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steal",
                                "target_lang_sense_clarification": "ideas, hearts, or attention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab1e30ac-91df-322e-87cd-c6e7a09a5e8c",
                    "sense_definition": "Найти или выделить немного времени для чего-либо, отвлекаясь от основных дел.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <украсть> пару часов, чтобы спокойно погулять по городу.",
                            "target_lang_translations": {
                                "en": "I want to <steal> a couple of hours to take a quiet walk around the city."
                            }
                        },
                        {
                            "text": "Ей с большим трудом удалось <украсть> момент для личного разговора.",
                            "target_lang_translations": {
                                "en": "She managed with great difficulty to <steal> a moment for a private conversation."
                            }
                        }
                    ],
                    "synonyms": [
                        "выкроить",
                        "улучить"
                    ],
                    "common_phrases": [
                        "украсть минутку",
                        "украсть час",
                        "украсть время"
                    ],
                    "target_lang_definitions": {
                        "en": "To find or set aside a small amount of time for something amidst other tasks."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steal",
                                "target_lang_sense_clarification": "time or moments"
                            },
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "time"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
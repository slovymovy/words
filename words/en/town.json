    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e196afe-1d67-3f82-be42-ac09102f9262",
                    "sense_definition": "A place with many houses, shops, and offices where people live and work, which is larger than a village but smaller than a city.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "place_settlement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We live in a small <w>town</w> near the ocean.",
                            "target_lang_translations": {
                                "ru": "Мы живем в маленьком <w>городке</w> у океана."
                            }
                        },
                        {
                            "text": "The <w>town</w> has a library, two schools, and a large park.",
                            "target_lang_translations": {
                                "ru": "В <w>городе</w> есть библиотека, две школы и большой парк."
                            }
                        }
                    ],
                    "synonyms": [
                        "ATC"
                    ],
                    "common_phrases": [
                        "small town",
                        "home town",
                        "ghost town",
                        "town hall",
                        "town square"
                    ],
                    "target_lang_definitions": {
                        "ru": "Населённый пункт с множеством домов, магазинов и офисов, где живут и работают люди; больше, чем деревня, но меньше, чем крупный город."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "город",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "городок",
                                "target_lang_sense_clarification": "небольшой город"
                            },
                            {
                                "target_lang_word": "посёлок",
                                "target_lang_sense_clarification": "небольшое поселение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e27f03f-263d-3536-9af3-390e68bc559a",
                    "sense_definition": "The central part of a city or town where the main shops and businesses are located; or the nearest city used as a reference point.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_settlement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am going into <w>town</w> to buy some new clothes.",
                            "target_lang_translations": {
                                "ru": "Я еду в <w>город</w>, чтобы купить новую одежду."
                            }
                        },
                        {
                            "text": "We stayed out of <w>town</w> until the traffic cleared.",
                            "target_lang_translations": {
                                "ru": "Мы оставались за <w>городом</w>, пока не рассосались пробки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go to town",
                        "in town",
                        "out of town",
                        "leave town"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральная часть города, где расположены основные магазины и предприятия; или ближайший город, используемый как ориентир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "центр",
                                "target_lang_sense_clarification": "центральная часть города"
                            },
                            {
                                "target_lang_word": "город",
                                "target_lang_sense_clarification": "деловой центр или ближайший город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0eea4dcb-db5c-3792-9582-5917d7516ed4",
                    "sense_definition": "A house that is part of a row of identical houses joined by side walls; a short form of townhouse.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to buy a <w>town</w> because it was cheaper than a detached house.",
                            "target_lang_translations": {
                                "ru": "Они решили купить <w>таунхаус</w>, потому что это было дешевле, чем отдельный дом."
                            }
                        },
                        {
                            "text": "The new development features a mix of condos and <w>towns</w>.",
                            "target_lang_translations": {
                                "ru": "В новом жилом комплексе сочетаются квартиры и <w>таунхаусы</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for townhouse"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дом, являющийся частью ряда идентичных домов, соединенных боковыми стенами; сокращение от \"таунхаус\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таунхаус",
                                "target_lang_sense_clarification": "тип жилья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d410b795-8fbd-36d7-95a9-ee80e8dd60aa",
                    "sense_definition": "The people who live in a specific town or city, often considered as a group distinct from university students or faculty.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The whole <w>town</w> turned out to celebrate the local team's victory.",
                            "target_lang_translations": {
                                "ru": "Весь <w>город</w> вышел праздновать победу местной команды."
                            }
                        },
                        {
                            "text": "Relations between <w>town</w> and gown have improved significantly in recent years.",
                            "target_lang_translations": {
                                "ru": "Отношения между <w>городом</w> и университетом значительно улучшились за последние годы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "town and gown"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жители определенного города, часто рассматриваемые как группа, отличная от студентов или преподавателей университета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "город",
                                "target_lang_sense_clarification": "жители города (собирательное)"
                            },
                            {
                                "target_lang_word": "горожане",
                                "target_lang_sense_clarification": "жители"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3c1dfa8b-d90d-3e89-b2ed-05e356319bd6",
                    "sense_definition": "A surname.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Town</w> has worked at the company for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Таун</w> проработал в компании двадцать лет."
                            }
                        },
                        {
                            "text": "I believe Sarah <w>Town</w> is the author of this book.",
                            "target_lang_translations": {
                                "ru": "Я полагаю, Сара <w>Таун</w> — автор этой книги."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Таун",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61d70c27-650f-3a72-9d6f-f6b411d750fa",
                    "sense_definition": "A colloquial way to refer to London, especially central London.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "He has gone up to <w>Town</w> for a business meeting.",
                            "target_lang_translations": {
                                "ru": "Он поехал в <w>Лондон</w> на деловую встречу."
                            }
                        },
                        {
                            "text": "When you live in the countryside, a trip to <w>Town</w> is a special event.",
                            "target_lang_translations": {
                                "ru": "Когда живешь в сельской местности, поездка в <w>Лондон</w> — особое событие."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название Лондона, особенно центрального Лондона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лондон",
                                "target_lang_sense_clarification": "город"
                            },
                            {
                                "target_lang_word": "центр",
                                "target_lang_sense_clarification": "центр Лондона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ad64a67-0a20-3c2c-a38d-03e48741de55",
                    "sense_definition": "A short name for Alabang Town Center, a shopping mall in the Philippines.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We agreed to meet at <w>Town</w> for dinner and a movie.",

            "senses": [
                {
                    "sense_id": "3db976b0-b8cd-340a-8adc-bc5a823feb74",
                    "sense_definition": "Causing or capable of causing death.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The snake's venom is potentially <w>lethal</w> to humans.",
                            "target_lang_translations": {
                                "ru": "Яд этой змеи потенциально <w>смертелен</w> для человека."
                            }
                        },
                        {
                            "text": "Police officers are authorized to use <w>lethal</w> force only when absolutely necessary.",
                            "target_lang_translations": {
                                "ru": "Сотрудники полиции уполномочены применять <w>смертоносную</w> силу только в случае крайней необходимости."
                            }
                        }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "95603e83-e48a-3888-b14e-ed60cbbd13a0",
                    "sense_definition": "Iemand of iets onverwacht zien, vinden of ontmoeten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kwam</w> gisteren toevallig mijn oude buurman <w>tegen</w> in de supermarkt.",
                            "target_lang_translations": {
                                "en": "I happened to <w>run into</w> my old neighbor at the supermarket yesterday.",
                                "ru": "Вчера в супермаркете я случайно <w>встретил</w> своего старого соседа."
                            }
                        },
                        {
                            "text": "Tijdens onze wandeling zijn we geen enkele andere toerist <w>tegengekomen</w>.",
                            "target_lang_translations": {
                                "en": "During our walk we didn't <w>encounter</w> a single other tourist.",
                                "ru": "Во время нашей прогулки мы не <w>встретили</w> ни одного другого туриста."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontmoeten",
                        "aantreffen",
                        "stuiten op"
                    ],
                    "antonyms": [
                        "mislopen"
                    ],
                    "common_phrases": [
                        "iemand tegenkomen",
                        "iets tegenkomen",
                        "toevallig tegenkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To meet someone or find something unexpectedly.",
                        "ru": "Неожиданно увидеть, найти или встретить кого-то или что-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run into",
                                "target_lang_sense_clarification": "to meet someone by chance"
                            },
                            {
                                "target_lang_word": "come across",
                                "target_lang_sense_clarification": "to find or meet by chance"
                            },
                            {
                                "target_lang_word": "encounter",
                                "target_lang_sense_clarification": "formal: to meet or find"
                            },
                            {
                                "target_lang_word": "meet",
                                "target_lang_sense_clarification": "to come face to face with"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "встречать / встретить",
                                "target_lang_sense_clarification": "случайно увидеть кого-то"
                            },
                            {
                                "target_lang_word": "попадаться / попасться",
                                "target_lang_sense_clarification": "встречаться на пути"
                            },
                            {
                                "target_lang_word": "наталкиваться / натолкнуться",
                                "target_lang_sense_clarification": "неожиданно обнаружить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57d0eca5-f0fd-37cb-9c44-bd78049732f2",
                    "sense_definition": "Geconfronteerd worden met je eigen grenzen, beperkingen of fouten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je altijd maar doorgaat zonder rust, zul je jezelf uiteindelijk <w>tegenkomen</w>.",
                            "target_lang_translations": {
                                "en": "If you keep going without rest, you will eventually <w>hit a wall</w>.",
                                "ru": "Если будешь продолжать без отдыха, в конце концов ты <w>столкнешься</w> с собственным пределом."
                            }
                        },
                        {
                            "text": "Hij is zichzelf flink <w>tegengekomen</w> toen hij probeerde die zware taak alleen te volbrengen.",
                            "target_lang_translations": {
                                "en": "He really <w>confronted his limits</w> when he tried to complete that heavy task alone.",
                                "ru": "Он жестко <w>столкнулся</w> с реальностью, когда пытался выполнить эту тяжелую задачу в одиночку."
                            }
                        }
                    ],
                    "synonyms": [
                        "vastlopen"
                    ],
                    "common_phrases": [
                        "jezelf tegenkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be confronted with one's own limits or weaknesses.",
                        "ru": "Столкнуться с собственными границами, ограничениями или ошибками."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "confront one's limits",
                                "target_lang_sense_clarification": "idiomatic: to face personal limitations"
                            },
                            {
                                "target_lang_word": "hit a wall",
                                "target_lang_sense_clarification": "idiomatic: to be unable to continue due to exhaustion or limits"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сталкиваться / столкнуться",
                                "target_lang_sense_clarification": "с трудностями или реальностью"
                            },
                            {
                                "target_lang_word": "осознавать / осознать",
                                "target_lang_sense_clarification": "свои пределы (в контексте)"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7a441d8b-e600-357c-9994-5af004c1ab53",
                    "sense_definition": "Rodzaj lekkiego, miękkiego obuwia noszonego w domu dla wygody.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po powrocie z pracy lubię od razu założyć wygodne <w>pantofle</w>.",
                            "target_lang_translations": {
                                "ru": "Вернувшись с работы, я люблю сразу надеть удобные <w>тапочки</w>."
                            }
                        },
                        {
                            "text": "Babcia zrobiła mi na drutach ciepłe, wełniane <w>pantofle</w> na zimę.",
                            "target_lang_translations": {
                                "ru": "Бабушка связала мне на зиму теплые шерстяные <w>тапочки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bambosz",
                        "ciapeć",
                        "kapeć",
                        "papeć",
                        "papuć"
                    ],
                    "common_phrases": [
                        "ciepłe pantofle",
                        "domowe pantofle",
                        "wygodne pantofle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид легкой, мягкой обуви, которую носят дома для удобства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тапочка",
                                "target_lang_sense_clarification": "домашняя обувь"
                            },
                            {
                                "target_lang_word": "тапка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79ee10c1-2286-3188-b707-df20aac9b303",
                    "sense_definition": "Elegancki, lekki but na cienkiej podeszwie, zakładany zazwyczaj do stroju wizytowego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do tej wieczorowej sukni pasują czarne, lakierowane <w>pantofle</w>.",
                            "target_lang_translations": {
                                "ru": "К этому вечернему платью подходят черные лакированные <w>туфли</w>."
                            }
                        },
                        {
                            "text": "W bajce o Kopciuszku najważniejszą rolę odgrywa szklany <w>pantofelek</w>.",
                            "target_lang_translations": {
                                "ru": "В сказке о Золушке самую важную роль играет хрустальная <w>туфелька</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szklany pantofelek",
                        "lakierowane pantofle",
                        "skórzane pantofle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элегантная легкая туфля на тонкой подошве, обычно надеваемая к выходному костюму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туфля",
                                "target_lang_sense_clarification": "женская обувь"
                            },
                            {
                                "target_lang_word": "туфелька",
                                "target_lang_sense_clarification": "изящная туфля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9651e00-6bbb-33bf-b49f-1b0f96fbc4b2",
                    "sense_definition": "Mężczyzna, który jest całkowicie podporządkowany woli swojej żony lub partnerki i nie ma własnego zdania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy koledzy śmieją się z niego, że jest pod <w>pantoflem</w> żony.",
                            "target_lang_translations": {
                                "ru": "Все друзья смеются над ним, что он у жены под <w>каблуком</w>."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>pantoflem</w> i zdecyduj sam, gdzie pojedziemy na wakacje.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>подкаблучником</w> и реши сам, куда мы поедем в отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "pantoflarz"
                    ],
                    "common_phrases": [
                        "być pod pantoflem",
                        "siedzieć pod pantoflem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который полностью подчиняется воле своей жены или партнерши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкаблучник",
                                "target_lang_sense_clarification": "мужчина, подчиняющийся жене"
                            }
                        ]
                    }
                }
            ]
        }
    ]

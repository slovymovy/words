                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapłan uroczyście podniósł złoty <w>kielich</w> podczas mszy."
                        },
                        {
                            "text": "Wznoszę ten <w>kielich</w> wina za zdrowie młodej pary."
                        }
                    ],
                    "synonyms": [
                        "puchar"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "46bb9256-ad40-3ab0-ad21-89d69c8b9392",
                    "sense_definition": "Op het moment dat iets in het verleden gebeurde.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij ging naar huis <w>toen</w> het vijf uur was.",
                            "target_lang_translations": {
                                "ru": "Он пошел домой, <w>когда</w> было пять часов.",
                                "en": "He went home <w>when</w> it was five o'clock."
                            }
                        },
                        {
                            "text": "Hoe oud was je <w>toen</w> je vegetariër werd?",
                            "target_lang_translations": {
                                "ru": "Сколько тебе было лет, <w>когда</w> ты стал вегетарианцем?",
                                "en": "How old were you <w>when</w> you became a vegetarian?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wanneer"
                    ],
                    "common_phrases": [
                        "net toen",
                        "juist toen"
                    ],
                    "target_lang_definitions": {
                        "ru": "В тот момент, когда что-то происходило в прошлом.",
                        "en": "At the moment that something happened in the past."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союз времени (в прошлом)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "when",
                                "target_lang_sense_clarification": "conjunction of time"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a03212b5-1c6a-31f2-902a-1728bc45b43d",
                    "sense_definition": "Op dat specifieke moment in het verleden of direct daarna.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is <w>toen</w> direct naar huis gegaan.",
                            "target_lang_translations": {
                                "ru": "Он <w>тогда</w> сразу пошел домой.",
                                "en": "He <w>then</w> went home immediately."
                            }
                        },
                        {
                            "text": "En <w>toen</w> gebeurde er iets heel vreemds.",
                            "target_lang_translations": {
                                "ru": "И <w>тогда</w> случилось нечто очень странное.",
                                "en": "And <w>then</w> something very strange happened."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarna",
                        "vervolgens",
                        "daarop",
                        "dan"
                    ],
                    "common_phrases": [
                        "en toen",
                        "vanaf toen"
                    ],
                    "target_lang_definitions": {
                        "ru": "В тот конкретный момент в прошлом или сразу после этого.",
                        "en": "At that specific moment in the past or immediately afterwards."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда",
                                "target_lang_sense_clarification": "в тот момент"
                            },
                            {
                                "target_lang_word": "потом",
                                "target_lang_sense_clarification": "затем, после этого"
                            },
                            {
                                "target_lang_word": "затем",
                                "target_lang_sense_clarification": "далее"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "then",
                                "target_lang_sense_clarification": "at that moment / next"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e56ac135-f058-3a36-a9cf-d006e334b4fe",
                    "sense_definition": "In die periode in het verleden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Toen</w> was dat nog heel gewoon.",
                            "target_lang_translations": {
                                "ru": "<w>Тогда</w> это было еще вполне обычным делом.",
                                "en": "<w>Back then</w>, that was still very common."
                            }
                        },
                        {
                            "text": "De mensen van <w>toen</w> dachten daar heel anders over.",
                            "target_lang_translations": {
                                "ru": "Люди <w>того времени</w> думали об этом совершенно иначе.",
                                "en": "People <w>back then</w> thought very differently about that."
                            }
                        }
                    ],
                    "synonyms": [
                        "destijds"
                    ],
                    "antonyms": [
                        "nu",
                        "heden"
                    ],
                    "common_phrases": [
                        "van toen",
                        "zoals toen"
                    ],
                    "target_lang_definitions": {
                        "ru": "В тот период в прошлом.",
                        "en": "In that period in the past."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда",
                                "target_lang_sense_clarification": "в то время, раньше"
                            },
                            {
                                "target_lang_word": "в то время",
                                "target_lang_sense_clarification": "в ту эпоху"
                            }
                        ],
                        "en": [
                            {

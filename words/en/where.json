                    "sense_id": "2252db86-3712-3f50-9a42-204af7751f46",
                    "sense_definition": "In, at, or to what place.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Where</w> are you going?",
                            "target_lang_translations": {
                                "ru": "<w>Куда</w> ты идешь?"
                            }
                        },
                        {
                            "text": "He asked me <w>where</w> I lived.",
                            "target_lang_translations": {
                                "ru": "Он спросил меня, <w>где</w> я живу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "where are you",
                        "where is it",
                        "where to"
                    ],
                    "target_lang_definitions": {
                        "ru": "В каком месте, в каком направлении или из какого места."
                    },
                    "translations": {

                },
                {
                    "sense_id": "18bfcb56-9723-3d47-9841-713f2602bf84",
                    "sense_definition": "Lacking formal organization or structure.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Children often learn social skills through <w>informal</w> play.",
                            "target_lang_translations": {

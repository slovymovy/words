        "presumably",
        "presumptuous"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0d0a42bc-71ea-33fe-805f-6dc9d04ca269",
                    "sense_definition": "Simple past and past participle of <w>presume</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>presumed</w> that the meeting was cancelled because no one was in the room.",
                            "target_lang_translations": {
                                "ru": "Она <w>предположила</w>, что встреча отменена, так как в комнате никого не было."
                            }
                        },
                        {
                            "text": "He <w>presumed</w> to speak for the entire group without asking them first.",
                            "target_lang_translations": {
                                "ru": "Он <w>осмелился</w> говорить от имени всей группы, не спросив их сначала."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола presume."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предполагать",
                                "target_lang_sense_clarification": "считать что-либо истинным без доказательств"
                            },
                            {
                                "target_lang_word": "осмеливаться",
                                "target_lang_sense_clarification": "позволять себе сделать что-либо, брать на себя смелость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "999a465b-3e09-38b0-9217-8d5c798daede",
                    "sense_definition": "Believed to be true, valid, or likely to happen, although not yet definitely proven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>presumed</w> winner of the race celebrated before the official results were announced.",
                            "target_lang_translations": {
                                "ru": "<w>Предполагаемый</w> победитель гонки праздновал до объявления официальных результатов."
                            }
                        },
                        {
                            "text": "In a court of law, the defendant is <w>presumed</w> innocent until the jury reaches a verdict.",
                            "target_lang_translations": {
                                "ru": "В суде обвиняемый <w>считается</w> невиновным, пока присяжные не вынесут вердикт."
                            }
                        }
                    ],
                    "synonyms": [
                        "assumed",
                        "supposed",
                        "likely",
                        "probable"
                    ],
                    "antonyms": [
                        "confirmed",
                        "proven",
                        "certain"
                    ],
                    "common_phrases": [
                        "presumed innocent",
                        "presumed dead",
                        "presumed father",
                        "presumed consent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считающийся истинным, действительным или вероятным, хотя это еще не доказано окончательно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предполагаемый",
                                "target_lang_sense_clarification": "ожидаемый или считающийся вероятным"
                            },
                            {
                                "target_lang_word": "считающийся",
                                "target_lang_sense_clarification": "воспринимаемый как таковой (например, невиновным)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
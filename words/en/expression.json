                    "synonyms": [
                        "articulation",
                        "utterance",
                        "statement",
                        "declaration"
                    ],
                    "antonyms": [
                        "suppression",
                        "concealment"
                    ],
                    "common_phrases": [
                        "freedom of expression",
                        "self-expression",
                        "expression of interest"
                    ],
                    "target_lang_definitions": {

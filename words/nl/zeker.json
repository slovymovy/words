{
    "word_family": [
        "zekerheid",
        "verzekeren",
        "onzeker",
        "zekeren",
        "voorzeker"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0bc63a30-e1c0-3abe-90d5-1b309450d1cf",
                    "sense_definition": "Waarvan men overtuigd is dat het waar is; waarover geen twijfel bestaat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "certainty_fact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik weet <w>zeker</w> dat ik de deur op slot heb gedaan.",
                            "target_lang_translations": {
                                "ru": "Я <w>уверен</w>, что запер дверь.",
                                "en": "I am <w>sure</w> that I locked the door."
                            }
                        },
                        {
                            "text": "Het is nog niet <w>zeker</w> of we morgen op vakantie kunnen gaan.",
                            "target_lang_translations": {
                                "ru": "Еще не <w>точно</w>, сможем ли мы завтра поехать в отпуск.",
                                "en": "It is not yet <w>certain</w> whether we can go on holiday tomorrow."
                            }
                        }
                    ],
                    "antonyms": [
                        "onzeker"
                    ],
                    "common_phrases": [
                        "zeker weten",
                        "er zeker van zijn",
                        "voor zeker aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, в чем кто-то убежден; не вызывающий сомнений.",
                        "en": "That which one is convinced is true; about which no doubt exists."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенный",
                                "target_lang_sense_clarification": "убежденный в чем-либо"
                            },
                            {
                                "target_lang_word": "несомненный",
                                "target_lang_sense_clarification": "о факте или событии"
                            },
                            {
                                "target_lang_word": "точный",
                                "target_lang_sense_clarification": "достоверный"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "certain",
                                "target_lang_sense_clarification": "established as a fact"
                            },
                            {
                                "target_lang_word": "sure",
                                "target_lang_sense_clarification": "convinced in one's mind"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fdca478-fbc5-33db-a630-30a911a393e3",
                    "sense_definition": "Een bepaalde, maar niet nader genoemde of gespecificeerde.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "indefinite_specifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is een <w>zekere</w> moed nodig om de waarheid te spreken.",
                            "target_lang_translations": {
                                "ru": "Нужна <w>определенная</w> смелость, чтобы говорить правду.",
                                "en": "A <w>certain</w> courage is needed to speak the truth."
                            }
                        },
                        {
                            "text": "Op een <w>zeker</w> moment zul je toch een beslissing moeten nemen.",
                            "target_lang_translations": {
                                "ru": "В <w>определенный</w> момент тебе все же придется принять решение.",
                                "en": "At a <w>certain</w> moment you will have to make a decision anyway."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een zekere mate van",
                        "tot op zekere hoogte",
                        "op een zeker moment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Какой-то конкретный, но не названный или не уточненный.",
                        "en": "A specific, but not further named or specified one."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определенный",
                                "target_lang_sense_clarification": "конкретный, но не названный"
                            },
                            {
                                "target_lang_word": "некий",
                                "target_lang_sense_clarification": "какой-то"
                            },
                            {
                                "target_lang_word": "известный",
                                "target_lang_sense_clarification": "в значении 'некоторый'"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "certain",
                                "target_lang_sense_clarification": "specific but unnamed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aba93e18-6470-3dc7-bd8e-146abb1ffa62",
                    "sense_definition": "Vol zelfvertrouwen; zich veilig voelend.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "confidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de goede resultaten voelde hij zich een stuk <w>zekerder</w>.",
                            "target_lang_translations": {
                                "ru": "После хороших результатов он чувствовал себя намного <w>увереннее</w>.",
                                "en": "After the good results he felt much more <w>confident</w>."
                            }
                        },
                        {
                            "text": "Ze stond <w>zeker</w> op haar benen ondanks de gladde vloer.",
                            "target_lang_translations": {
                                "ru": "Она <w>уверенно</w> стояла на ногах, несмотря на скользкий пол.",
                                "en": "She stood <w>steadily</w> on her legs despite the slippery floor."
                            }
                        }
                    ],
                    "antonyms": [
                        "onzeker"
                    ],
                    "common_phrases": [
                        "zich zeker voelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уверенный в себе; чувствующий себя в безопасности.",
                        "en": "Full of self-confidence; feeling safe."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенный",
                                "target_lang_sense_clarification": "в себе или своих силах"
                            },
                            {
                                "target_lang_word": "твердый",
                                "target_lang_sense_clarification": "устойчивый, не колеблющийся"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "confident",
                                "target_lang_sense_clarification": "self-assured"
                            },
                            {
                                "target_lang_word": "secure",
                                "target_lang_sense_clarification": "safe, steady"
                            },
                            {
                                "target_lang_word": "steady",
                                "target_lang_sense_clarification": "firm, stable"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a7a51fe4-5692-3795-918d-3a7741a4bad3",
                    "sense_definition": "Zonder twijfel; stellig; als bevestiging.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is <w>zeker</w> een goed idee van jou.",
                            "target_lang_translations": {
                                "ru": "Это <w>безусловно</w> хорошая твоя идея.",
                                "en": "That is <w>certainly</w> a good idea of yours."
                            }
                        },
                        {
                            "text": "Ga je mee naar het feest? Ja <w>zeker</w>!",
                            "target_lang_translations": {
                                "ru": "Ты пойдешь на вечеринку? Да, <w>конечно</w>!",
                                "en": "Are you coming to the party? Yes <w>sure</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ja zeker",
                        "zeker en vast",
                        "zeer zeker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без сомнения; безусловно; как подтверждение.",
                        "en": "Without doubt; definitely; as affirmation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечно",
                                "target_lang_sense_clarification": "выражение согласия"
                            },
                            {
                                "target_lang_word": "безусловно",
                                "target_lang_sense_clarification": "без сомнений"
                            },
                            {
                                "target_lang_word": "наверняка",
                                "target_lang_sense_clarification": "точно, несомненно"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "certainly",
                                "target_lang_sense_clarification": "without doubt"
                            },
                            {
                                "target_lang_word": "definitely",
                                "target_lang_sense_clarification": "emphatic affirmation"
                            },
                            {
                                "target_lang_word": "sure",
                                "target_lang_sense_clarification": "informal affirmation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78bc2d6c-391b-3193-9602-ba46801a17ac",
                    "sense_definition": "Minstens; een aantal of hoeveelheid aanduidend als ondergrens.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_estimation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er waren <w>zeker</w> vijfhonderd mensen aanwezig bij het concert.",
                            "target_lang_translations": {
                                "ru": "На концерте присутствовало <w>по меньшей мере</w> пятьсот человек.",
                                "en": "There were <w>at least</w> five hundred people present at the concert."
                            }
                        },
                        {
                            "text": "De reis naar Parijs duurt <w>zeker</w> vier uur met de auto.",
                            "target_lang_translations": {
                                "ru": "Поездка в Париж на машине занимает <w>как минимум</w> четыре часа.",
                                "en": "The trip to Paris takes <w>at least</w> four hours by car."
                            }
                        }
                    ],
                    "synonyms": [
                        "minstens",
                        "ten minste"
                    ],
                    "common_phrases": [
                        "zeker wel",
                        "zeker weten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Как минимум; указывая на нижнюю границу количества.",
                        "en": "At least; indicating a number or quantity as a lower limit."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как минимум",
                                "target_lang_sense_clarification": "не меньше чем"
                            },
                            {
                                "target_lang_word": "по меньшей мере",
                                "target_lang_sense_clarification": "самое малое"
                            },
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "гарантированно (о количестве)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "at least",
                                "target_lang_sense_clarification": "minimum amount"
                            },
                            {
                                "target_lang_word": "certainly",
                                "target_lang_sense_clarification": "emphasizing the minimum"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d36e5bda-2252-3bab-a915-d8b8be077d61",
                    "sense_definition": "Vooral; in het bijzonder.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hou van Italiaans eten, <w>zeker</w> van pizza en pasta.",
                            "target_lang_translations": {
                                "ru": "Я люблю итальянскую еду, <w>особенно</w> пиццу и пасту.",
                                "en": "I love Italian food, <w>especially</w> pizza and pasta."
                            }
                        },
                        {
                            "text": "Het is belangrijk om goed te slapen, <w>zeker</w> voor een examen.",
                            "target_lang_translations": {
                                "ru": "Важно хорошо спать, <w>особенно</w> перед экзаменом.",
                                "en": "It is important to sleep well, <w>especially</w> before an exam."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zeker als",
                        "zeker omdat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особенно; в частности.",
                        "en": "Especially; in particular."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "особенно",
                                "target_lang_sense_clarification": "прежде всего"
                            },
                            {
                                "target_lang_word": "в частности",
                                "target_lang_sense_clarification": "конкретно"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "especially",
                                "target_lang_sense_clarification": "particularly"
                            },
                            {
                                "target_lang_word": "particularly",
                                "target_lang_sense_clarification": "specifically"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "14550780-3f97-30db-b856-33e7e23468c0",
                    "sense_definition": "Vervoeging van <w>zekeren</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>zeker</w> mijn klimpartner zodat hij veilig naar boven kan.",
                            "target_lang_translations": {

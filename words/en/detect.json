{
    "word_family": [
        "detection",
        "detective",
        "detector",
        "detectable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e81c7b48-724f-303f-aafc-0e51715f37ae",
                    "sense_definition": "To discover or notice something, especially if it is small, hidden, or difficult to perceive.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "discovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The security system is designed to <w>detect</w> any movement in the building at night.",
                            "target_lang_translations": {
                                "ru": "Система безопасности разработана так, чтобы <w>обнаруживать</w> любое движение в здании ночью."
                            }
                        },
                        {
                            "text": "Doctors can <w>detect</w> a problem early if you have regular checkups.",
                            "target_lang_translations": {
                                "ru": "Врачи могут <w>выявить</w> проблему на ранней стадии, если вы регулярно проходите осмотры."
                            }
                        }
                    ],
                    "synonyms": [
                        "discover",
                        "notice",
                        "spot",
                        "identify",
                        "reveal"
                    ],
                    "antonyms": [
                        "miss",
                        "overlook",
                        "ignore"
                    ],
                    "common_phrases": [
                        "detect a change",
                        "detect a signal",
                        "detect a problem",
                        "detect presence",
                        "detect fraud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаруживать или замечать что-либо, особенно если объект мал, скрыт или его трудно заметить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнаруживать",
                                "target_lang_sense_clarification": "находить что-то скрытое или неизвестное"
                            },
                            {
                                "target_lang_word": "выявлять",
                                "target_lang_sense_clarification": "устанавливать наличие чего-либо (проблемы, факта)"
                            },
                            {
                                "target_lang_word": "засекать",
                                "target_lang_sense_clarification": "регистрировать сигнал или замечать движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e923725-ebd9-3c94-8caa-0953dadadc5a",
                    "sense_definition": "To work as a detective to solve a crime or mystery.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "investigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was hired to <w>detect</w> and find the missing documents.",
                            "target_lang_translations": {
                                "ru": "Его наняли, чтобы <w>разыскать</w> и найти пропавшие документы."
                            }
                        },
                        {
                            "text": "In the story, the main character spends his time <w>detecting</w> rather than doing his actual job.",
                            "target_lang_translations": {
                                "ru": "По сюжету главный герой проводит время, <w>занимаясь расследованием</w>, вместо того чтобы выполнять свою основную работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "investigate",
                        "sleuth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять работу детектива для раскрытия преступления или тайны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расследовать",
                                "target_lang_sense_clarification": "вести следствие, изучать обстоятельства"
                            },
                            {
                                "target_lang_word": "вести сыск",
                                "target_lang_sense_clarification": "заниматься сыскной деятельностью"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
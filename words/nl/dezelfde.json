    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "b823871e-7f08-3d80-8d97-155aee4beae3",
                    "sense_definition": "Duidt aan dat een persoon, zaak of situatie identiek is aan een eerder genoemde of bekende entiteit (gebruikt bij de-woorden in het enkelvoud en alle woorden in het meervoud)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "identity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>dezelfde</w> jurk als zij.",
                            "target_lang_translations": {
                                "ru": "У меня <w>такое же</w> платье, как у нее."
                            }
                        },
                        {
                            "text": "Zij wonen al jaren in <w>dezelfde</w> straat als mijn ouders.",
                            "target_lang_translations": {
                                "ru": "Они уже много лет живут на <w>той же</w> улице, что и мои родители."
                            }
                        },
                        {
                            "text": "Op <w>dezelfde</w> dag dat hij vertrok, begon het hard te regenen.",
                            "target_lang_translations": {
                                "ru": "В <w>тот же</w> день, когда он уехал, начался сильный дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "identieke",
                        "diezelfde"
                    ],
                    "antonyms": [
                        "andere",
                        "verschillende"
                    ],
                    "common_phrases": [
                        "op dezelfde manier",
                        "in dezelfde tijd",
                        "dezelfde dag",
                        "dezelfde fout"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на то, что лицо, предмет или ситуация идентичны ранее упомянутым или известным (используется с существительными рода de в единственном числе и всеми существительными во множественном числе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тот же (самый)",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "та же (самая)",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "те же (самые)",
                                "target_lang_sense_clarification": "множественное число"
                            },
                            {
                                "target_lang_word": "такой же",
                                "target_lang_sense_clarification": "идентичный по виду/типу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "ab1b7bde-b4a9-3fcb-a062-5f8bb018b24e",
                    "sense_definition": "De persoon of zaak die identiek is aan de eerder genoemde; niet een ander (zelfstandig gebruikt)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "identity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben weer voor <w>dezelfde</w> gekozen.",
                            "target_lang_translations": {
                                "ru": "Мы снова выбрали <w>того же</w>."
                            }
                        },
                        {
                            "text": "Als mijn schoenen versleten zijn, koop ik gewoon weer <w>dezelfde</w>.",
                            "target_lang_translations": {
                                "ru": "Когда мои туфли износятся, я просто куплю <w>те же самые</w>."
                            }
                        },
                        {
                            "text": "Dat zijn precies <w>dezelfde</w> als die we gisteren zagen.",
                            "target_lang_translations": {
                                "ru": "Это точно <w>те же самые</w>, что мы видели вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "diezelfde"
                    ],
                    "antonyms": [
                        "een andere"
                    ],
                    "common_phrases": [
                        "precies dezelfde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или предмет, идентичный ранее упомянутому; не другой (используется самостоятельно)"
                    },
                    "translations": {

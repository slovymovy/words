{
    "word_family": [
        "skupienie",
        "skupisko",
        "skupiony",
        "skup"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bab2efdb-c66d-3084-aa29-10220036d6c3",
                    "sense_definition": "Koncentrować swoją uwagę, myśli i wysiłek umysłowy na konkretnym działaniu lub obiekcie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "focus_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <skupiać> się na nauce, żeby zdać ten trudny egzamin.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <сосредотачиваться> на учебе, чтобы сдать этот трудный экзамен."
                            }
                        },
                        {
                            "text": "Podczas jazdy samochodem kierowca powinien <skupiać> się wyłącznie na drodze.",
                            "target_lang_translations": {
                                "ru": "Во время вождения водитель должен <концентрироваться> исключительно на дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "koncentrować się"
                    ],
                    "antonyms": [
                        "rozpraszać się",
                        "dekoncentrować się"
                    ],
                    "common_phrases": [
                        "skupiać się na pracy",
                        "skupiać się na zadaniu",
                        "skupiać się na sobie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Концентрировать свое внимание, мысли и умственные усилия на конкретном действии или объекте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосредоточиваться",
                                "target_lang_sense_clarification": "на чем-то (мысленно)"
                            },
                            {
                                "target_lang_word": "концентрироваться",
                                "target_lang_sense_clarification": "на задаче"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01ef1d3e-db7f-3c8d-9bb6-c47344d9716a",
                    "sense_definition": "Kierować uwagę, wzrok lub myśli intensywnie na kogoś lub coś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "focus_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel starał się <skupiać> uwagę uczniów na najważniejszym temacie lekcji.",
                            "target_lang_translations": {
                                "ru": "Учитель старался <сосредоточивать> внимание учеников на самой важной теме урока."
                            }
                        },
                        {
                            "text": "Ten artysta potrafi <skupiać> na sobie wzrok całej publiczności.",
                            "target_lang_translations": {
                                "ru": "Этот артист умеет <сосредоточивать> на себе взгляд всей публики."
                            }
                        }
                    ],
                    "synonyms": [
                        "koncentrować",
                        "ogniskować"
                    ],
                    "antonyms": [
                        "rozpraszać"
                    ],
                    "common_phrases": [
                        "skupiać uwagę",
                        "skupiać wzrok",
                        "skupiać myśli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять внимание, взгляд или мысли интенсивно на кого-то или что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосредоточивать",
                                "target_lang_sense_clarification": "внимание или взгляд"
                            },
                            {
                                "target_lang_word": "акцентировать",
                                "target_lang_sense_clarification": "внимание"
                            },
                            {
                                "target_lang_word": "фокусировать",
                                "target_lang_sense_clarification": "взгляд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1fe7431-68a1-3f83-9062-f0bf141d6a90",
                    "sense_definition": "Gromadzić osoby lub rzeczy w jednym miejscu lub wokół wspólnego celu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gather_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta organizacja charytatywna <skupia> ludzi, którzy chcą pomagać innym.",
                            "target_lang_translations": {
                                "ru": "Эта благотворительная организация <объединяет> людей, которые хотят помогать другим."
                            }
                        },
                        {
                            "text": "Silny magnes <skupia> drobne metalowe elementy leżące w pobliżu.",
                            "target_lang_translations": {
                                "ru": "Сильный магнит <собирает> мелкие металлические элементы, лежащие поблизости."
                            }
                        }
                    ],
                    "synonyms": [
                        "gromadzić",
                        "zbierać",
                        "zrzeszać"
                    ],
                    "antonyms": [
                        "rozpraszać",
                        "rozdzielać"
                    ],
                    "common_phrases": [
                        "skupiać wokół siebie",
                        "skupiać wysiłki",
                        "skupiać władzę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать людей или предметы в одном месте или вокруг общей цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "людей, усилия"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "предметы, людей"
                            },
                            {
                                "target_lang_word": "сплачивать",
                                "target_lang_sense_clarification": "группу людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a52edac-89ad-3a40-82c5-4fe94efbf4e9",
                    "sense_definition": "O ludziach lub rzeczach: zbierać się razem w jednym miejscu lub grupie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gather_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Większość życia towarzyskiego w tym mieście <skupia> się na rynku.",
                            "target_lang_translations": {
                                "ru": "Большая часть светской жизни в этом городе <сосредоточивается> на рыночной площади."
                            }
                        },
                        {
                            "text": "Tłum zaczął <skupiać> się przed wejściem do teatru na długo przed spektaklem.",
                            "target_lang_translations": {
                                "ru": "Толпа начала <собираться> перед входом в театр задолго до спектакля."
                            }
                        }
                    ],
                    "synonyms": [
                        "gromadzić się",
                        "zbierać się"
                    ],
                    "antonyms": [
                        "rozchodzić się"
                    ],
                    "common_phrases": [
                        "skupiać się wokół idei",
                        "życie skupia się"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О людях или предметах: собираться вместе в одном месте или группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосредоточиваться",
                                "target_lang_sense_clarification": "в одном месте"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "в группу"
                            },
                            {
                                "target_lang_word": "концентрироваться",
                                "target_lang_sense_clarification": "локализоваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc4a7630-a8b2-3b70-aa2a-0381501d2119",
                    "sense_definition": "Powodować zbieganie się promieni świetlnych, fal lub wiązek energii w jednym punkcie (ognisku).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "optics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Soczewka w lupie <skupia> promienie słoneczne, co pozwala wzniecić ogień.",
                            "target_lang_translations": {
                                "ru": "Линза в лупе <фокусирует> солнечные лучи, что позволяет разжечь огонь."
                            }
                        },
                        {
                            "text": "Zwierciadło teleskopu <skupia> słabe światło gwiazd, umożliwiając ich obserwację.",
                            "target_lang_translations": {
                                "ru": "Зеркало телескопа <собирает> слабый свет звезд, делая возможным их наблюдение."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogniskować"
                    ],
                    "antonyms": [
                        "rozpraszać"
                    ],
                    "common_phrases": [
                        "skupiać światło",
                        "skupiać wiązkę",
                        "soczewka skupiająca"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "techniczny (optyka, fizyka)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять световые лучи, волны или пучки энергии сходиться в одной точке (фокусе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фокусировать",
                                "target_lang_sense_clarification": "оптика"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "свет, лучи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
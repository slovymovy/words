    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bb88c74-b056-3eef-a901-c0aadf3b26b9",
                    "sense_definition": "Substancja jadalna pochodzenia zwierzęcego lub roślinnego, używana w kuchni, głównie do smażenia i pieczenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do smażenia pączków potrzebny jest głęboki <w>tłuszcz</w>.",
                            "target_lang_translations": {
                                "ru": "Для жарки пончиков требуется большое количество <w>жира</w> (фритюр)."
                            }
                        },
                        {
                            "text": "Jajecznicę można w zasadzie smażyć na dowolnym <w>tłuszczu</w>.",
                            "target_lang_translations": {
                                "ru": "Яичницу, в принципе, можно жарить на любом <w>жире</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tłuszcz roślinny",
                        "tłuszcz zwierzęcy",
                        "smażyć na tłuszczu",
                        "rozgrzany tłuszcz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобная субстанция животного или растительного происхождения, используемая в кулинарии, в основном для жарки и выпечки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жир",
                                "target_lang_sense_clarification": "пищевой продукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "494eb631-0bce-3d42-834c-2759a4a2fc73",
                    "sense_definition": "Tkanka w organizmie ludzi i zwierząt, która magazynuje energię i chroni narządy; potocznie nadmiar tej tkanki w ciele.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tissue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Masz sporą nadwagę, musisz więc spalić trochę <w>tłuszczu</w>.",
                            "target_lang_translations": {
                                "ru": "У тебя значительный лишний вес, поэтому тебе нужно сжечь немного <w>жира</w>."
                            }
                        },
                        {
                            "text": "Zimą dzikie zwierzęta gromadzą <w>tłuszcz</w>, aby przetrwać chłody.",
                            "target_lang_translations": {
                                "ru": "Зимой дикие животные накапливают <w>жир</w>, чтобы пережить холода."
                            }
                        }
                    ],
                    "synonyms": [
                        "sadło"
                    ],
                    "common_phrases": [
                        "tkanka tłuszczowa",
                        "spalać tłuszcz",
                        "nadmiar tłuszczu",
                        "obrosnąć w tłuszcz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ткань в организме людей и животных, которая накапливает энергию и защищает органы; в разговорной речи — избыток этой ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жир",
                                "target_lang_sense_clarification": "ткань организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "723a0077-fdc2-3d73-a86f-d16dee58ffc1",
                    "sense_definition": "Grupa związków organicznych, estrów glicerolu i kwasów tłuszczowych, będących jednym z podstawowych składników pożywienia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Tłuszcze</w> są niezbędnym elementem zbilansowanej diety człowieka.",
                            "target_lang_translations": {
                                "ru": "<w>Жиры</w> являются необходимым элементом сбалансированной диеты человека."
                            }
                        },
                        {
                            "text": "W chemii organicznej badamy strukturę cząsteczek <w>tłuszczu</w>.",
                            "target_lang_translations": {
                                "ru": "В органической химии мы изучаем структуру молекул <w>жира</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tłuszcze nasycone",
                        "tłuszcze nienasycone",
                        "kwasy tłuszczowe",
                        "przemiana tłuszczów"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chemia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа органических соединений, эфиров глицерина и жирных кислот, являющихся одним из основных компонентов пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жиры",
                                "target_lang_sense_clarification": "питательные вещества"
                            },
                            {
                                "target_lang_word": "жир",
                                "target_lang_sense_clarification": "химическое вещество"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f27171b6-674f-362d-93c8-274cefcdc733",
                    "sense_definition": "Miasto w Polsce, położone w województwie mazowieckim, w powiecie wołomińskim.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Pociąg podmiejski zatrzymuje się na stacji <w>Tłuszcz</w>.",
                            "target_lang_translations": {
                                "ru": "Пригородный поезд останавливается на станции <w>Тлущ</w>."
                            }
                        },
                        {
                            "text": "Moi dziadkowie mieszkają w małym domku w <w>Tłuszczu</w>.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка живут в маленьком домике в <w>Тлуще</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gmina Tłuszcz",
                        "stacja w Tłuszczu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в Польше, расположенный в Мазовецком воеводстве, в Воломинском повяте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тлущ",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }
    ]

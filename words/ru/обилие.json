                        "изобилие"
                    ],
                    "antonyms": [
                        "недостаток",
                        "отсутствие"
                    ],
                    "common_phrases": [
                        "обилие информации",
                        "обилие товаров",
                        "обилие света"
                    ],
                    "target_lang_definitions": {

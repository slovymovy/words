    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6f5acc46-ea82-332b-8e56-e3e7631f9167",
                    "sense_definition": "Having physical strength and the ability to endure difficult conditions, fatigue, or hardship.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "enduring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>hardy</w> travelers managed to cross the desert with very limited supplies.",
                            "target_lang_translations": {
                                "ru": "<w>Выносливые</w> путешественники смогли пересечь пустыню с очень ограниченными запасами."
                            }
                        },
                        {
                            "text": "Camels are a <w>hardy</w> breed of animal that can withstand extreme heat and dehydration.",
                            "target_lang_translations": {
                                "ru": "Верблюды — это <w>выносливая</w> порода животных, способная выдерживать сильную жару и обезвоживание."
                            }
                        }
                    ],
                    "synonyms": [
                        "robust",
                        "rugged",
                        "strong",
                        "hearty"
                    ],
                    "common_phrases": [
                        "hardy breed",
                        "hardy soul",
                        "hardy traveler"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий физической силой и способностью переносить тяжелые условия, усталость или лишения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выносливый",
                                "target_lang_sense_clarification": "способный много вынести"
                            },
                            {
                                "target_lang_word": "стойкий",
                                "target_lang_sense_clarification": "непоколебимый, твердый"
                            },
                            {
                                "target_lang_word": "закалённый",
                                "target_lang_sense_clarification": "привыкший к трудностям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "050fa9c7-6f55-3051-9296-e38bb7093547",
                    "sense_definition": "Able to survive cold weather or poor growing conditions (used especially of plants).",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "enduring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should choose a <w>hardy</w> plant if you want it to survive the winter frost.",
                            "target_lang_translations": {
                                "ru": "Вам следует выбрать <w>морозоустойчивое</w> растение, если вы хотите, чтобы оно пережило зимние заморозки."
                            }
                        },
                        {
                            "text": "This <w>hardy</w> perennial blooms every spring regardless of the weather.",
                            "target_lang_translations": {
                                "ru": "Этот <w>выносливый</w> многолетник цветет каждую весну независимо от погоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "robust",
                        "strong"
                    ],
                    "antonyms": [
                        "tender"
                    ],
                    "common_phrases": [
                        "hardy plant",
                        "hardy perennial",
                        "winter hardy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный переносить холодную погоду или плохие условия произрастания (о растениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морозоустойчивый",
                                "target_lang_sense_clarification": "устойчивый к холоду"

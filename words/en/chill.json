    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "43c2d05f-b999-3b0d-9153-9effd6769e7f",
                    "sense_definition": "To relax, calm down, or stop worrying.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_relax",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>chill</w> out and stop stressing about the exam results.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>расслабиться</w> и перестать нервничать из-за результатов экзамена."
                            }
                        },
                        {
                            "text": "I just want to go home, listen to music, and <w>chill</w> for the evening.",
                            "target_lang_translations": {
                                "ru": "Я просто хочу пойти домой, послушать музыку и <w>отдохнуть</w> вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "relax",
                        "unwind",
                        "calm down"
                    ],
                    "antonyms": [
                        "stress",
                        "panic"
                    ],
                    "common_phrases": [
                        "chill out",
                        "just chill"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Расслабиться, успокоиться или перестать волноваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расслабляться",
                                "target_lang_sense_clarification": "отдыхать, снимать напряжение"
                            },
                            {
                                "target_lang_word": "остыть",
                                "target_lang_sense_clarification": "успокоиться, перестать нервничать"
                            },
                            {
                                "target_lang_word": "отдыхать",
                                "target_lang_sense_clarification": "проводить время без дел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7b09e5a-3853-34cd-982b-8fb0bb6a7e79",
                    "sense_definition": "To make something cold or to lower its temperature.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_temp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>chill</w> the white wine in the fridge before the guests arrive.",

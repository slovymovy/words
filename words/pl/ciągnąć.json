    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8a1adddd-d674-338a-91e3-e914fde79ba8",
                    "sense_definition": "Przemieszczać kogoś lub coś w swoim kierunku, używając siły fizycznej; wlec coś za sobą.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koń z trudem <w>ciągnął</w> ciężki wóz pod górę.",
                            "target_lang_translations": {
                                "ru": "Лошадь с трудом <w>тянула</w> тяжелую повозку в гору."
                            }
                        },
                        {
                            "text": "Dziecko <w>ciągnęło</w> za sobą ulubioną zabawkę.",
                            "target_lang_translations": {
                                "ru": "Ребенок <w>тащил</w> за собой любимую игрушку."
                            }
                        }
                    ],
                    "synonyms": [
                        "wlec",
                        "holować",
                        "szarpać"
                    ],
                    "antonyms": [
                        "pchać",
                        "popychać"
                    ],
                    "common_phrases": [
                        "ciągnąć wózek",
                        "ciągnąć za włosy",
                        "ciągnąć linę",
                        "ciągnąć za rękę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать кого-то или что-то к себе, применяя физическую силу; тащить что-то за собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "приближать к себе с усилием"
                            },
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "волочить за собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4828584-7247-30cd-b26b-995f7e238c18",
                    "sense_definition": "Zajmować dużą przestrzeń, rozpościerać się na długim odcinku (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "extension_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malownicze pola <w>ciągnęły</w> się aż po horyzont.",
                            "target_lang_translations": {
                                "ru": "Живописные поля <w>тянулись</w> до самого горизонта."
                            }
                        },
                        {
                            "text": "Kolejka do kasy <w>ciągnęła</w> się przez cały sklep.",
                            "target_lang_translations": {
                                "ru": "Очередь в кассу <w>тянулась</w> через весь магазин."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozciągać się",
                        "rozpościerać się",
                        "snuć się"
                    ],
                    "common_phrases": [
                        "droga się ciągnie",
                        "las się ciągnie",
                        "pasmo górskie się ciągnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимать большое пространство, простираться на большое расстояние (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "простираться в пространстве"
                            },
                            {
                                "target_lang_word": "простираться",
                                "target_lang_sense_clarification": "занимать пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df73068c-f7c5-3402-86b0-0cab6347ce90",
                    "sense_definition": "Przemieszczać się powoli lub w dużej grupie w określonym kierunku (o ludziach, wojskach, chmurach lub ptakach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciemne chmury <w>ciągnęły</w> po niebie, zapowiadając burzę.",
                            "target_lang_translations": {
                                "ru": "Темные тучи <w>тянулись</w> по небу, предвещая грозу."
                            }
                        },
                        {
                            "text": "Jesienią ptaki <w>ciągną</w> na południe do ciepłych krajów.",
                            "target_lang_translations": {
                                "ru": "Осенью птицы <w>тянутся</w> на юг в теплые края."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmierzać",
                        "wędrować",
                        "nadciągać"
                    ],
                    "common_phrases": [
                        "ciągnąć na południe",
                        "wojska ciągną",
                        "chmury ciągną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медленно или большой группой двигаться в определенном направлении (о людях, войсках, облаках или птицах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "двигаться вереницей или массой"
                            },
                            {
                                "target_lang_word": "двигаться",
                                "target_lang_sense_clarification": "перемещаться в направлении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "748fa194-9f94-3fd3-9328-4ceb3b757b38",
                    "sense_definition": "Wybierać losowo jeden z wielu elementów, na przykład podczas loterii.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <w>ciągnąć</w> zapałki, żeby zdecydować, kto pójdzie pierwszy.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>тянуть</w> спички, чтобы решить, кто пойдет первым."
                            }
                        },
                        {
                            "text": "Każdy uczeń po kolei <w>ciągnął</w> los z pytaniem egzaminacyjnym.",
                            "target_lang_translations": {
                                "ru": "Каждый ученик по очереди <w>тянул</w> билет с экзаменационным вопросом."
                            }
                        }
                    ],
                    "synonyms": [
                        "losować"
                    ],
                    "common_phrases": [
                        "ciągnąć losy",
                        "ciągnąć zapałki",
                        "ciągnąć karty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбирать наугад один из многих элементов, например, во время лотереи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "жребий или билет"
                            },
                            {
                                "target_lang_word": "вытягивать",
                                "target_lang_sense_clarification": "доставать наугад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdcdbf83-3bca-3470-b690-11980b40d9c7",
                    "sense_definition": "Kontynuować rozpoczętą czynność, rozmowę lub dzieło; sprawiać, że coś trwa dalej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "continuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie możemy <w>ciągnąć</w> tej bezsensownej dyskusji w nieskończoność.",
                            "target_lang_translations": {
                                "ru": "Мы не можем <w>тянуть</w> эту бессмысленную дискуссию до бесконечности."
                            }
                        },
                        {
                            "text": "Autor postanowił <w>ciągnąć</w> wątek głównego bohatera w kolejnym tomie.",
                            "target_lang_translations": {
                                "ru": "Автор решил <w>продолжать</w> сюжетную линию главного героя в следующем томе."
                            }
                        }
                    ],
                    "synonyms": [
                        "kontynuować",
                        "przedłużać"
                    ],
                    "common_phrases": [
                        "ciągnąć rozmowę",
                        "ciągnąć temat",
                        "ciągnąć sprawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжать начатое действие, разговор или дело; заставлять что-то длиться дальше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продолжать",
                                "target_lang_sense_clarification": "делать дальше"
                            },
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "разг. поддерживать или затягивать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da689386-3b3d-3fb5-aa66-31590cc0967a",
                    "sense_definition": "Budzić w kimś silne zainteresowanie, pragnienie bycia w danym miejscu lub z daną osobą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <w>ciągnęło</w> go w wysokie góry i do dzikiej przyrody.",
                            "target_lang_translations": {
                                "ru": "Его всегда <w>тянуло</w> в высокие горы и к дикой природе."
                            }
                        },
                        {
                            "text": "Mimo ostrzeżeń, nastolatka wciąż <w>ciągnęło</w> do złego towarzystwa.",
                            "target_lang_translations": {
                                "ru": "Несмотря на предупреждения, подростка все равно <w>тянуло</w> в плохую компанию."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyciągać",
                        "nęcić",
                        "kusić"
                    ],
                    "common_phrases": [
                        "ciągnie wilka do lasu",
                        "ciągnąć do domu",
                        "ciągnąć do ludzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-то сильный интерес, желание быть в определенном месте или с определенным человеком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "влечь к себе (безличн.)"
                            },
                            {
                                "target_lang_word": "влечь",
                                "target_lang_sense_clarification": "притягивать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b08e8291-fda6-3f62-b86d-4174d11df16e",
                    "sense_definition": "Trwać zbyt długo, przedłużać się w czasie w sposób nużący (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "extension_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zebranie w pracy <w>ciągnęło</w> się godzinami bez żadnych konkretnych ustaleń.",
                            "target_lang_translations": {
                                "ru": "Собрание на работе <w>тянулось</w> часами без каких-либо конкретных решений."
                            }
                        },
                        {
                            "text": "Zima w tym roku wyjątkowo się <w>ciągnie</w> i wszyscy czekają na wiosnę.",
                            "target_lang_translations": {
                                "ru": "Зима в этом году необычайно <w>тянется</w>, и все ждут весны."
                            }
                        }
                    ],
                    "synonyms": [
                        "dłużyć się",
                        "przeciągać się",
                        "wlec się"
                    ],
                    "common_phrases": [
                        "czas się ciągnie",
                        "sprawa się ciągnie",
                        "dni się ciągną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длиться слишком долго, нудно продолжаться во времени (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "медленно проходить (о времени)"
                            },
                            {
                                "target_lang_word": "длиться",
                                "target_lang_sense_clarification": "продолжаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e403285-6f1f-36cc-aeb1-77836a33c134",
                    "sense_definition": "Rozprowadzać lub instalować przewody, rury lub linie przesyłowe na jakiejś przestrzeni.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "installation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elektrycy zaczęli <w>ciągnąć</w> nowe kable w całym budynku.",
                            "target_lang_translations": {
                                "ru": "Электрики начали <w>тянуть</w> новые кабели во всем здании."
                            }
                        },
                        {
                            "text": "Gmina planuje <w>ciągnąć</w> wodociąg do najdalszych zabudowań wsi.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет планирует <w>тянуть</w> водопровод к самым дальним постройкам деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "prowadzić",
                        "instalować",
                        "kłaść"
                    ],
                    "common_phrases": [
                        "ciągnąć kable",
                        "ciągnąć prąd",
                        "ciągnąć rury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прокладывать или устанавливать провода, трубы или линии передачи на каком-либо пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "прокладывать коммуникации"
                            },
                            {
                                "target_lang_word": "прокладывать",
                                "target_lang_sense_clarification": "строить линию/путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4117ac6-768f-3432-b48f-026fedf9b128",
                    "sense_definition": "Uzyskiwać korzyści materialne, pieniądze lub informacje od kogoś, często w sposób natarczywy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma przez lata <w>ciągnęła</w> ogromne zyski z eksportu ropy.",
                            "target_lang_translations": {
                                "ru": "Фирма годами <w>извлекала</w> огромную прибыль из экспорта нефти."
                            }
                        },
                        {
                            "text": "On wciąż próbuje <w>ciągnąć</w> pieniądze od swoich starszych rodziców.",
                            "target_lang_translations": {
                                "ru": "Он все еще пытается <w>тянуть</w> деньги со своих пожилых родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "czerpać",
                        "wyciągać",
                        "doić"
                    ],
                    "common_phrases": [
                        "ciągnąć zyski",
                        "ciągnąć korzyści",
                        "ciągnąć pieniądze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used regarding money or benefits"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать материальную выгоду, деньги или информацию от кого-то, часто настойчивым образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытягивать",
                                "target_lang_sense_clarification": "деньги или информацию"
                            },
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "разг. вымогать"
                            },
                            {
                                "target_lang_word": "извлекать",
                                "target_lang_sense_clarification": "получать выгоду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39cf7792-5c5a-3ab0-be4b-45490e1657f8",
                    "sense_definition": "Pić płyn powoli, wciągając go do ust, lub pić alkohol w dużych ilościach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedział przy barze i powoli <w>ciągnął</w> zimne piwo z kufla.",
                            "target_lang_translations": {
                                "ru": "Он сидел у бара и медленно <w>тянул</w> холодное пиво из кружки."
                            }
                        },
                        {
                            "text": "Dziecko <w>ciągnęło</w> sok przez kolorową słomkę.",
                            "target_lang_translations": {
                                "ru": "Ребенок <w>тянул</w> сок через цветную соломинку."
                            }
                        }
                    ],
                    "synonyms": [
                        "sączyć",
                        "pić"
                    ],
                    "common_phrases": [
                        "ciągnąć piwo",
                        "ciągnąć z butelki",
                        "ciągnąć przez słomkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "especially when referring to alcohol"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пить жидкость медленно, втягивая ее в рот, или пить алкоголь в больших количествах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуть",
                                "target_lang_sense_clarification": "медленно пить"
                            },
                            {
                                "target_lang_word": "потягивать",
                                "target_lang_sense_clarification": "пить маленькими глотками"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
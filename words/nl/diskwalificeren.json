                {
                    "sense_id": "3dc224a8-1280-3b11-820e-230f798b08b4",
                    "sense_definition": "Iemand uitsluiten van deelname aan een wedstrijd, examen of functie omdat diegene de regels heeft overtreden of niet aan de voorwaarden voldoet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "uitsluiten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jury besloot de atleet te <w>diskwalificeren</w> nadat hij drie keer vals was gestart.",
                            "target_lang_translations": {
                                "ru": "Жюри решило <w>дисквалифицировать</w> спортсмена после того, как он трижды совершил фальстарт."
                            }
                        },
                        {
                            "text": "Als je de regels niet volgt, zullen ze je onmiddellijk voor de competitie <w>diskwalificeren</w>.",
                            "target_lang_translations": {
                                "ru": "Если ты не будешь соблюдать правила, тебя немедленно <w>дисквалифицируют</w> с соревнований."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitsluiten",
                        "royeren"
                    ],
                    "antonyms": [
                        "kwalificeren",
                        "toelaten"
                    ],
                    "common_phrases": [
                        "wegens doping diskwalificeren",
                        "iemand laten diskwalificeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исключить кого-либо из участия в соревновании, экзамене или должности из-за нарушения правил или несоответствия условиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисквалифицировать",
                                "target_lang_sense_clarification": "спорт, соревнования"
                            },
                            {
                                "target_lang_word": "отстранить",
                                "target_lang_sense_clarification": "от участия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87f6be6a-65fa-3246-b512-c6918f484b9e",
                    "sense_definition": "Iets of iemand als ongeschikt, onwaardig of ongeloofwaardig bestempelen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "afkeuren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door zijn ongenuanceerde uitspraken wist hij zichzelf als serieuze gesprekspartner te <w>diskwalificeren</w>.",

                {
                    "sense_id": "5543a949-c317-3b13-885b-068bb4681c7f",
                    "sense_definition": "Een afzonderlijk deel of een specifieke afdeling van een grotere organisatie, tekst of gebied.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze <w>sectie</w> is verantwoordelijk voor de administratieve afhandeling van de projecten.",
                            "target_lang_translations": {
                                "en": "Our <w>section</w> is responsible for the administrative handling of the projects.",
                                "ru": "Наша <w>секция</w> отвечает за административное сопровождение проектов."
                            }
                        },
                        {
                            "text": "In de bibliotheek kun je dit boek vinden in de <w>sectie</w> voor geschiedenis.",
                            "target_lang_translations": {
                                "en": "In the library you can find this book in the history <w>section</w>.",
                                "ru": "В библиотеке вы можете найти эту книгу в <w>разделе</w> истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdeling"
                    ],
                    "common_phrases": [
                        "administratieve sectie",
                        "militaire sectie"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate part or a specific division of a larger organization, text, or area.",
                        "ru": "Отдельная часть или подразделение организации, текста или области."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "section",
                                "target_lang_sense_clarification": "part or subdivision"

{
    "word_family": [
        "obsługa",
        "obsługiwać",
        "usługa",
        "służyć",
        "służący"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9ca17611-1252-3602-adfd-8fbd695b197f",
                    "sense_definition": "Zająć się klientem lub gościem, spełniając jego oczekiwania, przyjmując zamówienie lub wykonując zleconą usługę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "service_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner podszedł do naszego stolika, aby nas szybko <w>obsłużyć</w>.",
                            "target_lang_translations": {
                                "ru": "Официант подошел к нашему столику, чтобы быстро нас <w>обслужить</w>."
                            }
                        },
                        {
                            "text": "Ekspedientka w sklepie starała się uprzejmie <w>obsłużyć</w> każdego klienta.",
                            "target_lang_translations": {
                                "ru": "Продавщица в магазине старалась вежливо <w>обслужить</w> каждого клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "zająć się",
                        "ugościć"
                    ],
                    "antonyms": [
                        "zlekceważyć",
                        "zignorować"
                    ],
                    "common_phrases": [
                        "obsłużyć klienta",
                        "szybko obsłużyć",
                        "obsłużyć gości",
                        "obsłużyć przy kasie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заняться клиентом или гостем, выполняя его ожидания, принимая заказ или оказывая услугу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обслужить",
                                "target_lang_sense_clarification": "клиента, гостя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9de1da2a-59b8-33af-9dfa-b3e9c38e43f5",
                    "sense_definition": "Użyć urządzenia, maszyny lub programu komputerowego, wykonując na nich określone działania w celu ich poprawnego działania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_operation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz przeczytać instrukcję, żeby wiedzieć, jak bezpiecznie <w>obsłużyć</w> tę maszynę.",
                            "target_lang_translations": {
                                "ru": "Ты должен прочитать инструкцию, чтобы знать, как безопасно <w>управлять</w> этой машиной."
                            }
                        },
                        {
                            "text": "Mój dziadek nauczył się <w>obsłużyć</w> swój nowy telefon komórkowy.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка научился <w>пользоваться</w> своим новым мобильным телефоном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obsłużyć komputer",
                        "obsłużyć urządzenie",
                        "obsłużyć maszynę",
                        "łatwo obsłużyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать устройство, машину или компьютерную программу, выполняя действия, необходимые для их работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "машиной, механизмом"
                            },
                            {
                                "target_lang_word": "пользоваться",
                                "target_lang_sense_clarification": "устройством, телефоном"
                            },
                            {
                                "target_lang_word": "работать",
                                "target_lang_sense_clarification": "с программой, на компьютере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad7ea54e-f801-37ea-860c-bf2d7b2ab3e9",
                    "sense_definition": "Wykonać czynność dla samego siebie, bez pomocy innych osób; często używane w kontekście jedzenia lub korzystania z usług samoobsługowych.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "service_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na tej stacji benzynowej kierowca musi <w>obsłużyć</w> się sam.",
                            "target_lang_translations": {
                                "ru": "На этой заправке водитель должен <w>обслужить</w> себя сам."
                            }
                        },
                        {
                            "text": "Podczas przyjęcia goście mogli podejść do bufetu i <w>obsłużyć</w> się do woli.",
                            "target_lang_translations": {
                                "ru": "Во время приема гости могли подойти к буфету и <w>накладывать</w> себе еду вволю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obsłużyć się samemu",
                        "można się obsłużyć"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнить действие для самого себя без посторонней помощи; часто используется в контексте еды или самообслуживания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обслужить",
                                "target_lang_sense_clarification": "себя (возвратный глагол)"
                            },
                            {
                                "target_lang_word": "накладывать",
                                "target_lang_sense_clarification": "себе еду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0a4d726-3f5d-3740-9c00-61adf6d6e08e",
                    "sense_definition": "Zapewnić usługi, transport lub przetwarzanie danych dla określonej liczby osób, lub na danym obszarze.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "logistics_capacity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowoczesne lotnisko może <w>obsłużyć</w> miliony pasażerów każdego roku.",
                            "target_lang_translations": {
                                "ru": "Современный аэропорт может <w>обслужить</w> миллионы пассажиров каждый год."
                            }
                        },
                        {
                            "text": "Ten serwer jest zbyt słaby, aby <w>obsłużyć</w> tak duży ruch w sieci.",
                            "target_lang_translations": {
                                "ru": "Этот сервер слишком слаб, чтобы <w>обработать</w> такой большой трафик в сети."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obsłużyć pasażerów",
                        "obsłużyć ruch",
                        "obsłużyć zamówienia",
                        "obsłużyć region"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечить услуги, перевозку или обработку данных для определенного количества людей или в определенном регионе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обслужить",
                                "target_lang_sense_clarification": "пассажиров, поток людей"
                            },
                            {
                                "target_lang_word": "обработать",
                                "target_lang_sense_clarification": "заказы, данные, трафик"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "antonyms": [
                        "введение",
                        "наложение",
                        "установление"
                    ],
                    "common_phrases": [
                        "снятие запрета",
                        "снятие санкций",
                        "снятие судимости",
                        "снятие блокады"
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "opwinden",
        "opwindend",
        "opgewonden"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6d5b2fb4-3e73-3d00-bbee-12412b17c938",
                    "sense_definition": "Een toestand van hevige emotie, spanning of enthousiasme.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In alle <w>opwinding</w> vergat hij helemaal op de tijd te letten.",
                            "target_lang_translations": {
                                "en": "In all the <w>excitement</w>, he completely forgot to watch the time.",
                                "ru": "В общем <w>волнении</w> он совсем забыл следить за временем."
                            }
                        },
                        {
                            "text": "Er ontstond grote <w>opwinding</w> toen de uitslag van de wedstrijd bekend werd.",
                            "target_lang_translations": {
                                "en": "Great <w>excitement</w> arose when the result of the match became known.",
                                "ru": "Поднялось сильное <w>волнение</w>, когда стали известны результаты матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "spanning",
                        "onrust",
                        "commotie",
                        "enthousiasme",
                        "agitatie"
                    ],
                    "antonyms": [
                        "kalmte",
                        "rust"
                    ],
                    "common_phrases": [
                        "in alle opwinding",
                        "grote opwinding",
                        "vol opwinding"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of intense emotion, tension, or enthusiasm.",
                        "ru": "Состояние сильной эмоции, напряжения или энтузиазма."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "excitement",
                                "target_lang_sense_clarification": "state of enthusiasm or eagerness"
                            },
                            {
                                "target_lang_word": "agitation",
                                "target_lang_sense_clarification": "state of anxiety or nervous excitement"
                            },
                            {
                                "target_lang_word": "commotion",
                                "target_lang_sense_clarification": "noisy disturbance or confusion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "волнение",
                                "target_lang_sense_clarification": "эмоциональное состояние, беспокойство"
                            },
                            {
                                "target_lang_word": "возбуждение",
                                "target_lang_sense_clarification": "повышенная активность, энтузиазм"
                            },
                            {
                                "target_lang_word": "ажиотаж",
                                "target_lang_sense_clarification": "сильное общественное возбуждение, суматоха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d6897b4-4656-351a-a4b5-25399d220456",
                    "sense_definition": "De handeling van het <w>opwinden</w>, bijvoorbeeld van een draad of een mechanisme.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De automatische <w>opwinding</w> van het horloge werkt niet meer goed.",
                            "target_lang_translations": {
                                "en": "The automatic <w>winding</w> of the watch no longer works properly.",
                                "ru": "Автоматический <w>завод</w> часов больше не работает должным образом."
                            }
                        },
                        {
                            "text": "Zorg voor een strakke <w>opwinding</w> van de kabel op de haspel.",
                            "target_lang_translations": {
                                "en": "Ensure a tight <w>winding</w> of the cable onto the reel.",
                                "ru": "Обеспечьте плотную <w>намотку</w> кабеля на катушку."
                            }
                        }
                    ],
                    "antonyms": [
                        "afwikkeling"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of winding something up, such as a thread or a mechanism.",
                        "ru": "Действие по наматыванию, например, нити или завод механизма."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "winding",
                                "target_lang_sense_clarification": "action of coiling or tightening a mechanism"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "завод",
                                "target_lang_sense_clarification": "механизма, часов"
                            },
                            {
                                "target_lang_word": "намотка",
                                "target_lang_sense_clarification": "кабеля, нити"
                            }
                        ]
                    }
                }
            ]
        }
    ]

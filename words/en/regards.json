    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1e37245-7570-3d72-8e2b-907809f07506",
                    "sense_definition": "A greeting used at the end of a letter or email.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "greetings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please accept my kind <w>regards</w> as we conclude this business.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, примите мои искренние <w>пожелания</w> в завершение этого дела."
                            }
                        },
                        {
                            "text": "She signed the email with warm <w>regards</w> before sending it.",
                            "target_lang_translations": {
                                "ru": "Она подписала письмо словами «с тёплыми <w>пожеланиями</w>» перед отправкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "best wishes",
                        "sincerely"
                    ],
                    "common_phrases": [
                        "kind regards",
                        "best regards",
                        "warm regards"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заключительная фраза вежливости в конце письма или электронного сообщения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с уважением",
                                "target_lang_sense_clarification": "стандартная официальная форма"
                            },
                            {
                                "target_lang_word": "с наилучшими пожеланиями",
                                "target_lang_sense_clarification": "менее формально, дружелюбно"
                            },
                            {
                                "target_lang_word": "всего доброго",
                                "target_lang_sense_clarification": "нейтральное завершение письма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b0dbe50-7889-3128-b2d3-4da6585593f7",
                    "sense_definition": "Good wishes or greetings sent to someone through another person.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "greetings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please give my <w>regards</w> to your brother when you see him.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай мой <w>привет</w> твоему брату, когда увидишь его."
                            }
                        },
                        {
                            "text": "He sends his <w>regards</w> to everyone in the office.",
                            "target_lang_translations": {
                                "ru": "Он передаёт всем в офисе <w>привет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "greetings",
                        "best wishes",
                        "compliments"
                    ],
                    "common_phrases": [
                        "give my regards",
                        "send regards"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добрые пожелания или приветствия, передаваемые кому-либо через другого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "дружеское приветствие"
                            },
                            {
                                "target_lang_word": "поклон",
                                "target_lang_sense_clarification": "почтительное или устаревшее приветствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df27d4d5-11c6-326b-adb7-1ae141d6013d",
                    "sense_definition": "Plural of <w>regard</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In some <w>regards</w>, the new plan is better than the old one.",
                            "target_lang_translations": {
                                "ru": "В некоторых <w>отношениях</w> новый план лучше старого."
                            }
                        },
                        {
                            "text": "The two reports are similar in many <w>regards</w>.",
                            "target_lang_translations": {
                                "ru": "Эти два отчёта схожи во многих <w>отношениях</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aspects",
                        "respects"
                    ],
                    "common_phrases": [
                        "in some regards",
                        "in many regards"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного regard (аспекты, отношения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "точка зрения, аспект"
                            },
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "в каком-либо отношении или смысле"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "736abb75-ca8d-32b2-96fc-215de64f088b",
                    "sense_definition": "Third-person singular simple present indicative of <w>regard</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>regards</w> him as one of her closest friends.",
                            "target_lang_translations": {
                                "ru": "Она <w>считает</w> его одним из своих самых близких друзей."
                            }
                        },
                        {
                            "text": "The law <w>regards</w> all citizens as equal.",
                            "target_lang_translations": {
                                "ru": "Закон <w>рассматривает</w> всех граждан как равных."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "third-person singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма третьего лица единственного числа глагола regard (считает, рассматривает)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение, расценивать"
                            },
                            {
                                "target_lang_word": "рассматривать",
                                "target_lang_sense_clarification": "воспринимать определенным образом"
                            },
                            {
                                "target_lang_word": "относиться",
                                "target_lang_sense_clarification": "проявлять отношение"
                            }
                        ]
                    }
                }
            ]
        }

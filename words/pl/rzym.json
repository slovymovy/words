                    "sense_id": "1d22a76b-c2bb-386c-badc-21178aa34db2",
                    "sense_definition": "stolica Włoch, duże miasto o znaczeniu historycznym położone nad rzeką Tyber",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "city_location",
                    "name_type": "place_name",

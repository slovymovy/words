{
    "word_family": [
        "bleat",
        "bleating",
        "bleater"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a8e964ae-8657-3263-a4ed-7b4edce819e9",
                    "sense_definition": "In a manner resembling the weak, wavering cry of a sheep or goat; often used to describe a complaining tone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lost lamb cried <w>bleatingly</w> for its mother across the empty field.",
                            "target_lang_translations": {
                                "ru": "Потерявшийся ягненок <w>жалобно</w> звал свою мать посреди пустого поля."
                            }
                        },
                        {
                            "text": "He spoke <w>bleatingly</w> about his minor injuries, annoying everyone in the room.",
                            "target_lang_translations": {
                                "ru": "Он <w>жалобно</w> рассказывал о своих мелких ушибах, раздражая всех в комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "plaintively",
                        "weakly"
                    ],
                    "antonyms": [
                        "forcefully",
                        "assertively"
                    ],
                    "common_phrases": [
                        "cried bleatingly",
                        "spoke bleatingly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Манера, напоминающая слабое, дрожащее блеяние овцы или козы; часто используется для описания жалобного тона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалобно",
                                "target_lang_sense_clarification": "выражая страдание или недовольство слабым голосом"
                            },
                            {
                                "target_lang_word": "блеющим голосом",
                                "target_lang_sense_clarification": "дрожащим, слабым тоном, напоминающим блеяние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c3a2495-5bc5-3fbe-b7a3-0a3c412427f6",
                    "sense_definition": "In a very obvious, flagrant, or conspicuous manner.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The director released a new movie that is <w>bleatingly</w> American in its style and themes.",
                            "target_lang_translations": {
                                "ru": "Режиссер выпустил новый фильм, который <w>вопиюще</w> американский по своему стилю и тематике."
                            }
                        },
                        {
                            "text": "It was <w>bleatingly</w> obvious that the team had not prepared for the presentation.",
                            "target_lang_translations": {
                                "ru": "Было <w>совершенно</w> очевидно, что команда не подготовилась к презентации."
                            }
                        }
                    ],
                    "synonyms": [
                        "blatantly",
                        "flagrantly",
                        "conspicuously"
                    ],
                    "antonyms": [
                        "subtly",
                        "obscurely"
                    ],
                    "common_phrases": [
                        "bleatingly obvious"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень явно, вопиюще или заметно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вопиюще",
                                "target_lang_sense_clarification": "вызывающе очевидно или грубо"
                            },
                            {
                                "target_lang_word": "совершенно",
                                "target_lang_sense_clarification": "используется для усиления очевидности факта"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
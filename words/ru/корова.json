    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4a347203-4236-3d6b-b4bb-d592c4068824",
                    "sense_definition": "Крупное домашнее животное, которое даёт молоко; самка быка.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В деревне у бабушки есть своя <корова>, которая даёт вкусное молоко.",
                            "target_lang_translations": {
                                "en": "In the village, my grandmother has her own <w>cow</w> that gives delicious milk."
                            }
                        },
                        {
                            "text": "Фермер рано утром пошёл в хлев доить <корову>.",
                            "target_lang_translations": {
                                "en": "The farmer went to the barn early in the morning to milk the <w>cow</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "бык"
                    ],
                    "common_phrases": [
                        "доить корову",
                        "священная корова",
                        "дойная корова",
                        "как корова языком слизала"
                    ],
                    "target_lang_definitions": {
                        "en": "A large domestic animal kept for milk or meat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cow",
                                "target_lang_sense_clarification": "animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8dfb6b2-1ded-3b80-9bcf-6831a093e240",
                    "sense_definition": "Толстая, неуклюжая или глупая женщина.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта <корова> наступила мне на ногу в автобусе и даже не извинилась.",
                            "target_lang_translations": {
                                "en": "That <w>cow</w> stepped on my foot on the bus and didn't even apologize."
                            }
                        },
                        {
                            "text": "Она двигалась по кухне медленно и неаккуратно, как настоящая <корова>.",
                            "target_lang_translations": {
                                "en": "She moved around the kitchen slowly and clumsily, like a real <w>cow</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "неповоротливая корова"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "rude, derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A derogatory term for a clumsy, stupid, or unpleasant woman."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cow",
                                "target_lang_sense_clarification": "insult"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17c17cf7-141b-3b93-abf8-eae8d0dca2a7",
                    "sense_definition": "Самка некоторых крупных диких рогатых животных (лося, оленя).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотники увидели на опушке леса лосиную <корову> с маленьким телёнком.",
                            "target_lang_translations": {
                                "en": "The hunters saw a moose <w>cow</w> with a small calf at the edge of the forest."
                            }
                        },
                        {
                            "text": "Огромная морская <корова> медленно проплыла мимо лодки.",
                            "target_lang_translations": {
                                "en": "A huge sea <w>cow</w> swam slowly past the boat."
                            }
                        }
                    ],
                    "antonyms": [
                        "бык"
                    ],
                    "common_phrases": [
                        "морская корова",
                        "лосиная корова"
                    ],
                    "target_lang_definitions": {
                        "en": "The female of certain large animals (e.g., moose, whale)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cow",
                                "target_lang_sense_clarification": "female animal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
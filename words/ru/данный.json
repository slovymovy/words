            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e6b57946-864d-330e-bbf6-b5a171d01515",
                    "sense_definition": "Именно тот, о котором идёт речь; рассматриваемый в настоящее время.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На <данный> момент мы не можем предоставить точную информацию.",
                            "target_lang_translations": {
                                "en": "At the <present> moment we cannot provide exact information."
                            }
                        },
                        {
                            "text": "В <данном> случае вам лучше обратиться к специалисту.",
                            "target_lang_translations": {
                                "en": "In <this> case, you had better contact a specialist."
                            }
                        }
                    ],
                    "synonyms": [
                        "э́тот",
                        "настоя́щий"
                    ],
                    "common_phrases": [
                        "на данный момент",
                        "в данном случае",
                        "в данное время"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to the specific person, thing, or time being discussed or currently under consideration."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "this",
                                "target_lang_sense_clarification": "Indicates the specific thing just mentioned or present"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "Refers to the current time or the matter at hand (formal)"
                            },
                            {
                                "target_lang_word": "current",
                                "target_lang_sense_clarification": "Happening or existing now"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec2784b2-8f07-36b7-9b0b-cd5e4d9f81e3",
                    "sense_definition": "Заранее определённый, известный или установленный (часто в условиях задачи).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "specified",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученик должен решить задачу при <данных> условиях.",
                            "target_lang_translations": {
                                "en": "The student must solve the problem under the <given> conditions."
                            }
                        },
                        {
                            "text": "За <данный> отрезок времени ничего не изменилось.",
                            "target_lang_translations": {
                                "en": "Nothing has changed during the <given> period of time."
                            }
                        }
                    ],
                    "synonyms": [
                        "определённый",
                        "известный"
                    ],
                    "common_phrases": [
                        "при данных условиях"
                    ],
                    "target_lang_definitions": {
                        "en": "Specified, known, or established beforehand, often used as a condition in logic or mathematics."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "given",
                                "target_lang_sense_clarification": "Specified or assumed as a fact"
                            },
                            {
                                "target_lang_word": "particular",
                                "target_lang_sense_clarification": "Specific or distinct from others"
                            }
                        ]
                    }
                }
            ]
        },
        {

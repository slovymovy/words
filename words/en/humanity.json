            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ca511cc-404e-373d-8960-8a0b2d3bee0e",
                    "sense_definition": "Humankind; human beings as a group.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Climate change is a serious threat to the future of <w>humanity</w>.",
                            "target_lang_translations": {
                                "ru": "Изменение климата — серьезная угроза будущему <w>человечества</w>."
                            }
                        },
                        {
                            "text": "The internet connects <w>humanity</w> in ways that were previously impossible.",
                            "target_lang_translations": {
                                "ru": "Интернет соединяет <w>человечество</w> способами, которые раньше были невозможны."
                            }
                        }
                    ],
                    "synonyms": [
                        "humankind",
                        "mankind",
                        "human race",
                        "Man"
                    ],
                    "common_phrases": [
                        "crimes against humanity",
                        "history of humanity",
                        "all of humanity",
                        "service to humanity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человеческий род; люди как единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человечество",
                                "target_lang_sense_clarification": "все люди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c574426e-8ed7-3807-9d10-b8e9a0035370",
                    "sense_definition": "The quality of being benevolent; humane traits of character.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She treated the prisoners of war with great <w>humanity</w> and kindness.",
                            "target_lang_translations": {
                                "ru": "Она относилась к военнопленным с большой <w>человечностью</w> и добротой."
                            }
                        },
                        {
                            "text": "We must never lose our sense of <w>humanity</w> when dealing with others.",
                            "target_lang_translations": {
                                "ru": "Мы никогда не должны терять чувство <w>человечности</w> при общении с другими."
                            }
                        }
                    ],
                    "synonyms": [
                        "humaneness"
                    ],
                    "common_phrases": [
                        "treat with humanity",
                        "show humanity",
                        "sense of humanity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество доброжелательности; гуманные черты характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человечность",
                                "target_lang_sense_clarification": "доброта, отзывчивость"
                            },
                            {
                                "target_lang_word": "гуманность",
                                "target_lang_sense_clarification": "человеколюбие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22040e28-4d98-3e98-855b-53fd64605f7c",
                    "sense_definition": "The human condition or nature.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The novel explores the depths of our shared <w>humanity</w>.",
                            "target_lang_translations": {
                                "ru": "Роман исследует глубины нашей общей <w>человеческой природы</w>."
                            }
                        },
                        {
                            "text": "He appealed to their common <w>humanity</w> to stop the fighting.",
                            "target_lang_translations": {
                                "ru": "Он взывал к их общей <w>человечности</w>, чтобы остановить сражение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shared humanity",
                        "common humanity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человеческая природа или сущность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человеческая природа",
                                "target_lang_sense_clarification": "сущность человека"
                            },
                            {
                                "target_lang_word": "человечность",
                                "target_lang_sense_clarification": "принадлежность к людям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "407f36d1-469b-3993-ad17-55cc5f9ba40c",
                    "sense_definition": "Any academic subject belonging to the humanities.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Philosophy is often classified as a <w>humanity</w> rather than a science.",
                            "target_lang_translations": {
                                "ru": "Философию часто классифицируют как <w>гуманитарную науку</w>, а не естественную."
                            }
                        },
                        {
                            "text": "She decided to study a <w>humanity</w> at the university.",
                            "target_lang_translations": {
                                "ru": "Она решила изучать <w>гуманитарную дисциплину</w> в университете."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Учебная дисциплина, относящаяся к гуманитарным наукам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гуманитарная наука",
                                "target_lang_sense_clarification": "учебный предмет"
                            },
                            {
                                "target_lang_word": "гуманитарная дисциплина",
                                "target_lang_sense_clarification": "область изучения"
                            }
                        ]
                    }
                }
            ]
        }

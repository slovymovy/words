    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6b81181-fd62-36ce-9f86-9bdb312b614a",
                    "sense_definition": "Предложение, выдвинутое для обсуждения, или начало какого-либо нового дела.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство поддержало эту важную социальную <w>инициативу</w>.",
                            "target_lang_translations": {
                                "en": "The government supported this important social <w>initiative</w>."
                            }
                        },
                        {
                            "text": "Мы решили выступить с <w>инициативой</w> по озеленению городского парка.",
                            "target_lang_translations": {
                                "en": "We decided to come forward with an <w>initiative</w> to green the city park."
                            }
                        }
                    ],
                    "synonyms": [
                        "почин"
                    ],
                    "common_phrases": [
                        "выступить с инициативой",
                        "поддержать инициативу",
                        "законодательная инициатива"
                    ],
                    "target_lang_definitions": {
                        "en": "A proposal or new plan put forward to solve a problem or improve a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "initiative",
                                "target_lang_sense_clarification": "new plan or project"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79f58445-50be-3160-be7e-b091cda5dfa4",
                    "sense_definition": "Способность и готовность к самостоятельным, активным действиям; предприимчивость.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На новой работе от сотрудников требовалось постоянно проявлять <w>инициативу</w>.",
                            "target_lang_translations": {
                                "en": "At the new job, employees were required to constantly show <w>initiative</w>."
                            }
                        },
                        {
                            "text": "Он помог нам по собственной <w>инициативе</w>, хотя никто его об этом не просил.",
                            "target_lang_translations": {
                                "en": "He helped us on his own <w>initiative</w>, although no one asked him to."
                            }
                        }
                    ],
                    "synonyms": [
                        "инициативность"
                    ],
                    "common_phrases": [
                        "проявлять инициативу",
                        "по собственной инициативе",
                        "отсутствие инициативы"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to decide and act on your own without waiting for someone to tell you what to do."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "initiative",
                                "target_lang_sense_clarification": "ability to act independently"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d37e944c-f027-3876-a128-d7b7b8380b6c",
                    "sense_definition": "Руководящая роль в действиях; преимущество, позволяющее управлять ходом событий (часто в спорте или противостоянии).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В середине матча наша команда смогла перехватить <w>инициативу</w> и забить гол.",
                            "target_lang_translations": {
                                "en": "In the middle of the match, our team managed to seize the <w>initiative</w> and score a goal."
                            }
                        },
                        {
                            "text": "Опытный шахматист владел <w>инициативой</w> на протяжении всей партии.",
                            "target_lang_translations": {
                                "en": "The experienced chess player held the <w>initiative</w> throughout the entire game."
                            }
                        }
                    ],
                    "synonyms": [
                        "лидерство"
                    ],
                    "common_phrases": [
                        "перехватить инициативу",
                        "владеть инициативой",
                        "упустить инициативу"
                    ],
                    "target_lang_definitions": {
                        "en": "The power or opportunity to act or take charge before others do, often giving an advantage in a competition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "initiative",
                                "target_lang_sense_clarification": "advantage or controlling position"
                            },
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "position of advantage"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
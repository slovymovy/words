                },
                {
                    "sense_id": "aa9db7e8-0f66-3b26-9dde-ead7179fe03d",
                    "sense_definition": "The movement of a substance or chemical from one material or location into another.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "scientific_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Plastic packaging prevents the <w>migration</w> of moisture into the food.",
                            "target_lang_translations": {
                                "ru": "Пластиковая упаковка предотвращает <w>миграцию</w> влаги в пищу."
                            }
                        },
                        {
                            "text": "Tests were conducted to measure the <w>migration</w> of chemicals from the container.",
                            "target_lang_translations": {
                                "ru": "Были проведены тесты для измерения <w>миграции</w> химических веществ из контейнера."
                            }
                        }
                    ],
                    "synonyms": [
                        "diffusion",
                        "transfer"
                    ],
                    "common_phrases": [
                        "chemical migration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение вещества или химического элемента из одного материала в другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миграция",
                                "target_lang_sense_clarification": "вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95021c66-9ec1-38f9-a7b5-dabb0bce7f5c",
                    "sense_definition": "The movement of cells to specific locations within an organism.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "scientific_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cell <w>migration</w> is essential for wound healing and immune response.",
                            "target_lang_translations": {
                                "ru": "<w>Миграция</w> клеток необходима для заживления ран и иммунного ответа."
                            }

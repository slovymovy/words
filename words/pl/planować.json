                },
                {
                    "sense_id": "4a5ce3b0-3e9d-38d8-af29-cd3a2dcaadd1",
                    "sense_definition": "Tworzyć projekt techniczny, przestrzenny lub wizualny jakiegoś obiektu, obszaru lub struktury.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "design",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany architekt <w>planuje</w> dla zagranicznego inwestora nowoczesny biurowiec w samym centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Известный архитектор <w>проектирует</w> для иностранного инвестора современное офисное здание в самом центре города."
                            }
                        },
                        {
                            "text": "Gmina <w>planuje</w> budowę nowego parku rozrywki na nieużywanych dotąd terenach podmiejskich.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет <w>планирует</w> строительство нового парка развлечений на неиспользуемых пока пригородных территориях."
                            }
                        }
                    ],
                    "synonyms": [
                        "projektować"
                    ],
                    "common_phrases": [
                        "planować ogród",
                        "planować rozbudowę",
                        "planować wnętrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать технический, пространственный или визуальный проект какого-либо объекта, территории или сооружения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проектировать",
                                "target_lang_sense_clarification": "создавать проект здания или устройства"
                            },
                            {
                                "target_lang_word": "планировать",
                                "target_lang_sense_clarification": "организовывать пространство"
                            }
                        ]
                    }

                    "antonyms": [
                        "she"
                    ],
                    "common_phrases": [
                        "a he or a she"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или животное мужского пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самец",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "мужчина",
                                "target_lang_sense_clarification": "о людях"
                            },
                            {
                                "target_lang_word": "мальчик",
                                "target_lang_sense_clarification": "о ребенке мужского пола"
                            }
                        ]

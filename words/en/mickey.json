    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "401ee3e0-0c43-3b63-9a37-9f2e20d1847d",
                    "sense_definition": "Mockery or teasing; used almost exclusively in the idiom 'take the mickey'.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "mockery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't get upset, he is just taking the <w>mickey</w> out of you.",
                            "target_lang_translations": {
                                "ru": "Не расстраивайся, он просто <w>подшучивает</w> над тобой."
                            }
                        },
                        {
                            "text": "They were taking the <w>mickey</w> out of his strange accent.",
                            "target_lang_translations": {
                                "ru": "Они <w>издевались</w> над его странным акцентом."
                            }
                        }
                    ],
                    "synonyms": [
                        "mockery",
                        "teasing",
                        "ridicule"
                    ],
                    "antonyms": [
                        "praise",
                        "respect"
                    ],
                    "common_phrases": [
                        "take the mickey"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Cockney rhyming slang origin (Mickey Bliss = Piss)"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "UK, Commonwealth"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Насмешка или поддразнивание; используется почти исключительно в идиоме 'take the mickey' (издеваться, подшучивать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насмешка",
                                "target_lang_sense_clarification": "обидная шутка"
                            },
                            {
                                "target_lang_word": "издевка",
                                "target_lang_sense_clarification": "злая насмешка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "031a33d2-e007-3f8c-b12c-09b53f0b131a",
                    "sense_definition": "A small bottle of liquor, typically holding 375 ml (13 oz), shaped to fit in a pocket.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He bought a <w>mickey</w> of rye whiskey for the camping trip.",
                            "target_lang_translations": {
                                "ru": "Он купил <w>чекушку</w> ржаного виски для похода."
                            }
                        },
                        {
                            "text": "It is easier to carry a <w>mickey</w> in your jacket than a full-sized bottle.",
                            "target_lang_translations": {
                                "ru": "Легче нести <w>чекушку</w> в куртке, чем полноразмерную бутылку."
                            }
                        }
                    ],
                    "synonyms": [
                        "flask"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Canada"
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая бутылка спиртного (обычно 375 мл), плоской формы, чтобы помещаться в карман."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чекушка",
                                "target_lang_sense_clarification": "маленькая бутылка водки или другого крепкого алкоголя"

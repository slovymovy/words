                    "antonyms": [
                        "vertical"
                    ],
                    "common_phrases": [
                        "horizontal transmission",
                        "horizontal gene transfer"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к передаче генетического материала или инфекции между особями одного поколения, а не от родителей к потомству."

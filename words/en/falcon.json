{
    "word_family": [
        "falconer",
        "falconry",
        "falconet"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4feea9ae-394a-3b8d-a08d-2d2ce62345c1",
                    "sense_definition": "A bird of prey with long, pointed wings, known for its great speed and ability to dive.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The *falcon* spotted its prey from high in the sky.",
                            "target_lang_translations": {
                                "ru": "<w>Сокол</w> заметил свою добычу с высоты в небе."
                            }
                        },
                        {
                            "text": "A peregrine *falcon* can dive at incredible speeds.",
                            "target_lang_translations": {
                                "ru": "<w>Сокол</w>-сапсан может пикировать с невероятной скоростью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "peregrine falcon",
                        "falconry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хищная птица с длинными острыми крыльями, известная своей огромной скоростью и способностью пикировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сокол",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ec39384-061c-315d-9b80-c51004b352d0",
                    "sense_definition": "A light cannon used historically, particularly from the 15th to the 17th century.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displayed a rusted *falcon* recovered from a shipwreck.",
                            "target_lang_translations": {
                                "ru": "В музее был выставлен ржавый <w>фалькон</w>, поднятый с затонувшего корабля."
                            }
                        },
                        {
                            "text": "Soldiers positioned the *falcon* on the castle wall to defend the gate.",
                            "target_lang_translations": {
                                "ru": "Солдаты установили <w>фалькон</w> на крепостной стене для защиты ворот."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical weapon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкое артиллерийское орудие (пушка), использовавшееся в прошлом, особенно в XV–XVII веках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фалькон",
                                "target_lang_sense_clarification": "старинное орудие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "79a62e86-4a7b-30bb-a834-4052fc8dd093",
                    "sense_definition": "To hunt wild animals using a trained falcon.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nobles *falconed* in the open fields during the autumn season.",
                            "target_lang_translations": {
                                "ru": "Дворяне <w>охотились с соколами</w> в открытых полях в осенний сезон."
                            }
                        },
                        {
                            "text": "She *falconed* for rabbits using her prized bird.",
                            "target_lang_translations": {
                                "ru": "Она <w>охотилась с соколом</w> на кроликов, используя свою лучшую птицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "hawk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охотиться на диких животных с помощью обученного сокола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охотиться с соколом",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2d486374-313f-3feb-a472-a09eb3b2c2c8",
                    "sense_definition": "A surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. *Falcon* will see you in his office now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Фалкон</w> примет вас в своем кабинете сейчас."
                            }
                        },
                        {
                            "text": "The *Falcon* family has lived in this town for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Фалкон</w> живет в этом городе уже несколько поколений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фалкон",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cde2407-68dd-3064-a4a4-c7ba61012fc9",
                    "sense_definition": "A place name used for various towns and communities, such as in the United States or Australia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They stopped for gas in the small town of *Falcon*.",
                            "target_lang_translations": {
                                "ru": "Они остановились заправиться в маленьком городке <w>Фалкон</w>."
                            }
                        },
                        {
                            "text": "The map shows that *Falcon* is just a few miles down the road.",
                            "target_lang_translations": {
                                "ru": "Карта показывает, что <w>Фалкон</w> находится всего в нескольких милях по дороге."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название населенных пунктов, встречающееся, например, в США или Австралии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фалкон",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
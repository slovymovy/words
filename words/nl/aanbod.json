            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0296af5c-a02b-3172-9105-150e1060407e",
                    "sense_definition": "Een voorstel om iets te geven, te doen of te leveren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij deed mij een mooi <w>aanbod</w> voor mijn oude auto.",
                            "target_lang_translations": {
                                "en": "He made me a nice <w>offer</w> for my old car.",
                                "ru": "Он сделал мне хорошее <w>предложение</w> насчет моей старой машины."
                            }
                        },
                        {
                            "text": "Wij kunnen helaas niet op uw <w>aanbod</w> ingaan.",
                            "target_lang_translations": {
                                "en": "Unfortunately, we cannot accept your <w>offer</w>.",
                                "ru": "К сожалению, мы не можем принять ваше <w>предложение</w>."
                            }
                        },
                        {
                            "text": "Zij sloeg het <w>aanbod</w> om mee te gaan beleefd af.",
                            "target_lang_translations": {
                                "en": "She politely declined the <w>offer</w> to come along.",
                                "ru": "Она вежливо отклонила <w>предложение</w> пойти вместе."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanbieding"
                    ],
                    "common_phrases": [
                        "een aanbod doen",
                        "een aanbod afslaan",
                        "op een aanbod ingaan"
                    ],
                    "target_lang_definitions": {
                        "en": "A proposal to give, do, or provide something.",
                        "ru": "Предложение что-либо дать, сделать или предоставить."
                    },
                    "translations": {

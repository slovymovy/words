                },
                {
                    "sense_id": "6563a448-32f6-304b-9d9c-a59777361536",
                    "sense_definition": "A division of a game in sports such as ice hockey.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team scored two goals in the final <w>period</w>.",
                            "target_lang_translations": {
                                "ru": "Команда забила два гола в последнем <w>периоде</w>."
                            }
                        },
                        {
                            "text": "They won the game in the first overtime <w>period</w>.",
                            "target_lang_translations": {
                                "ru": "Они выиграли игру в первом <w>периоде</w> овертайма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first period",
                        "second period",
                        "third period"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть матча в спортивных играх, например, в хоккее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пери́од",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1c1d7f9-fca5-381d-a604-167138df71a9",
                    "sense_definition": "The length of time required for a repeating wave or cycle to complete.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>period</w> of the earth's rotation is approximately twenty-four hours.",
                            "target_lang_translations": {
                                "ru": "<w>Период</w> вращения Земли составляет примерно двадцать четыре часа."
                            }
                        },
                        {
                            "text": "Scientists measured the <w>period</w> of the pendulum's swing.",
                            "target_lang_translations": {
                                "ru": "Ученые измерили <w>период</w> колебания маятника."
                            }
                        }
                    ],
                    "antonyms": [
                        "frequency"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Время, необходимое для завершения полного цикла повторяющегося процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пери́од",
                                "target_lang_sense_clarification": "физика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [

                    ],
                    "common_phrases": [
                        "the who and why"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личность конкретного человека, который обсуждается или о котором спрашивают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "личность",
                                "target_lang_sense_clarification": "сущность, субъект"
                            },
                            {
                                "target_lang_word": "вопрос «кто»",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },
        {

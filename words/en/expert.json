{
    "word_family": [
        "expertise",
        "expertly",
        "expertness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "693af04b-096b-3642-ba7d-ef5234d7b83d",
                    "sense_definition": "A person with extensive knowledge or ability in a given subject.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_with_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a recognized <w>expert</w> in the field of marine biology.",
                            "target_lang_translations": {
                                "ru": "Она признанный <w>эксперт</w> в области морской биологии."
                            }
                        },
                        {
                            "text": "We need to hire a technical <w>expert</w> to fix the server issues.",
                            "target_lang_translations": {
                                "ru": "Нам нужно нанять технического <w>специалиста</w> для устранения проблем с сервером."
                            }
                        }
                    ],
                    "synonyms": [
                        "specialist",
                        "maven"
                    ],
                    "antonyms": [
                        "nonexpert"
                    ],
                    "common_phrases": [
                        "expert in",
                        "expert on",
                        "medical expert",
                        "technical expert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий обширными знаниями или способностями в данной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспе́рт",
                                "target_lang_sense_clarification": "человек с глубокими знаниями"
                            },
                            {
                                "target_lang_word": "специали́ст",
                                "target_lang_sense_clarification": "профессионал в определенной сфере"
                            },
                            {
                                "target_lang_word": "знато́к",
                                "target_lang_sense_clarification": "человек, тонко разбирающийся в чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f712187-def4-3f07-87f9-c24dcf1000d4",
                    "sense_definition": "A player ranking just below master, especially in chess.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He achieved the rank of <w>expert</w> after winning the local tournament.",
                            "target_lang_translations": {
                                "ru": "Он получил звание <w>эксперта</w> после победы в местном турнире."
                            }
                        },
                        {
                            "text": "The <w>expert</w> player challenged the grandmaster to a match.",
                            "target_lang_translations": {
                                "ru": "Игрок уровня <w>эксперт</w> вызвал гроссмейстера на матч."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок, чей рейтинг находится чуть ниже уровня мастера (особенно в шахматах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кандидат в мастера",
                                "target_lang_sense_clarification": "приблизительный аналог ранга"
                            },
                            {
                                "target_lang_word": "экспе́рт",
                                "target_lang_sense_clarification": "название ранга в некоторых системах классификации"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dcec3f37-f09d-3432-86d0-e9451a22717b",
                    "sense_definition": "Having or showing special skill or knowledge because of training or experience.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "skilled_attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is an <w>expert</w> driver who can handle any road condition.",
                            "target_lang_translations": {
                                "ru": "Он <w>опытный</w> водитель, который может справиться с любыми дорожными условиями."
                            }
                        },
                        {
                            "text": "My sister is <w>expert</w> at solving difficult puzzles.",
                            "target_lang_translations": {
                                "ru": "Моя сестра — <w>мастер</w> в решении сложных головоломок."
                            }
                        }
                    ],
                    "synonyms": [
                        "proficient",
                        "skillful"
                    ],
                    "antonyms": [
                        "inexpert"
                    ],
                    "common_phrases": [
                        "expert at",
                        "expert player",
                        "more expert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий особыми навыками или знаниями благодаря обучению или опыту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́пытный",
                                "target_lang_sense_clarification": "имеющий большой опыт"
                            },
                            {
                                "target_lang_word": "иску́сный",
                                "target_lang_sense_clarification": "умелый, мастерский"
                            },
                            {
                                "target_lang_word": "зна́ющий",
                                "target_lang_sense_clarification": "обладающий знаниями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46922c74-8a61-312c-9a63-5189ea922291",
                    "sense_definition": "Involving or displaying the skill and knowledge of an expert.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skilled_attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique table requires <w>expert</w> restoration to look new again.",
                            "target_lang_translations": {
                                "ru": "Антикварному столу требуется <w>профессиональная</w> реставрация, чтобы он снова выглядел как новый."
                            }
                        },
                        {
                            "text": "We are grateful for your <w>expert</w> advice on this complex matter.",
                            "target_lang_translations": {
                                "ru": "Мы благодарны за ваш <w>экспертный</w> совет по этому сложному вопросу."
                            }
                        }
                    ],
                    "synonyms": [
                        "skillful"
                    ],
                    "antonyms": [
                        "inexpert"
                    ],
                    "common_phrases": [
                        "expert advice",
                        "expert knowledge",
                        "expert opinion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Включающий в себя или демонстрирующий навыки и знания эксперта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспе́ртный",
                                "target_lang_sense_clarification": "относящийся к экспертизе или мнению эксперта"
                            },
                            {
                                "target_lang_word": "квалифици́рованный",
                                "target_lang_sense_clarification": "выполненный на высоком профессиональном уровне"
                            },
                            {
                                "target_lang_word": "профессиона́льный",
                                "target_lang_sense_clarification": "свойственный профессионалу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "76b7736c-b927-3b72-a651-d74bcf7aa0c2",
                    "sense_definition": "To have something reviewed or checked by an expert.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee is <w>experting</w> the financial records to find errors.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>проводит экспертизу</w> финансовых отчетов для выявления ошибок."
                            }
                        },
                        {
                            "text": "They spent weeks <w>experting</w> the manuscript for authenticity.",
                            "target_lang_translations": {
                                "ru": "Они потратили недели, <w>проверяя подлинность</w> рукописи."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать что-либо проверке или оценке экспертом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проводи́ть эксперти́зу",
                                "target_lang_sense_clarification": "официально оценивать"
                            },
                            {
                                "target_lang_word": "подверга́ть эксперти́зе",
                                "target_lang_sense_clarification": "отдавать на проверку специалистам"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
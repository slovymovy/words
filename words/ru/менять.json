            "senses": [
                {
                    "sense_id": "a2605c45-a9d4-3d6e-8fd6-e4891f189eb5",
                    "sense_definition": "Замещать старое, испорченное или ненужное на новое или другое",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "substitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой необходимо <w>менять</w> летнюю резину на зимнюю.",
                            "target_lang_translations": {
                                "en": "In winter, it is necessary to <w>change</w> summer tires to winter ones."
                            }
                        },
                        {
                            "text": "Отец учил меня <w>менять</w> масло в машине самостоятельно.",
                            "target_lang_translations": {
                                "en": "My father taught me to <w>change</w> the oil in the car myself."
                            }
                        }
                    ],
                    "synonyms": [
                        "заменять",
                        "сменять",
                        "замещать"
                    ],
                    "antonyms": [
                        "оставлять"
                    ],
                    "common_phrases": [
                        "менять работу",
                        "менять одежду",
                        "менять колесо",
                        "менять постельное бельё"
                    ],
                    "target_lang_definitions": {
                        "en": "To replace something old, broken, or unwanted with something new or different"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "to replace with another"
                            },
                            {
                                "target_lang_word": "replace",
                                "target_lang_sense_clarification": "to substitute"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3f02250-32bb-3864-99b0-fee471c6e07e",
                    "sense_definition": "Отдавать один объект и получать взамен другой",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exchange",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы идут в банк <w>менять</w> доллары на местные деньги.",
                            "target_lang_translations": {
                                "en": "Tourists go to the bank to <w>exchange</w> dollars for local money."
                            }
                        },
                        {
                            "text": "Мы решили не <w>менять</w> квартиру, а сделать в ней ремонт.",
                            "target_lang_translations": {
                                "en": "We decided not to <w>change</w> apartments, but to renovate."
                            }
                        }

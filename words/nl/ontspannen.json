    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d5410509-f77b-3f84-8ab7-51b26b85d141",
                    "sense_definition": "Tot rust komen en lichamelijke of geestelijke spanning verminderen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relax_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange werkdag wil ik me gewoon even <w>ontspannen</w>.",
                            "target_lang_translations": {
                                "en": "After a long workday, I just want to <w>relax</w> for a bit.",
                                "ru": "После долгого рабочего дня я просто хочу немного <w>расслабиться</w>."
                            }
                        },
                        {
                            "text": "Muziek is een ideaal middel om je te <w>ontspannen</w>.",
                            "target_lang_translations": {
                                "en": "Music is an ideal way to <w>unwind</w>.",
                                "ru": "Музыка — идеальное средство, чтобы <w>расслабиться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "relaxen"
                    ],
                    "antonyms": [
                        "inspannen",
                        "stressen"
                    ],
                    "common_phrases": [
                        "zich ontspannen",
                        "lekker ontspannen"
                    ],
                    "target_lang_definitions": {
                        "en": "To come to rest and reduce physical or mental tension.",
                        "ru": "Прийти в состояние покоя и уменьшить физическое или психическое напряжение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relax",
                                "target_lang_sense_clarification": "to rest and become calm"
                            },
                            {
                                "target_lang_word": "unwind",
                                "target_lang_sense_clarification": "to release tension after work or stress"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расслабляться",
                                "target_lang_sense_clarification": "снимать напряжение, отдыхать"
                            },
                            {
                                "target_lang_word": "отдыхать",
                                "target_lang_sense_clarification": "восстанавливать силы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a4acb43-a270-3887-b5fb-74a76e80e5dd",
                    "sense_definition": "Iets of iemand minder gespannen maken, vaak met betrekking tot spieren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relax_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ga rustig liggen en probeer al je spieren te <w>ontspannen</w>.",
                            "target_lang_translations": {
                                "en": "Lie down quietly and try to <w>relax</w> all your muscles.",
                                "ru": "Спокойно ляг и попытайся <w>расслабить</w> все свои мышцы."
                            }
                        },
                        {
                            "text": "De massage hielp om zijn pijnlijke rugspieren te <w>ontspannen</w>.",
                            "target_lang_translations": {
                                "en": "The massage helped to <w>loosen</w> his painful back muscles.",
                                "ru": "Массаж помог <w>расслабить</w> его болезненные мышцы спины."
                            }
                        }
                    ],
                    "synonyms": [
                        "relaxen",
                        "relaxen"
                    ],
                    "antonyms": [
                        "aanspannen"
                    ],
                    "common_phrases": [
                        "spieren ontspannen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something or someone less tense, often regarding muscles.",
                        "ru": "Снимать напряжение с чего-либо или кого-либо, часто в отношении мышц."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relax",
                                "target_lang_sense_clarification": "to make muscles less tense"
                            },
                            {
                                "target_lang_word": "loosen",
                                "target_lang_sense_clarification": "to release physical tightness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расслаблять",
                                "target_lang_sense_clarification": "делать менее напряженным (мышцы)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [

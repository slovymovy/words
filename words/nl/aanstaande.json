        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f5c5cf2f-90e5-3b72-8f81-9f62b9e19093",
                    "sense_definition": "Verbogen vorm van <w>aanstaand</w>: eerstvolgend of komend (met betrekking tot tijd).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij gaan <w>aanstaande</w> zondag naar Deventer om de wedstrijd te zien.",
                            "target_lang_translations": {
                                "en": "We are going to Deventer <w>this coming</w> Sunday to see the match.",
                                "ru": "В <w>следующее</w> воскресенье мы едем в Девентер, чтобы посмотреть матч."
                            }
                        },
                        {
                            "text": "Zondag <w>aanstaande</w> ga ik naar de verjaardag van mijn moeder.",
                            "target_lang_translations": {
                                "en": "<w>Next</w> Sunday I am going to my mother's birthday.",
                                "ru": "В <w>ближайшее</w> воскресенье я иду на день рождения мамы."
                            }
                        }
                    ],
                    "synonyms": [
                        "volgend",
                        "komend",
                        "eerstvolgend"
                    ],
                    "antonyms": [
                        "afgelopen",
                        "vorig"
                    ],
                    "common_phrases": [
                        "aanstaande maandag",
                        "zondag aanstaande",
                        "aanstaande week"
                    ],
                    "target_lang_definitions": {
                        "en": "Inflected form of 'aanstaand': next, coming, or upcoming (regarding time).",
                        "ru": "Склоняемая форма <w>aanstaand</w>: следующий или ближайший (по времени)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "next",
                                "target_lang_sense_clarification": "immediately following in time"
                            },
                            {
                                "target_lang_word": "this coming",
                                "target_lang_sense_clarification": "the soonest specific day occurring"
                            },
                            {
                                "target_lang_word": "upcoming",
                                "target_lang_sense_clarification": "happening soon"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "следующий",
                                "target_lang_sense_clarification": "о времени (дни недели, месяцы)"
                            },
                            {
                                "target_lang_word": "предстоящий",
                                "target_lang_sense_clarification": "о событиях"

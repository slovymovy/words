{
    "word_family": [
        "republican",
        "republicanism",
        "republicanize"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca15a45b-6adb-3fd7-af96-5f0c4cdfe273",
                    "sense_definition": "A country where power is held by the people or their elected representatives, rather than by a monarch.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The United States is a <w>republic</w> governed by elected officials.",
                            "target_lang_translations": {
                                "ru": "Соединенные Штаты — это <w>республика</w>, управляемая выборными должностными лицами."
                            }
                        },
                        {
                            "text": "After the revolution, the country abolished the monarchy and became a <w>republic</w>.",
                            "target_lang_translations": {
                                "ru": "После революции страна упразднила монархию и стала <w>республикой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "commonwealth"
                    ],
                    "antonyms": [
                        "monarchy",
                        "empire"
                    ],
                    "common_phrases": [
                        "banana republic",
                        "federal republic",
                        "democratic republic",
                        "Dominican Republic",
                        "Czech Republic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна, в которой власть принадлежит народу или его избранным представителям, а не монарху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "республика",
                                "target_lang_sense_clarification": "форма правления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8d8eb22-68c4-37c4-8a98-fa1efcbde6a5",
                    "sense_definition": "An autonomous region or administrative division within a larger federation, particularly in Russia or the former Yugoslavia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "administrative_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Republic</w> of Karelia is located in the northwestern part of Russia.",
                            "target_lang_translations": {
                                "ru": "<w>Республика</w> Карелия расположена в северо-западной части России."
                            }
                        },
                        {
                            "text": "Each <w>republic</w> within the former federation had its own local government.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>республика</w> в составе бывшей федерации имела свое местное правительство."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Автономный регион или административная единица в составе более крупной федерации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "республика",
                                "target_lang_sense_clarification": "административная единица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d8cdb921-eb77-35af-99d7-7efc13660925",
                    "sense_definition": "The historical state of ancient Rome characterized by a republican form of government, existing before the Roman Empire.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "historical_entity",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "The Senate was the governing body of the Roman <w>Republic</w>.",
                            "target_lang_translations": {
                                "ru": "Сенат был руководящим органом Римской <w>республики</w>."
                            }
                        },
                        {
                            "text": "Many modern legal systems are influenced by the laws of the <w>Republic</w>.",
                            "target_lang_translations": {
                                "ru": "Многие современные правовые системы находятся под влиянием законов <w>Республики</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Roman Republic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческое государство Древнего Рима с республиканской формой правления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "республика",
                                "target_lang_sense_clarification": "историческое государство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c834cf3-408e-32b2-bcad-f7e60b3d97f2",
                    "sense_definition": "A name used for several cities, towns, and communities in the United States.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We stopped for gas in the small town of <w>Republic</w>.",
                            "target_lang_translations": {
                                "ru": "Мы остановились заправиться в маленьком городке <w>Репаблик</w>."
                            }
                        },
                        {
                            "text": "He works for a company based in <w>Republic</w>, Missouri.",
                            "target_lang_translations": {
                                "ru": "Он работает в компании, базирующейся в городе <w>Репаблик</w>, штат Миссури."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "United States"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название нескольких городов и населенных пунктов в США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Репаблик",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "a761795e-529a-3c10-8753-2c25007793e5",
                    "sense_definition": "Not having knowledge or information about something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was completely <w>unaware</w> of the changes to the schedule.",
                            "target_lang_translations": {
                                "ru": "Я совершенно <w>не знал</w> об изменениях в расписании."
                            }
                        },
                        {
                            "text": "Many people remain <w>unaware</w> of their legal rights in this situation.",
                            "target_lang_translations": {
                                "ru": "Многие люди остаются <w>в неведении</w> относительно своих законных прав в этой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "clueless"
                    ],
                    "antonyms": [
                        "aware",
                        "informed"
                    ],
                    "common_phrases": [
                        "unaware of",
                        "blissfully unaware",
                        "remain unaware",
                        "totally unaware"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не обладающий знаниями или информацией о чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не зна́ющий",
                                "target_lang_sense_clarification": "не имеющий информации"
                            },
                            {
                                "target_lang_word": "не осведомлённый",
                                "target_lang_sense_clarification": "не информированный"
                            },

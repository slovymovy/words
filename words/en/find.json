    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6f9dedb4-b55d-392c-b19f-158571f46bc2",
                    "sense_definition": "To discover or locate something that one has been looking for.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I finally <w>found</w> my car keys under the couch."
                        },
                        {
                            "text": "Can you help me <w>find</w> a good place to eat?"
                        }
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "find a way",
                        "find out"
                    ]
                },
                {
                    "sense_id": "5da17300-761e-3de3-9d27-ab070481d4a1",
                    "sense_definition": "To encounter or discover something by accident or chance.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>found</w> a beautiful shell on the beach this morning."
                        },
                        {
                            "text": "She arrived home to <w>find</w> that the door was unlocked."
                        }
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "find oneself"
                    ]
                },
                {
                    "sense_id": "3af809ac-d86a-3e50-b31b-122cb67a2acb",
                    "sense_definition": "To have a particular feeling or opinion about something; to consider.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>find</w> it difficult to wake up early in the winter."
                        },
                        {
                            "text": "Do you <w>find</w> this book interesting?"
                        }
                    ],
                    "common_phrases": [
                        "find it hard",
                        "find it easy"
                    ]
                },
                {
                    "sense_id": "905ad7a1-5da1-3971-8adf-7427679a4801",
                    "sense_definition": "To succeed in getting or obtaining something required or desired, such as time or money.",

                            },
                            {
                                "target_lang_word": "ущелье",
                                "target_lang_sense_clarification": "узкая горная долина"
                            },
                            {
                                "target_lang_word": "проход",
                                "target_lang_sense_clarification": "путь через препятствие"
                            }
                        ]
                    }
                },
                {

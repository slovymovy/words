{
    "word_family": [
        "overgaan",
        "overgankelijk",
        "overgangsfase",
        "overgangsregeling",
        "overgangsperiode"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f78f663d-f190-3fb2-947a-1845e5d95161",
                    "sense_definition": "Het proces van verandering van de ene toestand of situatie naar een andere.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>overgang</w> tussen licht en donker is op dit schilderij heel goed te zien.",
                            "target_lang_translations": {
                                "en": "The <w>transition</w> between light and dark is clearly visible in this painting.",
                                "ru": "<w>Переход</w> между светом и тенью очень хорошо виден на этой картине."
                            }
                        },
                        {
                            "text": "De <w>overgang</w> naar de middelbare school was voor hem best spannend.",
                            "target_lang_translations": {
                                "en": "The <w>transition</w> to secondary school was quite exciting for him.",
                                "ru": "<w>Переход</w> в среднюю школу был для него довольно волнительным."
                            }
                        }
                    ],
                    "synonyms": [
                        "transitie"
                    ],
                    "common_phrases": [
                        "een soepele overgang",
                        "overgang van oud naar nieuw"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of changing from one state or situation to another.",
                        "ru": "Процесс перехода из одного состояния или ситуации в другое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transition",
                                "target_lang_sense_clarification": "process of change"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "процесс изменения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d74a55d-99e5-3724-b3d0-00922adfc8cc",
                    "sense_definition": "Een plaats waar men een weg, spoor of water kan oversteken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij de onbewaakte <w>overgang</w> moet je altijd goed naar links en rechts kijken.",
                            "target_lang_translations": {
                                "en": "At the unguarded <w>level crossing</w>, you must always look carefully left and right.",
                                "ru": "На неохраняемом <w>переезде</w> нужно всегда внимательно смотреть налево и направо."
                            }
                        },
                        {
                            "text": "De trein toeterde luid toen hij de <w>overgang</w> naderde.",
                            "target_lang_translations": {
                                "en": "The train sounded its horn loudly as it approached the <w>level crossing</w>.",
                                "ru": "Поезд громко посигналил, приближаясь к <w>переезду</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "oversteek",
                        "passage"
                    ],
                    "common_phrases": [
                        "beveiligde overgang",
                        "bewaakte overgang"
                    ],
                    "target_lang_definitions": {
                        "en": "A place where a road, railway track, or body of water can be crossed.",
                        "ru": "Место пересечения дороги, железнодорожных путей или водной преграды."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "level crossing",
                                "target_lang_sense_clarification": "railway crossing"
                            },
                            {
                                "target_lang_word": "crossing",
                                "target_lang_sense_clarification": "general place to cross"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переезд",
                                "target_lang_sense_clarification": "железнодорожный"
                            },
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "пешеходный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d43fe181-d15c-3f0e-926e-5336d1224983",
                    "sense_definition": "De levensfase waarin een vrouw stopt met menstrueren en onvruchtbaar wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn tante heeft veel last van de <w>overgang</w> en slaapt daardoor slecht.",
                            "target_lang_translations": {
                                "en": "My aunt is suffering a lot from <w>menopause</w> and sleeps poorly as a result.",
                                "ru": "Моя тетя тяжело переносит <w>климакс</w> и из-за этого плохо спит."
                            }
                        },
                        {
                            "text": "Vrouwen die in de <w>overgang</w> zijn, krijgen vaak te maken met opvliegers.",
                            "target_lang_translations": {
                                "en": "Women who are going through <w>menopause</w> often experience hot flashes.",
                                "ru": "Женщины в период <w>менопаузы</w> часто сталкиваются с приливами."
                            }
                        }
                    ],
                    "synonyms": [
                        "menopauze",
                        "climacterium"
                    ],
                    "common_phrases": [
                        "in de overgang zijn",
                        "last hebben van de overgang"
                    ],
                    "target_lang_definitions": {
                        "en": "The phase of life when a woman stops menstruating and becomes infertile.",
                        "ru": "Период в жизни женщины, связанный с прекращением менструации и окончанием репродуктивного возраста."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "menopause",
                                "target_lang_sense_clarification": "biological phase"
                            },
                            {
                                "target_lang_word": "the change",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "климакс",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "менопауза",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
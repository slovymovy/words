    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "13d856c6-ae68-3960-9fcf-1737ae5d7cfd",
                    "sense_definition": "Connected with the sea, especially in relation to commercial shipping, navigation, or military activity.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country has a long and proud <w>maritime</w> history involving trade and exploration.",
                            "target_lang_translations": {
                                "ru": "У страны долгая и славная <w>морская</w> история, связанная с торговлей и исследованиями."
                            }
                        },
                        {
                            "text": "International <w>maritime</w> law governs how ships operate in open waters.",
                            "target_lang_translations": {
                                "ru": "Международное <w>морское</w> право регулирует деятельность судов в открытых водах."
                            }
                        }
                    ],
                    "synonyms": [
                        "nautical",
                        "naval",
                        "marine"
                    ],
                    "common_phrases": [
                        "maritime law",
                        "maritime history",
                        "maritime museum",
                        "maritime industry",
                        "maritime security"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с морем, особенно в отношении торгового судоходства, навигации или военной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морской",
                                "target_lang_sense_clarification": "относящийся к мореплаванию или флоту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f224a5e-fc95-3fd7-8b45-7ca83b331ba9",
                    "sense_definition": "Located near or bordering the sea.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>maritime</w> provinces are known for their beautiful beaches and fresh seafood.",
                            "target_lang_translations": {
                                "ru": "<w>Приморские</w> провинции известны своими красивыми пляжами и свежими морепродуктами."
                            }
                        },
                        {
                            "text": "People living in <w>maritime</w> areas often experience milder winters than those inland.",
                            "target_lang_translations": {
                                "ru": "Люди, живущие в <w>приморских</w> районах, часто переживают более мягкие зимы, чем жители внутренних районов."
                            }
                        }
                    ],
                    "synonyms": [
                        "coastal",
                        "seaside"
                    ],
                    "antonyms": [
                        "inland",
                        "landlocked"
                    ],
                    "common_phrases": [
                        "maritime climate",
                        "maritime region",
                        "maritime border"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный рядом с морем или граничащий с ним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приморский",
                                "target_lang_sense_clarification": "расположенный у моря, прибрежный"
                            },
                            {
                                "target_lang_word": "морской",
                                "target_lang_sense_clarification": "свойственный местности у моря (о климате)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1462c14-ed7a-324b-9482-6d2b60334c81",
                    "sense_definition": "Living or growing near the seashore.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many <w>maritime</w> birds nest on these cliffs during the spring season.",
                            "target_lang_translations": {
                                "ru": "Многие <w>морские</w> птицы гнездятся на этих скалах в весенний сезон."
                            }
                        },
                        {
                            "text": "The <w>maritime</w> forest protects the island from strong winds and soil erosion.",
                            "target_lang_translations": {
                                "ru": "<w>Прибрежный</w> лес защищает остров от сильных ветров и эрозии почвы."
                            }
                        }
                    ],
                    "synonyms": [
                        "littoral"
                    ],
                    "common_phrases": [
                        "maritime vegetation",
                        "maritime forest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обитающий или растущий на морском побережье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морской",
                                "target_lang_sense_clarification": "обитающий у моря (о фауне)"
                            },
                            {
                                "target_lang_word": "прибрежный",
                                "target_lang_sense_clarification": "растущий у берега (о флоре)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
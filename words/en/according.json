                {
                    "sense_id": "cecf1c75-0c1a-36fb-be26-37dbf9286e8d",
                    "sense_definition": "Present participle and gerund of <w>accord</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everything went <w>according</w> to plan.",
                            "target_lang_translations": {
                                "ru": "Всё шло <w>согласно</w> плану."
                            }
                        },
                        {
                            "text": "<w>According</w> to the weather forecast, it will rain tomorrow.",
                            "target_lang_translations": {
                                "ru": "<w>Согласно</w> прогнозу погоды, завтра будет дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "according to",
                        "according as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола accord; обычно используется в составе предлогов для указания на соответствие или источник информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласно",
                                "target_lang_sense_clarification": "предлог; указывает на соответствие чему-либо или источник"
                            },
                            {
                                "target_lang_word": "в соответствии с",
                                "target_lang_sense_clarification": "фраза; указывает на согласованность с правилами или планом"
                            },
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "предлог; указывает на источник информации или план"
                            }
                        ]
                    }
                }
            ]
        },
        {

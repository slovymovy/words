                {
                    "sense_id": "8cc97d52-db62-3684-8c95-66c589e6f7a0",
                    "sense_definition": "Popis zręczności lub iluzji, wykonywany w celu zadziwienia lub rozbawienia publiczności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "entertainment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iluzjonista pokazał nam niesamowitą <w>sztuczkę</w> z kartami i znikającym królikiem.",
                            "target_lang_translations": {
                                "ru": "Иллюзионист показал нам невероятный <w>фокус</w> с картами и исчезающим кроликом."
                            }
                        },
                        {
                            "text": "Mój pies nauczył się nowej <w>sztuczki</w> i teraz potrafi podawać łapę na komendę.",
                            "target_lang_translations": {
                                "ru": "Моя собака выучила новый <w>трюк</w> и теперь умеет давать лапу по команде."
                            }
                        }
                    ],
                    "synonyms": [
                        "trik"
                    ],
                    "common_phrases": [
                        "sztuczka magiczna",
                        "sztuczka karciana",
                        "sztuczka cyrkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонстрация ловкости или иллюзии, выполняемая с целью удивить или развлечь публику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фокус",
                                "target_lang_sense_clarification": "магия, иллюзия"
                            },
                            {
                                "target_lang_word": "трюк",
                                "target_lang_sense_clarification": "ловкость, дрессировка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66448655-bab9-3836-91ab-e72b189176cc",
                    "sense_definition": "Sprytny i skuteczny sposób na łatwiejsze lub szybsze wykonanie jakiegoś zadania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz zdradził nam prostą <w>sztuczkę</w> na idealne obranie czosnku w kilka sekund.",
                            "target_lang_translations": {
                                "ru": "Повар открыл нам простой <w>трюк</w>, как идеально почистить чеснок за несколько секунд."
                            }
                        },
                        {
                            "text": "Znam pewną <w>sztuczkę</w>, dzięki której komputer będzie działał znacznie szybciej.",
                            "target_lang_translations": {
                                "ru": "Я знаю одну <w>хитрость</w>, благодаря которой компьютер будет работать гораздо быстрее."
                            }
                        }
                    ],
                    "synonyms": [
                        "trik"
                    ],
                    "common_phrases": [
                        "prosta sztuczka",
                        "sztuczka kulinarna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хитрый и эффективный способ выполнить задачу проще или быстрее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хитрость",
                                "target_lang_sense_clarification": "смекалка"
                            },
                            {
                                "target_lang_word": "трюк",
                                "target_lang_sense_clarification": "метод"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f655adaa-2211-3bce-8530-8bdcbbe00336",
                    "sense_definition": "Door nadenken iets nieuws vinden of verzinnen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creatie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ingenieurs hebben een slimme oplossing <w>bedacht</w> voor het technische probleem.",
                            "target_lang_translations": {
                                "en": "The engineers <w>devised</w> a clever solution for the technical problem.",
                                "ru": "Инженеры <w>придумали</w> умное решение для технической проблемы."
                            }
                        },
                        {
                            "text": "Wij moeten samen een goed plan <w>bedenken</w> om het project te redden.",
                            "target_lang_translations": {
                                "en": "We must <w>come up with</w> a good plan together to save the project.",
                                "ru": "Мы должны вместе <w>разработать</w> хороший план, чтобы спасти проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzinnen",
                        "uitvinden",
                        "ontwerpen"
                    ],
                    "common_phrases": [
                        "een plan bedenken",
                        "iets nieuws bedenken",
                        "een oplossing bedenken"
                    ],
                    "target_lang_definitions": {
                        "en": "To invent, plan, or think of something new.",
                        "ru": "Придумать или изобрести что-то новое с помощью размышлений."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "think of",
                                "target_lang_sense_clarification": "to create an idea or solution in one's mind"
                            },
                            {
                                "target_lang_word": "devise",
                                "target_lang_sense_clarification": "to plan or design something complex using intellect"
                            },
                            {
                                "target_lang_word": "come up with",
                                "target_lang_sense_clarification": "to produce a plan or idea"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "придумать",
                                "target_lang_sense_clarification": "создать мысленно"
                            },
                            {
                                "target_lang_word": "изобрести",
                                "target_lang_sense_clarification": "создать что-то технически новое"
                            },
                            {
                                "target_lang_word": "разработать",
                                "target_lang_sense_clarification": "составить план или метод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c9f64c0-229d-3489-a1d7-459f5352404b",
                    "sense_definition": "De gedachten ergens over laten gaan of iets in overweging nemen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "overweging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet goed <w>bedenken</w> wat de gevolgen van deze beslissing kunnen zijn.",
                            "target_lang_translations": {
                                "en": "You must carefully <w>consider</w> what the consequences of this decision might be.",
                                "ru": "Ты должен хорошо <w>обдумать</w>, какими могут быть последствия этого решения."
                            }
                        },
                        {
                            "text": "Het is nauwelijks te <w>bedenken</w> dat het hier vroeger allemaal bos was.",
                            "target_lang_translations": {
                                "en": "It is hard to <w>imagine</w> that this used to be all forest.",
                                "ru": "Трудно <w>представить</w>, что раньше здесь был сплошной лес."
                            }
                        }
                    ],
                    "synonyms": [
                        "overwegen",
                        "overpeinzen",
                        "zich realiseren"
                    ],
                    "antonyms": [
                        "negeren"
                    ],
                    "common_phrases": [
                        "te bedenken dat",
                        "van tevoren bedenken"
                    ],
                    "target_lang_definitions": {
                        "en": "To think about, consider, or realize something.",
                        "ru": "Обдумать что-либо, принять во внимание или представить себе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "to think about something carefully"
                            },
                            {
                                "target_lang_word": "imagine",
                                "target_lang_sense_clarification": "to form a mental image or conceive of something"
                            },
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "to become aware of a fact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обдумать",
                                "target_lang_sense_clarification": "взвесить все за и против"
                            },
                            {
                                "target_lang_word": "представить себе",
                                "target_lang_sense_clarification": "вообразить"
                            },
                            {
                                "target_lang_word": "осознать",
                                "target_lang_sense_clarification": "понять в полной мере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26054238-431a-3031-9286-1f04e6286096",
                    "sense_definition": "Van mening veranderen of op een eerder besluit terugkomen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij wilde eerst meegaan, maar hij <w>bedacht</w> zich op het laatste moment.",
                            "target_lang_translations": {
                                "en": "He wanted to go along at first, but he <w>changed his mind</w> at the last moment.",
                                "ru": "Сначала он хотел пойти с нами, но в последний момент <w>передумал</w>."
                            }
                        },
                        {
                            "text": "Zij heeft zich <w>bedacht</w> en gaat toch niet verhuizen naar het buitenland.",
                            "target_lang_translations": {
                                "en": "She has <w>changed her mind</w> and is not moving abroad after all.",
                                "ru": "Она <w>передумала</w> и всё-таки не переезжает за границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "herzien"
                    ],
                    "antonyms": [
                        "volharden"
                    ],
                    "common_phrases": [
                        "zich plotseling bedenken",
                        "zich alsnog bedenken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive (zich bedenken)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To change one's mind about a previous decision.",
                        "ru": "Изменить мнение или отказаться от прежнего решения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "change one's mind",
                                "target_lang_sense_clarification": "to reverse a previous opinion or decision"
                            },
                            {
                                "target_lang_word": "reconsider",
                                "target_lang_sense_clarification": "to think again about a decision, often leading to a change"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "передумать",
                                "target_lang_sense_clarification": "изменить решение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32c909f6-37d4-33ab-9c5e-4242abda0f4e",
                    "sense_definition": "Iemand iets schenken of toekennen, vaak in een testament.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "schenking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rijke vrouw heeft al haar kleinkinderen in haar testament <w>bedacht</w>.",
                            "target_lang_translations": {
                                "en": "The wealthy woman <w>remembered</w> all her grandchildren in her will.",
                                "ru": "Богатая женщина <w>упомянула</w> всех своих внуков в завещании."
                            }
                        },
                        {
                            "text": "Bij het afscheid werden alle vrijwilligers met een mooi cadeau <w>bedacht</w>.",
                            "target_lang_translations": {
                                "en": "At the farewell, all volunteers were <w>remembered</w> with a beautiful gift.",
                                "ru": "На прощание всех волонтеров <w>одарили</w> прекрасным подарком."
                            }
                        }
                    ],
                    "synonyms": [
                        "begunstigen",
                        "toebedelen"
                    ],
                    "antonyms": [
                        "onterven"
                    ],
                    "common_phrases": [
                        "in een testament bedenken"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "formal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give a gift or legacy to someone, often in a will.",
                        "ru": "Одарить кого-либо или выделить долю (часто в завещании)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remember",
                                "target_lang_sense_clarification": "to provide for someone in a will or with a gift"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "одарить",
                                "target_lang_sense_clarification": "наградить подарком"
                            },
                            {
                                "target_lang_word": "упомянуть",
                                "target_lang_sense_clarification": "включить в завещание"
                            }
                        ]
                    }
                }
            ]
        }
    ]

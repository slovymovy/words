                {
                    "sense_id": "fda9deef-fc1b-3767-9128-881c066bda9e",
                    "sense_definition": "The constellation Lupus, located in the southern sky.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Astronomers observed a new star in the <w>Wolf</w>.",
                            "target_lang_translations": {
                                "ru": "Астрономы наблюдали новую звезду в созвездии <w>Волк</w>."
                            }
                        },
                        {
                            "text": "The <w>Wolf</w> is best seen from the southern hemisphere during summer.",
                            "target_lang_translations": {
                                "ru": "Созвездие <w>Волк</w> лучше всего видно из южного полушария летом."

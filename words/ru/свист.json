                            {
                                "target_lang_word": "whistle",
                                "target_lang_sense_clarification": "sound"
                            },
                            {
                                "target_lang_word": "whistling",
                                "target_lang_sense_clarification": "action or sound"
                            }
                        ]
                    }
                },
                {

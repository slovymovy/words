    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "74d225bf-31c9-3c07-a5ad-c5534a4e8a65",
                    "sense_definition": "Перемещаться вниз, вращаясь или скользя по наклонной поверхности",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мяч медленно <скатывался> с крутой горы прямо в реку.",
                            "target_lang_translations": {
                                "en": "The ball was slowly <rolling down> the steep hill straight into the river."
                            }
                        },
                        {
                            "text": "Зимой дети любят <скатываться> с ледяной горки на санках.",
                            "target_lang_translations": {
                                "en": "In winter, children love to <slide down> the ice slide on sleds."
                            }
                        }
                    ],
                    "synonyms": [
                        "спускаться",
                        "съезжать"
                    ],
                    "antonyms": [
                        "подниматься",
                        "взбираться"
                    ],
                    "common_phrases": [
                        "скатываться с горы",
                        "скатываться вниз",
                        "скатываться по склону"
                    ],
                    "target_lang_definitions": {
                        "en": "To move downwards by turning over and over or sliding on a surface"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll down",
                                "target_lang_sense_clarification": "movement by turning over"
                            },
                            {
                                "target_lang_word": "slide down",
                                "target_lang_sense_clarification": "movement on a smooth surface (e.g., on a sled)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05689bf1-b32a-3553-b53f-38cfa3ad05cf",
                    "sense_definition": "Сворачиваться, принимая округлую форму (шара, трубки, кольца)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "change_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При виде опасности ёж тут же <скатывается> в колючий клубок.",
                            "target_lang_translations": {
                                "en": "At the sight of danger, the hedgehog immediately <curls up> into a prickly ball."
                            }
                        },
                        {
                            "text": "Сухие листья на земле начали <скатываться> от жары.",
                            "target_lang_translations": {
                                "en": "Dry leaves on the ground began to <curl up> from the heat."
                            }
                        }
                    ],
                    "synonyms": [
                        "сворачиваться",
                        "скручиваться"
                    ],
                    "antonyms": [
                        "разворачиваться",
                        "раскручиваться"
                    ],
                    "common_phrases": [
                        "скатываться в клубок",
                        "скатываться в рулон",
                        "скатываться в шарик"
                    ],
                    "target_lang_definitions": {
                        "en": "To curl or wind into a rounded shape like a ball or tube"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "curl up",
                                "target_lang_sense_clarification": "into a ball shape (animals, leaves)"
                            },
                            {
                                "target_lang_word": "roll up",
                                "target_lang_sense_clarification": "into a tube or ball"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab3c5284-3feb-37a7-9264-1d6686d27da1",
                    "sense_definition": "Переходить в худшее, менее достойное состояние; опускаться (нравственно, профессионально или социально)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "degradation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Без постоянного развития компания начала <скатываться> к банкротству.",
                            "target_lang_translations": {
                                "en": "Without constant development, the company began to <slide> towards bankruptcy."
                            }
                        },
                        {
                            "text": "Он боялся <скатываться> в нищету после потери престижной работы.",
                            "target_lang_translations": {
                                "en": "He was afraid of <sliding> into poverty after losing his prestigious job."
                            }
                        }
                    ],
                    "synonyms": [
                        "опускаться",
                        "деградировать"
                    ],
                    "antonyms": [
                        "развиваться",
                        "совершенствоваться"
                    ],
                    "common_phrases": [
                        "скатываться в пропасть",
                        "скатываться до уровня",
                        "скатываться в кризис"
                    ],
                    "target_lang_definitions": {
                        "en": "To deteriorate or decline into a worse state or condition"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "gradual decline into a negative state"
                            },
                            {
                                "target_lang_word": "slip",
                                "target_lang_sense_clarification": "fall into a worse condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f384899f-5e5c-354d-ab17-a727dcc9fd35",
                    "sense_definition": "Сбиваться в мелкие комки (о ворсе ткани, шерсти или волосах)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "texture_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Шерсть на дешёвом свитере начала <скатываться> уже после первой стирки.",
                            "target_lang_translations": {
                                "en": "The wool on the cheap sweater began to <pill> after just the first wash."
                            }
                        },
                        {
                            "text": "Длинные волосы могут <скатываться>, если их долго не расчёсывать.",
                            "target_lang_translations": {
                                "en": "Long hair can <mat> if it is not brushed for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "сваливаться",
                        "сбиваться"
                    ],
                    "common_phrases": [
                        "ткань скатывается",
                        "шерсть скатывается"
                    ],
                    "target_lang_definitions": {
                        "en": "To form small lumps or tangles (of fabric, wool, or hair)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pill",
                                "target_lang_sense_clarification": "form small balls of fluff (fabric)"
                            },
                            {
                                "target_lang_word": "mat",
                                "target_lang_sense_clarification": "become tangled into clumps (hair, fur)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "6d449c82-36fd-3908-9621-60575b04406e",
                    "sense_definition": "Przestać coś trzymać; zwolnić uścisk dłoni lub chwyt na przedmiocie, pozwalając mu wypaść lub się oddalić.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "release_grip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz natychmiast <puścić> ten gorący garnek, bo się poparzysz.",
                            "target_lang_translations": {
                                "ru": "Ты должен немедленно <отпустить> эту горячую кастрюлю, иначе обожжешься."
                            }
                        },
                        {
                            "text": "Policjant kazał złodziejowi <puścić> skradzioną torebkę.",
                            "target_lang_translations": {
                                "ru": "Полицейский приказал вору <выпустить> украденную сумочку."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwolnić",
                        "uwolnić",
                        "wypuścić"
                    ],
                    "antonyms": [
                        "trzymać",
                        "chwycić",
                        "złapać",
                        "zacisnąć"
                    ],
                    "common_phrases": [
                        "puścić rękę",
                        "puścić linę",
                        "puścić wolno",
                        "puścić oczko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать держать что-либо; разжать руку или хватку на предмете, позволяя ему упасть или удалиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отпустить",
                                "target_lang_sense_clarification": "перестать удерживать"
                            },
                            {
                                "target_lang_word": "выпустить",
                                "target_lang_sense_clarification": "дать выпасть из рук"
                            }
                        ]
                    }
                },
                {

            "senses": [
                {
                    "sense_id": "17e227de-5a8c-3209-863e-dfbb5ea433aa",
                    "sense_definition": "Umieszczać roślinę, sadzonkę, cebulkę lub nasiono w ziemi, aby się zakorzeniła i rosła.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiosną będziemy <sadzić> pomidory i ogórki w naszym ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Весной мы будем <сажать> помидоры и огурцы в нашем саду."
                            }
                        },
                        {
                            "text": "Moi dziadkowie lubią <sadzić> młode drzewka owocowe na działce.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка любят <сажать> молодые фруктовые деревья на даче."
                            }
                        }
                    ],
                    "synonyms": [
                        "wsadzać",
                        "zakopywać"
                    ],
                    "antonyms": [
                        "wyrywać",
                        "wykopywać",
                        "zbierać"
                    ],
                    "common_phrases": [
                        "sadzić drzewa",
                        "sadzić kwiaty",
                        "sadzić ziemniaki",
                        "sadzić las"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать растение, саженец, луковицу или семя в землю, чтобы оно укоренилось и росло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сажать",
                                "target_lang_sense_clarification": "общеупотребительное"
                            },
                            {
                                "target_lang_word": "садить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6287415-83de-389c-ba63-d84088149b9b",
                    "sense_definition": "Robić rażące błędy językowe, zwłaszcza ortograficzne (używane niemal wyłącznie w połączeniu ze słowem byki).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "errors",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka zdenerwowała się, że uczeń zaczął <sadzić> byki w dyktandzie.",
                            "target_lang_translations": {
                                "ru": "Учительница рассердилась, что ученик начал <лепить> ошибки в диктанте."

{
    "word_family": [
        "shyness",
        "shyly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "065b769a-9861-3d0a-8754-3ff6964bab2a",
                    "sense_definition": "Nervous or uncomfortable about meeting and speaking to people.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>shy</w> child hid behind her mother's legs when the guests arrived.",
                            "target_lang_translations": {
                                "ru": "<w>Застенчивый</w> ребёнок спрятался за ноги матери, когда пришли гости."
                            }
                        },
                        {
                            "text": "He is too <w>shy</w> to ask the teacher for help in front of the class.",
                            "target_lang_translations": {
                                "ru": "Он слишком <w>стеснителен</w>, чтобы попросить учителя о помощи перед всем классом."
                            }
                        }
                    ],
                    "synonyms": [
                        "bashful",
                        "reserved",
                        "timid",
                        "introverted",
                        "retiring"
                    ],
                    "antonyms": [
                        "confident",
                        "outgoing",
                        "bold",
                        "gregarious",
                        "forward#Adjective"
                    ],
                    "common_phrases": [
                        "shy with strangers",
                        "feel shy",
                        "shy smile"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нервничающий или чувствующий себя неловко при встрече и разговоре с людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застенчивый",
                                "target_lang_sense_clarification": "робкий от природы"
                            },
                            {
                                "target_lang_word": "стеснительный",
                                "target_lang_sense_clarification": "испытывающий неловкость"
                            },
                            {
                                "target_lang_word": "робкий",
                                "target_lang_sense_clarification": "несмелый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4869df0b-9339-3ee8-863e-268eb3673155",
                    "sense_definition": "Easily frightened or scared; timid (often used for animals).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The deer is a very <w>shy</w> animal and runs away at the slightest noise.",
                            "target_lang_translations": {
                                "ru": "Олень — очень <w>пугливое</w> животное и убегает при малейшем шуме."
                            }
                        },
                        {
                            "text": "Stray cats are often <w>shy</w> and will not let humans touch them.",
                            "target_lang_translations": {
                                "ru": "Бродячие кошки часто <w>пугливы</w> и не дают людям дотронуться до себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "timid",
                        "fearful",
                        "skittish"
                    ],
                    "antonyms": [
                        "brave",
                        "fearless",
                        "bold"
                    ],
                    "common_phrases": [
                        "shy animal",
                        "shy horse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко пугающийся; боязливый (часто используется по отношению к животным)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пугливый",
                                "target_lang_sense_clarification": "легко поддающийся испугу"
                            },
                            {
                                "target_lang_word": "боязливый",
                                "target_lang_sense_clarification": "испытывающий страх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31ce661d-4203-3921-8ec8-794032b3a2fe",
                    "sense_definition": "Less than a specific amount or number; short of.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_lack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team finished just <w>shy</w> of the championship title this season.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне команде не хватило <w>совсем немного</w> до чемпионского титула."
                            }
                        },
                        {
                            "text": "Her height is just <w>shy</w> of six feet tall.",
                            "target_lang_translations": {
                                "ru": "Её рост <w>чуть меньше</w> шести футов."
                            }
                        }
                    ],
                    "synonyms": [
                        "short",
                        "lacking"
                    ],
                    "common_phrases": [
                        "just shy of",
                        "two shy of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Меньше определенного количества или числа; немного не хватает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чуть меньше",
                                "target_lang_sense_clarification": "недостает до полного количества"
                            },
                            {
                                "target_lang_word": "почти",
                                "target_lang_sense_clarification": "около, но не совсем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d5b7b81-ca94-327f-b5af-8a11fc32fab5",
                    "sense_definition": "Cautious or unwilling to do something due to mistrust or fear.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Investors are <w>shy</w> of putting money into such risky ventures.",
                            "target_lang_translations": {
                                "ru": "Инвесторы <w>остерегаются</w> вкладывать деньги в такие рискованные предприятия."
                            }
                        },
                        {
                            "text": "After the accident, he was <w>shy</w> of driving at night.",
                            "target_lang_translations": {
                                "ru": "После аварии он <w>боялся</w> водить машину ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "wary",
                        "cautious",
                        "suspicious",
                        "hesitant"
                    ],
                    "antonyms": [
                        "confident"
                    ],
                    "common_phrases": [
                        "shy of doing",
                        "fight shy of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осторожный или не желающий что-то делать из-за недоверия или страха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осторожный",
                                "target_lang_sense_clarification": "проявляющий осмотрительность"
                            },
                            {
                                "target_lang_word": "опасающийся",
                                "target_lang_sense_clarification": "испытывающий опасение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6e2f74d9-ec22-3e75-969d-983f4e01f8a5",
                    "sense_definition": "To avoid doing something because of nervousness, fear, or dislike.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do not <w>shy</w> away from difficult challenges in life.",
                            "target_lang_translations": {
                                "ru": "Не <w>уклоняйся</w> от трудных жизненных испытаний."
                            }
                        },
                        {
                            "text": "The politician tried to <w>shy</w> away from answering the difficult question.",
                            "target_lang_translations": {
                                "ru": "Политик пытался <w>уйти</w> от ответа на сложный вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "recoil",
                        "hesitate"
                    ],
                    "common_phrases": [
                        "shy away from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избегать делать что-либо из-за нервозности, страха или неприязни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уклоняться",
                                "target_lang_sense_clarification": "стараться избежать"
                            },
                            {
                                "target_lang_word": "избегать",
                                "target_lang_sense_clarification": "сторониться чего-либо"
                            },
                            {
                                "target_lang_word": "сторониться",
                                "target_lang_sense_clarification": "держаться в стороне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0dd1f4d-384d-373e-9f49-3180fcbf57d3",
                    "sense_definition": "To move suddenly back or sideways in fear or alarm (especially of a horse).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horse <w>shied</w> when a plastic bag blew across the road.",
                            "target_lang_translations": {
                                "ru": "Лошадь <w>шарахнулась</w>, когда через дорогу пролетел пластиковый пакет."
                            }
                        },
                        {
                            "text": "She <w>shied</w> in surprise when the door slammed shut behind her.",
                            "target_lang_translations": {
                                "ru": "Она <w>отпрянула</w> от неожиданности, когда дверь захлопнулась за её спиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "startle",
                        "recoil",
                        "flinch"
                    ],
                    "common_phrases": [
                        "horse shied",
                        "shied at the noise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно отпрянуть или шарахнуться в сторону от страха (особенно о лошади)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шарахаться",
                                "target_lang_sense_clarification": "резко отскакивать в сторону (о животных)"
                            },
                            {
                                "target_lang_word": "отпрянуть",
                                "target_lang_sense_clarification": "резко отодвинуться назад"
                            },
                            {
                                "target_lang_word": "пугаться",
                                "target_lang_sense_clarification": "испытывать внезапный страх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e054a459-093a-359a-b91a-7231d6b4c9c7",
                    "sense_definition": "To throw something with a sudden movement.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_throw",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The angry boy picked up a rock and <w>shied</w> it at the fence.",
                            "target_lang_translations": {
                                "ru": "Рассерженный мальчик поднял камень и <w>швырнул</w> его в забор."
                            }
                        },
                        {
                            "text": "He <w>shied</w> the empty bottle into the recycling bin.",
                            "target_lang_translations": {
                                "ru": "Он <w>бросил</w> пустую бутылку в мусорный бак."
                            }
                        }
                    ],
                    "synonyms": [
                        "fling",
                        "toss",
                        "throw",
                        "chuck"
                    ],
                    "common_phrases": [
                        "shy a stone"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросить что-то резким движением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "швырять",
                                "target_lang_sense_clarification": "сильно бросать"
                            },
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "кидать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3d36f46c-bd32-3cff-9b56-8e933d997c3a",
                    "sense_definition": "A fairground game where people throw balls at objects (often coconuts) to win prizes.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_throw",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We won a goldfish at the coconut <w>shy</w> during the village fair.",
                            "target_lang_translations": {
                                "ru": "Мы выиграли золотую рыбку на аттракционе <w>сбей кокос</w> во время деревенской ярмарки."
                            }
                        },
                        {
                            "text": "The children spent all their money at the <w>shy</w> trying to hit the cans.",
                            "target_lang_translations": {
                                "ru": "Дети потратили все свои деньги в <w>тире</w>, пытаясь сбить банки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coconut shy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ярмарочная игра, в которой люди бросают мячи в предметы (часто кокосы), чтобы выиграть призы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аттракцион \"сбей кокос\"",
                                "target_lang_sense_clarification": "ярмарочная игра"
                            },
                            {
                                "target_lang_word": "тир",
                                "target_lang_sense_clarification": "место для стрельбы или метания по мишеням"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcbf0fbf-219e-3165-9fa1-21921171181f",
                    "sense_definition": "A sudden startled movement, especially by a horse.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With a sudden <w>shy</w>, the horse almost threw its rider.",
                            "target_lang_translations": {
                                "ru": "Сделав внезапный <w>рывок в сторону</w>, лошадь чуть не сбросила всадника."
                            }
                        },
                        {
                            "text": "The nervous animal gave a <w>shy</w> at the loud noise.",
                            "target_lang_translations": {
                                "ru": "Нервное животное <w>шарахнулось</w> от громкого звука."
                            }
                        }
                    ],
                    "synonyms": [
                        "start",
                        "jump"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное испуганное движение, особенно лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рывок в сторону",
                                "target_lang_sense_clarification": "резкое движение"
                            },
                            {
                                "target_lang_word": "шараханье",
                                "target_lang_sense_clarification": "испуганное движение в сторону"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0f60a453-cac7-30eb-8d9e-277e3fc33028",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Shy</w> lived at the end of the street for many years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Шай</w> много лет жил в конце улицы."
                            }
                        },
                        {
                            "text": "We looked up the name <w>Shy</w> in the local phone book.",
                            "target_lang_translations": {
                                "ru": "Мы искали фамилию <w>Шай</w> в местном телефонном справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Шай",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
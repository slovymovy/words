{
    "word_family": [
        "bundel",
        "bundeling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2eede4c-1abe-316b-bb72-4cd0627d9322",
                    "sense_definition": "Dingen samenvoegen tot één geheel of verzameling, vaak gebruikt voor teksten of voorwerpen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze nieuwe uitgave zijn al zijn krantenartikelen <w>gebundeld</w>.",
                            "target_lang_translations": {
                                "en": "In this new edition, all his newspaper articles have been <w>compiled</w>.",
                                "ru": "В этом новом издании все его газетные статьи <w>собраны</w>."
                            }
                        },
                        {
                            "text": "Je moet de losse takken goed <w>bundelen</w> voordat je ze naar de straat brengt.",
                            "target_lang_translations": {
                                "en": "You need to <w>bundle</w> the loose branches well before taking them to the street.",
                                "ru": "Нужно хорошо <w>связать</w> отдельные ветки, прежде чем выносить их на улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "samenvoegen",
                        "verzamelen",
                        "bijeenbrengen"
                    ],
                    "antonyms": [
                        "verspreiden",
                        "splitsen"
                    ],
                    "common_phrases": [
                        "artikelen bundelen",
                        "verhalen bundelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To combine things into a single whole or collection, often used for texts or objects.",
                        "ru": "Объединять предметы в одно целое или коллекцию; часто используется для текстов (собирать в сборник) или физических объектов (связывать в пучок)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bundle",
                                "target_lang_sense_clarification": "physical objects"
                            },
                            {
                                "target_lang_word": "compile",
                                "target_lang_sense_clarification": "texts or writings"
                            },
                            {
                                "target_lang_word": "collect",
                                "target_lang_sense_clarification": "gathering into a set"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "в сборник, коллекцию"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "в пучок, связку"
                            },
                            {
                                "target_lang_word": "группировать",
                                "target_lang_sense_clarification": "объединять по признаку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c622dc03-0df9-3975-ba3b-fbb72fa678a8",
                    "sense_definition": "Samenbrengen of verenigen om sterker te staan of een doel te bereiken, vooral van krachten of kennis.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bedrijven willen hun krachten <w>bundelen</w> om een betere positie op de markt te krijgen.",
                            "target_lang_translations": {
                                "en": "The companies want to <w>join</w> forces to gain a better market position.",
                                "ru": "Компании хотят <w>объединить</w> свои силы, чтобы занять лучшую позицию на рынке."
                            }
                        },
                        {
                            "text": "Wetenschappers uit de hele wereld <w>bundelen</w> hun kennis om een vaccin te ontwikkelen.",
                            "target_lang_translations": {
                                "en": "Scientists from all over the world are <w>pooling</w> their knowledge to develop a vaccine.",
                                "ru": "Ученые со всего мира <w>объединяют</w> свои знания, чтобы разработать вакцину."
                            }
                        }
                    ],
                    "synonyms": [
                        "verenigen",
                        "samenvoegen"
                    ],
                    "common_phrases": [
                        "krachten bundelen",
                        "kennis bundelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring together or unite to become stronger or achieve a goal, especially regarding forces or knowledge.",
                        "ru": "Соединять ресурсы, усилия или знания для достижения общей цели или усиления позиций."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "join",
                                "target_lang_sense_clarification": "forces"
                            },
                            {
                                "target_lang_word": "pool",
                                "target_lang_sense_clarification": "resources or knowledge"
                            },
                            {
                                "target_lang_word": "combine",
                                "target_lang_sense_clarification": "efforts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "силы, усилия, ресурсы"
                            },
                            {
                                "target_lang_word": "консолидировать",
                                "target_lang_sense_clarification": "укреплять путем объединения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

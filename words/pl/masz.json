            "senses": [
                {
                    "sense_id": "d44c57b0-38e8-3616-bbe7-b7b7fdce35c1",
                    "sense_definition": "Słowo wypowiadane w momencie podawania lub wręczania komuś jakiegoś przedmiotu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Masz>, to jest prezent specjalnie dla ciebie.",
                            "target_lang_translations": {
                                "ru": "<Держи>, это подарок специально для тебя."
                            }
                        },
                        {
                            "text": "<Masz> klucze i zamknij dokładnie drzwi.",
                            "target_lang_translations": {
                                "ru": "<На> ключи и закрой хорошенько дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "proszę"
                    ],
                    "common_phrases": [
                        "masz tu",
                        "no masz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое при передаче или вручении кому-либо предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держи",
                                "target_lang_sense_clarification": "при передаче предмета"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "разговорное, при подаче"
                            },
                            {
                                "target_lang_word": "вот",
                                "target_lang_sense_clarification": "указание при передаче"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4443fdf-0f93-3306-8d9a-77f1ae05db2d",
                    "sense_definition": "Okrzyk wykrzykiwany przy uderzaniu kogoś, wyrażający agresję lub wymierzanie kary.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Masz> za swoje, ty kłamco!",
                            "target_lang_translations": {
                                "ru": "<Получай> за всё, лжец!"
                            }
                        },
                        {
                            "text": "<Masz>, żebyś popamiętał to na długo!",
                            "target_lang_translations": {
                                "ru": "<На>, чтобы ты запомнил это надолго!"
                            }

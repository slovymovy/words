{
    "word_family": [
        "litterer",
        "litterbug",
        "littery"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9ce066d-ffce-36a2-8d46-aadaa25cabdb",
                    "sense_definition": "Waste or debris, such as paper, cans, and bottles, that is left lying in an open or public place.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "trash_mess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not drop <w>litter</w> in the street.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не бросайте <w>мусор</w> на улице."
                            }
                        },
                        {
                            "text": "There is a <w>litter</w> bin near the park entrance.",
                            "target_lang_translations": {
                                "ru": "У входа в парк есть урна для <w>мусора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trash",
                        "particularly in plaster",
                        "trash",
                        "trash"
                    ],
                    "common_phrases": [
                        "drop litter",
                        "pick up litter",
                        "litter bin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мусор или отходы, такие как бумага, банки и бутылки, оставленные в общественном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мусор",
                                "target_lang_sense_clarification": "бытовые отходы"
                            },
                            {
                                "target_lang_word": "сор",
                                "target_lang_sense_clarification": "мелкий мусор, пыль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6601206d-0570-3983-92f9-cad454ef6a45",
                    "sense_definition": "Absorbent material, such as clay granules, used in a box for indoor pets to use as a toilet.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bedding_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to change the cat <w>litter</w> regularly to keep the house smelling fresh.",
                            "target_lang_translations": {
                                "ru": "Нужно регулярно менять кошачий <w>наполнитель</w>, чтобы в доме пахло свежестью."
                            }
                        },
                        {
                            "text": "We bought a bag of clay <w>litter</w> for the new kitten.",
                            "target_lang_translations": {
                                "ru": "Мы купили пакет глиняного <w>наполнителя</w> для нового котенка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cat litter",
                        "litter box",
                        "litter tray"
                    ],
                    "target_lang_definitions": {
                        "ru": "Впитывающий материал, например, гранулы глины, используемый в лотке для домашних животных в качестве туалета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наполнитель",
                                "target_lang_sense_clarification": "для кошачьего туалета"
                            },
                            {
                                "target_lang_word": "подстилка",
                                "target_lang_sense_clarification": "материал в лотке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2f355e0-306c-3cbd-9369-e2a5973f8de3",
                    "sense_definition": "A group of animals born to the same mother at the same time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animals_birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our dog gave birth to a <w>litter</w> of six puppies last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером наша собака принесла <w>помёт</w> из шести щенков."
                            }
                        },
                        {
                            "text": "The runt of the <w>litter</w> is often the smallest and weakest one.",
                            "target_lang_translations": {
                                "ru": "Самый слабый в <w>помёте</w> часто бывает самым маленьким."
                            }
                        }
                    ],
                    "common_phrases": [
                        "litter of puppies",
                        "litter of kittens",
                        "runt of the litter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа детёнышей, рождённых одной матерью одновременно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помёт",
                                "target_lang_sense_clarification": "рождение детёнышей"
                            },
                            {
                                "target_lang_word": "выводок",
                                "target_lang_sense_clarification": "группа детёнышей"
                            },
                            {
                                "target_lang_word": "приплод",
                                "target_lang_sense_clarification": "потомство скота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ff80a07-07ae-38b1-aa98-e65172d8b44e",
                    "sense_definition": "The layer of dead leaves and organic matter on the ground in a forest.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bedding_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many small insects live in the leaf <w>litter</w> on the forest floor.",
                            "target_lang_translations": {
                                "ru": "Многие мелкие насекомые живут в листовой <w>подстилке</w> на лесной почве."
                            }
                        },
                        {
                            "text": "Fungi help decompose the thick layer of <w>litter</w> under the trees.",
                            "target_lang_translations": {
                                "ru": "Грибы помогают разлагать толстый слой <w>подстилки</w> под деревьями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leaf litter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой опавших листьев и органического вещества на земле в лесу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подстилка",
                                "target_lang_sense_clarification": "лесная почва"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01154d81-8e98-3f2a-84d1-352b4777127c",
                    "sense_definition": "Straw or similar loose material used as bedding for farm animals.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bedding_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer spread fresh <w>litter</w> in the horse's stall.",
                            "target_lang_translations": {
                                "ru": "Фермер постелил свежую <w>подстилку</w> в стойле лошади."
                            }
                        },
                        {
                            "text": "Dirty <w>litter</w> must be removed from the barn daily to prevent disease.",
                            "target_lang_translations": {
                                "ru": "Грязную <w>подстилку</w> нужно убирать из сарая ежедневно, чтобы предотвратить болезни."
                            }
                        }
                    ],
                    "synonyms": [
                        "straw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Солома или подобный рыхлый материал, используемый в качестве подстилки для сельскохозяйственных животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подстилка",
                                "target_lang_sense_clarification": "для животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c7afd33-3271-3247-ae83-e1998d33f4aa",
                    "sense_definition": "An enclosed bed or couch carried on poles by people or animals, used in the past.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The queen traveled in a curtained <w>litter</w> carried by four men.",
                            "target_lang_translations": {
                                "ru": "Королева путешествовала в занавешенном <w>паланкине</w>, который несли четверо мужчин."
                            }
                        },
                        {
                            "text": "In ancient times, the wealthy were often transported in a <w>litter</w>.",
                            "target_lang_translations": {
                                "ru": "В древние времена богатых людей часто перевозили в <w>паланкине</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "palanquin",
                        "stretcher"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крытые носилки или ложе на шестах, переносимые людьми или животными (использовались в прошлом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паланкин",
                                "target_lang_sense_clarification": "крытые носилки"
                            },
                            {
                                "target_lang_word": "носилки",
                                "target_lang_sense_clarification": "средство передвижения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "14d34170-7e76-34ef-b258-03d85e058525",
                    "sense_definition": "To throw or leave trash on the ground in a public place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "trash_mess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is illegal to <w>litter</w> in the national park.",
                            "target_lang_translations": {
                                "ru": "В национальном парке запрещено <w>мусорить</w>."
                            }
                        },
                        {
                            "text": "People who <w>litter</w> can be fined by the police.",
                            "target_lang_translations": {
                                "ru": "Люди, которые <w>мусорят</w>, могут быть оштрафованы полицией."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no littering",
                        "litter the streets"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросать или оставлять мусор на земле в общественном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мусорить",
                                "target_lang_sense_clarification": "оставлять мусор"
                            },
                            {
                                "target_lang_word": "сорить",
                                "target_lang_sense_clarification": "разбрасывать мусор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "686d07d8-3d59-3867-b0f8-5bcfe4f18aa4",
                    "sense_definition": "To make a place untidy by leaving many objects scattered around.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trash_mess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The floor was <w>littered</w> with toys and clothes.",
                            "target_lang_translations": {
                                "ru": "Пол был <w>завален</w> игрушками и одеждой."
                            }
                        },
                        {
                            "text": "Broken glass <w>littered</w> the street after the storm.",
                            "target_lang_translations": {
                                "ru": "Битое стекло <w>усеивало</w> улицу после шторма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "be littered with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать место неопрятным, оставляя множество разбросанных предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засорять",
                                "target_lang_sense_clarification": "захламлять"
                            },
                            {
                                "target_lang_word": "усеивать",
                                "target_lang_sense_clarification": "покрывать поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bac59f37-47ef-3a12-b320-874980af60b0",
                    "sense_definition": "To give birth to a group of young animals.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animals_birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wolf <w>littered</w> in a den deep in the forest.",
                            "target_lang_translations": {
                                "ru": "Волчица <w>ощенилась</w> в логове глубоко в лесу."
                            }
                        },
                        {
                            "text": "Sows usually <w>litter</w> twice a year.",
                            "target_lang_translations": {
                                "ru": "Свиноматки обычно <w>поросятся</w> дважды в год."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рожать группу детёнышей (о животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приносить потомство",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "щениться",
                                "target_lang_sense_clarification": "о собаках/волках"
                            },
                            {
                                "target_lang_word": "пороситься",
                                "target_lang_sense_clarification": "о свиньях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f56d6463-d386-3c0f-a8bd-0554f31a8a66",
                    "sense_definition": "Comparative form of <w>lit</w>: more lit.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Last night's party was even <w>litter</w> than the one before.",
                            "target_lang_translations": {
                                "ru": "Вчерашняя вечеринка была еще <w>круче</w>, чем предыдущая."
                            }
                        },
                        {
                            "text": "He promised the next concert would be <w>litter</w>.",
                            "target_lang_translations": {
                                "ru": "Он пообещал, что следующий концерт будет <w>круче</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "comparative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень от 'lit' (крутой): круче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круче",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
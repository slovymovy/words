            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8673241-0c68-311f-92ee-f0a854f89d38",
                    "sense_definition": "Iemand die teksten zoals boeken, verhalen of artikelen schrijft, vaak als beroep.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "author",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman is <w>schrijver</w>.",
                            "target_lang_translations": {
                                "en": "My neighbor is a <w>writer</w>.",
                                "ru": "Мой сосед — <w>писатель</w>."
                            }
                        },
                        {
                            "text": "De <w>schrijver</w> presenteerde zijn nieuwe boek gisteren in de bibliotheek.",
                            "target_lang_translations": {
                                "en": "The <w>author</w> presented his new book yesterday at the library.",
                                "ru": "<w>Писатель</w> представил свою новую книгу вчера в библиотеке."
                            }
                        }
                    ],
                    "synonyms": [
                        "auteur"
                    ],
                    "antonyms": [
                        "lezerspubliek",
                        "lezerspubliek"
                    ],
                    "common_phrases": [
                        "bekende schrijver",
                        "groot schrijver",
                        "schrijver van beroep"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who writes texts such as books, stories, or articles, often as a profession.",
                        "ru": "Человек, который пишет тексты, такие как книги, рассказы или статьи, часто профессионально."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "writer",
                                "target_lang_sense_clarification": "general term for someone who writes professionally"
                            },
                            {
                                "target_lang_word": "author",
                                "target_lang_sense_clarification": "creator of a literary work like a book"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "писатель",
                                "target_lang_sense_clarification": "автор литературных произведений"
                            },
                            {
                                "target_lang_word": "автор",
                                "target_lang_sense_clarification": "создатель произведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5c0488b-bf94-3209-ae19-60d0e02525fe",
                    "sense_definition": "Iemand die op een bepaald moment bezig is met schrijven of een specifieke tekst heeft geschreven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_writing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>schrijver</w> van deze brief is vergeten zijn naam eronder te zetten.",
                            "target_lang_translations": {
                                "en": "The <w>writer</w> of this letter forgot to put his name at the bottom.",
                                "ru": "<w>Автор</w> этого письма забыл подписать свое имя."
                            }
                        },
                        {
                            "text": "Als <w>schrijver</w> van dit verslag ben ik verantwoordelijk voor de inhoud.",
                            "target_lang_translations": {
                                "en": "As the <w>author</w> of this report, I am responsible for the content.",
                                "ru": "Как <w>автор</w> этого отчета, я несу ответственность за его содержание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schrijver van de brief",
                        "onbekende schrijver"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is currently writing or has written a specific text.",
                        "ru": "Тот, кто пишет или написал определенный текст (например, письмо или отчет) в данный момент."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "writer",
                                "target_lang_sense_clarification": "person who wrote a specific document"
                            },
                            {
                                "target_lang_word": "author",
                                "target_lang_sense_clarification": "originator of a document or report"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "автор",
                                "target_lang_sense_clarification": "составитель текста"
                            },
                            {
                                "target_lang_word": "писавший",
                                "target_lang_sense_clarification": "человек, который писал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98452831-bce5-3e90-a7f1-e80d8f5c5752",
                    "sense_definition": "Iemand die in vroeger tijden documenten overschreef of administratief werk deed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clerk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>schrijver</w> werkte urenlang aan het kopiëren van het manuscript.",
                            "target_lang_translations": {
                                "en": "The <w>scribe</w> worked for hours copying the manuscript.",
                                "ru": "<w>Писец</w> часами работал над копированием рукописи."
                            }
                        },
                        {
                            "text": "Vroeger was een <w>schrijver</w> onmisbaar voor het bijhouden van de stadsarchieven.",
                            "target_lang_translations": {
                                "en": "In the past, a <w>clerk</w> was indispensable for maintaining the city archives.",
                                "ru": "Раньше <w>писарь</w> был незаменим для ведения городских архивов."
                            }
                        }
                    ],
                    "synonyms": [
                        "klerk",
                        "kopiist"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who copied documents or performed administrative work in former times.",
                        "ru": "Человек, который в прошлом переписывал документы или занимался канцелярской работой."
                    },

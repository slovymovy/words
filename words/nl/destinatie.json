    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "058712a7-7c71-3025-a6dc-eebab4b4e18b",
                    "sense_definition": "De plaats waar een reis naartoe gaat; het eindpunt van een tocht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "travel_goal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Parijs blijft een populaire <w>destinatie</w> voor toeristen uit de hele wereld.",
                            "target_lang_translations": {
                                "ru": "Париж остается популярным <w>направлением</w> для туристов со всего мира."
                            }
                        },
                        {
                            "text": "Het vliegtuig bereikte zijn uiteindelijke <w>destinatie</w> zonder enige vertraging.",
                            "target_lang_translations": {
                                "ru": "Самолет достиг своего конечного <w>пункта назначения</w> без каких-либо задержек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "populaire destinatie",
                        "nieuwe destinatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, куда направляется путешественник; конечный пункт поездки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пункт назначения",
                                "target_lang_sense_clarification": "конечная точка маршрута"
                            },
                            {
                                "target_lang_word": "место назначения",
                                "target_lang_sense_clarification": "место прибытия"
                            },
                            {
                                "target_lang_word": "направление",
                                "target_lang_sense_clarification": "в контексте туризма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e04236b5-8858-366f-a84d-aeaf8480966e",
                    "sense_definition": "Het vooraf bepaalde doel of lot van iemands leven; datgene waartoe iemand bestemd is.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fate_purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij had het gevoel dat het zijn <w>destinatie</w> was om een groot leider te worden.",
                            "target_lang_translations": {
                                "ru": "У него было чувство, что его <w>предназначением</w> было стать великим лидером."
                            }
                        },
                        {
                            "text": "Zij zocht jarenlang naar haar ware <w>destinatie</w> in het leven.",
                            "target_lang_translations": {
                                "ru": "Она годами искала свое истинное <w>предназначение</w> в жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "lotsbestemming",
                        "roeping",
                        "levenslot"
                    ],
                    "common_phrases": [
                        "goddelijke destinatie",
                        "ware destinatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предопределенная цель или судьба в жизни человека; то, для чего кто-то предназначен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предназначение",
                                "target_lang_sense_clarification": "высшая цель, смысл жизни"
                            },
                            {
                                "target_lang_word": "судьба",
                                "target_lang_sense_clarification": "рок, участь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
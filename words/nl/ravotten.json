                {
                    "sense_id": "b7b586c2-f075-320b-9240-8609aa94e2e4",
                    "sense_definition": "Wild, druk en vrolijk spelen, vaak buiten en met veel beweging.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen liepen de hele middag te <w>ravotten</w> in de tuin.",
                            "target_lang_translations": {
                                "en": "The children spent the whole afternoon <w>romping around</w> in the garden.",
                                "ru": "Дети весь день <w>резвились</w> в саду."
                            }
                        },
                        {
                            "text": "Op de speelplaats <w>ravotten</w> de leerlingen tijdens de pauze.",
                            "target_lang_translations": {
                                "en": "The students <w>romp</w> on the playground during the break.",
                                "ru": "На площадке ученики <w>бесятся</w> во время перемены."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoeien"
                    ],
                    "common_phrases": [
                        "buiten ravotten",
                        "lekker ravotten"
                    ],
                    "target_lang_definitions": {
                        "en": "To play in a wild, active, and cheerful way, often outdoors.",
                        "ru": "Шумно и весело играть, часто на свежем воздухе и очень активно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "romp",
                                "target_lang_sense_clarification": "play roughly and energetically"
                            },
                            {

                    "common_phrases": [
                        "торговая марка",
                        "известная марка",
                        "марка автомобиля",
                        "под маркой"
                    ],
                    "target_lang_definitions": {
                        "en": "A proprietary sign or name identifying the manufacturer of a product; a brand."

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "69c4ef54-8ff9-3fad-b3ee-6649f173ba0b",
                    "sense_definition": "Связанный с конвейером, осуществляемый с помощью движущейся ленты или поточного метода производства.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На новом заводе была запущена современная <w>конвейерная</w> линия по сборке автомобилей.",
                            "target_lang_translations": {
                                "en": "A modern car <w>assembly</w> line was launched at the new plant."
                            }
                        },
                        {
                            "text": "<w>Конвейерная</w> лента медленно двигалась, перемещая детали к следующему рабочему.",
                            "target_lang_translations": {
                                "en": "The <w>conveyor</w> belt moved slowly, transporting parts to the next worker."
                            }
                        }
                    ],
                    "synonyms": [
                        "поточный"
                    ],
                    "antonyms": [
                        "ручной"
                    ],
                    "common_phrases": [
                        "конвейерная лента",
                        "конвейерное производство",
                        "конвейерная сборка"
                    ],
                    "target_lang_definitions": {
                        "en": "Related to a conveyor or carried out using a moving belt or flow production method."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conveyor",
                                "target_lang_sense_clarification": "relating to the mechanical belt"
                            },
                            {
                                "target_lang_word": "assembly-line",
                                "target_lang_sense_clarification": "relating to the method of mass production"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f8ab3d8-03a9-36bc-8e74-967e4c56ef07",
                    "sense_definition": "Осуществляемый непрерывно, механически и однообразно, лишённый индивидуальности и творческого подхода (обычно с неодобрением).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Критики ругали режиссёра за <w>конвейерный</w> выпуск однотипных сериалов.",
                            "target_lang_translations": {
                                "en": "Critics scolded the director for the <w>assembly-line</w> production of identical TV series."
                            }
                        },
                        {
                            "text": "В этой клинике чувствуется бездушный <w>конвейерный</w> подход к пациентам.",
                            "target_lang_translations": {
                                "en": "A soulless <w>assembly-line</w> approach to patients is felt in this clinic."
                            }
                        }
                    ],
                    "synonyms": [
                        "шаблонный",
                        "штампованный",
                        "механический",
                        "безликий"
                    ],
                    "antonyms": [
                        "индивидуальный",
                        "штучный",
                        "уникальный",
                        "эксклюзивный"
                    ],
                    "common_phrases": [
                        "конвейерный метод",
                        "конвейерный подход"
                    ],
                    "target_lang_definitions": {
                        "en": "Carried out continuously, mechanically, and monotonously, lacking individuality or creativity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "assembly-line",
                                "target_lang_sense_clarification": "figurative, implying impersonal or mass-produced quality"
                            },
                            {
                                "target_lang_word": "cookie-cutter",
                                "target_lang_sense_clarification": "idiomatic, lacking originality or distinction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7da5a5f7-ed03-3ba6-9ffc-fa1e95e72cc1",
                    "sense_definition": "Доставить кого-либо или что-либо куда-либо, неся в руках или на себе.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <принеси> мне стакан воды из кухни.",
                            "target_lang_translations": {
                                "en": "Please <bring> me a glass of water from the kitchen."
                            }
                        },
                        {
                            "text": "Отец обещал вечером <принести> домой большую ёлку.",
                            "target_lang_translations": {
                                "en": "Father promised to <bring> home a big Christmas tree in the evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "доставить"
                    ],
                    "antonyms": [
                        "унести",
                        "отнести"
                    ],
                    "common_phrases": [
                        "принести воды",
                        "принести домой",
                        "принести с собой",
                        "принести стул"
                    ],
                    "target_lang_definitions": {
                        "en": "To deliver someone or something somewhere, carrying it in one's hands or on oneself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "to transport toward someone or somewhere"
                            },
                            {
                                "target_lang_word": "fetch",
                                "target_lang_sense_clarification": "to go and get something and bring it back"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "654932e1-1ae9-3557-b3fb-666486ed3a4a",
                    "sense_definition": "Дать какой-либо результат; стать причиной появления чего-либо (пользы, дохода, радости, плодов).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "result_yield",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это решение может <принести> компании большую пользу в будущем.",
                            "target_lang_translations": {
                                "en": "This decision could <bring> the company great benefit in the future."
                            }
                        },
                        {
                            "text": "Мы надеемся, что новый проект сможет <принести> доход уже через месяц.",
                            "target_lang_translations": {
                                "en": "We hope that the new project will be able to <yield> income in just a month."
                            }
                        }
                    ],
                    "synonyms": [
                        "дать"
                    ],
                    "common_phrases": [
                        "принести пользу",
                        "принести доход",
                        "принести удачу",
                        "принести счастье",
                        "принести плоды"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce a result; to cause something to appear (benefit, income, joy, fruit)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "to cause to happen or appear"
                            },
                            {
                                "target_lang_word": "yield",
                                "target_lang_sense_clarification": "to produce profit, results, or crops"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "to produce fruit or offspring"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df162f55-1d38-364c-afbb-1c094f5bc523",
                    "sense_definition": "Высказать или совершить что-либо (обычно используется в устойчивых сочетаниях с существительными, обозначающими извинение, клятву, жертву).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <принести> свои искренние извинения за опоздание.",
                            "target_lang_translations": {
                                "en": "I want to <offer> my sincere apologies for being late."
                            }
                        },
                        {
                            "text": "Ради спасения команды он был готов <принести> в жертву свои интересы.",
                            "target_lang_translations": {
                                "en": "For the sake of saving the team, he was ready to <make> a sacrifice of his interests."
                            }
                        }
                    ],
                    "synonyms": [
                        "выразить",
                        "совершить"
                    ],
                    "common_phrases": [
                        "принести извинения",
                        "принести клятву",
                        "принести присягу",
                        "принести в жертву",
                        "принести благодарность"
                    ],
                    "target_lang_definitions": {
                        "en": "To express or perform an action (used in set phrases with apologies, oaths, sacrifices)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "offer",
                                "target_lang_sense_clarification": "used with apologies or thanks"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "used with sacrifices or oaths"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "292ce6a1-c1a2-3e63-aca0-b4d602ac4f2f",
                    "sense_definition": "Заставить оказаться где-либо или появиться (о ветре, течении, запахе, звуке или судьбе).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "external_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свежий ветер <принёс> с моря запах соли и водорослей.",
                            "target_lang_translations": {
                                "en": "A fresh wind <brought> the smell of salt and seaweed from the sea."
                            }
                        },
                        {
                            "text": "Не знаю, какая нелёгкая судьба <принесла> его в наш город.",
                            "target_lang_translations": {
                                "en": "I don't know what difficult fate <brought> him to our city."
                            }
                        }
                    ],
                    "synonyms": [
                        "занести"
                    ],
                    "common_phrases": [
                        "ветром принесло",
                        "судьба принесла",
                        "течением принесло"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в разговорной речи при описании случайных событий"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cause to end up somewhere or appear (referring to wind, current, smell, sound, or fate)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "to convey or lead to a place"
                            },
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to transport by wind or current"
                            }
                        ]
                    }
                }
            ]
        }
    ]

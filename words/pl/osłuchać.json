            "senses": [
                {
                    "sense_id": "86fc5057-4712-3a3a-a1d9-6f22cbee42d9",
                    "sense_definition": "Zbadać pacjenta, słuchając szmerów i dźwięków wydawanych przez jego narządy wewnętrzne (np. serce, płuca), zazwyczaj przy użyciu stetoskopu",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_exam",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz postanowił dokładnie <w>osłuchać</w> płuca chorego, aby wykluczyć zapalenie oskrzeli.",
                            "target_lang_translations": {
                                "ru": "Врач решил тщательно <w>выслушать</w> легкие больного, чтобы исключить бронхит."
                            }
                        },
                        {
                            "text": "Podczas rutynowej wizyty doktor <w>osłuchał</w> moje serce i zmierzył ciśnienie.",
                            "target_lang_translations": {
                                "ru": "Во время планового визита доктор <w>прослушал</w> мое сердце и измерил давление."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbadać"
                    ],
                    "common_phrases": [
                        "osłuchać pacjenta",
                        "osłuchać płuca",
                        "osłuchać serce",
                        "osłuchać stetoskopem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обследовать пациента, выслушивая шумы и звуки внутренних органов (например, сердца, легких), обычно с помощью стетоскопа"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выслушать",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "прослушать",
                                "target_lang_sense_clarification": "провести осмотр"
                            },
                            {
                                "target_lang_word": "послушать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6b7b9a2-dfa6-3c9d-bb21-a561d4bf86c7",
                    "sense_definition": "(w połączeniu z „się”) Przyzwyczaić się do brzmienia czegoś lub nabrać wprawy w rozumieniu czegoś poprzez wielokrotne słuchanie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "familiarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Warto dużo słuchać radia, aby <w>osłuchać</w> się z melodią obcego języka.",
                            "target_lang_translations": {
                                "ru": "Стоит много слушать радио, чтобы <w>привыкнуть</w> к мелодике иностранного языка."
                            }
                        },
                        {
                            "text": "Po kilku tygodniach pracy w fabryce <w>osłuchałem</w> się z hałasem maszyn i przestał mi przeszkadzać.",
                            "target_lang_translations": {
                                "ru": "После нескольких недель работы на фабрике я <w>привык</w> к шуму машин, и он перестал мне мешать."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyzwyczaić się",
                        "zaznajomić się"
                    ],
                    "common_phrases": [
                        "osłuchać się z językiem",
                        "osłuchać się z muzyką",
                        "osłuchać się z akcentem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привыкнуть к звучанию чего-либо или научиться понимать на слух благодаря многократному прослушиванию"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привыкнуть",
                                "target_lang_sense_clarification": "к звучанию, шуму"
                            },
                            {
                                "target_lang_word": "освоиться",
                                "target_lang_sense_clarification": "с языком, акцентом"
                            }
                        ]
                    }
                }
            ]

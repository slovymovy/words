{
    "word_family": [
        "espionage",
        "counterspy",
        "spyglass"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab6cc882-784c-368d-96d9-802b49b6f213",
                    "sense_definition": "A person who secretly watches and examines the actions of other individuals or organizations and gathers information on them.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police caught a <w>spy</w> working for a foreign government.",
                            "target_lang_translations": {
                                "ru": "Полиция поймала <w>шпиона</w>, работающего на иностранное правительство."
                            }
                        },
                        {
                            "text": "She felt like a <w>spy</w> watching her neighbors from the window.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя <w>шпионом</w>, наблюдая за соседями из окна."
                            }
                        }
                    ],
                    "synonyms": [
                        "secret agent"
                    ],
                    "common_phrases": [
                        "industrial spy",
                        "spy satellite",
                        "foreign spy",
                        "spy network"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который тайно наблюдает за действиями других людей или организаций и собирает о них информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпион",
                                "target_lang_sense_clarification": "человек, занимающийся шпионажем"
                            },
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "секретный сотрудник разведки"
                            },
                            {
                                "target_lang_word": "разведчик",
                                "target_lang_sense_clarification": "сотрудник разведки (часто с положительным оттенком)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42a19e36-58e0-3050-866b-e7e307c46e34",
                    "sense_definition": "A defensive player assigned to cover an offensive backfield player man-to-man when they are expected to engage in a running play.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The linebacker played as a <w>spy</w> to stop the quarterback from running.",
                            "target_lang_translations": {
                                "ru": "Лайнбекер играл роль <w>шпиона</w>, чтобы не дать квотербеку бежать."
                            }
                        },
                        {
                            "text": "The coach assigned a <w>spy</w> to watch the star player closely.",
                            "target_lang_translations": {
                                "ru": "Тренер назначил <w>шпиона</w>, чтобы тот внимательно следил за звездным игроком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "quarterback spy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок защиты, которому поручено персонально опекать игрока задней линии нападения, когда ожидается выносная игра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпион",
                                "target_lang_sense_clarification": "термин в американском футболе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f1088949-73e6-3785-8ba0-a20665c1f39b",
                    "sense_definition": "To watch someone or something secretly to gather information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>spy</w> on his sister to see who she was calling.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>шпионить</w> за сестрой, чтобы узнать, кому она звонит."
                            }
                        },
                        {
                            "text": "The government was accused of <w>spying</w> on its citizens.",
                            "target_lang_translations": {
                                "ru": "Правительство обвинили в том, что оно <w>шпионит</w> за своими гражданами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spy on someone",
                        "spy for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тайно наблюдать за кем-либо или чем-либо с целью сбора информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпионить",
                                "target_lang_sense_clarification": "заниматься шпионажем"
                            },
                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "вести тайное наблюдение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9e2a240-1c1b-3f91-a487-ad5efd1f62f6",
                    "sense_definition": "To suddenly see or notice someone or something that is hard to find.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>spied</w> a rare bird sitting in the tree.",
                            "target_lang_translations": {
                                "ru": "Я <w>заметил</w> редкую птицу, сидящую на дереве."
                            }
                        },
                        {
                            "text": "She finally <w>spied</w> the exit sign through the thick smoke.",
                            "target_lang_translations": {
                                "ru": "Она наконец <w>разглядела</w> знак выхода сквозь густой дым."
                            }
                        }
                    ],
                    "synonyms": [
                        "espy"
                    ],
                    "common_phrases": [
                        "spy with my little eye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно увидеть или заметить кого-то или что-то, что трудно обнаружить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заметить",
                                "target_lang_sense_clarification": "обнаружить взглядом"
                            },
                            {
                                "target_lang_word": "разглядеть",
                                "target_lang_sense_clarification": "увидеть с трудом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
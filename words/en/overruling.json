    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "901cfcf9-4f1d-3b73-91f1-59d83c1c488d",
                    "sense_definition": "Present participle and gerund of <w>overrule</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "overrule_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge is <w>overruling</w> the objection raised by the defense attorney.",
                            "target_lang_translations": {
                                "ru": "Судья <w>отклоняет</w> возражение, выдвинутое адвокатом защиты."
                            }
                        },
                        {
                            "text": "After <w>overruling</w> the committee's decision, the chairman implemented his own plan.",
                            "target_lang_translations": {
                                "ru": "<w>Отменив</w> решение комитета, председатель реализовал свой собственный план."
                            }
                        }
                    ],
                    "common_phrases": [
                        "overruling an objection",
                        "overruling a decision",
                        "overruling authority"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола overrule."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отклонение",
                                "target_lang_sense_clarification": "отказ принять (возражение)"
                            },
                            {
                                "target_lang_word": "отмена",
                                "target_lang_sense_clarification": "аннулирование решения"
                            },
                            {
                                "target_lang_word": "отклоняющий",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "066776df-76a0-3015-a748-d8964d00f6f4",
                    "sense_definition": "The act or instance of rejecting, voiding, or setting aside a decision or authority.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "overrule_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>overruling</w> of the lower court's verdict surprised many legal experts.",
                            "target_lang_translations": {
                                "ru": "<w>Отмена</w> вердикта суда низшей инстанции удивила многих экспертов в области права."
                            }
                        },
                        {
                            "text": "Such a judicial <w>overruling</w> is rare in cases involving minor procedural errors.",
                            "target_lang_translations": {
                                "ru": "Подобная судебная <w>отмена решения</w> редко встречается в делах, связанных с незначительными процессуальными ошибками."
                            }
                        }
                    ],
                    "synonyms": [
                        "reversal",
                        "rejection",
                        "annulment",
                        "countermand"
                    ],
                    "antonyms": [
                        "upholding",
                        "ratification",
                        "confirmation"
                    ],
                    "common_phrases": [
                        "judicial overruling",
                        "overruling of a precedent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или случай отклонения, аннулирования или отмены решения или полномочий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмена",
                                "target_lang_sense_clarification": "решения, приговора"
                            },
                            {
                                "target_lang_word": "отклонение",
                                "target_lang_sense_clarification": "возражения, ходатайства"
                            },
                            {
                                "target_lang_word": "аннулирование",
                                "target_lang_sense_clarification": "официальная отмена"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "04fd4cb0-683a-34ef-ad13-5c6cbe59341c",
                    "sense_definition": "Wulgarnie o opuszczaniu jakiegoś miejsca, często w pośpiechu, ze strachu lub pod przymusem; uciekać, wynosić się.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "leave_escape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobiło się tu niebezpiecznie, więc zaraz stąd <w>wypierdalam</w>.",
                            "target_lang_translations": {
                                "ru": "Здесь стало опасно, так что я сейчас же отсюда <w>съёбываю</w>."
                            }
                        },
                        {
                            "text": "Musimy szybko <w>wypierdalać</w>, zanim przyjedzie policja.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быстро <w>съёбывать</w>, пока не приехала полиция."
                            }
                        }
                    ],
                    "synonyms": [
                        "spierdalać",
                        "spieprzać",
                        "spierniczać",
                        "uciekać",
                        "czmychać",
                        "brać nogi za pas"
                    ],
                    "antonyms": [
                        "przychodzić",
                        "zostawać"
                    ],
                    "common_phrases": [
                        "wypierdalać stąd",
                        "wypierdalać z domu",
                        "szybko wypierdalać"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgaryzm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубо о покидании места, часто в спешке, из страха или по принуждению; убегать, сваливать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съёбывать",
                                "target_lang_sense_clarification": "грубо, уходить или убегать"
                            },
                            {
                                "target_lang_word": "уёбывать",
                                "target_lang_sense_clarification": "грубо, удаляться"
                            },
                            {
                                "target_lang_word": "валить",
                                "target_lang_sense_clarification": "слэнг, быстро уходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa6c3c71-ec14-3c08-8c05-479c90b40d33",
                    "sense_definition": "Wulgarnie o wyrzucaniu czegoś niepotrzebnego lub usuwaniu kogoś z jakiegoś miejsca (np. z pracy, z domu).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "throw_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę w końcu <w>wypierdalać</w> te stare śmieci z garażu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно наконец <w>вышвырнуть</w> этот старый хлам из гаража."
                            }
                        },
                        {
                            "text": "Szef chce go <w>wypierdalać</w> z roboty za ciągłe spóźnienia.",
                            "target_lang_translations": {
                                "ru": "Шеф хочет <w>выпиздить</w> его с работы за постоянные опоздания."
                            }
                        }
                    ],
                    "synonyms": [
                        "wywalać",
                        "wyrzucać",
                        "wypieprzać",
                        "wychrzaniać"
                    ],
                    "antonyms": [
                        "przyjmować",
                        "zatrudniać",
                        "zachowywać"
                    ],
                    "common_phrases": [
                        "wypierdalać na śmietnik",
                        "wypierdalać z pracy",
                        "wypierdalać za drzwi"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgaryzm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубо о выбрасывании чего-то ненужного или изгнании кого-то откуда-либо (например, с работы, из дома)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вышвыривать",
                                "target_lang_sense_clarification": "агрессивно выбрасывать или выгонять"
                            },
                            {
                                "target_lang_word": "выпизживать",
                                "target_lang_sense_clarification": "очень грубо, выгонять или увольнять"
                            },
                            {
                                "target_lang_word": "выкидывать",
                                "target_lang_sense_clarification": "разг., выбрасывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f5ab564-b12a-3866-b2a7-a859e872c33d",
                    "sense_definition": "Wulgarnie o przewracaniu się lub potykaniu (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stumble_fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na tym śliskim chodniku można się łatwo <w>wypierdalać</w>.",
                            "target_lang_translations": {
                                "ru": "На этом скользком тротуаре можно легко <w>наебнуться</w>."
                            }
                        },
                        {
                            "text": "Jak będziesz tak biegał po schodach, to będziesz się ciągle <w>wypierdalać</w>.",
                            "target_lang_translations": {
                                "ru": "Если будешь так бегать по лестнице, то будешь постоянно <w>наёбываться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wywalać",
                        "precz"
                    ],
                    "common_phrases": [
                        "wypierdalać się na ryj"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgaryzm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубо о падении или спотыкании (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наёбываться",
                                "target_lang_sense_clarification": "грубо, падать"
                            },
                            {
                                "target_lang_word": "пиздякаться",
                                "target_lang_sense_clarification": "грубо, падать с шумом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "eb6ebc82-6703-3957-8318-b8a489009a40",
                    "sense_definition": "Bardzo wulgarne i agresywne żądanie, aby ktoś natychmiast opuścił dane miejsce.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "command_leave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę cię tu widzieć, <w>wypierdalać</w> stąd!",
                            "target_lang_translations": {
                                "ru": "Не хочу тебя здесь видеть, <w>съёбывай</w> отсюда!"
                            }
                        },
                        {
                            "text": "Ochroniarz krzyknął do nas tylko jedno słowo: <w>wypierdalać</w>!",
                            "target_lang_translations": {
                                "ru": "Охранник крикнул нам только одно слово: <w>съёбывайте</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "won",
                        "precz",
                        "spierdalać"
                    ],
                    "common_phrases": [
                        "wypierdalać stąd",
                        "wypierdalać mi z oczu"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgaryzm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень грубое и агрессивное требование, чтобы кто-то немедленно покинул место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съебись",
                                "target_lang_sense_clarification": "грубый приказ уйти"
                            },
                            {
                                "target_lang_word": "пиздуй",
                                "target_lang_sense_clarification": "грубо, иди отсюда"
                            },
                            {
                                "target_lang_word": "уёбывай",
                                "target_lang_sense_clarification": "грубый приказ удалиться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fab461a1-818a-352a-8c72-4436949a39b2",
                    "sense_definition": "Słowo używane dla silnego podkreślenia, że ktoś lub coś posiada wszystkie typowe, często skrajne cechy danego pojęcia; prawdziwy, wykapany.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten bałagan w twoim pokoju to <istny> koszmar, musisz natychmiast posprzątać.",
                            "target_lang_translations": {
                                "ru": "Этот беспорядок в твоей комнате — <сущий> кошмар, ты должен немедленно убраться."
                            }
                        },
                        {
                            "text": "Jej nowy ogród wygląda jak <istny> raj na ziemi, pełen kolorowych kwiatów.",
                            "target_lang_translations": {
                                "ru": "Её новый сад выглядит как <настоящий> рай на земле, полный разноцветных цветов."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzeczony"
                    ],
                    "common_phrases": [
                        "istny cud",
                        "istny diabeł",
                        "istny raj",
                        "istny wariat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для сильного подчеркивания того, что кто-то или что-то обладает всеми типичными, часто крайними чертами данного понятия; настоящий, вылитый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настоящий",
                                "target_lang_sense_clarification": "подлинный, действительный"
                            },
                            {
                                "target_lang_word": "сущий",
                                "target_lang_sense_clarification": "истинный, самый настоящий (часто с негативным оттенком или для усиления)"
                            },
                            {
                                "target_lang_word": "вылитый",
                                "target_lang_sense_clarification": "очень похожий, точная копия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f4dae61-d2f5-353c-b491-39aad4a483b3",
                    "sense_definition": "Mężczyzna, który oświadczył się kobiecie i planuje z nią ślub.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej <istny> przyjechał wczoraj z dalekiej podróży, aby wreszcie ustalić datę ślubu.",
                            "target_lang_translations": {
                                "ru": "Её <жених> приехал вчера из дальнего путешествия, чтобы наконец назначить дату свадьбы."
                            }
                        },
                        {
                            "text": "Młoda dziewczyna czekała na swojego <istnego> przy bramie z wielką niecierpliwością.",
                            "target_lang_translations": {
                                "ru": "Молодая девушка ждала своего <суженого> у ворот с большим нетерпением."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzeczony"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "gwara poznańska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который сделал предложение женщине и планирует с ней свадьбу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жених",
                                "target_lang_sense_clarification": "мужчина, вступающий в брак"
                            },
                            {
                                "target_lang_word": "суженый",
                                "target_lang_sense_clarification": "предназначенный судьбой будущий муж"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "13e05e85-6284-3f85-aa38-7e771107125a",
                    "sense_definition": "Wskazuje na osobę lub rzecz, o której była mowa wcześniej; tamten, ów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wspomniany <istny> człowiek nie był nikomu znany w tej okolicy przed wojną.",
                            "target_lang_translations": {
                                "ru": "Упомянутый <тот> человек не был никому известен в этой округе до войны."
                            }
                        },
                        {
                            "text": "Gdy <istny> dzień nadszedł, wszyscy mieszkańcy zgromadzili się na rynku głównym.",
                            "target_lang_translations": {
                                "ru": "Когда настал <тот> день, все жители собрались на главной площади."
                            }
                        }
                    ],
                    "synonyms": [
                        "ów"
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на лицо или предмет, о котором шла речь ранее; тот, оный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тот",
                                "target_lang_sense_clarification": "указательное местоимение"
                            },
                            {
                                "target_lang_word": "оный",
                                "target_lang_sense_clarification": "устаревшее указательное местоимение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

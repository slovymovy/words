    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc53f724-267c-3f7f-88dc-70fee813a133",
                    "sense_definition": "imiesłów przymiotnikowy bierny od czasownika <w>urodzić</w>; określa kogoś, kto przyszedł na świat",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "origin_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek <urodzony> na wolności ma prawo do decydowania o sobie.",
                            "target_lang_translations": {
                                "ru": "Каждый человек, <рождённый> на свободе, имеет право распоряжаться собой."
                            }
                        },
                        {
                            "text": "W tym szpitalu został <urodzony> mój dziadek ponad sto lat temu.",
                            "target_lang_translations": {
                                "ru": "В этой больнице был <рождён> мой дедушка более ста лет назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "ur."
                    ],
                    "common_phrases": [
                        "nowo urodzony",
                        "żywo urodzony",
                        "urodzony w"
                    ],
                    "target_lang_definitions": {
                        "ru": "страдательное причастие от глагола <w>urodzić</w>; обозначает того, кто появился на свет"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рождённый",
                                "target_lang_sense_clarification": "появившийся на свет"
                            },
                            {
                                "target_lang_word": "родившийся",
                                "target_lang_sense_clarification": "тот, кто родился"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e8e73cb5-6e2f-302e-93c7-9ae070bec3f2",
                    "sense_definition": "pochodzący z danego miejsca, będący jego rdzennym mieszkańcem",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "origin_adjective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad to <urodzony> paryżanin, który przeprowadził się do Polski.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — <коренной> парижанин, который переехал в Польшу."
                            }
                        },
                        {
                            "text": "Jako <urodzony> optymista, zawsze wierzył w lepsze jutro.",
                            "target_lang_translations": {
                                "ru": "Будучи <прирождённым> оптимистом, он всегда верил в лучшее завтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "rodowity",
                        "rdzenny"
                    ],
                    "common_phrases": [
                        "urodzony warszawiak",
                        "urodzony anglik"
                    ],
                    "target_lang_definitions": {
                        "ru": "происходящий из определенного места, являющийся его коренным жителем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коренной",
                                "target_lang_sense_clarification": "исконный житель"
                            },
                            {
                                "target_lang_word": "урождённый",
                                "target_lang_sense_clarification": "по происхождению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "884df2ab-6714-38b8-82eb-15ff6d8086c0",
                    "sense_definition": "taki, który posiada wrodzone skłonności, talent i zamiłowanie do jakiejś dziedziny",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "talent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek to <urodzony> kierowca, czuje się za kierownicą bardzo pewnie.",
                            "target_lang_translations": {
                                "ru": "Марек — <прирождённый> водитель, он чувствует себя за рулём очень уверенно."
                            }
                        },
                        {
                            "text": "Ona jest <urodzoną> aktorką, scena to jej prawdziwy żywioł.",
                            "target_lang_translations": {
                                "ru": "Она — <прирождённая> актриса, сцена — её настоящая стихия."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapalony",
                        "zawołany"
                    ],
                    "common_phrases": [
                        "urodzony artysta",
                        "urodzony przywódca",
                        "urodzony zwycięzca"
                    ],
                    "target_lang_definitions": {
                        "ru": "обладающий врождёнными склонностями, талантом и любовью к какой-либо сфере"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прирождённый",
                                "target_lang_sense_clarification": "имеющий врождённый талант"
                            },
                            {
                                "target_lang_word": "истинный",
                                "target_lang_sense_clarification": "настоящий, по призванию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d38e830-fdef-3406-8ba5-9f0f516b9bd5",
                    "sense_definition": "tytuł używany dawniej przed imieniem i nazwiskiem osoby pochodzenia szlacheckiego",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noble_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych dokumentach widniał zapis: <urodzony> Jan Kowalski, herbu Jastrzębiec.",
                            "target_lang_translations": {
                                "ru": "В старых документах была запись: <благородный> Ян Ковальский, герба Ястшембец."
                            }
                        },
                        {
                            "text": "List został zaadresowany do wielmożnego pana, <urodzonego> w dobrach ziemskich.",
                            "target_lang_translations": {
                                "ru": "Письмо было адресовано вельможному пану, <рождённому> в земских владениях."
                            }
                        }
                    ],
                    "traits": [

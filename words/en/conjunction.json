            "senses": [
                {
                    "sense_id": "9534ee48-899f-3775-807a-1852f887db9a",
                    "sense_definition": "A word used to connect words, phrases, or clauses within a sentence.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The word 'and' is a very common <w>conjunction</w> in the English language.",
                            "target_lang_translations": {
                                "ru": "Слово 'and' (и) — очень распространенный <w>союз</w> в английском языке."
                            }
                        },
                        {
                            "text": "You can use a <w>conjunction</w> like 'because' to explain the reason for an action.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать <w>союз</w>, такой как 'because' (потому что), чтобы объяснить причину действия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coordinating conjunction",
                        "subordinating conjunction"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, служащее для соединения слов, фраз или частей предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "союз",
                                "target_lang_sense_clarification": "часть речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "354fb787-29fa-3e60-932b-64edd5534c78",
                    "sense_definition": "The situation where two or more things happen or exist together; the act of joining.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The charity worked in <w>conjunction</w> with local schools to raise money.",
                            "target_lang_translations": {
                                "ru": "Благотворительная организация работала в <w>сотрудничестве</w> с местными школами, чтобы собрать деньги."
                            }
                        },
                        {
                            "text": "A <w>conjunction</w> of fortunate events led to the team's unexpected victory.",
                            "target_lang_translations": {
                                "ru": "<w>Стечение</w> удачных обстоятельств привело к неожиданной победе команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "union",
                        "connection",
                        "joining"
                    ],
                    "common_phrases": [
                        "in conjunction with",
                        "in conjunction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда две или более вещи происходят или существуют вместе; акт соединения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочетание",
                                "target_lang_sense_clarification": "комбинация"
                            },
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "связь"
                            },
                            {
                                "target_lang_word": "стечение",
                                "target_lang_sense_clarification": "обстоятельств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d987ee8-517c-34ab-a481-ef58461d2545",
                    "sense_definition": "The alignment of two celestial bodies so that they appear near each other from Earth.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Astronomers gathered to observe the rare <w>conjunction</w> of Jupiter and Saturn.",
                            "target_lang_translations": {
                                "ru": "Астрономы собрались, чтобы пронаблюдать редкое <w>соединение</w> Юпитера и Сатурна."
                            }
                        },
                        {
                            "text": "The moon is in <w>conjunction</w> with Venus tonight.",
                            "target_lang_translations": {
                                "ru": "Сегодня ночью Луна находится в <w>соединении</w> с Венерой."
                            }
                        }

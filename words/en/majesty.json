{
    "word_family": [
        "majestic",
        "majestically"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4eeaf55a-ddd1-3a48-bc70-25ada0c86df5",
                    "sense_definition": "A title of respect used when speaking to or about a king, queen, or emperor.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "titles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guard bowed low as Her <w>Majesty</w> entered the room.",
                            "target_lang_translations": {
                                "ru": "Стражник низко поклонился, когда Её <w>Величество</w> вошла в комнату."
                            }
                        },
                        {
                            "text": "Your <w>Majesty</w>, the carriage is ready for your departure.",
                            "target_lang_translations": {
                                "ru": "Ваше <w>Величество</w>, карета готова к вашему отъезду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Your Majesty",
                        "Her Majesty",
                        "His Majesty",
                        "Their Majesties"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул уважения, используемый при обращении к королю, королеве или императору или при упоминании о них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "величество",
                                "target_lang_sense_clarification": "титул монарха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48e22824-d864-36d4-a8c8-61c0af8f7e7f",
                    "sense_definition": "The quality of being impressive, beautiful, dignified, and powerful.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "qualities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stood in silence, admiring the sheer <w>majesty</w> of the mountains.",
                            "target_lang_translations": {
                                "ru": "Мы стояли в тишине, восхищаясь истинным <w>величием</w> гор."
                            }
                        },
                        {
                            "text": "The old cathedral rises in all its <w>majesty</w> above the city square.",
                            "target_lang_translations": {
                                "ru": "Старый собор возвышается во всём своём <w>величии</w> над городской площадью."
                            }
                        }
                    ],
                    "synonyms": [
                        "grandeur",
                        "splendor",
                        "magnificence",
                        "dignity"
                    ],
                    "common_phrases": [
                        "in all its majesty",
                        "sheer majesty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, характеризующееся впечатляющей красотой, достоинством и мощью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "величие",
                                "target_lang_sense_clarification": "грандиозность, торжественность"
                            },
                            {
                                "target_lang_word": "величественность",
                                "target_lang_sense_clarification": "впечатляющий внешний вид"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
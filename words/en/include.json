{
    "word_family": [
        "inclusion",
        "inclusive",
        "inclusively",
        "inclusiveness",
        "includable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fdc72cab-0ef1-33ac-9942-d889f21acbba",
                    "sense_definition": "To have something or someone as a part of a whole; to contain.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "containment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The price of the hotel room <w>includes</w> breakfast and dinner.",
                            "target_lang_translations": {
                                "ru": "Стоимость номера в отеле <w>включает</w> завтрак и ужин."
                            }
                        },
                        {
                            "text": "Does this history book <w>include</w> a section on the Roman Empire?",
                            "target_lang_translations": {
                                "ru": "Эта книга по истории <w>включает</w> раздел о Римской империи?"
                            }
                        }
                    ],
                    "synonyms": [
                        "contain",
                        "comprise",
                        "involve",
                        "cover",
                        "encompass",
                        "embody",
                        "come with"
                    ],
                    "antonyms": [
                        "exclude",
                        "lack",
                        "omit"
                    ],
                    "common_phrases": [
                        "include tax",
                        "include details",
                        "not include",
                        "include everything"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь что-либо или кого-либо в качестве составной части; содержать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "иметь в составе"
                            },
                            {
                                "target_lang_word": "содержать",
                                "target_lang_sense_clarification": "иметь внутри"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a187588c-45c0-3a14-a590-98a4a88e0b96",
                    "sense_definition": "To make someone or something part of a group, list, or set.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>include</w> my name on the guest list for the party.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>включите</w> моё имя в список гостей вечеринки."
                            }
                        },
                        {
                            "text": "The coach decided to <w>include</w> the new player in the starting lineup.",
                            "target_lang_translations": {
                                "ru": "Тренер решил <w>включить</w> нового игрока в стартовый состав."
                            }
                        }
                    ],
                    "synonyms": [
                        "add",
                        "incorporate",
                        "count",
                        "admit"
                    ],
                    "antonyms": [
                        "exclude",
                        "drop",
                        "omit"
                    ],
                    "common_phrases": [
                        "include in the list",
                        "include someone in",
                        "include a note"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать кого-либо или что-либо частью группы, списка или набора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "добавлять в группу"
                            },
                            {
                                "target_lang_word": "включить",
                                "target_lang_sense_clarification": "добавить (сов. вид)"
                            },
                            {
                                "target_lang_word": "вносить",
                                "target_lang_sense_clarification": "добавлять в список"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99a1ae24-99ac-3564-8440-82861f5ea0fd",
                    "sense_definition": "To use a directive that allows the use of source code from another file.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>include</w> the standard library to use these functions.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>подключить</w> стандартную библиотеку, чтобы использовать эти функции."
                            }
                        },
                        {
                            "text": "The script will automatically <w>include</w> the configuration file at startup.",
                            "target_lang_translations": {
                                "ru": "Скрипт автоматически <w>подключит</w> файл конфигурации при запуске."
                            }
                        }
                    ],
                    "common_phrases": [
                        "include a library",
                        "include a file",
                        "include a header"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать директиву для вставки исходного кода из другого файла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подключать",
                                "target_lang_sense_clarification": "соединять с кодом"
                            },
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "добавлять библиотеку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a0d418f-da2e-3dfc-a33a-69a2ac500bbf",
                    "sense_definition": "To enclose or confine within limits.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could have willingly ranged, but these straits wherein I am <w>included</w> will not permit.",
                            "target_lang_translations": {
                                "ru": "Я бы охотно побродил, но эти теснины, в которые я <w>заключён</w>, не позволяют."
                            }
                        },
                        {
                            "text": "The ancient walls <w>include</w> the inner sanctum of the temple.",
                            "target_lang_translations": {
                                "ru": "Древние стены <w>заключают</w> в себе внутреннее святилище храма."
                            }
                        }
                    ],
                    "synonyms": [
                        "enclose",
                        "encircle",
                        "circumscribe"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Original Latin sense."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заключать в пределы или ограничивать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключать",
                                "target_lang_sense_clarification": "ограничивать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e52b5e80-19fb-3c24-9502-bb67f03e4499",
                    "sense_definition": "To conclude or terminate.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "conclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We will <w>include</w> all arguments with a final agreement.",
                            "target_lang_translations": {
                                "ru": "Мы <w>завершим</w> все споры окончательным соглашением."
                            }
                        },
                        {
                            "text": "The events of the day were <w>included</w> with a grand celebration.",
                            "target_lang_translations": {
                                "ru": "События дня <w>завершились</w> грандиозным празднованием."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "No longer used in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Завершать или заканчивать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завершать",
                                "target_lang_sense_clarification": "заканчивать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ae6f211-b1ed-3349-a1cf-1d6bada5a50b",
                    "sense_definition": "A piece of source code or other content that is dynamically retrieved for inclusion in another item.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The developer checked the <w>include</w> to ensure the library loaded correctly.",
                            "target_lang_translations": {
                                "ru": "Разработчик проверил <w>подключаемый файл</w>, чтобы убедиться, что библиотека загрузилась правильно."
                            }
                        },
                        {
                            "text": "Using a server-side <w>include</w> can simplify the management of website headers.",
                            "target_lang_translations": {
                                "ru": "Использование <w>включений</w> на стороне сервера может упростить управление заголовками веб-сайта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "server-side include",
                        "file include"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фрагмент исходного кода или директива, вставляемая в другой файл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подключаемый файл",
                                "target_lang_sense_clarification": "компонент кода"
                            },
                            {
                                "target_lang_word": "вставка",
                                "target_lang_sense_clarification": "включение кода"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
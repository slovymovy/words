            "senses": [
                {
                    "sense_id": "624f92f6-d267-3020-9051-88265c4d927d",
                    "sense_definition": "To court or curry favour for political or business advantage; to flatter obsequiously.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lobbyist spent the evening <w>duchessing</w> the senator to gain support for the bill.",
                            "target_lang_translations": {
                                "ru": "Лоббист провел вечер, <w>обхаживая</w> сенатора, чтобы заручиться поддержкой законопроекта."
                            }
                        },
                        {
                            "text": "He was <w>duchessed</w> by the media and treated like a celebrity during his visit.",
                            "target_lang_translations": {

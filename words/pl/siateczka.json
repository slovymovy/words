                },
                {
                    "sense_id": "c41eed1e-ffb0-362c-8292-455e5b7c404c",
                    "sense_definition": "materiał lub struktura wykonana z cienkich, przeplatanych nici, drutów lub włókien",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej wieczorowa sukienka była ozdobiona delikatną, czarną siateczką.",

                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частично переваренная пища, исторгнутая при рвоте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рвота",

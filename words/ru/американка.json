            "senses": [
                {
                    "sense_id": "d61568f8-b4d2-3f81-8615-7a27a6af3619",
                    "sense_definition": "Женщина или девушка, проживающая в Америке (обычно в США) или являющаяся гражданкой этого государства.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашу группу пришла учиться молодая <американка>, которая хочет выучить русский язык.",
                            "target_lang_translations": {
                                "en": "A young <American woman> who wants to learn Russian has joined our study group."
                            }
                        },
                        {
                            "text": "Эта <американка> очень любит путешествовать по старинным городам Европы.",
                            "target_lang_translations": {
                                "en": "This <American> loves traveling around the ancient cities of Europe."
                            }
                        }
                    ],
                    "synonyms": [
                        "жительница США"
                    ],
                    "common_phrases": [
                        "типичная американка",
                        "богатая американка",
                        "молодая американка",
                        "знакомая американка"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman or girl living in America (usually the USA) or who is a citizen of that state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "American",
                                "target_lang_sense_clarification": "female citizen of the US"
                            },
                            {
                                "target_lang_word": "American woman",
                                "target_lang_sense_clarification": "female person from the US"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba9d0416-dcd4-3386-9647-ea6703230ba1",
                    "sense_definition": "Разъемное соединение труб с накидной гайкой, широко используемое в сантехнике.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сантехник сказал, что для установки нового радиатора нам понадобится <американка>.",
                            "target_lang_translations": {
                                "en": "The plumber said that we would need a <union> to install the new radiator."
                            }
                        },
                        {
                            "text": "Кран типа <американка> позволяет легко демонтировать оборудование без вращения труб.",
                            "target_lang_translations": {
                                "en": "A <union> valve allows for easy dismantling of equipment without rotating the pipes."
                            }
                        }
                    ],
                    "synonyms": [
                        "муфта",
                        "сгон"
                    ],
                    "common_phrases": [

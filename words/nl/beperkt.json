                        "ru": "Форма глагола beperken: повелительное наклонение множественного числа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "limit",
                                "target_lang_sense_clarification": "command to set a boundary"
                            },
                            {
                                "target_lang_word": "restrict",
                                "target_lang_sense_clarification": "command to keep within limits"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ограничьте",

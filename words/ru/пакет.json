            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f0efd1d4-4306-3a77-abc0-4101d1859a63",
                    "sense_definition": "Небольшой мешок из бумаги, полиэтилена или другого мягкого материала для упаковки и переноски вещей.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В магазине мне дали большой бумажный <пакет> для покупок.",
                            "target_lang_translations": {
                                "en": "At the store, they gave me a large paper <bag> for shopping."
                            }
                        },
                        {
                            "text": "Она положила яблоки в прозрачный <пакет> и пошла к кассе.",
                            "target_lang_translations": {
                                "en": "She put the apples in a transparent <bag> and went to the checkout."
                            }
                        }
                    ],
                    "synonyms": [
                        "мешок",
                        "кулёк"
                    ],
                    "common_phrases": [
                        "полиэтиленовый пакет",
                        "бумажный пакет",
                        "пакет с продуктами",
                        "мусорный пакет"
                    ],
                    "target_lang_definitions": {
                        "en": "A bag made of paper, plastic, or other soft material for packing and carrying items."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bag",
                                "target_lang_sense_clarification": "general container"
                            },
                            {
                                "target_lang_word": "plastic bag",
                                "target_lang_sense_clarification": "specific material"
                            },
                            {
                                "target_lang_word": "carrier bag",
                                "target_lang_sense_clarification": "shopping context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6cdba9a-1e6e-3cbe-b79c-06785e311ae9",
                    "sense_definition": "Картонная упаковка прямоугольной формы для жидких продуктов (молока, сока, кефира).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На завтрак он обычно выпивает целый <пакет> апельсинового сока.",
                            "target_lang_translations": {
                                "en": "For breakfast, he usually drinks a whole <carton> of orange juice."
                            }
                        },
                        {
                            "text": "Пожалуйста, купи <пакет> молока по дороге домой.",
                            "target_lang_translations": {
                                "en": "Please buy a <carton> of milk on your way home."
                            }
                        }
                    ],
                    "synonyms": [
                        "упаковка",
                        "коробка"
                    ],
                    "common_phrases": [
                        "пакет молока",
                        "пакет сока",
                        "открыть пакет",
                        "тетрапак"
                    ],
                    "target_lang_definitions": {
                        "en": "A cardboard container, usually rectangular, for liquid products like milk or juice."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carton",
                                "target_lang_sense_clarification": "for liquids"
                            },
                            {

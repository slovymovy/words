            "senses": [
                {
                    "sense_id": "67c104bd-b4a3-32b6-9c4a-ff828149abd6",
                    "sense_definition": "Zestawiać ze sobą rzeczy lub osoby w celu znalezienia ich cech wspólnych lub różnic.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "comparison_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed zakupem warto dokładnie <porównywać> ceny w różnych sklepach.",
                            "target_lang_translations": {
                                "ru": "Перед покупкой стоит внимательно <сравнивать> цены в разных магазинах."
                            }
                        },
                        {
                            "text": "Nie można <porównywać> tych dwóch sytuacji, ponieważ są zupełnie inne.",
                            "target_lang_translations": {
                                "ru": "Нельзя <сравнивать> эти две ситуации, так как они совершенно разные."
                            }
                        }
                    ],
                    "synonyms": [
                        "zestawiać",
                        "konfrontować"
                    ],
                    "common_phrases": [
                        "porównywać ceny",
                        "porównywać oferty",
                        "porównywać wyniki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сопоставлять кого-либо или что-либо для выявления сходств или различий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравнивать",
                                "target_lang_sense_clarification": "сопоставлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09b5a045-18ab-3d3e-9904-4f6509cc3a00",
                    "sense_definition": "Oceniać samego siebie, swoje cechy lub osiągnięcia w odniesieniu do innych osób.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś ciągle <porównywać> się do swojego starszego brata.",
                            "target_lang_translations": {
                                "ru": "Ты не должен постоянно <сравнивать> себя со своим старшим братом."
                            }
                        },
                        {
                            "text": "Każdy człowiek jest inny, dlatego nie warto się z nikim <porównywać>.",
                            "target_lang_translations": {
                                "ru": "Каждый человек уникален, поэтому не стоит ни с кем себя <сравнивать>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "porównywać się z innymi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать самого себя, свои качества или достижения по отношению к другим людям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравнивать",
                                "target_lang_sense_clarification": "себя с другими"
                            }
                        ]
                    }
                }
            ]
        }

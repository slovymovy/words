    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "599e9b73-cf82-333e-ad62-06490f7cf1a3",
                    "sense_definition": "Urządzenie elektroniczne służące do odbierania sygnałów dźwiękowych i audycji przesyłanych za pomocą fal radiowych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano włączam <w>radio</w>, żeby posłuchać najnowszych wiadomości przy śniadaniu.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я включаю <w>радио</w>, чтобы послушать последние новости за завтраком."
                            }
                        },
                        {
                            "text": "To stare <w>radio</w> w kuchni ma bardzo słaby zasięg i często szumi.",
                            "target_lang_translations": {
                                "ru": "Это старое <w>радио</w> на кухне очень плохо ловит и часто шумит."
                            }
                        }
                    ],
                    "synonyms": [
                        "radioodbiornik",
                        "odbiornik radiowy"
                    ],
                    "common_phrases": [
                        "włączyć radio",
                        "słuchać radia",
                        "zepsute radio",
                        "radio samochodowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронное устройство для приема звуковых сигналов и передач, транслируемых посредством радиоволн."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радио",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "радиоприёмник",
                                "target_lang_sense_clarification": "аппарат"
                            },
                            {
                                "target_lang_word": "приёмник",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c012722-95c1-31aa-aca2-9464b19999d2",
                    "sense_definition": "Instytucja lub organizacja zajmująca się przygotowywaniem i nadawaniem programów dźwiękowych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra marzy o tym, aby w przyszłości pracować w <w>radiu</w>.",
                            "target_lang_translations": {
                                "ru": "Моя сестра мечтает в будущем работать на <w>радио</w>."
                            }
                        },
                        {
                            "text": "To lokalne <w>radio</w> nadaje dużo muzyki i informacji z naszego regionu.",
                            "target_lang_translations": {
                                "ru": "Это местное <w>радио</w> передает много музыки и новостей из нашего региона."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozgłośnia",
                        "stacja radiowa"
                    ],
                    "common_phrases": [
                        "pracować w radiu",
                        "radio publiczne",
                        "radio internetowe",
                        "polskie radio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение или организация, занимающаяся подготовкой и трансляцией звуковых программ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радио",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "радиостанция",
                                "target_lang_sense_clarification": "станция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1911d2bf-cadf-3c9e-acf7-d3a6a810939e",
                    "sense_definition": "System przekazywania informacji i dźwięku na odległość za pomocą fal elektromagnetycznych; środek masowego przekazu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wynalezienie <w>radia</w> zrewolucjonizowało sposób komunikacji na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Изобретение <w>радио</w> произвело революцию в способах коммуникации во всем мире."
                            }
                        },
                        {
                            "text": "Dzięki <w>radiu</w> marynarze mogą utrzymywać stały kontakt z lądem.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>радио</w> моряки могут поддерживать постоянную связь с сушей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fale radiowe",
                        "wynalazek radia",
                        "historia radia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система передачи информации и звука на расстояние с помощью электромагнитных волн; средство массовой информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радио",
                                "target_lang_sense_clarification": "технология"
                            },
                            {
                                "target_lang_word": "радиосвязь",
                                "target_lang_sense_clarification": "способ связи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b68c9f52-f990-3885-854f-580ce9132e3f",
                    "sense_definition": "Urządzenie nadawczo-odbiorcze służące do dwustronnej komunikacji głosowej, używane np. przez służby, wojsko lub na statkach.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant wezwał wsparcie, krzycząc głośno przez <w>radio</w> w radiowozie.",
                            "target_lang_translations": {
                                "ru": "Полицейский вызвал подкрепление, громко крича в <w>рацию</w> в патрульной машине."
                            }
                        },
                        {
                            "text": "Kapitan statku nadał komunikat przez <w>radio</w>, ostrzegając inne jednostki przed sztormem.",
                            "target_lang_translations": {
                                "ru": "Капитан корабля передал сообщение по <w>радио</w>, предупреждая другие суда о шторме."
                            }
                        }
                    ],
                    "synonyms": [
                        "radiostacja"
                    ],
                    "common_phrases": [
                        "nadać przez radio",
                        "radio policyjne",
                        "obsługiwać radio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приемопередающее устройство для двусторонней голосовой связи, используемое, например, службами, военными или на судах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рация",
                                "target_lang_sense_clarification": "переговорное устройство"
                            },
                            {
                                "target_lang_word": "радиостанция",
                                "target_lang_sense_clarification": "оборудование связи"
                            }
                        ]
                    }
                }
            ]
        }
    ]

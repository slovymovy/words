                    "sense_id": "f7bdd2f4-6951-3610-bc75-22990ad2cade",
                    "sense_definition": "Pojedynczy akt wciągnięcia powietrza do płuc.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę wziąć głęboki <w>oddech</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сделайте глубокий <w>вдох</w>."
                            }
                        },
                        {
                            "text": "Po szybkim biegu musiałem chwilę odpocząć, by złapać <w>oddech</w>.",
                            "target_lang_translations": {
                                "ru": "После быстрого бега мне пришлось немного отдохнуть, чтобы восстановить <w>дыхание</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wdech"
                    ],
                    "antonyms": [
                        "wydech"

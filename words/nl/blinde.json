                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепой",
                                "target_lang_sense_clarification": "лишенный зрения"
                            },
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "без проемов или выхода (напр. стена, переулок)"
                            }

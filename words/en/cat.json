            "pos": "noun",
            "senses": [
                {
                    "sense_id": "38f333ca-5948-39bb-b51a-e2710e79fae6",
                    "sense_definition": "A small, furry, carnivorous mammal often kept as a pet or for catching mice.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>cat</w> curled up on the rug and fell asleep.",
                            "target_lang_translations": {
                                "ru": "<w>Кошка</w> свернулась калачиком на коврике и уснула."
                            }
                        },
                        {
                            "text": "She has a pet <w>cat</w> that loves to play with string.",
                            "target_lang_translations": {
                                "ru": "У нее есть домашняя <w>кошка</w>, которая любит играть с веревочкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "feline",
                        "kitty",
                        "pussycat",
                        "puss",
                        "housecat"
                    ],
                    "antonyms": [
                        "dog"
                    ],
                    "common_phrases": [
                        "cat and mouse",
                        "cat got your tongue",
                        "curiosity killed the cat",
                        "let the cat out of the bag",
                        "raining cats and dogs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое пушистое хищное млекопитающее, которое часто держат как домашнее животное или для ловли мышей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошка",
                                "target_lang_sense_clarification": "домашнее животное"
                            },
                            {
                                "target_lang_word": "кот",
                                "target_lang_sense_clarification": "самец кошки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3826d71c-6e7b-3ede-b3d3-111aca120fd4",
                    "sense_definition": "Any large wild animal of the cat family, such as a lion, tiger, or leopard.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lions are the only big <w>cats</w> that live in groups.",
                            "target_lang_translations": {
                                "ru": "Львы — единственные большие <w>кошки</w>, которые живут группами."
                            }
                        },
                        {
                            "text": "The zoo has a new enclosure for the wild <w>cats</w>.",
                            "target_lang_translations": {
                                "ru": "В зоопарке появился новый вольер для диких <w>кошек</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "feline",
                        "wildcat"
                    ],
                    "common_phrases": [
                        "big cat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любое крупное дикое животное из семейства кошачьих, например лев, тигр или леопард."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошка",
                                "target_lang_sense_clarification": "животное семейства кошачьих"
                            },
                            {
                                "target_lang_word": "кошачьи",
                                "target_lang_sense_clarification": "семейство животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d31a84cb-8053-3be5-adae-70fbb3b775c1",
                    "sense_definition": "A medical scan that uses X-rays to create detailed images of the inside of the body (abbreviation of computed axial tomography).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor ordered a <w>CAT</w> scan to check for internal injuries.",
                            "target_lang_translations": {
                                "ru": "Врач назначил <w>КТ</w> для проверки на наличие внутренних повреждений."
                            }
                        },
                        {
                            "text": "She was nervous about having a <w>CAT</w> scan at the hospital.",
                            "target_lang_translations": {
                                "ru": "Она нервничала перед прохождением <w>КТ</w> в больнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "CT scan"
                    ],
                    "common_phrases": [
                        "CAT scan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинское сканирование, использующее рентгеновские лучи для создания детальных изображений внутренних органов (аббревиатура от компьютерной аксиальной томографии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "КТ",
                                "target_lang_sense_clarification": "компьютерная томография"
                            },
                            {
                                "target_lang_word": "томография",
                                "target_lang_sense_clarification": "медицинское сканирование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dfd9a04-3cf9-367c-80a6-2c6f5b7d5fcc",
                    "sense_definition": "A man or person, often used in jazz contexts or slang.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a cool <w>cat</w> who plays the saxophone.",
                            "target_lang_translations": {
                                "ru": "Он крутой <w>чувак</w>, который играет на саксофоне."
                            }
                        },
                        {
                            "text": "I don't know that <w>cat</w>, but he seems friendly.",
                            "target_lang_translations": {
                                "ru": "Я не знаю этого <w>типа</w>, но он кажется дружелюбным."
                            }
                        }
                    ],
                    "synonyms": [
                        "guy",
                        "dude",
                        "fellow"
                    ],
                    "common_phrases": [
                        "cool cat"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used in jazz contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина или человек, часто используется в джазовом контексте или сленге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувак",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {

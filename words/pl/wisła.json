                            {
                                "target_lang_word": "разлив",
                                "target_lang_sense_clarification": "реки"
                            },
                            {
                                "target_lang_word": "половодье",
                                "target_lang_sense_clarification": "сезонное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

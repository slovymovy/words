{
    "word_family": [
        "furnish",
        "furnishing",
        "furnisher",
        "furnished",
        "unfurnished"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d0ae67d-19d4-3dac-a870-01f828a69206",
                    "sense_definition": "Large movable objects such as chairs, tables, and beds that are used to make a room or building suitable for living or working.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to buy a new piece of <w>furniture</w> for the empty bedroom.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить новый предмет <w>мебели</w> для пустой спальни."
                            }
                        },
                        {
                            "text": "The office <w>furniture</w> was arranged to encourage collaboration among the staff.",
                            "target_lang_translations": {
                                "ru": "Офисная <w>мебель</w> была расставлена так, чтобы способствовать сотрудничеству между сотрудниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "furnishings"
                    ],
                    "common_phrases": [
                        "piece of furniture",
                        "office furniture",
                        "garden furniture",
                        "antique furniture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупные предметы, такие как столы, стулья и кровати, используемые для обстановки комнаты или здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мебель",
                                "target_lang_sense_clarification": "обстановка помещения"
                            },
                            {
                                "target_lang_word": "предмет мебели",
                                "target_lang_sense_clarification": "отдельная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3448277f-ae2a-3626-9c07-b399714213c7",
                    "sense_definition": "The small accessories or fittings attached to items like doors, windows, or cabinets, such as handles, locks, and hinges.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The brass door <w>furniture</w> was polished to a high shine.",
                            "target_lang_translations": {
                                "ru": "Латунная дверная <w>фурнитура</w> была начищена до блеска."
                            }
                        },
                        {
                            "text": "He replaced the old iron <w>furniture</w> on the cabinets with modern handles.",
                            "target_lang_translations": {
                                "ru": "Он заменил старую железную <w>фурнитуру</w> на шкафах современными ручками."
                            }
                        }
                    ],
                    "synonyms": [
                        "furnishings",
                        "furniture"
                    ],
                    "common_phrases": [
                        "door furniture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкие аксессуары или детали, прикрепляемые к дверям, окнам или шкафам, например, ручки, замки и петли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фурнитура",
                                "target_lang_sense_clarification": "дверная, оконная или мебельная"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5a53f980-e54b-3682-9af1-e90aa2e6c166",
                    "sense_definition": "Met een verhoogde temperatuur; erg warm gemaakt of geworden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_heat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het hardlopen was zijn gezicht helemaal rood en <w>verhit</w>.",
                            "target_lang_translations": {
                                "ru": "После бега его лицо было совершенно красным и <w>разгорячённым</w>."
                            }
                        },
                        {
                            "text": "Zorg dat de oven goed <w>verhit</w> is voordat je de taart erin zet.",
                            "target_lang_translations": {
                                "ru": "Убедись, что духовка хорошо <w>разогрета</w>, прежде чем ставить в неё пирог."
                            }
                        }
                    ],
                    "antonyms": [
                        "niet-verhit"
                    ],
                    "common_phrases": [
                        "rood en verhit",
                        "verhit gezicht"
                    ],
                    "target_lang_definitions": {
                        "ru": "С повышенной температурой; ставший очень горячим или разгорячённым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разгорячённый",
                                "target_lang_sense_clarification": "о человеке или частях тела"
                            },
                            {
                                "target_lang_word": "нагретый",
                                "target_lang_sense_clarification": "о предметах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d178a07d-6423-308a-a047-cde9709d0074",
                    "sense_definition": "Gekenmerkt door sterke emoties, zoals boosheid of enthousiasme.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_heat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politici raakten verwikkeld in een <w>verhit</w> debat over de begroting.",
                            "target_lang_translations": {
                                "ru": "Политики были втянуты в <w>жаркие</w> дебаты о бюджете."
                            }
                        },
                        {
                            "text": "Hij reageerde nogal <w>verhit</w> toen hem naar zijn fouten werd gevraagd.",
                            "target_lang_translations": {
                                "ru": "Он отреагировал довольно <w>горячо</w>, когда его спросили об ошибках."
                            }
                        }
                    ],
                    "synonyms": [
                        "opgewonden"
                    ],
                    "antonyms": [
                        "koelbloedig"
                    ],
                    "common_phrases": [
                        "een verhit debat",
                        "verhitte discussie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся сильными эмоциями, такими как гнев или энтузиазм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаркий",
                                "target_lang_sense_clarification": "о споре или дискуссии"
                            },
                            {
                                "target_lang_word": "ожесточённый",
                                "target_lang_sense_clarification": "о конфликте или дебатах"
                            }
                        ]
                    }
                }
            ]
        },
        {

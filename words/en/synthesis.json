    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "698e9036-d622-3e66-a6e6-5dbe082903ac",
                    "sense_definition": "The formation of something complex or coherent by combining simpler things.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_combination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The essay was a perfect <w>synthesis</w> of historical facts and personal opinion.",
                            "target_lang_translations": {
                                "ru": "Это эссе представляло собой идеальный <w>синтез</w> исторических фактов и личного мнения."
                            }
                        },
                        {
                            "text": "Her cooking represents a creative <w>synthesis</w> of traditional and modern techniques.",
                            "target_lang_translations": {
                                "ru": "Её кулинария представляет собой творческий <w>синтез</w> традиционных и современных техник."
                            }
                        }
                    ],
                    "synonyms": [
                        "combination",
                        "fusion",
                        "blend",
                        "integration",
                        "mixture"
                    ],
                    "antonyms": [
                        "analysis"
                    ],
                    "common_phrases": [
                        "synthesis of ideas",
                        "cultural synthesis",
                        "synthesis of arts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формирование чего-либо сложного или целостного путем объединения более простых вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "си́нтез",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e3679eb-ae25-3756-996a-a9fd11e3cc12",
                    "sense_definition": "The reaction of elements or compounds to form more complex compounds.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "scientific_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Plants use sunlight for the <w>synthesis</w> of nutrients from carbon dioxide and water.",
                            "target_lang_translations": {
                                "ru": "Растения используют солнечный свет для <w>синтеза</w> питательных веществ из углекислого газа и воды."
                            }
                        },
                        {
                            "text": "The laboratory focuses on the <w>synthesis</w> of new drugs to treat diseases.",
                            "target_lang_translations": {
                                "ru": "Лаборатория занимается <w>синтезом</w> новых лекарств для лечения болезней."
                            }
                        }
                    ],
                    "synonyms": [
                        "formation",
                        "creation"
                    ],
                    "common_phrases": [
                        "protein synthesis",
                        "chemical synthesis",
                        "organic synthesis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реакция элементов или соединений с образованием более сложных веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "си́нтез",
                                "target_lang_sense_clarification": "химия/биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75ee7246-bd54-344b-a4ea-49df2b3acd57",
                    "sense_definition": "The creation of a complex waveform, sound, or speech by combining simpler signals.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Modern software allows for the realistic <w>synthesis</w> of human speech.",
                            "target_lang_translations": {
                                "ru": "Современное программное обеспечение позволяет осуществлять реалистичный <w>синтез</w> человеческой речи."
                            }
                        },
                        {
                            "text": "He studied sound <w>synthesis</w> to create unique electronic music.",
                            "target_lang_translations": {
                                "ru": "Он изучал <w>синтез</w> звука, чтобы создавать уникальную электронную музыку."
                            }
                        }
                    ],
                    "synonyms": [
                        "generation"
                    ],
                    "common_phrases": [
                        "speech synthesis",
                        "voice synthesis",
                        "sound synthesis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создание сложной формы волны, звука или речи путем объединения простых сигналов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "си́нтез",
                                "target_lang_sense_clarification": "звук и речь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27b49636-c52d-32e4-9f38-db8aeb1fdbca",
                    "sense_definition": "The combination of a thesis and an antithesis to form a higher truth or resolution.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "philosophical_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hegel's philosophy describes progress as a <w>synthesis</w> emerging from conflict.",
                            "target_lang_translations": {
                                "ru": "Философия Гегеля описывает прогресс как <w>синтез</w>, возникающий из конфликта."
                            }
                        },
                        {
                            "text": "The final agreement was a <w>synthesis</w> of the two opposing arguments.",
                            "target_lang_translations": {
                                "ru": "Окончательное соглашение стало <w>синтезом</w> двух противоположных аргументов."
                            }
                        }
                    ],
                    "synonyms": [
                        "resolution",
                        "reconciliation"
                    ],
                    "common_phrases": [
                        "thesis antithesis synthesis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединение тезиса и антитезиса для формирования высшей истины или разрешения противоречия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "си́нтез",
                                "target_lang_sense_clarification": "философия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0926563-8299-3717-92e1-63d02d049f94",

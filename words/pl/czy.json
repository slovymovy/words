{
    "word_family": [],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "79bd1287-6eab-39df-ae03-8e70a58e6f32",
                    "sense_definition": "Słowo rozpoczynające pytanie, na które oczekuje się odpowiedzi „tak” lub „nie”.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Czy</w> masz dzisiaj czas na spotkanie?",
                            "target_lang_translations": {
                                "ru": "Есть <w>ли</w> у тебя сегодня время на встречу?"
                            }
                        },
                        {
                            "text": "<w>Czy</w> to prawda, że wyjeżdżasz za granicę?",
                            "target_lang_translations": {
                                "ru": "Правда <w>ли</w>, что ты уезжаешь за границу?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "czy to prawda",
                        "czy można",
                        "czy warto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительная частица, используемая в начале предложения для формирования общего вопроса (часто не переводится, а передается интонацией или частицей «ли»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "частица, используемая для формирования вопроса (обычно стоит после слова, к которому относится)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4dbcc92-a73e-397b-80b1-f0a180887858",
                    "sense_definition": "Spójnik łączący alternatywne, wykluczające się elementy lub zdania, najczęściej używany w pytaniach.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "disjunctive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wolisz napić się kawy <w>czy</w> herbaty?",
                            "target_lang_translations": {
                                "ru": "Ты предпочитаешь выпить кофе <w>или</w> чаю?"
                            }
                        },
                        {
                            "text": "Nie wiem, czy to było wczoraj <w>czy</w> przedwczoraj.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, было ли это вчера <w>или</w> позавчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "albo",
                        "lub",
                        "bądź"
                    ],
                    "antonyms": [
                        "ani"
                    ],
                    "common_phrases": [
                        "tak czy nie",
                        "dziś czy jutro",
                        "raz czy dwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий альтернативные, взаимоисключающие элементы или предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "или",
                                "target_lang_sense_clarification": "союз, указывающий на выбор между вариантами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56485efb-faf6-3f9e-bdf2-05b4d53e5942",
                    "sense_definition": "Spójnik wprowadzający zdanie podrzędne, wyrażający wątpliwość, niepewność lub pytanie zależne.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zastanawiam się, <w>czy</w> zdążymy na ostatni autobus.",
                            "target_lang_translations": {
                                "ru": "Я думаю, успеем <w>ли</w> мы на последний автобус."
                            }
                        },
                        {
                            "text": "Ona zapytała, <w>czy</w> chcę pójść z nią do kina.",
                            "target_lang_translations": {
                                "ru": "Она спросила, хочу <w>ли</w> я пойти с ней в кино."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie wiem czy",
                        "pytanie czy",
                        "zobaczymy czy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, вводящий придаточное предложение и выражающий сомнение или косвенный вопрос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "частица в значении союза, используемая в косвенных вопросах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
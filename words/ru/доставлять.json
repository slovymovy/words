    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d68ed978-a4b6-36fc-a708-3d5322d883d2",
                    "sense_definition": "Привозить или приносить товары, почту или грузы к месту назначения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Курьер обещал <w>доставить</w> наш заказ в течение часа.",
                            "target_lang_translations": {
                                "en": "The courier promised to <w>deliver</w> our order within an hour."
                            }
                        },
                        {
                            "text": "Почтальон каждое утро <w>доставляет</w> свежие газеты и письма.",
                            "target_lang_translations": {
                                "en": "The postman <w>delivers</w> fresh newspapers and letters every morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "прикалывать",
                        "прикалывать"
                    ],
                    "common_phrases": [
                        "доставлять почту",
                        "доставлять груз",
                        "доставлять заказ",
                        "доставлять на дом"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring or carry goods, mail, or cargo to a specific destination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "goods, mail, orders"
                            },
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "general transport to a place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd5c31ff-0ae8-3adc-861e-35cec39b8903",
                    "sense_definition": "Вызывать у кого-либо определённые чувства, ощущения или состояния (радость, боль, беспокойство).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прогулка по вечернему городу <w>доставляет</w> мне огромное удовольствие.",
                            "target_lang_translations": {
                                "en": "A walk through the evening city <w>gives</w> me great pleasure."
                            }
                        },
                        {
                            "text": "Я не хотел <w>доставлять</w> вам лишние хлопоты своей просьбой.",
                            "target_lang_translations": {
                                "en": "I didn't want to <w>cause</w> you unnecessary trouble with my request."
                            }
                        }
                    ],
                    "synonyms": [
                        "причинять",
                        "вызывать",
                        "давать"
                    ],
                    "common_phrases": [
                        "доставлять удовольствие",
                        "доставлять неудобства",
                        "доставлять радость",
                        "доставлять хлопоты",
                        "доставлять боль"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause someone to experience specific feelings, sensations, or states (such as joy, pain, or trouble)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "pleasure, joy"
                            },
                            {
                                "target_lang_word": "cause",
                                "target_lang_sense_clarification": "trouble, pain, inconvenience"
                            },
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "happiness, satisfaction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2a8dacc-2021-3bb5-9279-9034e0cc8508",
                    "sense_definition": "Приводить, сопровождать или транспортировать людей в определённое место (часто официально или вынужденно).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скорая помощь должна срочно <w>доставить</w> пострадавшего в больницу.",
                            "target_lang_translations": {
                                "en": "The ambulance must urgently <w>take</w> the victim to the hospital."
                            }
                        },
                        {
                            "text": "Полиция обязана <w>доставлять</w> нарушителей порядка в участок для выяснения обстоятельств.",
                            "target_lang_translations": {
                                "en": "The police are required to <w>bring</w> public order violators to the station to clarify the circumstances."
                            }
                        }
                    ],
                    "common_phrases": [
                        "доставлять в больницу",
                        "доставлять в отделение",
                        "доставлять домой"
                    ],
                    "target_lang_definitions": {
                        "en": "To lead, accompany, or transport people to a specific place (often officially, by force, or out of necessity)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "someone to a hospital, station, etc."
                            },
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "formal conveyance of people"
                            },
                            {
                                "target_lang_word": "convey",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb7e91bb-82e9-36d3-88ee-290024e9834e",
                    "sense_definition": "Предоставлять или обеспечивать наличие чего-либо (возможности, случая).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот проект <w>доставляет</w> нам возможность проявить свои творческие способности.",
                            "target_lang_translations": {
                                "en": "This project <w>provides</w> us with the opportunity to demonstrate our creative abilities."
                            }
                        },
                        {
                            "text": "Судьба редко <w>доставляет</w> такой удобный случай для знакомства.",
                            "target_lang_translations": {
                                "en": "Fate rarely <w>affords</w> such a convenient chance for an introduction."
                            }
                        }
                    ],
                    "synonyms": [
                        "предоставлять",
                        "обеспечивать",
                        "давать"
                    ],
                    "common_phrases": [
                        "доставлять возможность",
                        "доставлять случай"
                    ],
                    "target_lang_definitions": {

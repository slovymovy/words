    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "498fb073-d5aa-3067-b231-2e6df35428c6",
                    "sense_definition": "Немного, в незначительной степени; чуть-чуть.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>слегка</w> устал после долгой дороги домой.",
                            "target_lang_translations": {
                                "en": "I was <w>slightly</w> tired after the long journey home."
                            }
                        },
                        {
                            "text": "Дверь в комнату была <w>слегка</w> приоткрыта.",
                            "target_lang_translations": {
                                "en": "The door to the room was <w>slightly</w> ajar."
                            }
                        }
                    ],
                    "synonyms": [
                        "немного",
                        "чуть-чуть"
                    ],
                    "antonyms": [
                        "сильно"
                    ],
                    "common_phrases": [
                        "слегка улыбнуться",
                        "слегка кивнуть",
                        "слегка изменить",
                        "слегка удивиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To a small extent or degree; a little."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slightly",
                                "target_lang_sense_clarification": "general degree"
                            },
                            {
                                "target_lang_word": "a bit",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "a little",
                                "target_lang_sense_clarification": "amount or degree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63cfa194-ecb0-3267-8250-f4e394c41059",
                    "sense_definition": "Не сильно, без большого усилия или нажима (о физическом действии).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>слегка</w> коснулся её руки, чтобы привлечь внимание.",
                            "target_lang_translations": {
                                "en": "He <w>lightly</w> touched her hand to get her attention."
                            }
                        },
                        {
                            "text": "Ветер <w>слегка</w> покачивал ветки деревьев в саду.",
                            "target_lang_translations": {
                                "en": "The wind was <w>gently</w> rocking the tree branches in the garden."
                            }
                        }
                    ],
                    "synonyms": [
                        "легонько"
                    ],
                    "antonyms": [
                        "сильно",
                        "с силой"
                    ],
                    "common_phrases": [
                        "слегка ударить",
                        "слегка толкнуть",
                        "слегка коснуться",
                        "слегка нажать"
                    ],
                    "target_lang_definitions": {
                        "en": "Not strongly; without much force or pressure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lightly",
                                "target_lang_sense_clarification": "physical action"
                            },
                            {
                                "target_lang_word": "gently",
                                "target_lang_sense_clarification": "soft action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21b56004-8de0-3b65-9364-42d67e4507a9",
                    "sense_definition": "Попутно, вскользь, не углубляясь в детали.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В разговоре он лишь <w>слегка</w> упомянул о своих планах.",
                            "target_lang_translations": {
                                "en": "In the conversation, he only <w>briefly</w> mentioned his plans."
                            }
                        },
                        {
                            "text": "Мы знакомы с этой темой лишь <w>слегка</w>.",
                            "target_lang_translations": {
                                "en": "We are only <w>superficially</w> familiar with this topic."
                            }
                        }
                    ],
                    "synonyms": [
                        "вскользь"
                    ],
                    "common_phrases": [
                        "слегка упомянуть",
                        "слегка коснуться темы",
                        "знать слегка"
                    ],
                    "target_lang_definitions": {
                        "en": "In passing or superficially; without going into details."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "briefly",
                                "target_lang_sense_clarification": "duration or speech"
                            },
                            {
                                "target_lang_word": "superficially",
                                "target_lang_sense_clarification": "depth of knowledge"
                            },
                            {
                                "target_lang_word": "lightly",
                                "target_lang_sense_clarification": "figurative touch"
                            }
                        ]
                    }
                }
            ]
        }
    ]

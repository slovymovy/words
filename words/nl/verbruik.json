    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "67c6964b-e3e7-31e6-a083-8eefd7fbfa9b",
                    "sense_definition": "De totale hoeveelheid van iets (zoals energie, water of goederen) die is opgemaakt of gebruikt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vorige maand had ik een <w>verbruik</w> van 126 kWu elektriciteit.",
                            "target_lang_translations": {
                                "ru": "В прошлом месяце мое <w>потребление</w> электроэнергии составило 126 кВт⋅ч."
                            }
                        },
                        {
                            "text": "Het <w>verbruik</w> van water is tijdens de warme zomermaanden sterk toegenomen.",
                            "target_lang_translations": {
                                "ru": "<w>Расход</w> воды сильно вырос в теплые летние месяцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "consumptie"
                    ],
                    "antonyms": [
                        "productie",
                        "besparing"
                    ],
                    "common_phrases": [
                        "gemiddeld verbruik",
                        "dagelijks verbruik",
                        "hoog verbruik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее количество чего-либо (например, энергии, воды или товаров), которое было израсходовано или использовано."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потребление",
                                "target_lang_sense_clarification": "использование ресурсов"
                            },
                            {
                                "target_lang_word": "расход",
                                "target_lang_sense_clarification": "затраченное количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baba0653-afd2-3c81-95f8-1028ea76fed1",
                    "sense_definition": "De snelheid of mate waarin een apparaat of voertuig brandstof of energie nodig heeft om te werken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat is het <w>verbruik</w> van jouw auto op de snelweg?",
                            "target_lang_translations": {
                                "ru": "Какой <w>расход</w> топлива у твоей машины на трассе?"
                            }
                        },
                        {
                            "text": "Deze nieuwe koelkast heeft een heel laag <w>verbruik</w> en is dus goedkoper in gebruik.",
                            "target_lang_translations": {
                                "ru": "У этого нового холодильника очень низкое <w>потребление</w> энергии, поэтому он дешевле в эксплуатации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zuinig in verbruik",
                        "specifiek verbruik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скорость или степень, с которой устройство или транспортное средство расходует топливо или энергию для работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расход",
                                "target_lang_sense_clarification": "топлива или энергии"
                            },
                            {
                                "target_lang_word": "потребление",
                                "target_lang_sense_clarification": "энергопотребление"
                            }
                        ]
                    }
                }
            ]
        },
        {

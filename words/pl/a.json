{
    "word_family": [],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "3efbdaba-3d6b-3202-b41e-08987512683c",
                    "sense_definition": "Spójnik łączący zdania lub części zdania o znaczeniu przeciwstawnym; wskazuje na różnicę, kontrast lub sprzeczność między elementami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "conj_contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat lubi kawę, <w>a</w> ja wolę herbatę.",
                            "target_lang_translations": {
                                "ru": "Мой брат любит кофе, <w>а</w> я предпочитаю чай."
                            }
                        },
                        {
                            "text": "Chcieliśmy pójść na spacer, <w>a</w> zaczął padać deszcz.",
                            "target_lang_translations": {
                                "ru": "Мы хотели пойти на прогулку, <w>но</w> пошел дождь."
                            }
                        },
                        {
                            "text": "To nie jest czarne, <w>a</w> białe.",
                            "target_lang_translations": {
                                "ru": "Это не черное, <w>а</w> белое."
                            }
                        }
                    ],
                    "synonyms": [
                        "ale",
                        "natomiast",
                        "zaś"
                    ],
                    "common_phrases": [
                        "tak a nie inaczej",
                        "mały a cieszy",
                        "wolno a dokładnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий предложения или части предложения с противоположным значением; указывает на различие, контраст или противоречие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "противопоставление"
                            },
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "противопоставление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ab67458-94e5-3e3d-a16a-943013970319",
                    "sense_definition": "Spójnik łączący elementy, które występują razem, uzupełniają się lub następują po sobie (często używany w stałych wyrażeniach).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "conj_add",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pomiędzy kuchnią <w>a</w> salonem stoi duży stół.",
                            "target_lang_translations": {
                                "ru": "Между кухней <w>и</w> гостиной стоит большой стол."
                            }
                        },
                        {
                            "text": "W tym miejscu panuje cisza <w>a</w> spokój.",
                            "target_lang_translations": {
                                "ru": "В этом месте царит тишина <w>и</w> покой."
                            }
                        }
                    ],
                    "synonyms": [
                        "i",
                        "oraz"
                    ],
                    "common_phrases": [
                        "między młotem a kowadłem",
                        "zima a lato",
                        "między bogiem a prawdą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий элементы, которые выступают вместе или дополняют друг друга (часто используется в устойчивых выражениях, например, с предлогом «между»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "соединительный союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f8449f5-28a8-3550-83dc-f8a7d92ed0bc",
                    "sense_definition": "Spójnik używany na początku pytania lub zdania, służący do zmiany tematu lub zwrócenia się do innej osoby w nawiązaniu do poprzedniej wypowiedzi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "conj_shift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ja już zjadłem obiad, <w>a</w> ty jesteś głodny?",
                            "target_lang_translations": {
                                "ru": "Я уже пообедал, <w>а</w> ты голоден?"
                            }
                        },
                        {
                            "text": "Wszystko jest gotowe, <w>a</w> co z biletami?",
                            "target_lang_translations": {
                                "ru": "Всё готово, <w>а</w> что с билетами?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "a ty",
                        "a co z tobą",
                        "a jakże"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, используемый в начале вопроса или предложения для смены темы или обращения к собеседнику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "смена темы или вопрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f43f2b7d-858c-3539-b8b1-031f35dd901c",
                    "sense_definition": "Spójnik wprowadzający zdanie wynikowe lub warunkowe; sugeruje, że jedna czynność jest bezpośrednim skutkiem drugiej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conj_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szukajcie wytrwale, <w>a</w> na pewno znajdziecie rozwiązanie.",
                            "target_lang_translations": {
                                "ru": "Ищите упорно, <w>и</w> наверняка найдете решение."
                            }
                        },
                        {
                            "text": "Spróbuj go dotknąć, <w>a</w> zobaczysz, co się stanie.",
                            "target_lang_translations": {
                                "ru": "Попробуй до него дотронуться, <w>и</w> увидишь, что произойдет."
                            }
                        }
                    ],
                    "synonyms": [
                        "więc"
                    ],
                    "common_phrases": [
                        "mów a masz",
                        "szukajcie a znajdziecie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, вводящий следствие или условное предложение; указывает на то, что одно действие является непосредственным результатом другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "следствие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "58f7a144-fab0-3782-bc61-195791586e73",
                    "sense_definition": "Słowo wzmacniające wypowiedź, często wyrażające emocje takie jak zniecierpliwienie, zdziwienie lub przypomnienie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>A</w> nie mówiłem, że dzisiaj będzie padać?",
                            "target_lang_translations": {
                                "ru": "<w>А</w> я ведь говорил, что сегодня будет дождь?"
                            }
                        },
                        {
                            "text": "<w>A</w> daj ty mi wreszcie święty spokój!",
                            "target_lang_translations": {
                                "ru": "<w>А</w> оставь ты меня наконец в покое!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "a nie mówiłem",
                        "a idźże",
                        "a daj spokój"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, усиливающее высказывание, часто выражающее эмоции, такие как нетерпение, удивление или напоминание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "усилительная частица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d8eac472-a189-330a-ae52-18b1f1bd7367",
                    "sense_definition": "Dźwięk wyrażający nagłe przypomnienie sobie czegoś, zrozumienie sytuacji lub zaskoczenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>A</w>, teraz już rozumiem, o co ci chodziło.",
                            "target_lang_translations": {
                                "ru": "<w>А</w>, теперь я понимаю, что ты имел в виду."
                            }
                        },
                        {
                            "text": "<w>A</w>! Zapomniałem kupić chleb na kolację.",
                            "target_lang_translations": {
                                "ru": "<w>А</w>! Я забыл купить хлеб на ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "o!"
                    ],
                    "common_phrases": [
                        "a to ciekawe",
                        "a jednak",
                        "a to pech"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, выражающий внезапное воспоминание, понимание ситуации или удивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "междометие"
                            },
                            {
                                "target_lang_word": "ах",
                                "target_lang_sense_clarification": "междометие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "70b8b148-c91e-35c1-bc44-1f27daf606cf",
                    "sense_definition": "Pierwsza litera polskiego alfabetu; samogłoska.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_letter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To słowo zaczyna się na małą literę <w>a</w>.",
                            "target_lang_translations": {
                                "ru": "Это слово начинается с маленькой буквы <w>а</w>."
                            }
                        },
                        {
                            "text": "Dzieci w szkole uczą się pisać <w>a</w>.",
                            "target_lang_translations": {
                                "ru": "Дети в школе учатся писать <w>а</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "od a do zet",
                        "powiedzieć a"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первая буква польского алфавита; гласный звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "буква"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6f6c8f1-5a6c-3daf-b4bf-c55212b54e2e",
                    "sense_definition": "Nazwa dźwięku muzycznego; szósty stopień gamy C-dur.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skrzypek nastroił swój instrument do dźwięku <w>a</w>.",
                            "target_lang_translations": {
                                "ru": "Скрипач настроил свой инструмент на звук <w>ля</w>."
                            }
                        },
                        {
                            "text": "Ten utwór muzyczny jest napisany w tonacji <w>a</w>.",
                            "target_lang_translations": {
                                "ru": "Это музыкальное произведение написано в тональности <w>ля</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tonacja a-moll",
                        "dźwięk a"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "music terminology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название музыкального звука; шестая ступень гаммы до мажор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ля",
                                "target_lang_sense_clarification": "нота"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
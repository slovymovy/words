    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "410e62a8-7fd8-3859-9588-517ee167e214",
                    "sense_definition": "Een vrouwelijke godheid.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "deity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Venus is de Romeinse <w>godin</w> van de liefde.",
                            "target_lang_translations": {
                                "en": "Venus is the Roman <w>goddess</w> of love.",
                                "ru": "Венера — римская <w>богиня</w> любви."
                            }
                        },
                        {
                            "text": "In de oudheid bouwden mensen tempels voor elke belangrijke <w>godin</w>.",
                            "target_lang_translations": {
                                "en": "In ancient times, people built temples for every important <w>goddess</w>.",
                                "ru": "В древности люди строили храмы для каждой важной <w>богини</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "god"
                    ],
                    "common_phrases": [
                        "godin van de jacht",
                        "godin van de liefde",
                        "Griekse godin"
                    ],
                    "target_lang_definitions": {
                        "en": "A female deity.",
                        "ru": "Женское божество."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "goddess",
                                "target_lang_sense_clarification": "female deity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "богиня",
                                "target_lang_sense_clarification": "женское божество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa415893-aab8-3d32-a41f-cc4731b65ddb",
                    "sense_definition": "Een vrouw die men buitengewoon bewondert of aanbidt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij behandelt zijn vrouw alsof ze een <w>godin</w> is.",
                            "target_lang_translations": {
                                "en": "He treats his wife as if she were a <w>goddess</w>.",
                                "ru": "Он относится к жене так, будто она <w>богиня</w>."
                            }
                        },
                        {
                            "text": "Op het podium zag de zangeres eruit als een ware <w>godin</w>.",
                            "target_lang_translations": {
                                "en": "On stage, the singer looked like a true <w>goddess</w>.",
                                "ru": "На сцене певица выглядела как истинная <w>богиня</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who is extraordinarily admired or adored.",
                        "ru": "Женщина, которой чрезвычайно восхищаются или которую боготворят."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "goddess",
                                "target_lang_sense_clarification": "figurative: adored woman"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "богиня",
                                "target_lang_sense_clarification": "предмет обожания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d616ab83-efa4-3db1-8371-cae8d2903a7b",
                    "sense_definition": "Naam voor het vrouwelijke opperwezen in bepaalde religies, zoals wicca.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "deity_name",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "In wicca wordt de <w>Godin</w> vaak vereerd naast de Gehoornde God.",
                            "target_lang_translations": {
                                "en": "In Wicca, the <w>Goddess</w> is often worshipped alongside the Horned God.",
                                "ru": "В викке <w>Богиня</w> часто почитается наряду с Рогатым Богом."
                            }
                        },
                        {
                            "text": "De cyclus van de maan wordt vaak geassocieerd met de <w>Godin</w>.",
                            "target_lang_translations": {
                                "en": "The cycle of the moon is often associated with the <w>Goddess</w>.",
                                "ru": "Лунный цикл часто ассоциируется с <w>Богиней</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Moedergodin"
                    ],
                    "target_lang_definitions": {
                        "en": "Name for the female supreme being in certain religions, such as Wicca.",
                        "ru": "Имя женского верховного божества в некоторых религиях, например, в викке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Goddess",
                                "target_lang_sense_clarification": "supreme being"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Богиня",
                                "target_lang_sense_clarification": "верховное существо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человек-бутерброд",
                                "target_lang_sense_clarification": "носитель рекламы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d25454f6-7c98-36c9-afe7-e71edf9469a3",
                    "sense_definition": "To place (an item) physically between two other, usually flat, items.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was <w>sandwiched</w> between two large passengers on the crowded bus.",
                            "target_lang_translations": {
                                "ru": "Я был <w>зажат</w> между двумя крупными пассажирами в переполненном автобусе."
                            }
                        },
                        {
                            "text": "The old photograph was <w>sandwiched</w> between the pages of the book.",
                            "target_lang_translations": {
                                "ru": "Старая фотография была <w>зажата</w> между страницами книги."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "174cc36d-071e-3fb0-b126-a9ca668d737c",
                    "sense_definition": "Тот, кто слушает кого-либо или что-либо, воспринимает речь или звуки.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг — очень внимательный <w>слушатель</w>, он никогда не перебивает.",
                            "target_lang_translations": {
                                "en": "My friend is a very attentive <w>listener</w>, he never interrupts."
                            }
                        },
                        {
                            "text": "Мне нужен хотя бы один <w>слушатель</w>, чтобы отрепетировать речь.",
                            "target_lang_translations": {
                                "en": "I need at least one <w>listener</w> to rehearse the speech."
                            }
                        }
                    ],
                    "antonyms": [
                        "рассказчик",
                        "говорящий"
                    ],
                    "common_phrases": [
                        "внимательный слушатель",
                        "благодарный слушатель",
                        "хороший слушатель"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who listens to a person or sound; a person who pays attention to what is being said."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listener",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "hearer",
                                "target_lang_sense_clarification": "one who perceives sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8f4981b-7241-31df-98c2-ebcce21163c1",
                    "sense_definition": "Человек, который слушает радиопередачи, подкасты, лекции или публичные выступления (часто используется во множественном числе для обозначения аудитории).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дорогие <w>слушатели</w>, наша передача подошла к концу.",
                            "target_lang_translations": {
                                "en": "Dear <w>listeners</w>, our program has come to an end."
                            }
                        },
                        {
                            "text": "У этой радиостанции миллионы постоянных <w>слушателей</w> по всей стране.",
                            "target_lang_translations": {
                                "en": "This radio station has millions of regular <w>listeners</w> across the country."
                            }
                        }
                    ],
                    "synonyms": [
                        "аудитория"
                    ],
                    "antonyms": [
                        "ведущий",
                        "лектор",
                        "выступающий"
                    ],
                    "common_phrases": [
                        "уважаемые слушатели",
                        "постоянный слушатель",
                        "слушатели радио"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who listens to radio broadcasts, podcasts, lectures, or public speeches."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listener",
                                "target_lang_sense_clarification": "of radio, podcasts, etc."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6d65e31-43ff-3517-8b31-c2ac20e5a460",
                    "sense_definition": "Учащийся курсов повышения квалификации или высшего военного учебного заведения.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый <w>слушатель</w> академии обязан сдать экзамен в конце семестра.",
                            "target_lang_translations": {
                                "en": "Every <w>student</w> of the academy is required to pass an exam at the end of the semester."
                            }
                        },
                        {
                            "text": "На конференцию пригласили <w>слушателей</w> курсов иностранных языков.",
                            "target_lang_translations": {
                                "en": "<w>Students</w> of foreign language courses were invited to the conference."
                            }
                        }
                    ],
                    "synonyms": [
                        "учащийся",
                        "курсант"
                    ],
                    "antonyms": [
                        "преподаватель"
                    ],
                    "common_phrases": [
                        "слушатель курсов",
                        "слушатель академии",
                        "вольный слушатель"
                    ],
                    "target_lang_definitions": {
                        "en": "A student enrolled in professional training courses or a higher military educational institution."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "student",
                                "target_lang_sense_clarification": "of an academy or course"
                            },
                            {
                                "target_lang_word": "attendee",
                                "target_lang_sense_clarification": "participant of a course"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "59c40dd1-a82c-3e01-b96a-e526165cd81d",
                    "sense_definition": "Dzień 24 grudnia, poprzedzający święto Bożego Narodzenia, obchodzony uroczyście w tradycji chrześcijańskiej",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "christmas_context",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "W <w>Wigilię</w> cała nasza rodzina dzieli się opłatkiem i składa sobie życzenia.",
                            "target_lang_translations": {
                                "ru": "В <w>сочельник</w> вся наша семья делит облатку и поздравляет друг друга."
                            }
                        },
                        {
                            "text": "Dzieci nie mogą się doczekać, kiedy w <w>Wigilię</w> znajdą prezenty pod choinką.",
                            "target_lang_translations": {
                                "ru": "Дети не могут дождаться, когда в <w>сочельник</w> найдут подарки под ёлкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "Gwiazdka",
                        "Wilia"
                    ],
                    "common_phrases": [
                        "spędzać Wigilię",
                        "w samą Wigilię",
                        "pasterka w Wigilię"
                    ],
                    "target_lang_definitions": {
                        "ru": "День 24 декабря, предшествующий празднику Рождества, торжественно отмечаемый в христианской традиции"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочельник",
                                "target_lang_sense_clarification": "праздник"
                            },
                            {
                                "target_lang_word": "канун Рождества",
                                "target_lang_sense_clarification": "день перед Рождеством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0aea2ce-8899-306d-bb36-7160aa48342d",
                    "sense_definition": "Imię żeńskie pochodzenia łacińskiego",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja ciocia <w>Wigilia</w> obchodzi swoje imieniny w grudniu.",
                            "target_lang_translations": {
                                "ru": "Моя тетя <w>Вигилия</w> празднует свои именины в декабре."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie spotkałem kobiety o imieniu <w>Wigilia</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не встречал женщину по имени <w>Вигилия</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя латинского происхождения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вигилия",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "0f77a786-5411-34a1-a98f-72947769e2a3",
                    "sense_definition": "Pozioma powierzchnia w budynku, po której chodzą ludzie, wykonana z drewna, kamienia, płytek lub innych materiałów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelnerka upuściła na <w>podłogę</w> tacę z przystawkami.",
                            "target_lang_translations": {
                                "ru": "Официантка уронила на <w>пол</w> поднос с закусками."
                            }
                        },
                        {
                            "text": "Muszę dzisiaj dokładnie umyć brudną <w>podłogę</w> w kuchni.",
                            "target_lang_translations": {
                                "ru": "Сегодня я должен тщательно вымыть грязный <w>пол</w> на кухне."
                            }
                        }
                    ],
                    "antonyms": [
                        "sufit"
                    ],
                    "common_phrases": [

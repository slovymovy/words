    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7fff8f09-63ef-331f-8c14-7477ba20beab",
                    "sense_definition": "Wypowiedź pozbawiona sensu, logiki lub wartości merytorycznej; treść trudna do zrozumienia ze względu na zawiłość lub brak konkretów.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "nonsense_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten artykuł to zwykły pseudonaukowy <w>bełkot</w>, z którego nic konkretnego nie wynika.",
                            "target_lang_translations": {
                                "ru": "Эта статья — обычный псевдонаучный <w>бред</w>, из которого не следует ничего конкретного."
                            }
                        },
                        {
                            "text": "Nie mam zamiaru dłużej słuchać tego politycznego <w>bełkotu</w> w telewizji.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь больше слушать этот политический <w>бред</w> по телевизору."
                            }
                        }
                    ],
                    "synonyms": [
                        "bzdura",
                        "nonsens",
                        "kit"
                    ],
                    "common_phrases": [
                        "pseudonaukowy bełkot",
                        "polityczny bełkot",
                        "marketingowy bełkot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Речь или текст, лишенные смысла, логики или ценности; бессмыслица, которую трудно понять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бред",
                                "target_lang_sense_clarification": "бессмысленные высказывания"
                            },
                            {
                                "target_lang_word": "бессмыслица",
                                "target_lang_sense_clarification": "отсутствие смысла"
                            },
                            {
                                "target_lang_word": "чепуха",
                                "target_lang_sense_clarification": "глупость, вздор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c19bdb5-a65d-374d-81e7-c075b84eb257",
                    "sense_definition": "Niewyraźna, niezrozumiała mowa, często wynikająca z choroby, zmęczenia lub upojenia alkoholowego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unclear_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z ust rannego wydobywał się jedynie cichy i niezrozumiały <w>bełkot</w>.",
                            "target_lang_translations": {
                                "ru": "С губ раненого срывалось лишь тихое и непонятное <w>бормотание</w>."
                            }
                        },
                        {
                            "text": "Jego pijacki <w>bełkot</w> sprawił, że nikt nie wiedział, o co mu chodzi.",
                            "target_lang_translations": {
                                "ru": "Из-за его пьяного <w>бормотания</w> никто не понимал, чего он хочет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pijacki bełkot",
                        "niezrozumiały bełkot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неразборчивая, непонятная речь, часто возникающая из-за болезни, усталости или алкогольного опьянения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бормотание",
                                "target_lang_sense_clarification": "тихая, неразборчивая речь"
                            },
                            {
                                "target_lang_word": "лепет",
                                "target_lang_sense_clarification": "бессвязная речь"
                            },
                            {
                                "target_lang_word": "невнятная речь",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6feeefb4-a3c4-3952-9cc5-29aa18a1f99d",
                    "sense_definition": "Cichy, jednostajny odgłos płynącej cieczy, zazwyczaj wody w strumieniu lub rzece.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "water_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uspokajał mnie monotonny <w>bełkot</w> górskiego strumienia płynącego za oknem.",
                            "target_lang_translations": {
                                "ru": "Меня успокаивало монотонное <w>журчание</w> горного ручья, текущего за окном."
                            }
                        },
                        {
                            "text": "W nocnej ciszy słychać było wyraźny <w>bełkot</w> wody uderzającej o kamienie.",
                            "target_lang_translations": {
                                "ru": "В ночной тишине было слышно отчетливое <w>журчание</w> воды, ударяющейся о камни."
                            }
                        }
                    ],
                    "synonyms": [
                        "bulgot"
                    ],
                    "common_phrases": [
                        "bełkot strumienia",
                        "bełkot wody"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or dated usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тихий, монотонный звук текущей воды, обычно в ручье или реке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журчание",
                                "target_lang_sense_clarification": "звук текущей воды"
                            }
                        ]
                    }
                }
            ]
        },

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бяка",
                                "target_lang_sense_clarification": "существительное, обозначающее что-то плохое (детское)"
                            },
                            {
                                "target_lang_word": "кака",
                                "target_lang_sense_clarification": "о грязи или чем-то противном (детское)"
                            },
                            {
                                "target_lang_word": "фу",
                                "target_lang_sense_clarification": "оценка предмета как неприятного"
                            }
                        ]
                    }
                }
            ]
        }
    ]

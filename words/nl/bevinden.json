{
    "word_family": [
        "bevinding",
        "welbevinden"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6c0ec598-c4ef-3837-a9c2-13a07d6a0ccb",
                    "sense_definition": "Op een bepaalde plaats aanwezig zijn of gesitueerd zijn (wederkerend: zich bevinden).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "locatie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kantoor <w>bevindt</w> zich in het centrum van de stad.",
                            "target_lang_translations": {
                                "en": "The office <w>is located</w> in the city center.",
                                "ru": "Офис <w>находится</w> в центре города."
                            }
                        },
                        {
                            "text": "Wij wisten niet precies waar we ons op de kaart <w>bevonden</w>.",
                            "target_lang_translations": {
                                "en": "We didn't know exactly where we <w>were located</w> on the map.",
                                "ru": "Мы точно не знали, где <w>находимся</w> на карте."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijn",
                        "liggen",
                        "staan",
                        "zich ophouden"
                    ],
                    "common_phrases": [
                        "zich ergens bevinden",
                        "zich thuis bevinden"
                    ],
                    "target_lang_definitions": {
                        "en": "To be situated or present in a specific place.",
                        "ru": "Находиться или располагаться в определенном месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be located",
                                "target_lang_sense_clarification": "regarding physical position"
                            },
                            {
                                "target_lang_word": "be situated",
                                "target_lang_sense_clarification": "regarding physical position"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "general presence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "быть в каком-либо месте"
                            },
                            {
                                "target_lang_word": "располагаться",
                                "target_lang_sense_clarification": "занимать место (о зданиях, организациях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "faf7ff56-168d-39d8-ab15-439ce128c775",
                    "sense_definition": "In een bepaalde toestand, omstandigheid of situatie verkeren (wederkerend: zich bevinden).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "toestand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De patiënt <w>bevindt</w> zich momenteel in kritieke toestand.",
                            "target_lang_translations": {
                                "en": "The patient <w>is</w> currently in critical condition.",
                                "ru": "Пациент в данный момент <w>находится</w> в критическом состоянии."
                            }
                        },
                        {
                            "text": "Door de crisis <w>bevonden</w> veel bedrijven zich in zwaar weer.",
                            "target_lang_translations": {
                                "en": "Due to the crisis, many companies <w>found themselves</w> in dire straits.",
                                "ru": "Из-за кризиса многие компании <w>находились</w> в тяжелом положении."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkeren",
                        "zijn"
                    ],
                    "common_phrases": [
                        "zich wel bevinden",
                        "zich in een lastig parket bevinden"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a specific state, condition, or situation.",
                        "ru": "Пребывать в определенном состоянии, обстоятельствах или ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find oneself",
                                "target_lang_sense_clarification": "in a situation or condition"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "state of being"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "пребывать в состоянии"
                            },
                            {
                                "target_lang_word": "пребывать",
                                "target_lang_sense_clarification": "быть в каком-либо состоянии (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9023eee9-f589-3b90-84bc-dab9334c69ed",
                    "sense_definition": "Na onderzoek of waarneming vaststellen of oordelen dat iets of iemand een bepaalde eigenschap heeft.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "oordeel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte werd door de rechtbank schuldig <w>bevonden</w>.",
                            "target_lang_translations": {
                                "en": "The suspect was <w>found</w> guilty by the court.",
                                "ru": "Подозреваемый был <w>признан</w> судом виновным."
                            }
                        },
                        {
                            "text": "Het ingediende voorstel werd na inspectie te duur <w>bevonden</w>.",
                            "target_lang_translations": {
                                "en": "The submitted proposal was <w>found</w> to be too expensive after inspection.",
                                "ru": "Представленное предложение после проверки было <w>сочтено</w> слишком дорогим."
                            }
                        }
                    ],
                    "synonyms": [
                        "achten",
                        "oordelen",
                        "constateren",
                        "vaststellen"
                    ],
                    "common_phrases": [
                        "schuldig bevonden worden",
                        "te licht bevonden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To determine, judge, or deem something to have a certain quality after investigation.",
                        "ru": "Признать или счесть кого-либо/что-либо обладающим определенным свойством на основе оценки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "to judge or declare"
                            },
                            {
                                "target_lang_word": "deem",
                                "target_lang_sense_clarification": "formal judgment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "признавать",
                                "target_lang_sense_clarification": "выносить суждение (напр. виновным)"
                            },
                            {
                                "target_lang_word": "счесть",
                                "target_lang_sense_clarification": "посчитать каким-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

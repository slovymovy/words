{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "584c9320-bc19-30c7-a489-af8701a39db0",
                    "sense_definition": "Een informele begroeting om iemand hallo te zeggen bij een ontmoeting.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hoi</w>, hoe gaat het met jou vandaag?",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>, как у тебя сегодня дела?"
                            }
                        },
                        {
                            "text": "Toen ik de kamer binnenkwam, zei ik meteen <w>hoi</w> tegen iedereen.",
                            "target_lang_translations": {
                                "ru": "Когда я вошел в комнату, я сразу сказал всем <w>привет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hallo",
                        "hé"
                    ],
                    "antonyms": [
                        "doei"
                    ],
                    "common_phrases": [
                        "hoi allemaal",
                        "hoi daar"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal situations"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное приветствие, используемое, чтобы поздороваться с кем-то при встрече."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "приветствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db775887-4371-3107-af1d-d88c3e96885a",
                    "sense_definition": "Een informele groet om afscheid te nemen bij het weggaan.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "farewell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was erg gezellig, tot morgen, <w>hoi</w>!",
                            "target_lang_translations": {
                                "ru": "Было очень здорово, до завтра, <w>пока</w>!"
                            }
                        },
                        {
                            "text": "Ik moet nu echt naar huis, <w>hoi</w>.",
                            "target_lang_translations": {
                                "ru": "Мне правда пора домой, <w>пока</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "doei",
                        "dag"
                    ],
                    "common_phrases": [
                        "nou hoi"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal situations"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное прощание, используемое при расставании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "прощание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a712d03-4e18-3496-b1b9-a14ec6c74533",
                    "sense_definition": "Een uitroep van vreugde of enthousiasme, vaak gebruikt bij verjaardagen of vieringen.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hiep hiep <w>hoi</w>, ze is vandaag jarig!",
                            "target_lang_translations": {
                                "ru": "Гип-гип <w>ура</w>, у нее сегодня день рождения!"
                            }
                        },
                        {
                            "text": "We riepen allemaal hard <w>hoi</w> toen we het goede nieuws hoorden.",
                            "target_lang_translations": {
                                "ru": "Мы все громко закричали <w>ура</w>, когда услышали хорошие новости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hiep hiep hoi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас радости или энтузиазма, часто используемый на днях рождения или праздниках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ура",
                                "target_lang_sense_clarification": "восклицание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
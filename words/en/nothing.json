{
    "word_family": [
        "nothingness"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "b78151c0-5ac3-3543-a433-12253df8d833",
                    "sense_definition": "Not any thing; no single thing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "absence_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is <w>nothing</w> in the fridge for dinner.",
                            "target_lang_translations": {
                                "ru": "В холодильнике нет <w>ничего</w> на ужин."
                            }
                        },
                        {
                            "text": "I heard <w>nothing</w> about the changes to the schedule.",
                            "target_lang_translations": {
                                "ru": "Я <w>ничего</w> не слышал об изменениях в расписании."
                            }
                        },
                        {
                            "text": "She said <w>nothing</w> and just walked away.",
                            "target_lang_translations": {
                                "ru": "Она <w>ничего</w> не сказала и просто ушла."
                            }
                        }
                    ],
                    "synonyms": [
                        "not a thing",
                        "nil",
                        "zero",
                        "zilch",
                        "nada",
                        "zip"
                    ],
                    "antonyms": [
                        "something",
                        "anything",
                        "everything"
                    ],
                    "common_phrases": [
                        "nothing else",
                        "nothing more",
                        "nothing special",
                        "nothing at all",
                        "for nothing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ни один предмет; отсутствие какой-либо вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "самая распространенная форма (в отрицательных предложениях)"
                            },
                            {
                                "target_lang_word": "ничто",
                                "target_lang_sense_clarification": "именительный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66551e4f-53f2-38a0-9167-e2896e342777",
                    "sense_definition": "The state of nonexistence; the absence of all matter, space, or content.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "absence_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "According to the theory, the universe was created from <w>nothing</w>.",
                            "target_lang_translations": {
                                "ru": "Согласно теории, вселенная была создана из <w>ничего</w>."
                            }
                        },
                        {
                            "text": "He stared into the dark <w>nothing</w> of the abyss.",
                            "target_lang_translations": {
                                "ru": "Он смотрел в темное <w>ничто</w> бездны."
                            }
                        }
                    ],
                    "synonyms": [
                        "void",
                        "emptiness",
                        "nonexistence",
                        "oblivion"
                    ],
                    "antonyms": [
                        "everything",
                        "something",
                        "anything"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние несуществования; отсутствие материи, пространства или содержания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничто",
                                "target_lang_sense_clarification": "философское понятие, небытие"
                            },
                            {
                                "target_lang_word": "пустота",
                                "target_lang_sense_clarification": "отсутствие всего"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0dd19e14-ba4a-3cb6-99be-cc7932f73cad",
                    "sense_definition": "Not at all; in no way or degree.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adverb_negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She looks <w>nothing</w> like her older sister.",
                            "target_lang_translations": {
                                "ru": "Она <w>совсем не</w> похожа на свою старшую сестру."
                            }
                        },
                        {
                            "text": "The final result was <w>nothing</w> near what we expected.",
                            "target_lang_translations": {
                                "ru": "Конечный результат был <w>совсем не</w> тем, что мы ожидали."
                            }
                        }
                    ],
                    "synonyms": [
                        "not at all",
                        "in no way"
                    ],
                    "common_phrases": [
                        "nothing like",
                        "nothing near",
                        "nothing much"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совсем нет; никоим образом или ни в какой степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совсем не",
                                "target_lang_sense_clarification": "наречие степени"
                            },
                            {
                                "target_lang_word": "ничуть",
                                "target_lang_sense_clarification": "разговорное усиление отрицания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0658fde9-0038-310e-b620-efc05d9bfb01",
                    "sense_definition": "Something of no importance, value, or consequence.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_triviality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't worry about the cost, it is a mere <w>nothing</w>.",
                            "target_lang_translations": {
                                "ru": "Не беспокойся о цене, это сущий <w>пустяк</w>."
                            }
                        },
                        {
                            "text": "They whispered sweet <w>nothings</w> to each other in the park.",
                            "target_lang_translations": {
                                "ru": "Они шептали друг другу <w>нежности</w> в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "trifle",
                        "triviality"
                    ],
                    "common_phrases": [
                        "sweet nothings",
                        "it is nothing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то неважное, не имеющее ценности или последствий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустяк",
                                "target_lang_sense_clarification": "незначительная вещь"
                            },
                            {
                                "target_lang_word": "мелочь",
                                "target_lang_sense_clarification": "что-то несущественное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcf0b2be-66c2-328b-9dde-848c685861fe",
                    "sense_definition": "A person of no significance or importance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt like a <w>nothing</w> in the company of such famous people.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>ничтожеством</w> в компании таких известных людей."
                            }
                        },
                        {
                            "text": "She rose from being a <w>nothing</w> to becoming a star.",
                            "target_lang_translations": {
                                "ru": "Она превратилась из <w>ничтожества</w> в звезду."
                            }
                        }
                    ],
                    "synonyms": [
                        "nobody",
                        "nonentity"
                    ],
                    "antonyms": [
                        "something",
                        "something"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, не имеющий значимости или веса в обществе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничтожество",
                                "target_lang_sense_clarification": "о человеке (пренебрежительно)"
                            },
                            {
                                "target_lang_word": "пустое место",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "8d733a4d-66ed-3a9a-84d7-df2d2aeeb623",
                    "sense_definition": "Used to indicate that something is not important or to dismiss a previous statement.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "interjection_dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What did you say? Oh, <w>nothing</w>.",
                            "target_lang_translations": {
                                "ru": "Что ты сказал? О, <w>ничего</w>."
                            }
                        },
                        {
                            "text": "<w>Nothing</w>, I was just thinking aloud.",
                            "target_lang_translations": {
                                "ru": "<w>Ничего</w>, я просто размышлял вслух."
                            }
                        }
                    ],
                    "synonyms": [
                        "never mind",
                        "forget it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что что-то неважно, или чтобы отмахнуться от предыдущего высказывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "в значении «неважно» или «нормально»"
                            },
                            {
                                "target_lang_word": "пустяки",
                                "target_lang_sense_clarification": "ответ на извинение или благодарность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "df89f4fe-88a8-3616-97bf-fcef3586022e",
                    "sense_definition": "Completely unimportant or insignificant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "adj_unimportant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>nothing</w> game that ended in a draw.",
                            "target_lang_translations": {
                                "ru": "Это была <w>никчемная</w> игра, которая закончилась вничью."
                            }
                        },
                        {
                            "text": "He lost his temper over a <w>nothing</w> comment.",
                            "target_lang_translations": {
                                "ru": "Он вышел из себя из-за <w>пустякового</w> комментария."
                            }
                        }
                    ],
                    "synonyms": [
                        "insignificant",
                        "unimportant",
                        "trivial"
                    ],
                    "antonyms": [
                        "something",
                        "something"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенно неважный или незначительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустяковый",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "никчемный",
                                "target_lang_sense_clarification": "бесполезный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
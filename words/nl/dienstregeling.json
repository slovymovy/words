{
    "word_family": [
        "dienst",
        "regeling",
        "dienstrooster",
        "regelen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b14a623a-a3e3-345b-8209-4dedb9f7642f",
                    "sense_definition": "Een schema met de vertrek- en aankomsttijden van het openbaar vervoer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein vertrekt precies volgens de <w>dienstregeling</w>.",
                            "target_lang_translations": {
                                "ru": "Поезд отправляется точно по <w>расписанию</w>."
                            }
                        },
                        {
                            "text": "Door werkzaamheden aan het spoor is de <w>dienstregeling</w> aangepast.",
                            "target_lang_translations": {
                                "ru": "Из-за ремонтных работ на путях <w>расписание</w> было изменено."
                            }
                        }
                    ],
                    "synonyms": [
                        "tijdschema",
                        "reisschema"
                    ],
                    "common_phrases": [
                        "volgens de dienstregeling",
                        "de nieuwe dienstregeling",
                        "wijziging in de dienstregeling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Схема с временем отправления и прибытия общественного транспорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расписание",
                                "target_lang_sense_clarification": "движения транспорта"
                            },
                            {
                                "target_lang_word": "график движения",
                                "target_lang_sense_clarification": "транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac6dba5b-fbb5-37b6-ad32-ebb71e651f7a",
                    "sense_definition": "Een boekje, bord of app waarin de tijden van het openbaar vervoer staan vermeld.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik keek even op de <w>dienstregeling</w> om te zien wanneer de laatste bus ging.",
                            "target_lang_translations": {
                                "ru": "Я заглянул в <w>расписание</w>, чтобы узнать, когда уходит последний автобус."
                            }
                        },
                        {
                            "text": "Op het station hangt de actuele <w>dienstregeling</w> in een vitrine.",
                            "target_lang_translations": {
                                "ru": "На вокзале в витрине висит актуальное <w>расписание</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoorboekje"
                    ],
                    "common_phrases": [
                        "op de dienstregeling kijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Буклет, табло или приложение, в котором указано время движения общественного транспорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расписание",
                                "target_lang_sense_clarification": "табло, стенд или буклет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b99a05f1-e1d7-3071-b8cd-661aa2fe1a60",
                    "sense_definition": "Een schema waarin staat wanneer werknemers hun diensten moeten draaien.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "work_schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De manager maakt elke maand een nieuwe <w>dienstregeling</w> voor het personeel.",
                            "target_lang_translations": {
                                "ru": "Менеджер каждый месяц составляет новый <w>график работы</w> для персонала."
                            }
                        },
                        {
                            "text": "Volgens de <w>dienstregeling</w> heb ik dit weekend vrij.",
                            "target_lang_translations": {
                                "ru": "Согласно <w>графику</w>, в эти выходные я свободен."
                            }
                        }
                    ],
                    "synonyms": [
                        "dienstrooster",
                        "werkrooster"
                    ],
                    "target_lang_definitions": {
                        "ru": "График, в котором указано, когда сотрудники должны выходить на смены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "график работы",
                                "target_lang_sense_clarification": "смены"
                            },
                            {
                                "target_lang_word": "график дежурств",
                                "target_lang_sense_clarification": "распределение обязанностей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
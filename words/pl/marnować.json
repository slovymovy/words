{
    "word_family": [
        "zmarnować",
        "marnotrawić",
        "marnotrawstwo",
        "marnotrawny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aa3d6676-d3ef-3f13-ba67-6203118fdbb0",
                    "sense_definition": "Bezużytecznie wydawać lub tracić zasoby, takie jak czas, pieniądze czy energia, nie osiągając żadnych korzyści.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "waste_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś <w>marnować</w> swojego cennego czasu na gry komputerowe.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>тратить</w> свое драгоценное время на компьютерные игры."
                            }
                        },
                        {
                            "text": "Staramy się nie <w>marnować</w> wody i zakręcamy kran podczas mycia zębów.",
                            "target_lang_translations": {
                                "ru": "Мы стараемся не <w>тратить</w> воду зря и закрываем кран, когда чистим зубы."
                            }
                        }
                    ],
                    "synonyms": [
                        "marnotrawić",
                        "trwonić"
                    ],
                    "antonyms": [
                        "oszczędzać",
                        "spożytkowywać"
                    ],
                    "common_phrases": [
                        "marnować czas",
                        "marnować pieniądze",
                        "marnować życie",
                        "marnować energię",
                        "nie marnuj wody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бесполезно расходовать ресурсы, такие как время, деньги или энергия, не получая никакой выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тратить",
                                "target_lang_sense_clarification": "расходовать (часто подразумевается, что зря)"
                            },
                            {
                                "target_lang_word": "тратить впустую",
                                "target_lang_sense_clarification": "расходовать без пользы"
                            },
                            {
                                "target_lang_word": "терять",
                                "target_lang_sense_clarification": "упускать (о времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79def06f-40ef-3111-a011-a64723e016f9",
                    "sense_definition": "Ulegać zepsuciu, niszczeć lub tracić na wartości z powodu braku wykorzystania (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "go_to_waste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tyle dobrego jedzenia <w>marnuje</w> się w restauracjach każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Столько хорошей еды <w>пропадает</w> в ресторанах каждый день."
                            }
                        },
                        {
                            "text": "Szkoda, żeby ten piękny dom stał pusty i <w>marnował</w> się przez lata.",
                            "target_lang_translations": {
                                "ru": "Жаль, если этот красивый дом будет стоять пустым и <w>пропадать</w> годами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jedzenie się marnuje",
                        "nic się nie zmarnuje",
                        "talent się marnuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Портиться, приходить в негодность или терять ценность из-за неиспользования (о вещах, еде, таланте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропадать",
                                "target_lang_sense_clarification": "оставаться неиспользованным, исчезать зря"
                            },
                            {
                                "target_lang_word": "портиться",
                                "target_lang_sense_clarification": "гнить или приходить в негодность (о еде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1081874e-1381-3159-a41a-49a5f765a253",
                    "sense_definition": "Nie wykorzystywać w pełni swoich możliwości, talentu lub potencjału.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "waste_potential",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel uważał, że uczeń <w>marnuje</w> swój potencjał, nie odrabiając prac domowych.",
                            "target_lang_translations": {
                                "ru": "Учитель считал, что ученик <w>растрачивает</w> свой потенциал, не делая домашние задания."
                            }
                        },
                        {
                            "text": "To smutne patrzeć, jak on <w>marnuje</w> swoją wielką szansę na karierę.",
                            "target_lang_translations": {
                                "ru": "Грустно смотреть, как он <w>упускает</w> свой большой шанс на карьеру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "marnować szansę",
                        "marnować okazję",
                        "marnować potencjał",
                        "marnować talent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не использовать в полной мере свои возможности, талант или потенциал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упускать",
                                "target_lang_sense_clarification": "не использовать шанс или возможность"
                            },
                            {
                                "target_lang_word": "растрачивать",
                                "target_lang_sense_clarification": "бездарно расходовать талант или потенциал"
                            },
                            {
                                "target_lang_word": "губить",
                                "target_lang_sense_clarification": "уничтожать (о таланте или жизни)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
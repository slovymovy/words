                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадроцикл",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11d4d37d-101d-3256-b336-c4f4dae66958",
                    "sense_definition": "Sposób przedstawienia na rysunku technicznym szczegółów przekroju lub widoku przedmiotu, polegający na narysowaniu krawędzi leżących na płaszczyźnie tnącej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_drawing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student musiał poprawnie narysować <kład> wału maszynowego na egzaminie.",
                            "target_lang_translations": {
                                "ru": "Студент должен был правильно нарисовать <w>сечение</w> вала машины на экзамене."
                            }
                        },
                        {
                            "text": "Różnica między <kładem> a przekrojem jest kluczowa w projektowaniu inżynierskim.",
                            "target_lang_translations": {
                                "ru": "Разница между <w>сечением</w> и разрезом является ключевой в инженерном проектировании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kład miejscowy",
                        "kład przesunięty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ изображения на техническом чертеже фигуры, получающейся при мысленном рассечении предмета плоскостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сечение",
                                "target_lang_sense_clarification": "черчение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5e372f7-9b6b-30b0-b9b7-2a6abe33ae8f",

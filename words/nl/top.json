            "senses": [
                {
                    "sense_id": "ae6f2c18-8b14-30d7-b679-2bad7ae078ef",
                    "sense_definition": "Het hoogste punt of bovenste uiteinde van iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_top",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben na uren klimmen de <w>top</w> van de berg bereikt.",
                            "target_lang_translations": {
                                "ru": "После нескольких часов восхождения мы достигли <w>вершины</w> горы."
                            }
                        },
                        {
                            "text": "Er ligt een dik pak sneeuw op de <w>top</w> van het dak.",
                            "target_lang_translations": {
                                "ru": "На <w>верхушке</w> крыши лежит толстый слой снега."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van top tot teen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая высокая точка или верхний конец чего-либо."
                    },
                    "translations": {

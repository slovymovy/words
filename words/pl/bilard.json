    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3141d860-79aa-3145-9122-e62df44acac1",
                    "sense_definition": "Gra towarzyska i sportowa polegająca na odbijaniu kul kijem na specjalnym stole pokrytym suknem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy piątek spotykamy się z przyjaciółmi, żeby pograć w <w>bilard</w>.",
                            "target_lang_translations": {
                                "ru": "Каждую пятницу мы встречаемся с друзьями, чтобы поиграть в <w>бильярд</w>."
                            }
                        },
                        {
                            "text": "Ten zawodnik to prawdziwy mistrz gry w <w>bilard</w>.",
                            "target_lang_translations": {
                                "ru": "Этот спортсмен — настоящий мастер игры в <w>бильярд</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać w bilard",
                        "partia bilarda",
                        "kule do bilarda",
                        "kij do bilarda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивная и развлекательная игра, заключающаяся в перемещении шаров по специальному столу, покрытому сукном, с помощью кия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бильярд",
                                "target_lang_sense_clarification": "игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ed5a916-409f-3dd6-842a-c229c0d52a1c",
                    "sense_definition": "Pokryty suknem stół z bandami przystosowany do gry w kule.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W rogu salonu stał ciężki, dębowy <w>bilard</w>.",
                            "target_lang_translations": {
                                "ru": "В углу гостиной стоял тяжёлый дубовый <w>бильярд</w>."
                            }
                        },
                        {
                            "text": "Musimy zamówić nowe sukno na nasz stary <w>bilard</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заказать новое сукно на наш старый <w>бильярд</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stół bilardowy"
                    ],
                    "common_phrases": [
                        "postawić bilard",
                        "sukno na bilard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стол, покрытый сукном и оборудованный бортами, предназначенный для игры в шары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бильярдный стол",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "бильярд",
                                "target_lang_sense_clarification": "стол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "411eb161-98a9-386b-ab63-366087826b83",
                    "sense_definition": "Automat do gry zręcznościowej z odbijającą się wewnątrz metalową kulą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W barze stał kolorowy <w>bilard</w>, który przyciągał uwagę dzieci.",
                            "target_lang_translations": {
                                "ru": "В баре стоял красочный <w>пинбол</w>, который привлекал внимание детей."
                            }
                        },
                        {
                            "text": "Wrzuciłem monetę, ale ten elektryczny <w>bilard</w> się zaciął.",
                            "target_lang_translations": {
                                "ru": "Я бросил монету, но этот <w>электрический бильярд</w> сломался."
                            }
                        }
                    ],
                    "synonyms": [
                        "flipper"
                    ],
                    "common_phrases": [
                        "bilard elektryczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аркадный игровой автомат, в котором игрок манипулирует металлическим шариком внутри застекленного корпуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пинбол",
                                "target_lang_sense_clarification": "игровой автомат"
                            },
                            {
                                "target_lang_word": "электрический бильярд",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        }
    ]

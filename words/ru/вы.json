    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "a61e9153-75f0-39b8-9820-edd22212b462",
                    "sense_definition": "Личное местоимение второго лица множественного числа; используется для обращения к нескольким лицам.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Куда <w>вы</w> идёте так поздно?",
                            "target_lang_translations": {
                                "en": "Where are <w>you</w> going so late?"
                            }
                        },
                        {
                            "text": "<w>Вы</w> все должны внимательно слушать учителя.",
                            "target_lang_translations": {
                                "en": "<w>You</w> all must listen carefully to the teacher."
                            }
                        }
                    ],
                    "antonyms": [
                        "мы"
                    ],
                    "common_phrases": [
                        "вы все",
                        "кто вы",
                        "где вы"
                    ],
                    "target_lang_definitions": {
                        "en": "Second-person plural pronoun; used to address multiple people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "plural"
                            },
                            {
                                "target_lang_word": "you all",
                                "target_lang_sense_clarification": "plural"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "608d7f4b-66d6-3c18-b1a2-83ae8bd31caf",
                    "sense_definition": "Форма вежливого, официального обращения к одному лицу (в отличие от «ты»).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скажите, <w>вы</w> работаете в этом магазине?",
                            "target_lang_translations": {
                                "en": "Tell me, do <w>you</w> work in this shop?"
                            }
                        },
                        {
                            "text": "Как <w>вас</w> зовут, молодой человек?",
                            "target_lang_translations": {
                                "en": "What is <w>your</w> name, young man?"
                            }
                        }
                    ],
                    "antonyms": [
                        "я",
                        "мы",
                        "мы"
                    ],
                    "common_phrases": [
                        "как вас зовут",
                        "можно вас",
                        "слушаю вас",
                        "что вы"
                    ],
                    "target_lang_definitions": {
                        "en": "Polite, formal form of address to a single person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "polite form"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "uzupełniać",
        "uzupełnienie",
        "uzupełniający"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "78aedac5-c59a-3e89-9b5f-fd9ea7a64526",
                    "sense_definition": "Wpisać brakujące informacje, słowa lub dane w puste miejsca w tekście lub dokumencie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę czytelnie <uzupełnić> ten formularz swoimi danymi osobowymi.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, разборчиво <заполните> этот бланк своими личными данными."
                            }
                        },
                        {
                            "text": "Na egzaminie student poprawnie <uzupełnił> wszystkie luki w zdaniach.",
                            "target_lang_translations": {
                                "ru": "На экзамене студент правильно <заполнил> все пропуски в предложениях."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypełnić",
                        "wpisać"
                    ],
                    "common_phrases": [
                        "uzupełnić formularz",
                        "uzupełnić luki",
                        "uzupełnić dane",
                        "uzupełnić wniosek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вписать недостающую информацию, слова или данные в пустые места в тексте или документе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заполнить",
                                "target_lang_sense_clarification": "бланк, формуляр, пропуски"
                            },
                            {
                                "target_lang_word": "вписать",
                                "target_lang_sense_clarification": "данные, недостающие слова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5a3575f-d79d-3776-85fe-6b1f111f5188",
                    "sense_definition": "Dodać coś, czego brakuje, aby całość była kompletna, pełna lub osiągnęła odpowiedni poziom.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy koniecznie <uzupełnić> zapasy jedzenia przed nadchodzącą zimą.",
                            "target_lang_translations": {
                                "ru": "Нам нужно обязательно <пополнить> запасы еды перед предстоящей зимой."
                            }
                        },
                        {
                            "text": "Nauczyciel zalecił mu, aby <uzupełnił> swoją wiedzę z historii.",
                            "target_lang_translations": {
                                "ru": "Учитель посоветовал ему <восполнить> свои знания по истории."
                            }
                        },
                        {
                            "text": "Kierowca zatrzymał się na stacji benzynowej, żeby <uzupełnić> paliwo.",
                            "target_lang_translations": {
                                "ru": "Водитель остановился на заправке, чтобы <пополнить> запас топлива."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopełnić",
                        "skompletować",
                        "dolać",
                        "dołożyć"
                    ],
                    "antonyms": [
                        "uszczuplić",
                        "wyczerpać"
                    ],
                    "common_phrases": [
                        "uzupełnić zapasy",
                        "uzupełnić wiedzę",
                        "uzupełnić braki",
                        "uzupełnić paliwo",
                        "uzupełnić kolekcję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавить что-то недостающее, чтобы целое стало полным или достигло нужного уровня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пополнить",
                                "target_lang_sense_clarification": "запасы, счет, коллекцию"
                            },
                            {
                                "target_lang_word": "восполнить",
                                "target_lang_sense_clarification": "пробелы в знаниях, нехватку"
                            },
                            {
                                "target_lang_word": "дополнить",
                                "target_lang_sense_clarification": "добавить к уже имеющемуся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f76ba166-f310-3bd2-a871-4041a613166f",
                    "sense_definition": "Pasować do siebie nawzajem, tworząc razem lepszą, spójną całość (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich odmienne charaktery wspaniale się <uzupełniły> w tym małżeństwie.",
                            "target_lang_translations": {
                                "ru": "Их разные характеры прекрасно <дополнили> друг друга в этом браке."
                            }
                        },
                        {
                            "text": "Wierzę, że nasze umiejętności zawodowe doskonale się <uzupełnią> w nowym projekcie.",
                            "target_lang_translations": {
                                "ru": "Я верю, что наши профессиональные навыки отлично <дополнят> друг друга в новом проекте."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopełnić się",
                        "zgrać się"
                    ],
                    "common_phrases": [
                        "wzajemnie się uzupełnić",
                        "idealnie się uzupełnić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходить друг другу, образуя вместе лучшую, единую целостность (обычно используется с «друг друга»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дополнить",
                                "target_lang_sense_clarification": "друг друга, составить единое целое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "1d3e63f5-ca3a-3181-87f2-a01c579f3921",
                    "sense_definition": "Obywatel monarchii, który podlega władzy panującego monarchy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako wierny <w>poddany</w>, rycerz zawsze stawał w obronie króla.",
                            "target_lang_translations": {
                                "ru": "Как верный <w>подданный</w>, рыцарь всегда вставал на защиту короля."
                            }
                        },
                        {
                            "text": "Wszyscy <w>poddani</w> zgromadzili się przed pałacem, aby wiwatować na cześć władcy.",
                            "target_lang_translations": {
                                "ru": "Все <w>подданные</w> собрались перед дворцом, чтобы приветствовать правителя."
                            }
                        }
                    ],
                    "antonyms": [
                        "monarcha",
                        "władca",
                        "król"
                    ],
                    "common_phrases": [
                        "wierny poddany",
                        "poddani króla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гражданин монархии, который подчиняется власти правящего монарха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подданный",
                                "target_lang_sense_clarification": "гражданин монархического государства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a71efe6c-3fc8-3c80-8673-d9785e806d1e",
                    "sense_definition": "Chłop w systemie feudalnym, zależny od pana i przywiązany do ziemi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych wiekach <w>poddany</w> musiał pracować na polu swojego pana.",
                            "target_lang_translations": {
                                "ru": "В былые времена <w>крепостной</w> должен был работать на поле своего господина."
                            }
                        },
                        {
                            "text": "Los <w>poddanego</w> zależał całkowicie od woli właściciela ziemskiego.",
                            "target_lang_translations": {
                                "ru": "Судьба <w>крепостного</w> полностью зависела от воли землевладельца."
                            }

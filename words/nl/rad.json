                    "synonyms": [
                        "wiel"
                    ],
                    "common_phrases": [
                        "rad van fortuin",
                        "vijfde rad aan de wagen"
                    ],
                    "target_lang_definitions": {
                        "en": "A circular object that rotates on an axis, used for movement or driving a machine.",
                        "ru": "Круглый предмет, который вращается вокруг оси и служит для движения или привода механизма."
                    },
                    "translations": {

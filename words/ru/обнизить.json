    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3f4f952d-35e6-3343-b75a-d30a7b453aeb",
                    "sense_definition": "Сделать что-либо ниже, чем следует, или укоротить по высоте.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "construction_height",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Надо следить, чтобы плотники случайно не <w>обнизили</w> потолок в новом доме.",
                            "target_lang_translations": {
                                "en": "We need to watch out so the carpenters don't accidentally <w>make</w> the ceiling <w>too low</w> in the new house."
                            }
                        },
                        {
                            "text": "― Надо мужикам сказать, завалину бы не <w>обнизили</w>, подпол промерзать станет.",
                            "target_lang_translations": {
                                "en": "We need to tell the men not to <w>build</w> the embankment <w>too low</w>, or the cellar will start freezing."
                            }
                        }
                    ],
                    "synonyms": [
                        "окоротить"
                    ],
                    "antonyms": [
                        "обвысить"
                    ],
                    "common_phrases": [
                        "обнизить сруб",
                        "обнизить фундамент"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "сельская речь"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make something lower than intended or to shorten its height."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make too low",
                                "target_lang_sense_clarification": "construction/height"
                            },
                            {
                                "target_lang_word": "build too low",
                                "target_lang_sense_clarification": "construction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5e2568a-bb46-3405-82c2-df1250eedfc1",
                    "sense_definition": "Выстрелить или попасть ниже цели (об охоте).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting_shooting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотник поторопился с выстрелом и <w>обнизил</w>, пуля ушла в землю.",
                            "target_lang_translations": {
                                "en": "The hunter rushed the shot and <w>shot low</w>; the bullet went into the ground."
                            }
                        },
                        {
                            "text": "Нет никакого сомнения, что дуло ружья потеряет цель и по большей части <w>обнизит</w>.",
                            "target_lang_translations": {
                                "en": "There is no doubt that the gun barrel will lose the target and mostly <w>shoot low</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "обвысить"
                    ],
                    "common_phrases": [
                        "обнизить при стрельбе",
                        "сильно обнизить"
                    ],
                    "target_lang_definitions": {
                        "en": "To shoot or hit below the target."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shoot low",
                                "target_lang_sense_clarification": "firearms/hunting"
                            },
                            {
                                "target_lang_word": "undershoot",
                                "target_lang_sense_clarification": "miss below target"
                            }
                        ]
                    }
                }
            ]
        }
    ]

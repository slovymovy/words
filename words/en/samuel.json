    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4a4a7531-fcb6-3f46-8090-dbd937d6402c",
                    "sense_definition": "A male given name from Hebrew.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Samuel</w> prefers to be called Sam by his friends.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Сэмюэл</w> предпочитает, чтобы друзья называли его Сэм."
                            }
                        },
                        {
                            "text": "<w>Samuel</w> is a very popular name for boys in many English-speaking countries.",
                            "target_lang_translations": {
                                "ru": "<w>Сэмюэл</w> — очень популярное имя для мальчиков во многих англоязычных странах."
                            }
                        }
                    ],
                    "synonyms": [
                        "Sam."
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя еврейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сэмюэл",
                                "target_lang_sense_clarification": "английское имя"
                            },
                            {
                                "target_lang_word": "Самуил",
                                "target_lang_sense_clarification": "русский эквивалент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20d6d9f3-384a-308f-a4e5-ec53094d9ecc",
                    "sense_definition": "The primary author and central character of the first book of Samuel in the Bible; a prophet and judge.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_figure",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "The prophet <w>Samuel</w> anointed David as the future king of Israel.",
                            "target_lang_translations": {
                                "ru": "Пророк <w>Самуил</w> помазал Давида на царство как будущего царя Израиля."
                            }
                        },
                        {
                            "text": "In the biblical story, God called out to <w>Samuel</w> while he was sleeping.",
                            "target_lang_translations": {
                                "ru": "В библейском рассказе Бог воззвал к <w>Самуилу</w>, когда тот спал."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский пророк и последний судья Израиля; главный персонаж Первой книги Царств (Самуила)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Самуил",
                                "target_lang_sense_clarification": "библейский пророк"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fa58c55-1321-32a8-8604-1ae8fa274039",
                    "sense_definition": "One of two books of the Old Testament and the Hebrew Tanakh.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "biblical_book",
                    "name_type": "work_of_art_name",
                    "examples": [
                        {
                            "text": "The pastor read a passage from the first book of <w>Samuel</w> during the service.",
                            "target_lang_translations": {
                                "ru": "Пастор прочитал отрывок из Первой книги <w>Самуила</w> во время службы."
                            }
                        },
                        {
                            "text": "Scholars analyze the historical context described in <w>Samuel</w> to understand the era.",
                            "target_lang_translations": {
                                "ru": "Ученые анализируют исторический контекст, описанный в книгах <w>Самуила</w>, чтобы понять ту эпоху."
                            }
                        }
                    ],
                    "common_phrases": [
                        "First Samuel",
                        "Second Samuel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух книг Ветхого Завета (в синодальном переводе — Первая и Вторая книги Царств)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Самуил",
                                "target_lang_sense_clarification": "название книги Библии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "116b65c9-2f71-3d73-9a2a-878f800405b0",
                    "sense_definition": "A surname.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Samuel</w> has been working at the company for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Сэмюэл</w> работает в компании уже более двадцати лет."
                            }
                        },
                        {
                            "text": "We looked up the <w>Samuel</w> family in the local telephone directory.",
                            "target_lang_translations": {
                                "ru": "Мы искали семью <w>Сэмюэл</w> в местном телефонном справочнике."
                            }
                        }
                    ],
                    "traits": [

            "senses": [
                {
                    "sense_id": "00850e84-c226-39db-a094-691985dd4100",
                    "sense_definition": "A general distrust of the integrity or professed motives of others, often believing they are motivated solely by self-interest.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>cynicism</w> about politicians has grown stronger after the recent scandal.",
                            "target_lang_translations": {
                                "ru": "Её <w>цинизм</w> по отношению к политикам усилился после недавнего скандала."
                            }
                        },
                        {
                            "text": "Despite his deep <w>cynicism</w>, he still volunteers to help the community every weekend.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свой глубокий <w>цинизм</w>, он всё же каждые выходные помогает обществу в качестве волонтёра."
                            }
                        }
                    ],
                    "synonyms": [
                        "skepticism",
                        "distrust",
                        "suspicion",
                        "misanthropy"
                    ],
                    "antonyms": [
                        "optimism",
                        "idealism",
                        "trust",
                        "naivety"
                    ],
                    "common_phrases": [
                        "deep cynicism",
                        "public cynicism",
                        "cynicism about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее недоверие к порядочности или заявленным мотивам других людей, часто основанное на убеждении, что ими движет исключительно личная выгода."
                    },
                    "translations": {

            "senses": [
                {
                    "sense_id": "dae5716e-96a0-373c-9ffd-91eb26b4c81b",
                    "sense_definition": "Воспринять звуки с помощью органов слуха.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "perception_auditory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вдруг я <услышал> странный звук за окном.",
                            "target_lang_translations": {
                                "en": "Suddenly I <heard> a strange sound outside the window."
                            }
                        },
                        {
                            "text": "Ты <услышал> мой голос в этой шумной толпе?",
                            "target_lang_translations": {
                                "en": "Did you <hear> my voice in this noisy crowd?"
                            }
                        }
                    ],
                    "synonyms": [
                        "услыхать",
                        "расслышать"
                    ],
                    "common_phrases": [
                        "услышать голос",
                        "услышать шум",
                        "рад услышать"
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive sounds through the ear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hear",
                                "target_lang_sense_clarification": "to perceive sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f78dd8c-0354-35d0-b0e4-d499264eecdc",
                    "sense_definition": "Получить информацию о чём-либо, узнать новость.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "perception_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <услышали> эту радостную новость только вчера вечером.",
                            "target_lang_translations": {
                                "en": "We <heard> this joyful news only yesterday evening."
                            }
                        },
                        {
                            "text": "Я <услышал> о твоём успехе от наших общих друзей.",
                            "target_lang_translations": {
                                "en": "I <heard> about your success from our mutual friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "узнать"
                    ],
                    "common_phrases": [
                        "услышать новость",
                        "услышать по радио",
                        "услышать от друга"
                    ],
                    "target_lang_definitions": {
                        "en": "To receive information about something; to learn news."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hear",
                                "target_lang_sense_clarification": "to receive information"
                            },
                            {
                                "target_lang_word": "learn",
                                "target_lang_sense_clarification": "to gain knowledge of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01e3badb-8c2b-35c0-8e54-8f3b09049b6e",
                    "sense_definition": "Принять во внимание, понять и учесть чьи-либо слова, просьбы или мнение.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "perception_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство должно <услышать> требования своих граждан.",
                            "target_lang_translations": {
                                "en": "The government must <hear> the demands of its citizens."
                            }
                        },
                        {
                            "text": "Они пытались <услышать> друг друга и найти компромисс в споре.",
                            "target_lang_translations": {
                                "en": "They tried to <hear> each other and find a compromise in the dispute."
                            }
                        }
                    ],
                    "synonyms": [
                        "заметить",
                        "обратить внимание"
                    ],
                    "common_phrases": [
                        "услышать просьбу",
                        "услышать друг друга",
                        "быть услышанным"
                    ],
                    "target_lang_definitions": {
                        "en": "To take into account, understand, and consider someone's words, requests, or opinion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hear",
                                "target_lang_sense_clarification": "to listen to or grant a request"
                            },
                            {
                                "target_lang_word": "heed",
                                "target_lang_sense_clarification": "to pay attention to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71be0f42-60c5-3a26-93d9-1c4aab2a1a59",
                    "sense_definition": "Почувствовать запах или ощутить что-либо физически (разговорное употребление).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "perception_olfactory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как только я вошёл в дом, я <услышал> запах свежих пирогов.",
                            "target_lang_translations": {
                                "en": "As soon as I entered the house, I <smelled> the scent of fresh pies."
                            }
                        },
                        {
                            "text": "Охотничья собака сразу <услышала> присутствие дикого зверя.",
                            "target_lang_translations": {
                                "en": "The hunting dog immediately <sensed> the presence of a wild beast."
                            }
                        }
                    ],
                    "synonyms": [
                        "почуять",
                        "почувствовать"
                    ],
                    "common_phrases": [
                        "услышать запах"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "используется в значении 'почувствовать запах'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive a smell or feel something physically (colloquial)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smell",
                                "target_lang_sense_clarification": "to perceive an odor"
                            },
                            {
                                "target_lang_word": "sense",
                                "target_lang_sense_clarification": "to perceive physically or intuitively"
                            },
                            {
                                "target_lang_word": "feel",
                                "target_lang_sense_clarification": "to experience a physical sensation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
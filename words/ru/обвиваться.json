            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ec6af578-0800-3fed-9f97-a9430cc77642",
                    "sense_definition": "Охватывать, плотно прилегая со всех сторон; обматываться или оплетаться вокруг кого-либо или чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_winding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Змея медленно <w>обвивалась</w> вокруг толстой ветки дерева.",
                            "target_lang_translations": {
                                "en": "The snake slowly <w>coiled</w> around the thick tree branch."
                            }
                        },
                        {
                            "text": "Зелёный плющ густо <w>обвивается</w> вокруг старого забора.",
                            "target_lang_translations": {
                                "en": "Green ivy thickly <w>twines</w> around the old fence."
                            }
                        }
                    ],
                    "synonyms": [
                        "оплетаться",
                        "обматываться",
                        "виться"
                    ],
                    "antonyms": [
                        "разматываться",
                        "расплетаться"
                    ],
                    "common_phrases": [
                        "обвиваться вокруг шеи",
                        "обвиваться вокруг дерева",
                        "обвиваться кольцами"
                    ],
                    "target_lang_definitions": {
                        "en": "To encompass, fitting tightly on all sides; to wind or twine around someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wind around",
                                "target_lang_sense_clarification": "general winding motion"
                            },
                            {
                                "target_lang_word": "coil around",
                                "target_lang_sense_clarification": "forming rings or loops, e.g., a snake"
                            },
                            {
                                "target_lang_word": "twine around",
                                "target_lang_sense_clarification": "interweaving or spiraling, e.g., plants"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5df5f544-a477-3678-99b7-2fc6f4928616",
                    "sense_definition": "Крепко обнимать кого-либо (обычно о руках).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_winding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её руки нежно <w>обвивались</w> вокруг его шеи при встрече.",
                            "target_lang_translations": {
                                "en": "Her arms gently <w>wrapped</w> around his neck when they met."
                            }
                        },
                        {
                            "text": "Ребёнок испугался и крепко <w>обвивался</w> вокруг ног матери.",
                            "target_lang_translations": {
                                "en": "The child got scared and tightly <w>wrapped himself</w> around his mother's legs."
                            }
                        }
                    ],
                    "synonyms": [
                        "обнимать"
                    ],
                    "antonyms": [
                        "отпускать",
                        "отстраняться"
                    ],
                    "common_phrases": [
                        "руки обвивались"
                    ],
                    "target_lang_definitions": {
                        "en": "To hug someone tightly (usually referring to arms wrapping around)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wrap around",
                                "target_lang_sense_clarification": "of arms or legs embracing someone"
                            },
                            {
                                "target_lang_word": "cling to",
                                "target_lang_sense_clarification": "holding on tightly with limbs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7602b3e6-50ee-3232-8018-4d76822ea475",
                    "sense_definition": "Покрываться или окутываться чем-либо легким и летучим (туманом, дымом, мглой).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вершина высокой горы <w>обвивалась</w> густым утренним туманом.",
                            "target_lang_translations": {
                                "en": "The peak of the high mountain <w>was wreathed</w> in thick morning fog."
                            }
                        },
                        {
                            "text": "Весь город медленно <w>обвивался</w> сизой дымкой заката.",
                            "target_lang_translations": {
                                "en": "The whole city slowly <w>became enveloped</w> in the blue-grey haze of the sunset."
                            }
                        }
                    ],
                    "synonyms": [
                        "окутываться",
                        "обволакиваться"
                    ],
                    "antonyms": [
                        "рассеиваться"
                    ],
                    "common_phrases": [
                        "обвиваться туманом",
                        "обвиваться дымом"
                    ],
                    "target_lang_definitions": {
                        "en": "To be covered or enveloped by something light and volatile (fog, smoke, haze)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be wreathed in",
                                "target_lang_sense_clarification": "surrounded by smoke or fog"
                            },
                            {
                                "target_lang_word": "be enveloped in",
                                "target_lang_sense_clarification": "completely covered by atmosphere"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47b3afdb-4920-3679-a23c-6035e50cb08b",
                    "sense_definition": "Страдательный залог к глаголу <w>обвивать</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот столб <w>обвивается</w> декоративной лентой для праздника.",
                            "target_lang_translations": {

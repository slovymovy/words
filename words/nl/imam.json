                {
                    "sense_id": "c9ca4fc7-aa24-35f6-9635-241d50cee7fa",
                    "sense_definition": "De voorganger bij het gebed in een moskee.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>imam</w> leidt het vrijdaggebed in de moskee.",
                            "target_lang_translations": {
                                "en": "The <w>imam</w> leads the Friday prayer in the mosque.",
                                "ru": "<w>Имам</w> ведет пятничную молитву в мечети."
                            }
                        },
                        {
                            "text": "Na de dienst stelden wij een vraag aan de <w>imam</w>.",
                            "target_lang_translations": {
                                "en": "After the service, we asked the <w>imam</w> a question.",
                                "ru": "После службы мы задали вопрос <w>имаму</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebedsleider",
                        "voorganger"
                    ],
                    "common_phrases": [
                        "de imam van de moskee"
                    ],
                    "target_lang_definitions": {
                        "en": "The person who leads the prayer in a mosque.",
                        "ru": "Предстоятель на молитве в мечети."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "imam",
                                "target_lang_sense_clarification": "prayer leader"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "имам",
                                "target_lang_sense_clarification": "руководитель молитвы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11737536-39da-30dd-b1a2-722e7407dc35",
                    "sense_definition": "Een geestelijk en politiek leider van een sjiitische gemeenschap.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De twaalfde <w>imam</w> is een centrale figuur in het sjiisme.",
                            "target_lang_translations": {

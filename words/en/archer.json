                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They stopped in the small town of <w>Archer</w> to refuel.",
                            "target_lang_translations": {
                                "ru": "Они остановились в маленьком городке <w>Арчер</w>, чтобы заправиться."
                            }

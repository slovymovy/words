{
    "word_family": [
        "kloven"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad94cf7b-a16f-3b50-ae99-d0603d8c0e95",
                    "sense_definition": "Een groot verschil of grote afstand tussen mensen, groepen of opvattingen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij sprak in zijn toespraak over de <w>kloof</w> tussen de politiek en de burger.",
                            "target_lang_translations": {
                                "ru": "В своей речи он говорил о <w>пропасти</w> между политикой и гражданами."
                            }
                        },
                        {
                            "text": "Er ontstaat een steeds grotere <w>kloof</w> tussen arm en rijk in de samenleving.",
                            "target_lang_translations": {
                                "ru": "В обществе возникает всё большая <w>пропасть</w> между бедными и богатыми."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de kloof dichten",
                        "kloof tussen arm en rijk",
                        "een diepe kloof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая разница или дистанция между людьми, группами или мнениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропасть",
                                "target_lang_sense_clarification": "переносное значение: глубокое различие"
                            },
                            {
                                "target_lang_word": "разрыв",
                                "target_lang_sense_clarification": "отсутствие связи, дистанция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80113be4-34af-380c-a6e5-21761e08e5c2",
                    "sense_definition": "Een diep, smal dal met steile wanden, vaak ontstaan door erosie van een rivier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voorzichtig lopen de toeristen over het glibberige pad in de diepe <w>kloof</w>.",
                            "target_lang_translations": {
                                "ru": "Туристы осторожно идут по скользкой тропе в глубоком <w>ущелье</w>."
                            }
                        },
                        {
                            "text": "De rivier stroomt al eeuwenlang door deze smalle <w>kloof</w> in de bergen.",
                            "target_lang_translations": {
                                "ru": "Река веками течет через это узкое <w>ущелье</w> в горах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "diepe kloof",
                        "in de kloof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокая узкая долина с крутыми стенами, часто образованная речной эрозией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ущелье",
                                "target_lang_sense_clarification": "узкая горная долина"
                            },
                            {
                                "target_lang_word": "каньон",
                                "target_lang_sense_clarification": "глубокая речная долина"
                            },
                            {
                                "target_lang_word": "расщелина",
                                "target_lang_sense_clarification": "трещина в скале или земле"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c133c179-ff6a-37d5-8b13-0d3a938b9e8b",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kloven</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kloof</w> het hout in kleine stukken voor de open haard.",
                            "target_lang_translations": {
                                "ru": "Я <w>колю</w> дрова на мелкие куски для камина."
                            }
                        },
                        {
                            "text": "<w>Kloof</w> jij dat grote blok hout even doormidden?",
                            "target_lang_translations": {
                                "ru": "Ты <w>расколешь</w> это большое полено пополам?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "hout kloven"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "also imperative and second person singular (inversion)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола <w>kloven</w> (колоть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колю",
                                "target_lang_sense_clarification": "дрова или твердые предметы"
                            },
                            {
                                "target_lang_word": "раскалываю",
                                "target_lang_sense_clarification": "разделяю на части ударом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f7adb04-76d3-3c89-bd18-33322b1268e4",
                    "sense_definition": "Enkelvoud verleden tijd van <w>kluiven</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond <w>kloof</w> urenlang tevreden op het grote bot.",
                            "target_lang_translations": {
                                "ru": "Собака часами довольно <w>грызла</w> большую кость."
                            }
                        },
                        {
                            "text": "Hij <w>kloof</w> het kippenpootje helemaal kaal tijdens het avondeten.",
                            "target_lang_translations": {
                                "ru": "За ужином он полностью <w>обглодал</w> куриную ножку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени единственного числа от глагола <w>kluiven</w> (грызть, обгладывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грыз",
                                "target_lang_sense_clarification": "кость или твердую пищу"
                            },
                            {
                                "target_lang_word": "обгладывал",
                                "target_lang_sense_clarification": "съедал мясо с кости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad86938b-d986-35dc-bc7e-f39e4b096711",
                    "sense_definition": "Enkelvoud verleden tijd van <w>klieven</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het snelle schip <w>kloof</w> moeiteloos door de hoge golven.",
                            "target_lang_translations": {
                                "ru": "Быстрый корабль без усилий <w>рассекал</w> высокие волны."
                            }
                        },
                        {
                            "text": "De pijl <w>kloof</w> de lucht voordat hij het doel raakte.",
                            "target_lang_translations": {
                                "ru": "Стрела <w>рассекла</w> воздух, прежде чем поразить цель."
                            }
                        }
                    ],
                    "synonyms": [
                        "kliefde"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени единственного числа от глагола <w>klieven</w> (рассекать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассекал",
                                "target_lang_sense_clarification": "разрезал воздух или воду"
                            },
                            {
                                "target_lang_word": "разрезал",
                                "target_lang_sense_clarification": "проходил сквозь что-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запрещённый",
                                "target_lang_sense_clarification": "страдательное причастие"
                            },
                            {
                                "target_lang_word": "запретить",
                                "target_lang_sense_clarification": "глагол (используется при переводе активных конструкций)"

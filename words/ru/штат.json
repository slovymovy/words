    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ae664a7-9238-3bd8-8c3c-a860e2a705f4",
                    "sense_definition": "Административно-территориальная единица с внутренним самоуправлением в ряде государств (например, в США, Индии, Бразилии).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geo_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Калифорния — это самый густонаселённый <штат> в стране.",
                            "target_lang_translations": {
                                "en": "California is the most populous <state> in the country."
                            }
                        },
                        {
                            "text": "Законы могут сильно различаться в зависимости от того, в каком <штате> вы находитесь.",
                            "target_lang_translations": {
                                "en": "Laws can vary greatly depending on which <state> you are in."
                            }
                        }
                    ],
                    "synonyms": [
                        "регион"
                    ],
                    "common_phrases": [
                        "штат Техас",
                        "законы штата",
                        "южные штаты"
                    ],
                    "target_lang_definitions": {
                        "en": "An administrative-territorial unit with internal self-government in certain countries (e.g., the USA, India, Brazil)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "state",
                                "target_lang_sense_clarification": "political division"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4355d2e9-1bc0-3cb4-8888-b8fef610f437",
                    "sense_definition": "Постоянный состав сотрудников организации, предприятия или учреждения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personnel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор подписал приказ зачислить нового инженера в <штат>.",
                            "target_lang_translations": {
                                "en": "The director signed an order to add the new engineer to the <staff>."
                            }
                        },
                        {
                            "text": "Наш <штат> насчитывает более ста квалифицированных специалистов.",
                            "target_lang_translations": {
                                "en": "Our <staff> numbers over a hundred qualified specialists."
                            }
                        }
                    ],
                    "synonyms": [
                        "коллектив",
                        "кадры"
                    ],
                    "common_phrases": [
                        "в штате",
                        "зачислить в штат",
                        "раздутый штат"
                    ],
                    "target_lang_definitions": {
                        "en": "The permanent body of employees in an organization, enterprise, or institution."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "staff",
                                "target_lang_sense_clarification": "employees"
                            },
                            {
                                "target_lang_word": "personnel",
                                "target_lang_sense_clarification": "human resources"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4540b850-7677-3b4d-9da4-cf7f1f5df08b",
                    "sense_definition": "Положение о численности и структуре сотрудников; расписание должностей (часто используется во множественном числе).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "personnel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за финансовых трудностей на заводе началось массовое сокращение <штатов>.",
                            "target_lang_translations": {
                                "en": "Due to financial difficulties, a massive reduction of <staff> began at the factory."
                            }
                        },
                        {
                            "text": "Этому отделу по <штату> полагается два секретаря и один помощник.",
                            "target_lang_translations": {
                                "en": "This department is entitled to two secretaries and one assistant on <staff>."
                            }
                        }
                    ],
                    "synonyms": [
                        "штатное расписание"
                    ],
                    "common_phrases": [
                        "сокращение штатов",
                        "по штату"
                    ],
                    "target_lang_definitions": {
                        "en": "Regulation regarding the number and structure of employees; the schedule of positions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "staff",
                                "target_lang_sense_clarification": "workforce structure"
                            },
                            {
                                "target_lang_word": "personnel",
                                "target_lang_sense_clarification": "staffing"
                            }
                        ]
                    }
                }
            ]
        },
        {

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "158ea2c7-0fa5-327c-b88f-f10d30282403",
                    "sense_definition": "The activity of operating and controlling a motor vehicle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "noun_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was arrested for reckless <w>driving</w> after the accident.",
                            "target_lang_translations": {
                                "ru": "Его арестовали за неосторожное <w>вождение</w> после аварии."
                            }
                        },
                        {
                            "text": "My father thinks that night <w>driving</w> is too dangerous for me.",
                            "target_lang_translations": {
                                "ru": "Мой отец считает, что ночное <w>вождение</w> слишком опасно для меня."
                            }
                        }
                    ],
                    "synonyms": [
                        "motoring"
                    ],
                    "common_phrases": [
                        "driving license",
                        "drunk driving",
                        "reckless driving",
                        "safe driving"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по управлению и контролю автотранспортным средством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вождение",
                                "target_lang_sense_clarification": "управление транспортным средством"
                            },
                            {
                                "target_lang_word": "езда",
                                "target_lang_sense_clarification": "процесс перемещения на транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baa792d9-84c3-32b4-b119-fc3ba7de600b",
                    "sense_definition": "The act of hitting a golf ball a long distance, usually from the tee.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>driving</w> has improved significantly since he bought new clubs.",
                            "target_lang_translations": {
                                "ru": "Его <w>драйв</w> значительно улучшился с тех пор, как он купил новые клюшки."
                            }
                        },
                        {
                            "text": "We spent the afternoon practicing our <w>driving</w> at the range.",
                            "target_lang_translations": {
                                "ru": "Мы провели день, отрабатывая <w>драйв</w> на тренировочном поле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "driving range"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный удар по мячу в гольфе на дальнюю дистанцию, обычно первый с подставки (ти)."

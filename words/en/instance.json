    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5641126-3e61-3990-a8ad-3ea8c6ecd5fe",
                    "sense_definition": "A specific case or example used to illustrate or explain something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "example_case",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "For <w>instance</w>, you could take the train instead of driving.",
                            "target_lang_translations": {
                                "ru": "К <w>примеру</w>, вы могли бы поехать на поезде вместо машины."
                            }
                        },
                        {
                            "text": "This is a clear <w>instance</w> of how the system fails.",
                            "target_lang_translations": {
                                "ru": "Это наглядный <w>пример</w> того, как система дает сбой."
                            }
                        }
                    ],
                    "synonyms": [
                        "example",
                        "case",
                        "illustration",
                        "sample"
                    ],
                    "common_phrases": [
                        "for instance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный случай или пример, используемый для иллюстрации или объяснения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пример",
                                "target_lang_sense_clarification": "образец для подражания или иллюстрация"
                            },
                            {
                                "target_lang_word": "случай",
                                "target_lang_sense_clarification": "конкретная ситуация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36f77f13-23ca-3fb0-b19d-32faab322b87",
                    "sense_definition": "A particular occasion or occurrence of an event.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "example_case",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There have been several <w>instances</w> of theft in the neighborhood recently.",
                            "target_lang_translations": {
                                "ru": "В последнее время в районе было несколько <w>случаев</w> краж."
                            }
                        },
                        {
                            "text": "In this particular <w>instance</w>, I am willing to overlook your mistake.",
                            "target_lang_translations": {
                                "ru": "В данном конкретном <w>случае</w> я готов закрыть глаза на вашу ошибку."
                            }

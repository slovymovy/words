                    "sense_id": "a9801cb3-9ea8-36c4-888a-5e74173cc2fd",
                    "sense_definition": "Iets of iemand naar een andere plaats meenemen of vervoeren.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal de kinderen morgen met de auto naar school <w>brengen</w>.",
                            "target_lang_translations": {
                                "en": "I will <w>take</w> the children to school by car tomorrow.",
                                "ru": "Завтра я <w>отвезу</w> детей в школу на машине."
                            }
                        },
                        {
                            "text": "De ober <w>bracht</w> ons snel de bestelde drankjes.",
                            "target_lang_translations": {
                                "en": "The waiter quickly <w>brought</w> us the ordered drinks.",
                                "ru": "Официант быстро <w>принёс</w> нам заказанные напитки."
                            }
                        },
                        {
                            "text": "Je moet hem onmiddellijk op de hoogte <w>brengen</w> van het nieuws.",
                            "target_lang_translations": {
                                "en": "You must immediately <w>inform</w> him of the news.",
                                "ru": "Ты должен немедленно <w>сообщить</w> ему новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "meenemen",
                        "vervoeren",
                        "bezorgen",
                        "afleveren"
                    ],
                    "antonyms": [
                        "halen",
                        "ophalen"
                    ],
                    "common_phrases": [
                        "naar bed brengen",
                        "op de hoogte brengen",
                        "in gevaar brengen",
                        "geluk brengen",
                        "tot stand brengen"
                    ],
                    "target_lang_definitions": {
                        "en": "To take, carry, or transport someone or something to another place.",
                        "ru": "Перемещать кого-либо или что-либо в определенное место (приносить, приводить, отвозить)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "to carry someone or something to a place (often towards the speaker)"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "to carry someone or something to a place (often away from the speaker)"
                            },
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "to move people or goods from one place to another"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "доставлять предмет в какое-либо место (обычно в руках)"
                            },
                            {
                                "target_lang_word": "отвозить",
                                "target_lang_sense_clarification": "доставлять кого-либо или что-либо куда-либо на транспорте"
                            },
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "сопровождать кого-либо до места (пешком)"
                            },
                            {
                                "target_lang_word": "доставлять",
                                "target_lang_sense_clarification": "перемещать грузы, товары или почту"
                            }
                        ]
                    }

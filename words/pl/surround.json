    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "34d5093d-755b-3e3c-adc0-8626c291f2fe",
                    "sense_definition": "Aparatura audio umożliwiająca uzyskanie wrażenia, że dźwięk dochodzi do słuchacza ze wszystkich stron, tworząc iluzję przestrzenności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "audio_technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy system <w>surround</w> sprawia, że oglądanie filmów to czysta przyjemność.",
                            "target_lang_translations": {
                                "ru": "Моя новая система <w>объемного звучания</w> превращает просмотр фильмов в чистое удовольствие."
                            }
                        },
                        {
                            "text": "Aby w pełni wykorzystać możliwości zestawu <w>surround</w>, musisz odpowiednio rozmieścić głośniki w pokoju.",
                            "target_lang_translations": {
                                "ru": "Чтобы в полной мере использовать возможности комплекта <w>surround</w>, нужно правильно расставить колонки в комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "system dźwięku przestrzennego"
                    ],
                    "antonyms": [
                        "stereo",
                        "mono"
                    ],
                    "common_phrases": [
                        "system surround",
                        "zestaw surround",
                        "kino domowe surround"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аудиоаппаратура, создающая иллюзию того, что звук исходит со всех сторон, обеспечивая эффект присутствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система объемного звучания",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "система surround",
                                "target_lang_sense_clarification": "заимствование, часто используется в названиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a3ac622-886b-3faf-a32d-a71b4a463144",
                    "sense_definition": "Dźwięk sprawiający wrażenie przestrzennego, uzyskany dzięki specjalnemu systemowi głośników.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "audio_technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Efekt <w>surround</w> w tej grze pozwala usłyszeć kroki przeciwnika za plecami.",
                            "target_lang_translations": {
                                "ru": "Эффект <w>объемного звука</w> в этой игре позволяет услышать шаги противника за спиной."
                            }
                        },
                        {
                            "text": "Nagranie zostało zrealizowane tak, aby zapewnić słuchaczom doskonały <w>surround</w>.",
                            "target_lang_translations": {
                                "ru": "Запись была сделана так, чтобы обеспечить слушателям превосходный <w>объемный звук</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dźwięk przestrzenny"
                    ],
                    "common_phrases": [
                        "efekt surround",
                        "jakość surround"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, создающий ощущение пространственности, полученный с помощью специальной системы динамиков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объемный звук",
                                "target_lang_sense_clarification": "звуковой эффект"
                            },
                            {
                                "target_lang_word": "пространственное звучание",
                                "target_lang_sense_clarification": "характеристика звука"
                            }
                        ]
                    }
                }
            ]
        },
        {

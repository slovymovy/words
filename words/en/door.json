            "senses": [
                {
                    "sense_id": "b8c883e3-832c-3ad4-a8d5-51b17429935b",
                    "sense_definition": "A movable barrier used to close and open an entrance to a building, room, or vehicle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_barrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please close the <w>door</w> behind you when you leave.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, закрой за собой <w>дверь</w>, когда будешь уходить."
                            }
                        },
                        {
                            "text": "She knocked on the front <w>door</w> but nobody answered.",
                            "target_lang_translations": {
                                "ru": "Она постучала во входную <w>дверь</w>, но никто не ответил."
                            }
                        }
                    ],
                    "synonyms": [
                        "entrance",
                        "entry",
                        "portal",
                        "gate"
                    ],
                    "common_phrases": [
                        "front door",
                        "back door",
                        "lock the door",
                        "open the door",
                        "answer the door"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижная преграда, используемая для закрытия и открытия входа в здание, комнату или транспортное средство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дверь",
                                "target_lang_sense_clarification": "физический объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6281ab8a-a29b-38ea-91c5-143afa488b3b",
                    "sense_definition": "A means of access, opportunity, or possibility to achieve something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Education opens the <w>door</w> to a better future.",
                            "target_lang_translations": {
                                "ru": "Образование открывает <w>дверь</w> в лучшее будущее."
                            }
                        },
                        {
                            "text": "We must keep the <w>door</w> open for further negotiations.",
                            "target_lang_translations": {
                                "ru": "Мы должны оставить <w>дверь</w> открытой для дальнейших переговоров."
                            }

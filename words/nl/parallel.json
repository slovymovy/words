                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Kreeftskeerkring is een bekende <w>parallel</w> op het noordelijk halfrond.",
                            "target_lang_translations": {
                                "ru": "Тропик Рака — известная <w>параллель</w> в Северном полушарии."
                            }

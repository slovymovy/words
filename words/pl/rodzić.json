                    "sense_id": "6bab1327-1426-3a13-bb06-8497bd1909fb",
                    "sense_definition": "Przychodzić na świat, zaczynać życie (używane z zaimkiem \"się\")",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_birth",
                    "name_type": "no",

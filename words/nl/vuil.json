                {
                    "sense_id": "fe518805-20b4-3067-9e09-93c48e327e11",
                    "sense_definition": "Niet schoon; bedekt met viezigheid, vlekken of stof.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "literal_dirty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je <vuile> handen wassen voordat je gaat eten.",
                            "target_lang_translations": {
                                "en": "You need to wash your <dirty> hands before you eat.",
                                "ru": "Тебе нужно помыть <грязные> руки перед едой."
                            }
                        },
                        {
                            "text": "Gooi die <vuile> kleren maar direct in de wasmand.",
                            "target_lang_translations": {
                                "en": "Just throw those <dirty> clothes directly into the laundry basket.",
                                "ru": "Брось эту <грязную> одежду прямо в корзину для белья."
                            }
                        }
                    ],
                    "synonyms": [
                        "vuiligheid"
                    ],
                    "antonyms": [
                        "schoon",
                        "proper",
                        "rein",
                        "zindelijk"
                    ],
                    "common_phrases": [
                        "vuile handen",
                        "vuile was",
                        "vuil water",
                        "zich vuil maken"
                    ],
                    "target_lang_definitions": {
                        "en": "Not clean; covered with dirt, stains, or dust.",
                        "ru": "Нечистый; покрытый грязью, пятнами или пылью."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dirty",
                                "target_lang_sense_clarification": "general term for not clean"
                            },
                            {
                                "target_lang_word": "filthy",
                                "target_lang_sense_clarification": "very dirty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "грязный",
                                "target_lang_sense_clarification": "общий термин для обозначения нечистого"
                            },
                            {
                                "target_lang_word": "нечистый",
                                "target_lang_sense_clarification": "противоположность чистому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a3bb2be-78df-398c-a307-6e433b7df34a",
                    "sense_definition": "Moreel verwerpelijk, gemeen of onfatsoenlijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_mean",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij speelde een <vuil> spelletje om de verkiezingen te winnen.",
                            "target_lang_translations": {
                                "en": "He played a <dirty> game to win the elections.",
                                "ru": "Он вел <грязную> игру, чтобы выиграть выборы."
                            }
                        },
                        {
                            "text": "Dat was een <vuile> streek van je collega.",
                            "target_lang_translations": {
                                "en": "That was a <dirty> trick by your colleague.",
                                "ru": "Это была <подлая> выходка твоего коллеги."
                            }
                        }
                    ],
                    "antonyms": [
                        "net"
                    ],
                    "common_phrases": [
                        "een vuile streek",
                        "vuile taal",
                        "vuil spel",
                        "iemand vuil aankijken"
                    ],
                    "target_lang_definitions": {
                        "en": "Morally reprehensible, mean, or indecent.",

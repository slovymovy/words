                    "sense_definition": "Een onhandig, dom of belachelijk persoon.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figuurlijk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doe niet zo idioot, je gedraagt je als een <w>hansworst</w>.",
                            "target_lang_translations": {
                                "en": "Don't be such an idiot, you're acting like a <w>buffoon</w>.",
                                "ru": "Не будь идиотом, ты ведёшь себя как <w>шут</w>."
                            }
                        },
                        {
                            "text": "Die <w>hansworst</w> heeft per ongeluk alle glazen omgestoten.",
                            "target_lang_translations": {
                                "en": "That <w>buffoon</w> accidentally knocked over all the glasses.",
                                "ru": "Этот <w>олух</w> случайно опрокинул все стаканы."
                            }
                        }
                    ],
                    "synonyms": [
                        "sufferd",
                        "kluns",
                        "dwaas",
                        "oen"
                    ],
                    "antonyms": [
                        "slimmerik"
                    ],
                    "common_phrases": [
                        "een echte hansworst",
                        "zich gedragen als een hansworst"
                    ],
                    "target_lang_definitions": {

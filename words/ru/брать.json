            "senses": [
                {
                    "sense_id": "74d8b707-0a09-38f3-a1fb-dfc1b145d9d1",
                    "sense_definition": "Хватать, захватывать руками или инструментом; принимать в руки, поднимать или перемещать к себе.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я всегда <беру> ключи, когда выхожу из дома.",
                            "target_lang_translations": {
                                "en": "I always <take> my keys when I leave the house."
                            }
                        },
                        {
                            "text": "Не забудь <брать> с собой зонт, сегодня будет дождь.",
                            "target_lang_translations": {
                                "en": "Don't forget to <take> an umbrella with you, it will rain today."
                            }
                        }
                    ],
                    "synonyms": [
                        "хватать",
                        "захватывать",
                        "поднимать"
                    ],
                    "antonyms": [
                        "класть",
                        "бросать",
                        "отпускать"
                    ],
                    "common_phrases": [
                        "брать в руки",
                        "брать с собой",
                        "брать трубку"
                    ],
                    "target_lang_definitions": {
                        "en": "To grab, seize, or accept into one's hands; to pick up or carry."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "lift"
                            },
                            {
                                "target_lang_word": "grab",
                                "target_lang_sense_clarification": "seize quickly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01ce6bea-ea06-3032-a4eb-633d4c8f910f",
                    "sense_definition": "Приобретать что-либо за деньги; покупать.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Где вы обычно <берёте> свежие овощи и фрукты?",
                            "target_lang_translations": {
                                "en": "Where do you usually <get> fresh vegetables and fruits?"
                            }
                        },
                        {
                            "text": "Мы решили не <брать> этот дорогой телевизор.",
                            "target_lang_translations": {
                                "en": "We decided not to <buy> this expensive TV."
                            }
                        }
                    ],
                    "synonyms": [
                        "покупать"
                    ],
                    "common_phrases": [
                        "брать билеты",
                        "брать в кредит",
                        "брать продукты"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное употребление вместо слова «покупать»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To acquire something in exchange for money; to buy."

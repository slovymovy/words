{
    "word_family": [
        "election",
        "elector",
        "electoral",
        "elective",
        "electorate",
        "reelect",
        "reelection",
        "unelected"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e3c217ed-b759-319f-9db6-718aa5e9517e",
                    "sense_definition": "To choose a candidate for a public office or other position by voting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "voting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens voted to <w>elect</w> a new mayor for the city.",
                            "target_lang_translations": {
                                "ru": "Граждане проголосовали за то, чтобы <w>избрать</w> нового мэра города."
                            }
                        },
                        {
                            "text": "They <w>elected</w> her as the chairperson of the committee.",
                            "target_lang_translations": {
                                "ru": "Они <w>избрали</w> её председателем комитета."
                            }
                        }
                    ],
                    "synonyms": [
                        "vote for",
                        "choose",
                        "select"
                    ],
                    "common_phrases": [
                        "elect a president",
                        "elect a representative",
                        "elect a candidate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбирать кандидата на государственную должность или другой пост путем голосования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избира́ть",
                                "target_lang_sense_clarification": "проводить выборы (несов. вид)"
                            },
                            {
                                "target_lang_word": "избра́ть",
                                "target_lang_sense_clarification": "выбрать голосованием (сов. вид)"
                            },
                            {
                                "target_lang_word": "выбира́ть",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3428358a-ccbe-3d8c-92c0-4ec248e6256b",
                    "sense_definition": "To make a formal choice or decision to do something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "choosing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After considering the risks, the patient <w>elected</w> to undergo the surgery.",
                            "target_lang_translations": {
                                "ru": "Взвесив риски, пациент <w>выбрал</w> проведение операции."
                            }
                        },
                        {
                            "text": "She <w>elected</w> to take early retirement rather than transfer to a new office.",
                            "target_lang_translations": {
                                "ru": "Она <w>выбрала</w> досрочный выход на пенсию, а не перевод в новый офис."
                            }
                        }
                    ],
                    "synonyms": [
                        "choose",
                        "decide",
                        "opt"
                    ],
                    "common_phrases": [
                        "elect to do",
                        "elect to take"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать официальный выбор или принять решение сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбира́ть",
                                "target_lang_sense_clarification": "делать выбор"
                            },
                            {
                                "target_lang_word": "вы́брать",
                                "target_lang_sense_clarification": "принять решение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "51623c92-6822-3af4-8974-ce363e31b151",
                    "sense_definition": "Having been voted into a specific office or position but not yet officially installed; usually placed after the noun.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president <w>elect</w> is currently selecting members for the new cabinet.",
                            "target_lang_translations": {
                                "ru": "<w>Избранный</w> президент в настоящее время подбирает членов нового кабинета."
                            }
                        },
                        {
                            "text": "She met with the governor <w>elect</w> to discuss the transition of power.",
                            "target_lang_translations": {
                                "ru": "Она встретилась с <w>избранным</w> губернатором, чтобы обсудить передачу власти."
                            }
                        }
                    ],
                    "synonyms": [
                        "incoming",
                        "designated"
                    ],
                    "common_phrases": [
                        "president elect",
                        "governor elect",
                        "bishop elect"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "postpositive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Получивший должность в результате выборов, но еще официально не вступивший в нее; обычно ставится после существительного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и́збранный",
                                "target_lang_sense_clarification": "о должностном лице"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb0a690c-d96c-3336-8cf3-b50e873eb6c4",
                    "sense_definition": "Carefully chosen or selected as the best or most preferred from a group.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Only a small, <w>elect</w> group of scientists was invited to the summit.",
                            "target_lang_translations": {
                                "ru": "Только небольшая <w>избранная</w> группа ученых была приглашена на саммит."
                            }
                        },
                        {
                            "text": "This exclusive club is open only to an <w>elect</w> circle of members.",
                            "target_lang_translations": {
                                "ru": "Этот эксклюзивный клуб открыт только для <w>избранного</w> круга членов."
                            }
                        }
                    ],
                    "synonyms": [
                        "select",
                        "chosen",
                        "elite"
                    ],
                    "common_phrases": [
                        "elect few",
                        "elect circle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно отобранный или выделенный как лучший из группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и́збранный",
                                "target_lang_sense_clarification": "элитный, лучший"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8517c6bb-4d2d-3bd1-ac33-9448d1981346",
                    "sense_definition": "People chosen by God for salvation or a specific holy purpose, particularly in Christian theology.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "theology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The preacher spoke about the eternal destiny of the <w>elect</w>.",
                            "target_lang_translations": {
                                "ru": "Проповедник говорил о вечной судьбе <w>избранных</w>."
                            }
                        },
                        {
                            "text": "He firmly believed that he was numbered among the <w>elect</w>.",
                            "target_lang_translations": {
                                "ru": "Он твердо верил, что причислен к <w>избранным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chosen ones"
                    ],
                    "antonyms": [
                        "reprobate"
                    ],
                    "common_phrases": [
                        "the elect",
                        "God's elect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, избранные Богом для спасения или особой святой цели, особенно в христианском богословии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и́збранные",
                                "target_lang_sense_clarification": "в религиозном контексте"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "4bc5d6a0-9abc-323b-970b-5c04a6fdfca9",
                    "sense_definition": "Находясь на задней стороне; за спиной кого-либо или чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_back",
                    "name_type": "no",
                    "examples": [
                        {

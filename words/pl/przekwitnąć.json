            "senses": [
                {
                    "sense_id": "7d72f3c9-8acb-3536-be57-a3a2f44da9f9",
                    "sense_definition": "O roślinach: zakończyć okres kwitnienia, zrzucić kwiaty lub zwiędnąć.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "botany_wither",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje ulubione tulipany w ogrodzie już niestety <w>przekwitły</w>.",
                            "target_lang_translations": {
                                "ru": "Мои любимые тюльпаны в саду, к сожалению, уже <w>отцвели</w>."
                            }
                        },
                        {
                            "text": "Kiedy drzewa owocowe <w>przekwitną</w>, na gałęziach zaczną pojawiać się małe owoce.",
                            "target_lang_translations": {
                                "ru": "Когда фруктовые деревья <w>отцветут</w>, на ветках начнут появляться маленькие плоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwiędnąć",
                        "obumrzeć"
                    ],
                    "antonyms": [
                        "zakwitnąć",
                        "rozkwitnąć"
                    ],
                    "common_phrases": [
                        "róże przekwitły",
                        "drzewa przekwitły"
                    ],
                    "target_lang_definitions": {
                        "ru": "О растениях: закончить цветение, сбросить цветы или увянуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отцвести",
                                "target_lang_sense_clarification": "о цветах и деревьях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c667f11b-7839-330e-801c-bda727234736",
                    "sense_definition": "Stracić dawną urodę, świeżość lub młodzieńczy wygląd; zestarzeć się.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_aging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niegdyś była wielką gwiazdą kina, ale z biegiem lat jej uroda <w>przekwitła</w>.",
                            "target_lang_translations": {
                                "ru": "Когда-то она была великой кинозвездой, но с годами её красота <w>увяла</w>."
                            }
                        },
                        {
                            "text": "Choć jej młodość dawno <w>przekwitła</w>, wciąż zachowała w sobie wiele elegancji.",
                            "target_lang_translations": {
                                "ru": "Хотя её молодость давно <w>отцвела</w>, она всё ещё сохранила в себе много элегантности."
                            }
                        }
                    ],
                    "synonyms": [
                        "zestarzeć się",
                        "zbrzydnąć"
                    ],
                    "antonyms": [
                        "rozkwitnąć",
                        "wypięknieć"
                    ],
                    "common_phrases": [
                        "uroda przekwitła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять былую красоту, свежесть или моложавый вид; постареть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увянуть",
                                "target_lang_sense_clarification": "о красоте"
                            },
                            {
                                "target_lang_word": "отцвести",
                                "target_lang_sense_clarification": "перен. утратить свежесть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cd4150f-b684-30e2-a632-cd98a8ac5f91",
                    "sense_definition": "O kobietach: przejść okres menopauzy (klimakterium), tracąc zdolność rozrodczą.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_menopause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz poinformował pacjentkę, że jej organizm już <w>przekwitł</w>.",
                            "target_lang_translations": {
                                "ru": "Врач сообщил пациентке, что у неё уже <w>наступил климакс</w>."
                            }
                        },
                        {
                            "text": "Wiele kobiet obawia się momentu, w którym <w>przekwitną</w>, ze względu na zmiany hormonalne.",
                            "target_lang_translations": {
                                "ru": "Многие женщины боятся момента, когда у них <w>наступит менопауза</w>, из-за гормональных изменений."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О женщинах: пройти период менопаузы (климакса), теряя детородную функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выйти из детородного возраста",
                                "target_lang_sense_clarification": "описательный перевод"
                            },
                            {
                                "target_lang_word": "отцвести",
                                "target_lang_sense_clarification": "разг. устар."
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "verdrukken",
        "verdrukker"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71234c9e-9b1f-3b87-8fca-f29a7a4e6ada",
                    "sense_definition": "Een situatie waarin mensen of dingen zo dicht op elkaar geperst worden dat er geen bewegingsruimte meer is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_crush",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de paniek in de menigte kwamen tientallen mensen in de <w>verdrukking</w>.",
                            "target_lang_translations": {
                                "en": "Due to the panic in the crowd, dozens of people got caught in the <w>crush</w>.",
                                "ru": "Из-за паники в толпе десятки людей попали в <w>давку</w>."
                            }
                        },
                        {
                            "text": "Bij het drukke concert raakten enkele fans vooraan in de <w>verdrukking</w>.",
                            "target_lang_translations": {
                                "en": "At the busy concert, some fans at the front got <w>crushed</w>.",
                                "ru": "На многолюдном концерте несколько фанатов в первых рядах оказались в <w>давке</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de verdrukking komen",
                        "in de verdrukking raken"
                    ],
                    "target_lang_definitions": {
                        "en": "A situation where people or things are pressed so closely together that there is no room to move.",
                        "ru": "Ситуация, когда люди или предметы прижаты друг к другу так плотно, что нет места для движения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crush",
                                "target_lang_sense_clarification": "physical pressure in a crowd"
                            },
                            {
                                "target_lang_word": "squeeze",
                                "target_lang_sense_clarification": "tight crowding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "давка",
                                "target_lang_sense_clarification": "скопление людей"
                            },
                            {
                                "target_lang_word": "теснота",
                                "target_lang_sense_clarification": "нехватка места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0927cc3-b62f-3c0f-8b5d-9907f2595898",
                    "sense_definition": "Het onrechtvaardig en wreed overheersen of onderwerpen van een groep mensen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "oppression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wrede heerser hield zijn volk jarenlang in de <w>verdrukking</w>.",
                            "target_lang_translations": {
                                "en": "The cruel ruler kept his people under <w>oppression</w> for years.",
                                "ru": "Жестокий правитель годами держал свой народ в <w>угнетении</w>."
                            }
                        },
                        {
                            "text": "Na een lange periode van <w>verdrukking</w> kwam de bevolking eindelijk in opstand.",
                            "target_lang_translations": {
                                "en": "After a long period of <w>oppression</w>, the population finally revolted.",
                                "ru": "После долгого периода <w>угнетения</w> население наконец восстало."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderdrukking",
                        "dwingelandij"
                    ],
                    "target_lang_definitions": {
                        "en": "The unjust and cruel domination or subjugation of a group of people.",
                        "ru": "Несправедливое и жестокое господство над группой людей или их подчинение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oppression",
                                "target_lang_sense_clarification": "cruel exercise of authority"
                            },
                            {
                                "target_lang_word": "tyranny",
                                "target_lang_sense_clarification": "cruel and oppressive government"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "угнетение",
                                "target_lang_sense_clarification": "жестокое обращение"
                            },
                            {
                                "target_lang_word": "притеснение",
                                "target_lang_sense_clarification": "ограничение прав"
                            },
                            {
                                "target_lang_word": "гнёт",
                                "target_lang_sense_clarification": "тяжелая власть"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "ef16ed2b-9ba2-3a5e-a1de-36bf3bec0fd9",
                    "sense_definition": "Процесс действия по глаголу «ходить»; передвижение пешком, часто длительное или многократное.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полезное для здоровья <w>хождение</w> босиком по траве укрепляет иммунитет.",
                            "target_lang_translations": {
                                "en": "Healthy <w>walking</w> barefoot on the grass strengthens the immune system."
                            }
                        },
                        {
                            "text": "Его долгое <w>хождение</w> по кабинетам чиновников наконец принесло результат.",
                            "target_lang_translations": {
                                "en": "His long <w>walking</w> through officials' offices finally brought results."
                            }
                        }
                    ],
                    "synonyms": [
                        "ходьба"
                    ],
                    "common_phrases": [
                        "хождение по мукам",
                        "хождение босиком",
                        "хождение по врачам",
                        "хождение в народ"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of walking; movement on foot, often prolonged or repeated."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walking",
                                "target_lang_sense_clarification": "general act of moving on foot"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e301cbc-7330-36ac-b2c0-4c3b13dc0062",
                    "sense_definition": "Оборот, циркуляция и использование денежных средств или ценных бумаг в экономике.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой курортной зоне иностранная валюта имеет свободное <w>хождение</w>.",
                            "target_lang_translations": {
                                "en": "In this resort zone, foreign currency has free <w>circulation</w>."
                            }
                        },
                        {
                            "text": "Старые бумажные купюры были постепенно изъяты из <w>хождения</w>.",
                            "target_lang_translations": {
                                "en": "Old paper notes were gradually withdrawn from <w>circulation</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "обращение"
                    ],
                    "common_phrases": [
                        "свободное хождение",
                        "иметь хождение",
                        "изъять из хождения",
                        "хождение валюты"
                    ],
                    "target_lang_definitions": {
                        "en": "The turnover, circulation, and use of money or securities in the economy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circulation",
                                "target_lang_sense_clarification": "of money or currency"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85ce81d7-1d8d-3841-9982-c0c67d432768",
                    "sense_definition": "Путешествие или паломничество (часто используется в названиях старинных литературных произведений).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Знаменитое «<w>Хождение</w> за три моря» Афанасия Никитина описывает его путь в Индию.",
                            "target_lang_translations": {
                                "en": "Afanasy Nikitin's famous '<w>Journey</w> Beyond Three Seas' describes his path to India."
                            }
                        },
                        {
                            "text": "Древний монах описал своё долгое <w>хождение</w> в святые земли в летописи.",
                            "target_lang_translations": {
                                "en": "The ancient monk described his long <w>pilgrimage</w> to the holy lands in a chronicle."
                            }
                        }
                    ],
                    "synonyms": [
                        "странствие",
                        "паломничество",
                        "путешествие"
                    ],
                    "common_phrases": [
                        "хождение за три моря",
                        "жанр хождения"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary genre or historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A journey or pilgrimage (often used in the titles of old literary works)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "journey",
                                "target_lang_sense_clarification": "literary or archaic"
                            },
                            {
                                "target_lang_word": "pilgrimage",
                                "target_lang_sense_clarification": "religious journey"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0cc1b6e-b3c5-3b7e-8cbc-a2c512420fa7",
                    "sense_definition": "Уход, забота и присмотр за кем-либо (обычно за больным или ребёнком).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Постоянное <w>хождение</w> за больным родственником требовало от неё много сил.",
                            "target_lang_translations": {
                                "en": "Constant <w>caring</w> for her sick relative required a lot of strength from her."
                            }
                        },
                        {
                            "text": "В обязанности няни входило <w>хождение</w> за детьми и обучение их грамоте.",
                            "target_lang_translations": {
                                "en": "The nanny's duties included <w>looking after</w> the children and teaching them literacy."
                            }
                        }
                    ],
                    "common_phrases": [
                        "хождение за больными"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "replaced by 'уход' in modern Russian"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Care, looking after, and supervision of someone (usually a sick person or a child)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "care",
                                "target_lang_sense_clarification": "looking after someone"
                            },
                            {
                                "target_lang_word": "nursing",
                                "target_lang_sense_clarification": "caring for the sick"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "799b523c-c7a5-38b7-93a7-0d80347b71bd",
                    "sense_definition": "Вопросительное слово, обозначающее образ действия, способ или состояние (каким образом?).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adv_interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Как</w> вас зову́т?"
                        },
                        {
                            "text": "Я не зна́ю, <w>как</w> э́то рабо́тает."
                        },
                        {
                            "text": "<w>Как</w> вы себя́ чу́вствуете сего́дня?"
                        }
                    ],
                    "synonyms": [
                        "каким образом"
                    ],
                    "common_phrases": [
                        "как дела",
                        "как обычно",
                        "как правило",
                        "как раз",
                        "как можно скорее"
                    ]
                },
                {
                    "sense_id": "90d7a4b5-9b19-346f-8307-304609f5a5f1",
                    "sense_definition": "Слово, используемое в восклицательных предложениях для обозначения высокой степени качества или действия.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adv_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Как</w> здо́рово мы провели́ вре́мя!"
                        },
                        {
                            "text": "<w>Как</w> жаль, что ты не пришёл."
                        },
                        {
                            "text": "<w>Как</w> бы́стро лети́т вре́мя!"
                        }
                    ],
                    "synonyms": [
                        "насколько",
                        "до чего"
                    ],
                    "common_phrases": [
                        "как жаль",
                        "как странно",
                        "как красиво"
                    ]
                },
                {
                    "sense_id": "26f89840-0a9c-38f6-ad12-0e731b061b19",
                    "sense_definition": "Насколько, в какой степени (о мере и степени).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adv_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Как</w> далеко́ отсю́да до вокза́ла?"
                        },
                        {
                            "text": "Я не знал, <w>как</w> э́то ва́жно для них."
                        }
                    ],
                    "synonyms": [
                        "насколько"
                    ],
                    "common_phrases": [
                        "как долго",
                        "как много",
                        "как часто"
                    ]
                },
                {
                    "sense_id": "3df25467-7963-3770-9383-0ef54fe9e8bc",
                    "sense_definition": "В сочетании с частицей «ни» выражает уступку: сколько бы ни, в какой бы степени ни.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adv_concessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Как</w> ни стра́нно, он оказа́лся прав."
                        },
                        {
                            "text": "<w>Как</w> он ни стара́лся, ничего́ не выходи́ло."
                        }
                    ],
                    "common_phrases": [
                        "как ни странно",
                        "как ни крути",
                        "как бы то ни было"
                    ]
                },
                {
                    "sense_id": "6c474e5b-fa86-3e5e-be0d-131e08144cc4",
                    "sense_definition": "Внезапно, неожиданно (обычно при описании прошедших событий для придания динамичности).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adv_narrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он сиде́л ти́хо, а пото́м <w>как</w> закричи́т!"
                        },
                        {
                            "text": "Заяц <w>как</w> вы́скочит из кусто́в прямо нам под но́ги."
                        }
                    ],
                    "synonyms": [
                        "вдруг",
                        "внезапно"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in lively storytelling"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "631b045e-ae28-3825-93a2-9d318af2c833",
                    "sense_definition": "Союз, используемый для сравнения; указывает на сходство с кем-либо или чем-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "conj_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она́ говори́т по-ру́сски <w>как</w> носи́тель языка́."
                        },
                        {
                            "text": "Вода́ в о́зере была́ прозра́чная <w>как</w> стекло́."
                        },
                        {
                            "text": "Бу́дьте <w>как</w> до́ма."
                        }
                    ],
                    "synonyms": [
                        "словно",
                        "будто",
                        "точно"
                    ],
                    "common_phrases": [
                        "белый как снег",
                        "нужен как воздух",
                        "как две капли воды"
                    ]
                },
                {
                    "sense_id": "3dfb174b-4a82-3b99-a122-fec42fc9ba27",
                    "sense_definition": "Союз, присоединяющий придаточное предложение к глаголам восприятия (видеть, слышать, чувствовать) или мысли.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conj_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я ви́дел, <w>как</w> она́ выходи́ла из до́ма."
                        },
                        {
                            "text": "Мы слы́шали, <w>как</w> игра́ла му́зыка в сосе́дней ко́мнате."
                        }
                    ],
                    "common_phrases": [
                        "видеть как",
                        "слышать как",
                        "смотреть как"
                    ]
                },
                {
                    "sense_id": "484befe0-a4b6-39f1-9203-15d93ea85616",
                    "sense_definition": "Союз, указывающий на роль, функцию или качество, в котором выступает лицо или предмет.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conj_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я говорю́ с ва́ми <w>как</w> врач."
                        },
                        {
                            "text": "Э́то зда́ние изве́стно <w>как</w> са́мое высо́кое в го́роде."
                        }
                    ],
                    "synonyms": [
                        "в качестве"
                    ],
                    "common_phrases": [
                        "известен как",
                        "работать как",
                        "использовать как"
                    ]
                },
                {
                    "sense_id": "d168d3bf-2272-3117-98ba-8c503b77d417",
                    "sense_definition": "Союз, указывающий на время совершения действия; в то время как, когда.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conj_temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я позвоню́ тебе́, <w>как</w> то́лько прие́ду."
                        },
                        {
                            "text": "Прошёл год с тех пор, <w>как</w> мы ви́делись."
                        }
                    ],
                    "synonyms": [
                        "когда"
                    ],
                    "common_phrases": [
                        "как только",
                        "с тех пор как",
                        "перед тем как",
                        "после того как",
                        "в то время как"
                    ]
                },
                {
                    "sense_id": "85d652ed-e8ed-3868-9e8b-c6b47a5f3b7b",
                    "sense_definition": "Часть составного союза «как..., так и...», выражающего объединение (и..., и...).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conj_correlative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Э́тот фильм нра́вится <w>как</w> де́тям, так и взро́слым."
                        },
                        {
                            "text": "В меню́ есть <w>как</w> мя́сные, так и вегетариа́нские блю́да."
                        }
                    ],
                    "common_phrases": [
                        "как так и"
                    ]
                },
                {
                    "sense_id": "18666e02-c00d-3c47-837b-7c274cdb931f",
                    "sense_definition": "По причине того что; так как.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conj_causal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "И <w>как</w> он до́лго не приходи́л, мы на́чали обе́дать без него́."
                        }
                    ],
                    "synonyms": [
                        "так как",
                        "поскольку"
                    ],
                    "common_phrases": [
                        "так как"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly found in literature; modern Russian prefers 'так как'"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "dfed28e7-ea16-372a-8c2e-53aa9c2ae179",
                    "sense_definition": "Употребляется для выражения сильного удивления, недоверия или возмущения.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "part_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Как</w>, вы ещё не ушли́?"
                        },
                        {
                            "text": "<w>Как</w>, ты не зна́ешь об э́том?"
                        }
                    ],
                    "synonyms": [
                        "разве",
                        "неужели"
                    ]
                }
            ]
        }
    ]
}
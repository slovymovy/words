{
    "word_family": [
        "valuable",
        "valuation",
        "valueless",
        "evaluate",
        "evaluation",
        "revalue",
        "devalue",
        "undervalue",
        "overvalue"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dea65738-d852-3c3a-8750-57d4a9762959",
                    "sense_definition": "The amount of money or goods that is considered to be a fair equivalent for something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "monetary_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The total <w>value</w> of the stolen jewelry is over one million dollars.",
                            "target_lang_translations": {
                                "ru": "Общая <w>стоимость</w> украденных драгоценностей составляет более миллиона долларов."
                            }
                        },
                        {
                            "text": "This car offers excellent <w>value</w> for money compared to other models.",
                            "target_lang_translations": {
                                "ru": "Этот автомобиль предлагает отличное <w>соотношение цены и качества</w> по сравнению с другими моделями."
                            }
                        }
                    ],
                    "synonyms": [
                        "worth"
                    ],
                    "common_phrases": [
                        "market value",
                        "face value",
                        "value for money",
                        "drop in value"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная сумма или эквивалент, который считается справедливой платой за что-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоимость",
                                "target_lang_sense_clarification": "денежное выражение ценности"
                            },
                            {
                                "target_lang_word": "цена",
                                "target_lang_sense_clarification": "сумма денег за товар"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00a32d5b-d73d-3349-b7e1-c574943f0238",
                    "sense_definition": "The quality of being useful, important, or desirable.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "importance_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her advice was of great <w>value</w> to me during the project.",
                            "target_lang_translations": {
                                "ru": "Ее совет имел для меня огромную <w>ценность</w> во время работы над проектом."
                            }
                        },
                        {
                            "text": "We place a high <w>value</w> on honesty and hard work.",
                            "target_lang_translations": {
                                "ru": "Мы придаем большое <w>значение</w> честности и трудолюбию."
                            }
                        }
                    ],
                    "synonyms": [
                        "worth"
                    ],
                    "common_phrases": [
                        "of great value",
                        "place value on",
                        "good value"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство быть полезным, важным или желаемым"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ценность",
                                "target_lang_sense_clarification": "важность, значимость"
                            },
                            {
                                "target_lang_word": "значение",
                                "target_lang_sense_clarification": "роль, важность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63939064-e066-386c-868b-044e62dd1850",
                    "sense_definition": "Beliefs about what is right and wrong or what is important in life.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "principles_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They share the same moral <w>values</w> regarding family and community.",
                            "target_lang_translations": {
                                "ru": "Они разделяют одни и те же моральные <w>ценности</w> в отношении семьи и общества."
                            }
                        },
                        {
                            "text": "The school aims to teach traditional <w>values</w> to its students.",
                            "target_lang_translations": {
                                "ru": "Школа стремится привить своим ученикам традиционные <w>ценности</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "family values",
                        "moral values",
                        "core values",
                        "cultural values"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used in plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убеждения о том, что правильно и неправильно, или о том, что важно в жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ценности",
                                "target_lang_sense_clarification": "моральные принципы, идеалы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b07c517d-c540-3173-a60d-06e357d7145a",
                    "sense_definition": "A numerical quantity or result determined by calculation or measurement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "math_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to find the exact <w>value</w> of x to solve the equation.",
                            "target_lang_translations": {
                                "ru": "Вам нужно найти точное <w>значение</w> x, чтобы решить уравнение."
                            }
                        },
                        {
                            "text": "The numerical <w>value</w> of pi is approximately 3.14.",
                            "target_lang_translations": {
                                "ru": "Числовое <w>значение</w> пи составляет приблизительно 3,14."
                            }
                        }
                    ],
                    "common_phrases": [
                        "absolute value",
                        "numerical value"
                    ],
                    "target_lang_definitions": {
                        "ru": "Числовая величина или результат, полученный путем вычисления или измерения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "значение",
                                "target_lang_sense_clarification": "числовая характеристика"
                            },
                            {
                                "target_lang_word": "величина",
                                "target_lang_sense_clarification": "размер, количественный показатель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "215fb432-d928-3845-9d50-7ca59b64ec79",
                    "sense_definition": "The relative darkness or lightness of a color.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "art_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used different color <w>values</w> to create a sense of depth.",
                            "target_lang_translations": {
                                "ru": "Художник использовал различные цветовые <w>тона</w>, чтобы создать ощущение глубины."
                            }
                        },
                        {
                            "text": "Adding white to a color changes its tonal <w>value</w>.",
                            "target_lang_translations": {
                                "ru": "Добавление белого к цвету меняет его тональную <w>светлоту</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tonal value",
                        "color value"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительная темнота или светлота цвета"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тон",
                                "target_lang_sense_clarification": "цветовой оттенок"
                            },
                            {
                                "target_lang_word": "светлота",
                                "target_lang_sense_clarification": "степень яркости цвета"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b7545602-af3a-3e0d-b985-7fc020bfefe2",
                    "sense_definition": "To consider someone or something to be important or beneficial.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "importance_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>value</w> your friendship more than you know.",
                            "target_lang_translations": {
                                "ru": "Я <w>ценю</w> твою дружбу больше, чем ты думаешь."
                            }
                        },
                        {
                            "text": "The company <w>values</w> employees who are creative and dedicated.",
                            "target_lang_translations": {
                                "ru": "Компания <w>ценит</w> творческих и преданных делу сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "appreciate",
                        "esteem",
                        "prize",
                        "respect",
                        "treasure"
                    ],
                    "antonyms": [
                        "despise",
                        "disrespect",
                        "undervalue"
                    ],
                    "common_phrases": [
                        "value highly",
                        "value advice",
                        "value opinion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать кого-либо или что-либо важным или полезным"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ценить",
                                "target_lang_sense_clarification": "признавать достоинства"
                            },
                            {
                                "target_lang_word": "дорожить",
                                "target_lang_sense_clarification": "беречь, бояться потерять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9727dbac-2eda-33a3-a3c6-a6fa056d7cba",
                    "sense_definition": "To estimate or determine the monetary worth of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "monetary_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The estate agent <w>valued</w> the house at half a million pounds.",
                            "target_lang_translations": {
                                "ru": "Агент по недвижимости <w>оценил</w> дом в полмиллиона фунтов."
                            }
                        },
                        {
                            "text": "They brought in an expert to <w>value</w> the antique furniture.",
                            "target_lang_translations": {
                                "ru": "Они пригласили эксперта, чтобы <w>оценить</w> антикварную мебель."
                            }
                        }
                    ],
                    "synonyms": [
                        "assess",
                        "evaluate",
                        "rate",
                        "valuate"
                    ],
                    "common_phrases": [
                        "value at",
                        "value property"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать или определять денежную стоимость чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "определять цену"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "84f9012f-c783-3338-8446-93e41d9a440d",
                    "sense_definition": "Naar de zijkant of aan de kant; weg van het midden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij legde het boek <w>terzijde</w> om te gaan slapen.",
                            "target_lang_translations": {
                                "en": "He put the book <w>aside</w> to go to sleep.",
                                "ru": "Он отложил книгу <w>в сторону</w>, чтобы лечь спать."
                            }
                        },
                        {
                            "text": "De vrouw stapte <w>terzijde</w> om de fietser door te laten.",
                            "target_lang_translations": {
                                "en": "The woman stepped <w>aside</w> to let the cyclist pass.",
                                "ru": "Женщина отошла <w>в сторону</w>, чтобы пропустить велосипедиста."
                            }
                        }
                    ],
                    "synonyms": [
                        "opzij",
                        "weg"
                    ],
                    "antonyms": [
                        "centraal"
                    ],
                    "common_phrases": [
                        "terzijde leggen",
                        "terzijde schuiven",
                        "terzijde werpen"
                    ],
                    "target_lang_definitions": {
                        "en": "To the side or out of the way; away from the center.",
                        "ru": "В сторону или прочь с дороги; прочь от центра."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aside",
                                "target_lang_sense_clarification": "to one side; out of the way"
                            },
                            {
                                "target_lang_word": "to the side",
                                "target_lang_sense_clarification": "physical movement away from the center"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в сторону",
                                "target_lang_sense_clarification": "физическое движение от центра"
                            },
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "удаление от чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ee77c7e-0bdf-3eae-9331-9304e36cf521",
                    "sense_definition": "Iemand helpen of steunen, vaak gebruikt in een vaste uitdrukking.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De advocaat stond de verdachte <w>terzijde</w> tijdens het proces.",
                            "target_lang_translations": {
                                "en": "The lawyer <w>assisted</w> the defendant during the trial.",
                                "ru": "Адвокат <w>поддерживал</w> обвиняемого во время процесса."
                            }
                        },
                        {
                            "text": "Zijn vrienden hebben hem in moeilijke tijden <w>terzijde</w> gestaan.",
                            "target_lang_translations": {
                                "en": "His friends <w>stood by</w> him in difficult times.",
                                "ru": "Его друзья <w>поддержали</w> его в трудные времена."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijstaan",
                        "helpen",
                        "steunen"
                    ],
                    "common_phrases": [
                        "iemand terzijde staan"
                    ],
                    "target_lang_definitions": {
                        "en": "To help or support someone, often used in the phrase 'iemand terzijde staan'.",
                        "ru": "Помогать или поддерживать кого-либо, часто используется в выражении 'iemand terzijde staan'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand by",
                                "target_lang_sense_clarification": "to support or remain loyal to someone"
                            },
                            {
                                "target_lang_word": "assist",
                                "target_lang_sense_clarification": "to help formally or professionally"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "оказывать помощь или быть лояльным"
                            },
                            {
                                "target_lang_word": "помогать",
                                "target_lang_sense_clarification": "оказывать содействие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b61f5b9b-e9ab-3147-a26e-4278640c12eb",
                    "sense_definition": "Als een opmerking tussendoor, zonder er diep op in te gaan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit <w>terzijde</w>, we moeten ons nu richten op het hoofdprobleem.",
                            "target_lang_translations": {
                                "en": "This <w>aside</w>, we must now focus on the main problem.",
                                "ru": "Оставим это <w>в стороне</w>, сейчас мы должны сосредоточиться на главной проблеме."
                            }
                        },
                        {
                            "text": "Hij merkte <w>terzijde</w> op dat de vergadering bijna voorbij was.",
                            "target_lang_translations": {
                                "en": "He remarked <w>in passing</w> that the meeting was almost over.",
                                "ru": "Он заметил <w>мимоходом</w>, что собрание почти закончилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "terloops",
                        "en passant"
                    ],
                    "antonyms": [
                        "uitvoerig"
                    ],
                    "common_phrases": [
                        "dit terzijde"
                    ],
                    "target_lang_definitions": {
                        "en": "As an incidental remark, not the main point.",
                        "ru": "Как попутное замечание, не углубляясь в суть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aside",
                                "target_lang_sense_clarification": "used to indicate a digression (e.g., 'joking aside')"
                            },
                            {
                                "target_lang_word": "in passing",
                                "target_lang_sense_clarification": "briefly and casually"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "между прочим",
                                "target_lang_sense_clarification": "кратко и попутно"
                            },
                            {
                                "target_lang_word": "к слову",
                                "target_lang_sense_clarification": "как попутное замечание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "85a8d82f-df38-3179-ac8d-1e334c01bcdb",
                    "sense_definition": "Een opmerking die tussendoor wordt gemaakt en afwijkt van het hoofdonderwerp.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik een klein <w>terzijde</w> maken over dit onderwerp?",
                            "target_lang_translations": {
                                "en": "May I make a small <w>aside</w> about this subject?",
                                "ru": "Могу я сделать небольшое <w>отступление</w> по этой теме?"
                            }
                        },
                        {
                            "text": "Zijn toespraak zat vol met grappige <w>terzijdes</w>.",
                            "target_lang_translations": {
                                "en": "His speech was full of funny <w>asides</w>.",
                                "ru": "Его речь была полна забавных <w>отступлений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijsprong",
                        "tussendoortje"
                    ],
                    "target_lang_definitions": {
                        "en": "A remark made in passing that deviates from the main topic.",
                        "ru": "Замечание, сделанное между делом, отклоняющееся от основной темы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aside",
                                "target_lang_sense_clarification": "a remark not directly related to the main subject"
                            },
                            {
                                "target_lang_word": "side note",
                                "target_lang_sense_clarification": "an incidental comment"
                            },
                            {
                                "target_lang_word": "digression",
                                "target_lang_sense_clarification": "a temporary departure from the main subject"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отступление",
                                "target_lang_sense_clarification": "временный отход от главной темы"
                            },
                            {
                                "target_lang_word": "ремарка",
                                "target_lang_sense_clarification": "попутное замечание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa0c6929-b8c3-3eb9-9465-f13cf07f2cb8",
                    "sense_definition": "Tekst in een toneelstuk die tot het publiek gericht wordt zonder dat de andere spelers dit horen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In een <w>terzijde</w> onthulde de acteur zijn ware plannen aan de zaal.",
                            "target_lang_translations": {
                                "en": "In an <w>aside</w>, the actor revealed his true plans to the audience.",
                                "ru": "В <w>реплике в сторону</w> актер раскрыл залу свои истинные планы."
                            }
                        },
                        {
                            "text": "De schrijver maakt veel gebruik van het <w>terzijde</w> om gedachten weer te geven.",
                            "target_lang_translations": {
                                "en": "The writer makes extensive use of the <w>aside</w> to convey thoughts.",
                                "ru": "Писатель широко использует <w>реплики в сторону</w> для передачи мыслей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A line spoken by an actor to the audience that other characters are not supposed to hear.",
                        "ru": "Реплика в пьесе, обращенная к зрителям, которую не слышат другие персонажи."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aside",
                                "target_lang_sense_clarification": "theatrical term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "реплика в сторону",
                                "target_lang_sense_clarification": "театральный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
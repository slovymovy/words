{
    "word_family": [
        "привет",
        "приветствие",
        "приветливый",
        "поприветствовать"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "802ada7c-a14a-3503-bb62-261ae0ed14ae",
                    "sense_definition": "Здороваться, выражать слова приветствия или дружелюбия при встрече.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель вошёл в класс и громко <приветствовал> своих учеников.",
                            "target_lang_translations": {
                                "en": "The teacher entered the classroom and loudly <greeted> his students."
                            }
                        },
                        {
                            "text": "Мы всегда радостно <приветствуем> гостей в нашем доме.",
                            "target_lang_translations": {
                                "en": "We always joyfully <welcome> guests in our home."
                            }
                        }
                    ],
                    "synonyms": [
                        "привечать"
                    ],
                    "antonyms": [
                        "прощаться"
                    ],
                    "common_phrases": [
                        "тепло приветствовать",
                        "приветствовать гостей",
                        "приветствовать друг друга"
                    ],
                    "target_lang_definitions": {
                        "en": "To say hello or express friendliness when meeting someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "greet",
                                "target_lang_sense_clarification": "to address with expressions of kind wishes upon meeting"
                            },
                            {
                                "target_lang_word": "welcome",
                                "target_lang_sense_clarification": "to receive a guest or newcomer with pleasure or hospitality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f768cd08-4204-361a-abe0-240a14cbfad9",
                    "sense_definition": "Одобрять, выражать согласие и поддержку какой-либо идее, решению или начинанию.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "approval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Руководство компании <приветствует> желание сотрудников учиться новому.",
                            "target_lang_translations": {
                                "en": "The company management <welcomes> the employees' desire to learn new things."
                            }
                        },
                        {
                            "text": "Я полностью <приветствую> ваше решение начать этот сложный проект.",
                            "target_lang_translations": {
                                "en": "I fully <welcome> your decision to start this complex project."
                            }
                        }
                    ],
                    "synonyms": [
                        "поддержать"
                    ],
                    "antonyms": [
                        "критиковать"
                    ],
                    "common_phrases": [
                        "приветствовать решение",
                        "приветствовать инициативу",
                        "всячески приветствовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To express approval, agreement, or support for an idea, decision, or undertaking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "welcome",
                                "target_lang_sense_clarification": "to accept a decision or idea willingly and gladly"
                            },
                            {
                                "target_lang_word": "hail",
                                "target_lang_sense_clarification": "to praise or acclaim enthusiastically (often used for public decisions or achievements)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b916cd4-6970-3954-906b-9c408d2c417f",
                    "sense_definition": "Отдавать воинскую честь в знак уважения (в военной среде).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты выстроились на плацу, чтобы <приветствовать> генерала.",
                            "target_lang_translations": {
                                "en": "The soldiers lined up on the parade ground to <salute> the general."
                            }
                        },
                        {
                            "text": "Молодой офицер <приветствовал> командира, приложив руку к фуражке.",
                            "target_lang_translations": {
                                "en": "The young officer <saluted> the commander, raising his hand to his cap."
                            }
                        }
                    ],
                    "synonyms": [
                        "салютовать",
                        "отдавать честь"
                    ],
                    "common_phrases": [
                        "приветствовать командира",
                        "приветствовать по уставу"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a formal gesture of respect, typically in a military context."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "salute",
                                "target_lang_sense_clarification": "to make a formal military gesture of respect"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
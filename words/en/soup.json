    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bf464b37-ec9a-3111-98be-2c85ec35a018",
                    "sense_definition": "A liquid dish made by boiling ingredients such as meat, vegetables, or fish in stock or water.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She prepared a warm vegetable <w>soup</w> for lunch.",
                            "target_lang_translations": {
                                "ru": "Она приготовила на обед теплый овощной <w>суп</w>."
                            }
                        },
                        {
                            "text": "I would like a bowl of tomato <w>soup</w> with bread.",
                            "target_lang_translations": {
                                "ru": "Я бы хотел тарелку томатного <w>супа</w> с хлебом."
                            }
                        }
                    ],
                    "synonyms": [
                        "broth",
                        "stew",
                        "chowder"
                    ],
                    "common_phrases": [
                        "chicken soup",
                        "tomato soup",
                        "vegetable soup",
                        "bowl of soup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкое блюдо, приготовленное путем варки ингредиентов (мяса, овощей или рыбы) в бульоне или воде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суп",
                                "target_lang_sense_clarification": "общее название жидких блюд"
                            },
                            {
                                "target_lang_word": "похлёбка",
                                "target_lang_sense_clarification": "простой, часто густой суп"
                            },
                            {
                                "target_lang_word": "первое",
                                "target_lang_sense_clarification": "разг. первое блюдо обеда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52f77b74-9781-3c58-8d11-8f1da5f575ea",
                    "sense_definition": "A difficult, unfortunate, or chaotic situation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He really landed himself in the <w>soup</w> by losing the company credit card.",
                            "target_lang_translations": {
                                "ru": "Он действительно попал в <w>переплёт</w>, потеряв корпоративную кредитную карту."
                            }
                        },
                        {
                            "text": "If the boss finds out about the mistake, we will all be in the <w>soup</w>.",
                            "target_lang_translations": {
                                "ru": "Если босс узнает об ошибке, мы все попадем в <w>переделку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mess",
                        "trouble",
                        "predicament",
                        "fix"
                    ],
                    "common_phrases": [
                        "in the soup"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудная, неудачная или хаотичная ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переделка",
                                "target_lang_sense_clarification": "разг. трудное положение"
                            },
                            {
                                "target_lang_word": "переплёт",
                                "target_lang_sense_clarification": "запутанная, неприятная история"
                            },
                            {
                                "target_lang_word": "беда",
                                "target_lang_sense_clarification": "неприятность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7decb563-9736-3e6a-abd4-cedc459c3c4a",
                    "sense_definition": "A liquid mixture of organic compounds believed to be the origin of life on Earth.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "scientific_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists believe life emerged from a primordial <w>soup</w> billions of years ago.",
                            "target_lang_translations": {
                                "ru": "Ученые полагают, что жизнь возникла из первичного <w>бульона</w> миллиарды лет назад."
                            }
                        },
                        {
                            "text": "The experiment attempted to recreate the chemical conditions of the primordial <w>soup</w>.",
                            "target_lang_translations": {
                                "ru": "В ходе эксперимента пытались воссоздать химические условия первичного <w>бульона</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "primordial soup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкая смесь органических соединений, считающаяся источником жизни на Земле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бульон",
                                "target_lang_sense_clarification": "в термине «первичный бульон»"
                            },
                            {
                                "target_lang_word": "суп",
                                "target_lang_sense_clarification": "реже, в термине «первичный суп»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "745c7f30-3835-3911-9919-21b8651f0dcf",
                    "sense_definition": "A very thick fog or cloud.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot struggled to see the runway through the grey <w>soup</w>.",
                            "target_lang_translations": {
                                "ru": "Пилот с трудом различал взлетно-посадочную полосу сквозь серую <w>муть</w>."
                            }
                        },
                        {
                            "text": "London was covered in a thick pea <w>soup</w> that stopped all traffic.",
                            "target_lang_translations": {
                                "ru": "Лондон был окутан густым <w>туманом</w>, который остановил всё движение."
                            }
                        }
                    ],
                    "synonyms": [
                        "fog",
                        "mist"
                    ],
                    "common_phrases": [
                        "pea soup"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень густой туман или облачность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туман",
                                "target_lang_sense_clarification": "густая атмосферная дымка"
                            },
                            {
                                "target_lang_word": "молоко",
                                "target_lang_sense_clarification": "перен. о непроглядном тумане"
                            },
                            {
                                "target_lang_word": "муть",
                                "target_lang_sense_clarification": "разг. о плохой видимости"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        "wokalistyka"
                    ],
                    "antonyms": [
                        "mowa",
                        "milczenie"
                    ],
                    "common_phrases": [
                        "nauka śpiewu",
                        "śpiew chóralny",
                        "lekcja śpiewu",
                        "czysty śpiew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавание мелодичных звуков голосом; вокальное искусство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пение",

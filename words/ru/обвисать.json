{
    "word_family": [
        "обвисание",
        "обвислый",
        "обвислость"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2cb18d3d-4a84-3e66-a2d1-97054793f38e",
                    "sense_definition": "Опускаться вниз, теряя натяжение или форму под собственной тяжестью (о предметах, ветках, проводах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "loss_of_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Под тяжестью мокрого снега ветки деревьев начали <w>обвисать</w>.",
                            "target_lang_translations": {
                                "en": "Under the weight of the wet snow, the tree branches began to <w>droop</w>."
                            }
                        },
                        {
                            "text": "Старая бельевая верёвка растянулась и стала <w>обвисать</w> посередине.",
                            "target_lang_translations": {
                                "en": "The old clothesline stretched out and began to <w>sag</w> in the middle."
                            }
                        }
                    ],
                    "synonyms": [
                        "провисать",
                        "свисать"
                    ],
                    "antonyms": [
                        "натягиваться"
                    ],
                    "common_phrases": [
                        "ветки обвисают",
                        "провода обвисают",
                        "одежда обвисает"
                    ],
                    "target_lang_definitions": {
                        "en": "To hang down loosely or lose tension under its own weight (of objects, branches, wires)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sag",
                                "target_lang_sense_clarification": "lose tension or shape"
                            },
                            {
                                "target_lang_word": "droop",
                                "target_lang_sense_clarification": "hang downwards limply"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ab39c25-e657-3897-8c76-aaac8ad59c4a",
                    "sense_definition": "Становиться дряблым, лишаться упругости (о коже, мышцах или частях тела).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "loss_of_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если очень быстро похудеть, кожа может <w>обвисать</w> складками.",
                            "target_lang_translations": {
                                "en": "If you lose weight very quickly, the skin may <w>sag</w> in folds."
                            }
                        },
                        {
                            "text": "С возрастом щёки начинают <w>обвисать</w>, меняя овал лица.",
                            "target_lang_translations": {
                                "en": "With age, cheeks begin to <w>sag</w>, changing the shape of the face."
                            }
                        }
                    ],
                    "synonyms": [
                        "дрябнуть"
                    ],
                    "antonyms": [
                        "подтягиваться"
                    ],
                    "common_phrases": [
                        "кожа обвисает",
                        "щёки обвисают",
                        "живот обвисает"
                    ],
                    "target_lang_definitions": {
                        "en": "To become loose, flabby, or lose firmness (of skin, muscles, or body parts)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sag",
                                "target_lang_sense_clarification": "skin or body parts"
                            },
                            {
                                "target_lang_word": "become flabby",
                                "target_lang_sense_clarification": "lose muscle tone"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
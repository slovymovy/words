    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "63c72164-4c46-32e4-8b27-43365384f55f",
                    "sense_definition": "Так, что никто не видит и не замечает; скрытно, стараясь не привлекать внимания.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он постарался <w>незаметно</w> выйти из комнаты, чтобы никого не разбудить.",
                            "target_lang_translations": {
                                "en": "He tried to leave the room <w>unnoticed</w> so as not to wake anyone."
                            }
                        },
                        {
                            "text": "В сумерки он <w>незаметно</w> исчез, захватив с собой ключ от двери.",
                            "target_lang_translations": {
                                "en": "In the twilight, he disappeared <w>unnoticed</w>, taking the door key with him."
                            }
                        }
                    ],
                    "synonyms": [
                        "скрытно",
                        "украдкой",
                        "неприметно"
                    ],
                    "antonyms": [
                        "заметно"
                    ],
                    "common_phrases": [
                        "пройти незаметно",
                        "сделать незаметно",
                        "незаметно исчезнуть",
                        "незаметно подойти"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that no one sees or notices; secretly, trying not to attract attention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unnoticed",
                                "target_lang_sense_clarification": "without being seen or observed"
                            },
                            {
                                "target_lang_word": "unobtrusively",
                                "target_lang_sense_clarification": "without attracting attention"
                            },
                            {
                                "target_lang_word": "secretly",
                                "target_lang_sense_clarification": "in a hidden manner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "432aad96-6b91-3af9-883c-df22d85e4443",
                    "sense_definition": "Постепенно, так, что трудно ощутить изменения или уловить течение времени.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gradual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За интересной беседой время пролетело <w>незаметно</w> до самого вечера.",
                            "target_lang_translations": {
                                "en": "During the interesting conversation, time flew by <w>unnoticed</w> until the very evening."
                            }
                        },
                        {
                            "text": "Пока мы разговаривали, на улице <w>незаметно</w> настала тёмная ночь.",
                            "target_lang_translations": {
                                "en": "While we were talking, dark night fell <w>imperceptibly</w> outside."
                            }
                        }
                    ],
                    "synonyms": [
                        "неощутимо",
                        "малозаметно",
                        "неосязаемо"
                    ],
                    "antonyms": [
                        "заметно"
                    ],
                    "common_phrases": [
                        "время пролетело незаметно",
                        "незаметно подкралась",
                        "пройти незаметно"
                    ],
                    "target_lang_definitions": {
                        "en": "Gradually, in a way that is difficult to perceive changes or the passage of time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "imperceptibly",
                                "target_lang_sense_clarification": "gradually or slightly, making it hard to detect"
                            },
                            {
                                "target_lang_word": "unnoticed",
                                "target_lang_sense_clarification": "often used with time passing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21c9a751-01f3-3a6f-a8ff-f5b269eb3fe2",
                    "sense_definition": "Оценочная характеристика чего-либо как плохо различимого, невидимого или неуловимого для наблюдателя.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "predicative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пятно на твоей рубашке совсем маленькое, его почти <w>незаметно</w>.",
                            "target_lang_translations": {
                                "en": "The stain on your shirt is quite small, it is almost <w>unnoticeable</w>."
                            }
                        },
                        {
                            "text": "Изменений в его поведении было <w>незаметно</w> даже близким друзьям.",
                            "target_lang_translations": {
                                "en": "Changes in his behavior were <w>unnoticeable</w> even to close friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "не видно",
                        "неприметно"
                    ],
                    "antonyms": [
                        "заметно"
                    ],
                    "common_phrases": [
                        "почти незаметно",
                        "совсем незаметно",
                        "ничего незаметно"
                    ],
                    "target_lang_definitions": {
                        "en": "Describing something as hard to distinguish, invisible, or elusive to the observer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unnoticeable",
                                "target_lang_sense_clarification": "hard to see or notice"
                            },
                            {
                                "target_lang_word": "invisible",
                                "target_lang_sense_clarification": "cannot be seen at all"
                            },
                            {
                                "target_lang_word": "not noticeable",
                                "target_lang_sense_clarification": "predicative usage"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5c056122-2713-3528-acd6-3baf402b82ba",
                    "sense_definition": "Краткая форма среднего рода прилагательного <w>незаметный</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это небольшое повреждение было <w>незаметно</w> при первом осмотре машины.",
                            "target_lang_translations": {
                                "en": "This small damage was <w>unnoticeable</w> during the first inspection of the car."
                            }
                        },
                        {
                            "text": "Его присутствие в зале было <w>незаметно</w>, так как он сидел очень тихо.",
                            "target_lang_translations": {
                                "en": "His presence in the hall was <w>unnoticeable</w> because he sat very quietly."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a20207c8-ef99-39ef-bf76-e2194c69060e",
                    "sense_definition": "Onrustig, bezorgd en gespannen over iets wat gaat gebeuren",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De student wacht <w>zenuwachtig</w> zijn examenresultaten af.",
                            "target_lang_translations": {
                                "ru": "Студент <w>нервно</w> ждет результатов своих экзаменов.",
                                "en": "The student is <w>nervously</w> awaiting his exam results."
                            }
                        },
                        {
                            "text": "Ik word altijd een beetje <w>zenuwachtig</w> als ik naar de tandarts moet.",
                            "target_lang_translations": {
                                "ru": "Я всегда начинаю немного <w>нервничать</w>, когда мне нужно к стоматологу.",
                                "en": "I always get a little <w>nervous</w> when I have to go to the dentist."
                            }
                        }
                    ],
                    "synonyms": [
                        "nerveus"
                    ],
                    "antonyms": [
                        "kalm",
                        "rustig"
                    ],
                    "common_phrases": [
                        "zenuwachtig zijn",
                        "zenuwachtig worden",
                        "zenuwachtig lachen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспокойный, взволнованный и напряженный из-за чего-то, что должно произойти",
                        "en": "Restless, worried, and tense about something that is going to happen"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взволнованный",
                                "target_lang_sense_clarification": "испытывающий беспокойство перед событием"
                            },
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "находящийся в состоянии напряжения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "nervous",
                                "target_lang_sense_clarification": "feeling worry or fear about an event"
                            },
                            {
                                "target_lang_word": "anxious",
                                "target_lang_sense_clarification": "feeling unease about an uncertain outcome"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8250e00-9f9c-3498-aff7-49533262378f",
                    "sense_definition": "Lijdend onder een gevoelig, snel geraakt zenuwstelsel",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is altijd nogal <w>zenuwachtig</w> geweest.",
                            "target_lang_translations": {
                                "ru": "Она всегда была довольно <w>нервной</w>.",
                                "en": "She has always been quite <w>nervous</w>."
                            }
                        },
                        {
                            "text": "Het paard is erg <w>zenuwachtig</w> en schrikt van elke onverwachte beweging.",
                            "target_lang_translations": {
                                "ru": "Лошадь очень <w>нервная</w> и пугается любого неожиданного движения.",
                                "en": "The horse is very <w>nervous</w> and startles at every unexpected movement."
                            }
                        }
                    ],
                    "synonyms": [
                        "nerveus"
                    ],
                    "antonyms": [
                        "evenwichtig"
                    ],
                    "common_phrases": [
                        "een zenuwachtig type"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся чувствительной, легко возбудимой нервной системой",
                        "en": "Suffering from a sensitive, easily affected nervous system"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "как черта характера или свойство организма"
                            },
                            {
                                "target_lang_word": "пугливый",
                                "target_lang_sense_clarification": "легко пугающийся (о животных или людях)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "nervous",
                                "target_lang_sense_clarification": "having a tendency to be easily agitated"
                            },
                            {
                                "target_lang_word": "high-strung",
                                "target_lang_sense_clarification": "tense and easily upset"
                            },
                            {
                                "target_lang_word": "skittish",
                                "target_lang_sense_clarification": "easily scared (often used for animals)"
                            }
                        ]
                    }

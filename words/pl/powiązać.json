    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b83061ae-46d8-33ef-90e9-8489dee3e983",
                    "sense_definition": "Połączyć fakty, informacje, zdarzenia lub elementy w logiczną całość, znajdując między nimi zależność.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja próbuje <powiązać> te dwa przestępstwa ze sobą, szukając wspólnych śladów.",
                            "target_lang_translations": {
                                "ru": "Полиция пытается <w>связать</w> эти два преступления между собой, ища общие следы."
                            }
                        },
                        {
                            "text": "Trudno jest logicznie <powiązać> te sprzeczne informacje w jeden spójny raport.",
                            "target_lang_translations": {
                                "ru": "Трудно логически <w>увязать</w> эту противоречивую информацию в один связный отчет."
                            }
                        }
                    ],
                    "synonyms": [
                        "połączyć",
                        "skojarzyć",
                        "złączyć"
                    ],
                    "common_phrases": [
                        "powiązać fakty",
                        "ściśle powiązane",
                        "powiązać koniec z końcem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединить факты, информацию, события или элементы в логическое целое, находя между ними зависимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "найти связь между фактами или событиями"
                            },
                            {
                                "target_lang_word": "увязать",
                                "target_lang_sense_clarification": "согласовать детали или информацию"
                            },
                            {
                                "target_lang_word": "соотнести",
                                "target_lang_sense_clarification": "сравнить и найти соответствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acf05237-c0d4-38ea-a47c-9b2657ec5b1e",
                    "sense_definition": "Złączyć ze sobą fizyczne przedmioty, używając do tego sznurka, liny lub węzła.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <powiązać> stare gazety sznurkiem, zanim wyniesiemy je do śmietnika.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>связать</w> старые газеты веревкой, прежде чем вынесем их на помойку."
                            }
                        },
                        {
                            "text": "Ogrodnik postanowił <powiązać> gałęzie pomidorów, aby nie opadały na ziemię.",
                            "target_lang_translations": {
                                "ru": "Садовник решил <w>подвязать</w> ветки помидоров, чтобы они не падали на землю."
                            }
                        }
                    ],
                    "synonyms": [
                        "związać",
                        "spiąć",
                        "złączyć",
                        "spleść"
                    ],
                    "common_phrases": [
                        "powiązać sznurkiem",
                        "powiązać w pęczki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединить физические предметы друг с другом, используя веревку, шнур или узел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "скрепить предметы вместе"
                            },
                            {
                                "target_lang_word": "перевязать",
                                "target_lang_sense_clarification": "обвязать пачку или группу предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "686eacf0-b4a2-3878-b8a5-6da6829fe367",
                    "sense_definition": "Stworzyć silną więź, relację lub wspólnotę interesów między osobami lub grupami.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda para postanowiła <powiązać> swoje losy i wzięła ślub w zeszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Молодая пара решила <w>связать</w> свои судьбы и поженилась в прошлом месяце."
                            }
                        },
                        {
                            "text": "Wspólne cele biznesowe mogą trwale <powiązać> ze sobą nawet dawnych konkurentów.",
                            "target_lang_translations": {
                                "ru": "Общие бизнес-цели могут прочно <w>связать</w> друг с другом даже бывших конкурентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "zespolić",
                        "złączyć"
                    ],
                    "common_phrases": [
                        "powiązać swoje losy",
                        "powiązać przyszłość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создать прочную связь, отношения или общность интересов между людьми или группами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "объединить судьбы или жизни"
                            },
                            {
                                "target_lang_word": "объединить",
                                "target_lang_sense_clarification": "сплотить на основе общих целей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d13ba92d-d247-3093-8b11-5a58f5a21b26",
                    "sense_definition": "Unieruchomić kogoś lub coś poprzez związanie kończyn lub przymocowanie do czegoś (często dotyczy wielu obiektów).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnicy zagrozili, że mogą <powiązać> zakładników, jeśli ci będą stawiać opór.",
                            "target_lang_translations": {
                                "ru": "Нападавшие угрожали, что могут <w>связать</w> заложников, если те будут оказывать сопротивление."
                            }
                        },
                        {
                            "text": "Rolnik musiał <powiązać> nogi zwierzęcia, aby bezpiecznie opatrzyć mu ranę.",
                            "target_lang_translations": {
                                "ru": "Фермер должен был <w>связать</w> ноги животному, чтобы безопасно обработать ему рану."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrępować",
                        "spętać",
                        "przymocować"
                    ],
                    "common_phrases": [
                        "powiązać ręce",
                        "powiązać jeńców"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обездвижить кого-то или что-то, связав конечности или прикрепив к чему-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "ограничить свободу движений веревкой"
                            },
                            {
                                "target_lang_word": "спутать",
                                "target_lang_sense_clarification": "связать ноги (обычно животному)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

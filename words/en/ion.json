    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8022cc5d-74b3-351c-a95c-c1f5af519eb3",
                    "sense_definition": "An atom or molecule that bears an electrical charge due to the loss or gain of one or more electrons.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The salt dissolved in water, separating into sodium and chloride <w>ions</w>.",
                            "target_lang_translations": {
                                "ru": "Соль растворилась в воде, распавшись на <w>ионы</w> натрия и хлора."
                            }
                        },
                        {
                            "text": "A lithium-ion battery powers most modern smartphones and laptops.",
                            "target_lang_translations": {
                                "ru": "Литий-<w>ионный</w> аккумулятор питает большинство современных смартфонов и ноутбуков."
                            }
                        }
                    ],
                    "synonyms": [
                        "charged particle"
                    ],
                    "common_phrases": [
                        "lithium-ion battery",
                        "positive ion",
                        "negative ion",
                        "heavy ion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Атом или молекула, имеющая электрический заряд вследствие потери или присоединения одного или нескольких электронов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ион",
                                "target_lang_sense_clarification": "физика, химия"
                            }
                        ]

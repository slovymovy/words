    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6c6017dc-fde6-382a-aaf7-71f7c404a739",
                    "sense_definition": "The central figure of Christianity, revered by Christians as the Son of God and the Messiah, and regarded as a prophet in Islam.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "religious_figure",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Christians believe that <w>Jesus</w> taught people to love one another.",
                            "target_lang_translations": {
                                "ru": "Христиане верят, что <w>Иисус</w> учил людей любить друг друга."
                            }
                        },
                        {
                            "text": "The story of the birth of <w>Jesus</w> is celebrated at Christmas.",
                            "target_lang_translations": {
                                "ru": "История рождения <w>Иисуса</w> празднуется в Рождество."
                            }
                        }
                    ],
                    "synonyms": [
                        "Jesus Christ",
                        "Christ",
                        "Son of God"
                    ],
                    "common_phrases": [
                        "Jesus of Nazareth",
                        "baby Jesus",
                        "thank Jesus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральная фигура христианства, почитаемая христианами как Сын Божий и Мессия, а в исламе — как пророк."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иисус",
                                "target_lang_sense_clarification": "религиозная фигура"
                            },
                            {
                                "target_lang_word": "Иисус Христос",
                                "target_lang_sense_clarification": "полное имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bd97b7b-6022-3544-9ef7-856b2cce7e41",
                    "sense_definition": "A male given name common in Spanish-speaking cultures.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I introduced my friend <w>Jesus</w> to the rest of the team.",
                            "target_lang_translations": {
                                "ru": "Я представил своего друга <w>Хесуса</w> остальной части команды."
                            }
                        },
                        {
                            "text": "Is <w>Jesus</w> coming to the party tonight?",
                            "target_lang_translations": {
                                "ru": "<w>Хесус</w> придет сегодня на вечеринку?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Spanish culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя, распространенное в испаноязычных культурах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хесус",
                                "target_lang_sense_clarification": "испанское имя"
                            },
                            {
                                "target_lang_word": "Иисус",
                                "target_lang_sense_clarification": "транслитерация написания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "48e58d40-0de3-39a5-9079-5ccfb471ea36",
                    "sense_definition": "An exclamation used to express strong emotion, such as surprise, shock, anger, or exasperation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jesus</w>, that car almost hit us!",
                            "target_lang_translations": {
                                "ru": "<w>Господи</w>, эта машина чуть не сбила нас!"
                            }
                        },
                        {
                            "text": "Oh <w>Jesus</w>, I completely forgot about the meeting.",
                            "target_lang_translations": {
                                "ru": "О <w>Боже</w>, я совсем забыл о встрече."
                            }
                        }
                    ],
                    "synonyms": [
                        "Christ",
                        "God",
                        "Jesus Christ"
                    ],
                    "common_phrases": [
                        "Jesus Christ",
                        "sweet Jesus",
                        "oh Jesus"
                    ],

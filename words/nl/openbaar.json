    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a6d1c0e0-8413-3d87-bf38-74596fedbc0c",
                    "sense_definition": "Voor iedereen toegankelijk en zichtbaar; niet geheim of privé.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik reis elke dag met het <w>openbaar</w> vervoer naar mijn werk.",
                            "target_lang_translations": {
                                "en": "I travel to work by <w>public</w> transport every day.",
                                "ru": "Я каждый день езжу на работу на <w>общественном</w> транспорте."
                            }
                        },
                        {
                            "text": "Het park is een <w>openbare</w> ruimte waar iedereen mag komen.",
                            "target_lang_translations": {
                                "en": "The park is a <w>public</w> space where everyone is allowed to go.",
                                "ru": "Парк — это <w>общественное</w> место, куда может прийти каждый."
                            }
                        },
                        {
                            "text": "Hij durft niet goed in het <w>openbaar</w> te spreken.",
                            "target_lang_translations": {
                                "en": "He is afraid to speak in <w>public</w>.",
                                "ru": "Он боится выступать на <w>публике</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "publiek"
                    ],
                    "antonyms": [
                        "niet-openbaar",
                        "niet-openbaar"
                    ],
                    "common_phrases": [
                        "openbaar vervoer",
                        "in het openbaar",
                        "openbare ruimte",
                        "openbare weg",
                        "openbare orde"
                    ],
                    "target_lang_definitions": {
                        "en": "Accessible and visible to everyone; not secret or private.",
                        "ru": "Доступный и видимый для всех; не секретный и не частный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "accessible to everyone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "общественный",
                                "target_lang_sense_clarification": "относящийся к обществу, доступный для людей (транспорт, места)"
                            },
                            {
                                "target_lang_word": "публичный",
                                "target_lang_sense_clarification": "открытый, совершаемый в присутствии публики"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "доступный для посещения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83e495a5-1d41-3f52-ad78-3306541dd586",
                    "sense_definition": "Uitgaande van de overheid en niet verbonden aan een specifieke godsdienst of levensbeschouwing.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn kinderen gaan naar een <w>openbare</w> school in de buurt.",
                            "target_lang_translations": {
                                "en": "My children go to a <w>public</w> school in the neighborhood.",
                                "ru": "Мои дети ходят в <w>государственную</w> школу по соседству."
                            }
                        },
                        {
                            "text": "Het <w>openbaar</w> ministerie heeft de straf geëist.",
                            "target_lang_translations": {
                                "en": "The <w>Public</w> Prosecution Service demanded the sentence.",
                                "ru": "<w>Государственное</w> обвинение потребовало наказания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "openbaar onderwijs",
                        "openbare school",
                        "openbaar ministerie",
                        "openbare bibliotheek"
                    ],
                    "target_lang_definitions": {
                        "en": "Provided by the government and not connected to a specific religion or philosophy.",
                        "ru": "Исходящий от государства и не связанный с конкретной религией или мировоззрением."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "government-funded or secular"
                            },
                            {
                                "target_lang_word": "state",
                                "target_lang_sense_clarification": "government-run (e.g. schools)"

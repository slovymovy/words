    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2c45bb8-6e4f-3da2-b8d0-ed0b01df29cd",
                    "sense_definition": "Войти внутрь помещения, здания или другого ограниченного пространства.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_enter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <зайти> в этот магазин, чтобы купить хлеба.",
                            "target_lang_translations": {
                                "en": "We decided to <go into> this shop to buy bread."
                            }
                        },
                        {
                            "text": "Когда на улице начался дождь, дети поспешили <зайти> в дом.",
                            "target_lang_translations": {
                                "en": "When the rain started outside, the children hurried to <go inside> the house."
                            }
                        }
                    ],
                    "antonyms": [
                        "выйти"
                    ],
                    "common_phrases": [
                        "зайти в комнату",
                        "зайти в дом",
                        "зайти в магазин",
                        "зайти внутрь"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter inside a room, building, or other confined space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go in",
                                "target_lang_sense_clarification": "general movement into a place"
                            },
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "more formal"
                            },
                            {
                                "target_lang_word": "come in",
                                "target_lang_sense_clarification": "entering towards the speaker"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "195674c8-6267-3fc2-a606-57e50ec682b4",
                    "sense_definition": "Навестить кого-либо или посетить какое-либо место ненадолго, по пути.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обещал <зайти> к бабушке после работы, чтобы узнать, как у неё дела.",
                            "target_lang_translations": {
                                "en": "I promised to <drop by> my grandmother's after work to see how she is doing."
                            }
                        },
                        {
                            "text": "Ты можешь <зайти> ко мне вечером на чашку чая?",
                            "target_lang_translations": {
                                "en": "Can you <come over> to my place in the evening for a cup of tea?"
                            }
                        }
                    ],
                    "synonyms": [
                        "посетить",
                        "заглянуть"
                    ],
                    "antonyms": [
                        "уйти"
                    ],
                    "common_phrases": [
                        "зайти в гости",
                        "зайти к другу",
                        "зайти на чай",
                        "зайти ненадолго"
                    ],
                    "target_lang_definitions": {
                        "en": "To visit someone or a place briefly, usually on the way to somewhere else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drop by",
                                "target_lang_sense_clarification": "casual visit"
                            },
                            {
                                "target_lang_word": "stop by",
                                "target_lang_sense_clarification": "brief visit on the way"
                            },
                            {
                                "target_lang_word": "call on",
                                "target_lang_sense_clarification": "formal visit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adbe51e6-ae0e-3d8d-a7d8-fe02488b1a33",
                    "sense_definition": "Открыть веб-страницу, приложение или выполнить вход в систему на компьютере или в интернете.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня не получается <зайти> в свой личный кабинет на сайте банка.",
                            "target_lang_translations": {
                                "en": "I can't seem to <log in> to my personal account on the bank's website."
                            }
                        },
                        {
                            "text": "Чтобы найти ответ на вопрос, нужно просто <зайти> в интернет.",
                            "target_lang_translations": {
                                "en": "To find the answer to the question, you just need to <go on> the internet."
                            }
                        }
                    ],
                    "antonyms": [
                        "выйти"
                    ],
                    "common_phrases": [
                        "зайти на сайт",
                        "зайти в интернет",
                        "зайти в настройки",
                        "зайти в систему"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in tech context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To open a webpage, app, or log in to a system on a computer or the internet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "log in",
                                "target_lang_sense_clarification": "access an account"
                            },
                            {
                                "target_lang_word": "go on",
                                "target_lang_sense_clarification": "visit a website or the internet"
                            },
                            {
                                "target_lang_word": "access",
                                "target_lang_sense_clarification": "enter a system"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09e1aeac-fe35-3684-b988-5edfa466498a",
                    "sense_definition": "Скрыться за чем-либо или опуститься за горизонт (о небесных телах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "disappear_set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркое солнце медленно <зашло> за высокие горы.",
                            "target_lang_translations": {
                                "en": "The bright sun slowly <set> behind the high mountains."
                            }
                        },
                        {
                            "text": "Мы решили <зайти> за угол здания, чтобы спрятаться от сильного ветра.",
                            "target_lang_translations": {
                                "en": "We decided to <go behind> the corner of the building to hide from the strong wind."
                            }
                        }
                    ],
                    "synonyms": [
                        "закатиться"
                    ],
                    "antonyms": [
                        "выйти"
                    ],
                    "common_phrases": [
                        "солнце зашло",
                        "зайти за угол",
                        "зайти за тучу",
                        "луна зашла"
                    ],
                    "target_lang_definitions": {
                        "en": "To hide behind something or descend below the horizon (referring to celestial bodies)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "of the sun or moon"
                            },
                            {
                                "target_lang_word": "go behind",
                                "target_lang_sense_clarification": "move behind an object"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c81d75a6-9341-3591-8b52-12539a377ded",
                    "sense_definition": "Уйти далеко или глубоко внутрь чего-либо; в переносном смысле — переступить границы допустимого.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "go_far_deep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грибники случайно <зашли> в самую гущу леса и заблудились.",
                            "target_lang_translations": {
                                "en": "The mushroom pickers accidentally <wandered> into the thick of the forest and got lost."
                            }
                        },
                        {
                            "text": "Наш спор <зашёл> слишком далеко, и мы перестали понимать друг друга.",
                            "target_lang_translations": {
                                "en": "Our argument has <gone> too far, and we have stopped understanding each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "забраться"
                    ],
                    "common_phrases": [
                        "зайти слишком далеко",
                        "зайти в тупик",
                        "зайти в лес",
                        "зайти в глубь"
                    ],
                    "target_lang_definitions": {
                        "en": "To go far or deep inside something; figuratively — to cross the boundaries of what is acceptable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "general movement or progression"
                            },
                            {
                                "target_lang_word": "wander",
                                "target_lang_sense_clarification": "move deeply into an area, often unintentionally"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "reach a certain state or point"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4964aea-ec06-360e-8f87-5a3a904cf6d4",
                    "sense_definition": "Приблизиться к кому-либо или чему-либо не прямо, а сбоку или с тыла.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "approach_flank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Противник попытался <зайти> с тыла, но мы были к этому готовы.",
                            "target_lang_translations": {
                                "en": "The enemy tried to <approach> from the rear, but we were ready for it."
                            }
                        },
                        {
                            "text": "Чтобы лучше рассмотреть скульптуру, нужно <зайти> с другой стороны.",
                            "target_lang_translations": {
                                "en": "To get a better look at the sculpture, you need to <approach> from the other side."
                            }
                        }
                    ],
                    "common_phrases": [
                        "зайти с тыла",
                        "зайти слева",
                        "зайти с фланга",
                        "зайти со спины"
                    ],
                    "target_lang_definitions": {
                        "en": "To approach someone or something not directly, but from the side or rear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "move towards from a specific direction"
                            },
                            {
                                "target_lang_word": "flank",
                                "target_lang_sense_clarification": "military or tactical context"
                            },
                            {
                                "target_lang_word": "come at",
                                "target_lang_sense_clarification": "approach from a direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93ec7147-0562-30e9-899b-5adfeee14517",
                    "sense_definition": "Понравиться, произвести хорошее впечатление (обычно о контенте, еде или творчестве).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "slang_like",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта новая песня мне сразу <зашла>, я слушаю её весь день.",
                            "target_lang_translations": {
                                "en": "This new song immediately <clicked with> me; I've been listening to it all day."
                            }
                        },
                        {
                            "text": "К сожалению, этот странный фильм никому из нашей компании не <зашёл>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, this strange film didn't <go down well> with anyone in our group."
                            }
                        }
                    ],
                    "common_phrases": [
                        "шутка зашла",
                        "фильм не зашёл",
                        "хорошо зашло"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "youth and internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be liked, to make a good impression (usually regarding content, food, or art)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go down well",
                                "target_lang_sense_clarification": "be received positively"
                            },
                            {
                                "target_lang_word": "click",
                                "target_lang_sense_clarification": "resonate with someone"
                            },
                            {
                                "target_lang_word": "hit the spot",
                                "target_lang_sense_clarification": "be exactly what was needed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40b4fb34-489c-341c-af14-621385176a7d",
                    "sense_definition": "Начаться или возникнуть (о разговоре, споре или теме).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "start_conversation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда речь <зашла> о деньгах, все в комнате сразу замолчали.",
                            "target_lang_translations": {
                                "en": "When the conversation <turned to> money, everyone in the room immediately fell silent."
                            }
                        },
                        {
                            "text": "Неожиданно у нас <зашёл> спор о политике, хотя мы хотели просто отдохнуть.",
                            "target_lang_translations": {
                                "en": "Unexpectedly, an argument about politics <arose> between us, although we just wanted to relax."
                            }
                        }
                    ],
                    "common_phrases": [
                        "речь зашла о",
                        "разговор зашёл о",
                        "спор зашёл"
                    ],
                    "target_lang_definitions": {
                        "en": "To begin or arise (regarding a conversation, argument, or topic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn to",
                                "target_lang_sense_clarification": "change of topic"
                            },
                            {
                                "target_lang_word": "come up",
                                "target_lang_sense_clarification": "arise in conversation"
                            },
                            {
                                "target_lang_word": "arise",
                                "target_lang_sense_clarification": "start (of a dispute)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2433de7-ae8c-3b55-9493-0f2b263cba4c",
                    "sense_definition": "Войти в порт или гавань (о судне); заплыть куда-либо (о рыбе).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_fish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан решил <зайти> в ближайший порт, чтобы пополнить запасы воды.",
                            "target_lang_translations": {
                                "en": "The captain decided to <put in> at the nearest port to replenish water supplies."
                            }
                        },
                        {
                            "text": "Рыбаки надеялись, что лосось скоро <зайдёт> в реку на нерест.",
                            "target_lang_translations": {
                                "en": "The fishermen hoped that the salmon would soon <enter> the river to spawn."
                            }
                        }
                    ],
                    "synonyms": [
                        "заплыть"
                    ],
                    "common_phrases": [
                        "зайти в порт",
                        "зайти в гавань",
                        "рыба зашла"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter a port or harbor (of a vessel); to swim into a place (of fish)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put in",
                                "target_lang_sense_clarification": "nautical term for entering port"
                            },
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "general movement into"
                            },
                            {
                                "target_lang_word": "call at",
                                "target_lang_sense_clarification": "stop at a port"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ecdee0e-c3a5-3749-9a12-cc592afeec2c",
                    "sense_definition": "Мужское имя.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Моего нового знакомого зовут <Зайти>.",
                            "target_lang_translations": {
                                "en": "My new acquaintance is named <Zayti>."
                            }
                        },
                        {
                            "text": "В списке гостей числился человек по имени <Зайти>.",
                            "target_lang_translations": {
                                "en": "A person named <Zayti> was listed on the guest list."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A male name."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Zayti",
                                "target_lang_sense_clarification": "transliteration of the name"
                            }
                        ]
                    }
                }
            ]
        }
    ]

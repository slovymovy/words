{
    "word_family": [
        "sorriness",
        "sorrily"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9e0c0cb4-05da-3f60-8164-26ddb0f4ce1f",
                    "sense_definition": "Feeling regret, penitence, or shame for one's actions or mistakes.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "apology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>sorry</w> that I stepped on your toes by accident.",
                            "target_lang_translations": {
                                "ru": "Мне <w>жаль</w>, что я случайно наступил вам на ногу."
                            }
                        },
                        {
                            "text": "She was truly <w>sorry</w> for forgetting his birthday.",
                            "target_lang_translations": {
                                "ru": "Она искренне <w>сожалела</w>, что забыла о его дне рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "apologetic",
                        "regretful",
                        "remorseful",
                        "contrite",
                        "penitent",
                        "repentant",
                        "compunctious"
                    ],
                    "common_phrases": [
                        "I am sorry",
                        "so sorry",
                        "terribly sorry",
                        "sorry for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий сожаление, раскаяние или вину за свои действия или ошибки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаль",
                                "target_lang_sense_clarification": "в значении «мне жаль»"
                            },
                            {
                                "target_lang_word": "виноватый",
                                "target_lang_sense_clarification": "чувствующий вину"
                            },
                            {
                                "target_lang_word": "сожалеющий",
                                "target_lang_sense_clarification": "испытывающий сожаление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1d58d7d-4088-30aa-a4b9-12eecc196792",
                    "sense_definition": "Feeling sadness, sympathy, or pity for someone else's misfortune.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sympathy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I feel <w>sorry</w> for the children who have nowhere to play.",
                            "target_lang_translations": {
                                "ru": "Мне <w>жаль</w> детей, которым негде играть."
                            }
                        },
                        {
                            "text": "We were <w>sorry</w> to hear the bad news about your job.",
                            "target_lang_translations": {
                                "ru": "Нам было <w>жаль</w> слышать плохие новости о вашей работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "heavy-hearted",
                        "melancholy",
                        "mournful"
                    ],
                    "common_phrases": [
                        "feel sorry for",
                        "sorry to hear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий грусть, сочувствие или жалость к чужому несчастью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаль",
                                "target_lang_sense_clarification": "выражение сочувствия"
                            },
                            {
                                "target_lang_word": "сочувствующий",
                                "target_lang_sense_clarification": "проявляющий сострадание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb1a9f5d-9d08-3254-8032-df106a8197e1",
                    "sense_definition": "In a poor, pitiful, or wretched condition.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The abandoned garden was in a <w>sorry</w> state after the storm.",
                            "target_lang_translations": {
                                "ru": "Заброшенный сад был в <w>плачевном</w> состоянии после бури."
                            }
                        },
                        {
                            "text": "He looked a <w>sorry</w> sight covered in mud and shivering.",
                            "target_lang_translations": {
                                "ru": "Он представлял собой <w>жалкое</w> зрелище, покрытый грязью и дрожащий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sorry state",
                        "sorry sight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в плохом, жалком или плачевном состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плачевный",
                                "target_lang_sense_clarification": "о состоянии"
                            },
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "вызывающий жалость видом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ed21f2b-e9c2-34f3-849f-5ea408c4918d",
                    "sense_definition": "Contemptibly inadequate or worthless; pathetic.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was a <w>sorry</w> excuse for a football player.",
                            "target_lang_translations": {
                                "ru": "Это было <w>жалкое</w> подобие футболиста."
                            }
                        },
                        {
                            "text": "He offered a <w>sorry</w> explanation that no one believed.",
                            "target_lang_translations": {
                                "ru": "Он предложил <w>жалкое</w> объяснение, которому никто не поверил."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sorry excuse for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Никчемный, неполноценный или не заслуживающий уважения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "никчемный, убогий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1579098e-c6b7-37eb-b459-04bf0dcefdbc",
                    "sense_definition": "Used to express apology, regret, or remorse.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "apology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Sorry</w>! I didn't see that you were on the phone.",
                            "target_lang_translations": {
                                "ru": "<w>Извините</w>! Я не видел, что вы говорите по телефону."
                            }
                        },
                        {
                            "text": "<w>Sorry</w> about the mess, I will clean it up later.",
                            "target_lang_translations": {
                                "ru": "<w>Простите</w> за беспорядок, я уберу позже."
                            }
                        }
                    ],
                    "synonyms": [
                        "soz"
                    ],
                    "common_phrases": [
                        "oh sorry",
                        "sorry about that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения извинения, сожаления или раскаяния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извините",
                                "target_lang_sense_clarification": "вежливое обращение"
                            },
                            {
                                "target_lang_word": "прости",
                                "target_lang_sense_clarification": "дружеское обращение"
                            },
                            {
                                "target_lang_word": "прошу прощения",
                                "target_lang_sense_clarification": "формальное"
                            },
                            {
                                "target_lang_word": "извини",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "925001ce-c8ea-32ae-adf1-63edc469557f",
                    "sense_definition": "Used to ask someone to repeat something that was not heard or understood.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Sorry</w>? Could you say that again please?",
                            "target_lang_translations": {
                                "ru": "<w>Простите</w>? Не могли бы вы повторить?"
                            }
                        },
                        {
                            "text": "<w>Sorry</w>? The connection is bad and I missed that.",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>? Связь плохая, и я пропустил это."
                            }
                        }
                    ],
                    "synonyms": [
                        "excuse me?",
                        "I beg your pardon?",
                        "I'm sorry?",
                        "say again",
                        "come again"
                    ],
                    "common_phrases": [
                        "sorry?"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы попросить собеседника повторить то, что не было услышано или понято."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простите?",
                                "target_lang_sense_clarification": "вежливый переспрос"
                            },
                            {
                                "target_lang_word": "что?",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "как?",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db84c1af-53fd-35d4-b546-e7523271f1bf",
                    "sense_definition": "Used as a polite request to pass someone or to interrupt.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "politeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Sorry</w>! Coming through with a heavy box.",
                            "target_lang_translations": {
                                "ru": "<w>Извините</w>! Прохожу с тяжелой коробкой."
                            }
                        },
                        {
                            "text": "<w>Sorry</w>, can I just squeeze past you to get to the door?",
                            "target_lang_translations": {
                                "ru": "<w>Простите</w>, можно я протиснусь мимо вас к двери?"
                            }
                        }
                    ],
                    "synonyms": [
                        "excuse me"
                    ],
                    "common_phrases": [
                        "sorry coming through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как вежливая просьба пропустить или разрешение прервать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извините",
                                "target_lang_sense_clarification": "просьба пропустить"
                            },
                            {
                                "target_lang_word": "простите",
                                "target_lang_sense_clarification": "вежливое обращение"
                            },
                            {
                                "target_lang_word": "разрешите",
                                "target_lang_sense_clarification": "просьба пройти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "426fef70-7969-3ead-a829-13220e4994de",
                    "sense_definition": "Used to soften a refusal or a disagreement.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "politeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Sorry</w>, but I really don't agree with your opinion.",
                            "target_lang_translations": {
                                "ru": "<w>Извините</w>, но я действительно не согласен с вашим мнением."
                            }
                        },
                        {
                            "text": "<w>Sorry</w>, we are closed for the night.",
                            "target_lang_translations": {
                                "ru": "<w>Извините</w>, мы закрыты на ночь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sorry but"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для смягчения отказа или выражения несогласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извините",
                                "target_lang_sense_clarification": "смягчение отказа"
                            },
                            {
                                "target_lang_word": "простите",
                                "target_lang_sense_clarification": "вежливая форма"
                            },
                            {
                                "target_lang_word": "к сожалению",
                                "target_lang_sense_clarification": "выражение сожаления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "213d74e8-eb53-3ca5-918b-732295275558",
                    "sense_definition": "Used to correct oneself immediately after making a mistake in speech.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are four, <w>sorry</w>, five branches of the store in this city.",
                            "target_lang_translations": {
                                "ru": "В этом городе четыре, <w>простите</w>, пять филиалов магазина."
                            }
                        },
                        {
                            "text": "Turn left, <w>sorry</w>, right at the next traffic light.",
                            "target_lang_translations": {
                                "ru": "Поверните налево, <w>извините</w>, направо на следующем светофоре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no sorry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для немедленного исправления собственной оговорки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простите",
                                "target_lang_sense_clarification": "при оговорке"
                            },
                            {
                                "target_lang_word": "извините",
                                "target_lang_sense_clarification": "при исправлении"
                            },
                            {
                                "target_lang_word": "то есть",
                                "target_lang_sense_clarification": "уточнение"
                            }
                        ]
                    }
                }
            ]
        },
        {

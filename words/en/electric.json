                {
                    "sense_id": "ddae64ab-d91f-33ee-b36c-4ddc395ca563",
                    "sense_definition": "Operated by, producing, or relating to electricity.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "power_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to buy a new <w>electric</w> kettle for the kitchen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить новый <w>электрический</w> чайник на кухню."
                            }
                        },
                        {
                            "text": "The company plans to sell only <w>electric</w> cars by next year.",
                            "target_lang_translations": {
                                "ru": "Компания планирует продавать только <w>электрические</w> автомобили к следующему году."
                            }
                        }
                    ],
                    "synonyms": [
                        "electrical"

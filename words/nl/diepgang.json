                    "sense_id": "7abfe294-3340-3556-87d9-c10b336d4fb7",
                    "sense_definition": "De mate van inhoudelijke kwaliteit, ernst of geestelijke rijkdom.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",

                        "ru": [
                            {
                                "target_lang_word": "неподвижно",
                                "target_lang_sense_clarification": "в сочетании с глаголами стояния/сидения"
                            },
                            {
                                "target_lang_word": "тихо",
                                "target_lang_sense_clarification": "в значении затихания"
                            }
                        ]
                    }
                }
            ]
        },

                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "физическое воздействие"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "nudge",
                                "target_lang_sense_clarification": "gentle push with elbow"
                            },
                            {

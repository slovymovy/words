                                "target_lang_word": "журнал",
                                "target_lang_sense_clarification": "бухгалтерия"
                            },
                            {
                                "target_lang_word": "журнал учёта",
                                "target_lang_sense_clarification": "регистрация данных"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "journal",
                                "target_lang_sense_clarification": "accounting"
                            },
                            {
                                "target_lang_word": "general journal",
                                "target_lang_sense_clarification": "bookkeeping"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f928f30-3706-3a18-8ac1-93bd1f6f6e8b",
                    "sense_definition": "Сооружение из камня, кирпича или металла для отопления помещения и приготовления пищи",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "heating_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В деревне у бабушки есть большая русская <печь>.",
                            "target_lang_translations": {
                                "en": "In the village, grandmother has a large Russian <stove>."
                            }
                        },
                        {
                            "text": "Зимой мы топили <печь> каждый день, чтобы в доме было тепло.",
                            "target_lang_translations": {
                                "en": "In winter, we heated the <stove> every day to keep the house warm."
                            }
                        }
                    ],
                    "synonyms": [
                        "печка"
                    ],
                    "antonyms": [
                        "холодильник"
                    ],
                    "common_phrases": [
                        "русская печь",
                        "топить печь",
                        "сидеть на печи",
                        "микроволновая печь"
                    ],
                    "target_lang_definitions": {
                        "en": "A structure made of stone, brick, or metal for heating a room and cooking food"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stove",
                                "target_lang_sense_clarification": "device for heating or cooking"
                            },
                            {
                                "target_lang_word": "oven",
                                "target_lang_sense_clarification": "chamber for baking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1157bbd-f97f-3653-bb48-a160c0148d1e",
                    "sense_definition": "Промышленное сооружение для сильного нагрева и тепловой обработки материалов",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "heating_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На металлургическом заводе работает новая доменная <печь>.",
                            "target_lang_translations": {
                                "en": "A new blast <furnace> is operating at the metallurgical plant."
                            }
                        },
                        {
                            "text": "Эта <печь> используется для обжига керамических изделий при высокой температуре.",
                            "target_lang_translations": {
                                "en": "This <kiln> is used for firing ceramic products at high temperatures."
                            }
                        }
                    ],
                    "common_phrases": [
                        "доменная печь",
                        "плавильная печь",
                        "муфельная печь"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An industrial structure for intense heating and processing of materials"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "furnace",
                                "target_lang_sense_clarification": "industrial heating, e.g., for metal"
                            },
                            {
                                "target_lang_word": "kiln",
                                "target_lang_sense_clarification": "oven for firing ceramics or drying"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5a26a0a-f7b1-38a4-9b3a-8bc6af7f625e",
                    "sense_definition": "Готовить пищу сухим нагреванием, обычно в духовке или на сковороде",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама научила меня <печь> вкусные пироги с яблоками.",
                            "target_lang_translations": {
                                "en": "Mom taught me to <bake> delicious apple pies."
                            }
                        },
                        {
                            "text": "На завтрак бабушка часто <печёт> тонкие блины.",
                            "target_lang_translations": {
                                "en": "For breakfast, grandmother often <makes> thin pancakes."
                            }
                        }
                    ],
                    "synonyms": [
                        "выпекать",
                        "испекать",
                        "припекать"
                    ],
                    "common_phrases": [
                        "печь пироги",
                        "печь хлеб",
                        "печь блины",
                        "печь торт"
                    ],
                    "target_lang_definitions": {
                        "en": "To cook food using dry heat, usually in an oven"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bake",
                                "target_lang_sense_clarification": "to cook in an oven"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b016b9b2-0bcb-3f0b-ba3e-4e31c1823837",
                    "sense_definition": "Сильно греть, обдавать жаром (обычно о солнце)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heat_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В полдень летнее солнце <печёт> особенно сильно.",
                            "target_lang_translations": {
                                "en": "At noon, the summer sun <beats down> especially hard."
                            }
                        },
                        {
                            "text": "Мне <пекло> голову, поэтому я надел шляпу.",
                            "target_lang_translations": {
                                "en": "The sun was <baking> my head, so I put on a hat."
                            }
                        }
                    ],
                    "synonyms": [
                        "палить",
                        "жечь",
                        "припекать"
                    ],
                    "antonyms": [
                        "холодить",
                        "морозить"
                    ],
                    "common_phrases": [
                        "солнце печёт",
                        "нещадно печь"
                    ],
                    "target_lang_definitions": {
                        "en": "To emit intense heat or be scorching hot (usually of the sun)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bake",
                                "target_lang_sense_clarification": "to be very hot"
                            },
                            {
                                "target_lang_word": "beat down",
                                "target_lang_sense_clarification": "of the sun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5b3a52d-5077-3572-80b7-70b61a2ec438",
                    "sense_definition": "Вызывать чувство жжения или боли в теле",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После укуса насекомого кожу начало сильно <печь>.",
                            "target_lang_translations": {
                                "en": "After the insect bite, the skin started to <sting> intensely."
                            }
                        },
                        {
                            "text": "У него <пекло> в груди от горячего чая.",
                            "target_lang_translations": {
                                "en": "His chest <burned> from the hot tea."
                            }
                        }
                    ],
                    "synonyms": [
                        "жечь"
                    ],
                    "common_phrases": [
                        "в груди печёт",
                        "глаза печёт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used impersonally"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a burning or stinging sensation in the body"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burn",
                                "target_lang_sense_clarification": "sensation of heat or pain"
                            },
                            {
                                "target_lang_word": "sting",
                                "target_lang_sense_clarification": "sharp pain"
                            }
                        ]
                    }
                }
            ]
        }

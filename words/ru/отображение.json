    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bdfb48a5-bf94-3474-86f7-ed9bf2d8447f",
                    "sense_definition": "Действие по глаголу <w>отображать</w>; визуальное представление информации (обычно на экране устройства) или воспроизведение действительности в художественных образах.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В настройках программы можно изменить <w>отображение</w> даты и времени.",
                            "target_lang_translations": {
                                "en": "You can change the date and time <w>display</w> in the program settings."
                            }
                        },
                        {
                            "text": "Правильное <w>отображение</w> цветов на мониторе очень важно для работы дизайнера.",
                            "target_lang_translations": {
                                "en": "Correct color <w>display</w> on the monitor is very important for a designer's work."
                            }
                        }
                    ],
                    "synonyms": [
                        "функция"
                    ],
                    "common_phrases": [
                        "отображение данных",
                        "настройки отображения",
                        "графическое отображение",
                        "корректное отображение"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of displaying; visual representation of information (usually on a device screen) or the reproduction of reality in artistic images."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "display",
                                "target_lang_sense_clarification": "visual output on a screen"
                            },
                            {
                                "target_lang_word": "representation",
                                "target_lang_sense_clarification": "act of depicting or showing data"
                            },
                            {
                                "target_lang_word": "rendering",
                                "target_lang_sense_clarification": "computer graphics generation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d8fb91f-9d00-396f-aa3d-eb9f117261f7",
                    "sense_definition": "Закон, по которому каждому элементу одного множества ставится в соответствие определенный элемент другого множества.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "math_mapping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <w>отображение</w> переводит прямые линии в окружности.",
                            "target_lang_translations": {
                                "en": "This <w>mapping</w> transforms straight lines into circles."
                            }
                        },
                        {
                            "text": "Студенты рассматривали взаимно однозначное <w>отображение</w> геометрических фигур.",
                            "target_lang_translations": {
                                "en": "The students examined a one-to-one <w>mapping</w> of geometric shapes."
                            }
                        }
                    ],
                    "synonyms": [
                        "функция"
                    ],
                    "common_phrases": [
                        "линейное отображение",
                        "конформное отображение",
                        "взаимно однозначное отображение"
                    ],
                    "target_lang_definitions": {
                        "en": "A rule or law by which every element of one set is associated with a specific element of another set."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mapping",
                                "target_lang_sense_clarification": "mathematical correspondence between sets"
                            },
                            {
                                "target_lang_word": "map",
                                "target_lang_sense_clarification": "mathematical function"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b586c8a-f8b5-395a-bddc-cb221861b940",
                    "sense_definition": "Запечатление, след явлений объективной действительности в сознании человека.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Искусство — это творческое <w>отображение</w> мира художником.",
                            "target_lang_translations": {
                                "en": "Art is a creative <w>reflection</w> of the world by an artist."
                            }
                        },
                        {
                            "text": "Философы спорили про объективное <w>отображение</w> реальности в наших мыслях.",
                            "target_lang_translations": {
                                "en": "Philosophers argued about the objective <w>reflection</w> of reality in our thoughts."
                            }
                        }
                    ],
                    "common_phrases": [
                        "отображение действительности",
                        "отображение реальности"
                    ],
                    "target_lang_definitions": {
                        "en": "The capturing or imprint of phenomena of objective reality in human consciousness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reflection",
                                "target_lang_sense_clarification": "mirroring of reality in consciousness or art"
                            },
                            {
                                "target_lang_word": "depiction",
                                "target_lang_sense_clarification": "artistic portrayal"
                            }
                        ]
                    }
                }
            ]
        }
    ]

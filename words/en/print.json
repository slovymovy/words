                        {
                            "text": "The company spends more money on digital advertising than on <w>print</w>.",
                            "target_lang_translations": {
                                "ru": "Компания тратит больше денег на цифровую рекламу, чем на <w>печать</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in print",
                        "out of print",
                        "print media"
                    ],
                    "target_lang_definitions": {
                        "ru": "Книги, газеты и другие материалы, созданные типографским способом; пресса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печать",
                                "target_lang_sense_clarification": "пресса, издания"

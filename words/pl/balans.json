    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "917893ca-ade0-3675-885a-a3d4ab7768d8",
                    "sense_definition": "Przechylanie się w celu utrzymania równowagi.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "equilibrium",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Narciarz musiał zachować <w>balans</w>, aby nie przewrócić się na stromym stoku.",
                            "target_lang_translations": {
                                "ru": "Лыжнику нужно было сохранять <w>равновесие</w>, чтобы не упасть на крутом склоне."
                            }
                        },
                        {
                            "text": "Ćwiczenia na piłce gimnastycznej pomagają poprawić <w>balans</w> i koordynację.",
                            "target_lang_translations": {
                                "ru": "Упражнения на гимнастическом мяче помогают улучшить <w>баланс</w> и координацию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "utrzymać balans",
                        "stracić balans",
                        "balans ciałem",
                        "złapać balans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движения тела или состояние для сохранения устойчивости; равновесие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равновесие",
                                "target_lang_sense_clarification": "состояние устойчивости"
                            },
                            {
                                "target_lang_word": "баланс",
                                "target_lang_sense_clarification": "в спорте и переносном смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa45c179-be5d-3b43-8fb2-3121d6267481",
                    "sense_definition": "Długi drążek lub tyczka używana przez linoskoczka do utrzymania równowagi; przeciwwaga.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "equilibrium",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Akrobata pewnie chwycił <w>balans</w> i wszedł na cienką linę rozpiętą nad areną.",
                            "target_lang_translations": {
                                "ru": "Акробат уверенно схватил <w>балансир</w> и ступил на тонкий канат, натянутый над ареной."
                            }
                        },
                        {
                            "text": "Ciężki <w>balans</w> pomaga linoskoczkowi obniżyć środek ciężkości i ustabilizować sylwetkę.",
                            "target_lang_translations": {
                                "ru": "Тяжелый <w>балансир</w> помогает канатоходцу понизить центр тяжести и стабилизировать положение тела."
                            }
                        }
                    ],
                    "synonyms": [
                        "balansjer"
                    ],
                    "common_phrases": [
                        "chwycić za balans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шест, используемый канатоходцем для удержания равновесия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балансир",
                                "target_lang_sense_clarification": "шест акробата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "589e2896-b993-3783-aa2a-9b5737a9b58b",
                    "sense_definition": "Element budowy zegarka mechanicznego regulujący jego chód.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "horology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zegarmistrz ostrożnie wyregulował <w>balans</w> w zabytkowym mechanizmie.",
                            "target_lang_translations": {
                                "ru": "Часовщик осторожно отрегулировал <w>баланс</w> в старинном механизме."
                            }
                        },
                        {
                            "text": "Uszkodzony <w>balans</w> sprawił, że zegarek zaczął się znacznie spóźniać.",
                            "target_lang_translations": {
                                "ru": "Из-за поврежденного <w>баланса</w> часы стали значительно отставать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koło balansu",
                        "oś balansu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деталь механических часов, регулирующая их ход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баланс",
                                "target_lang_sense_clarification": "деталь часов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a8bcb3d1-4c2c-33c7-bdca-fc7a275c7e72",
                    "sense_definition": "Передвигаться по воздуху в определённом направлении (о птицах, насекомых или объектах).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Птицы осенью <w>летят</w> на юг.",
                            "target_lang_translations": {
                                "en": "Birds <w>fly</w> south in the autumn."
                            }
                        },
                        {
                            "text": "В небе высоко <w>летит</w> воздушный шар.",
                            "target_lang_translations": {
                                "en": "A hot air balloon is <w>flying</w> high in the sky."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лететь по небу",
                        "лететь на юг",
                        "высоко лететь",
                        "быстро лететь"
                    ],
                    "target_lang_definitions": {
                        "en": "To move through the air in a specific direction (about birds, insects, or objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fly",
                                "target_lang_sense_clarification": "travel through the air"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5addd0e5-fb47-3328-87ae-35300dcf49cd",
                    "sense_definition": "Путешествовать на самолёте или другом летательном аппарате в качестве пассажира.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <w>летим</w> вечерним рейсом в Москву.",
                            "target_lang_translations": {
                                "en": "We are <w>flying</w> to Moscow on the evening flight."
                            }
                        },
                        {
                            "text": "Летом наша семья <w>летит</w> в отпуск к морю.",
                            "target_lang_translations": {
                                "en": "In summer, our family is <w>flying</w> to the sea for vacation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лететь в отпуск",
                        "лететь самолётом",
                        "лететь первым классом",
                        "лететь за границу"
                    ],
                    "target_lang_definitions": {
                        "en": "To travel by plane or other aircraft as a passenger."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fly",
                                "target_lang_sense_clarification": "travel by aircraft"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51b80eb7-eb72-3f50-88c0-af70219d2526",
                    "sense_definition": "Очень быстро проходить или заканчиваться (обычно о времени).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphor_speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как быстро <w>летит</w> время, когда ты счастлив!",
                            "target_lang_translations": {
                                "en": "How fast time <w>flies</w> when you are happy!"
                            }
                        },
                        {
                            "text": "Летние каникулы <w>летят</w> незаметно для школьников.",
                            "target_lang_translations": {
                                "en": "Summer holidays <w>fly</w> by unnoticed for schoolchildren."
                            }
                        }
                    ],
                    "common_phrases": [
                        "время летит",
                        "годы летят",
                        "дни летят",
                        "незаметно лететь"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass or end very quickly (usually about time)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fly",
                                "target_lang_sense_clarification": "pass quickly (of time)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70dcc70c-4f6d-35c4-be26-a21d848aa351",
                    "sense_definition": "Очень быстро двигаться, бежать или ехать (переносное значение).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphor_speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Навстречу мне по коридору <w>летел</w> курьер с документами.",
                            "target_lang_translations": {
                                "en": "A courier with documents was <w>rushing</w> towards me down the corridor."
                            }
                        },
                        {
                            "text": "Автомобиль <w>летел</w> по шоссе на огромной скорости.",
                            "target_lang_translations": {
                                "en": "The car was <w>flying</w> along the highway at huge speed."
                            }
                        }
                    ],
                    "synonyms": [
                        "мчаться"
                    ],
                    "common_phrases": [
                        "лететь сломя голову",
                        "лететь на встречу",
                        "лететь на работу",
                        "лететь стрелой"
                    ],
                    "target_lang_definitions": {
                        "en": "To move, run, or drive very fast (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fly",
                                "target_lang_sense_clarification": "move very fast"
                            },
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "move with urgent haste"
                            },
                            {
                                "target_lang_word": "tear",
                                "target_lang_sense_clarification": "move recklessly fast"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3019dc6b-1f44-31b4-9e77-b08a1a3039fe",
                    "sense_definition": "Падать с высоты вниз.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "negative_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок спотыкается и <w>летит</w> прямо на землю.",
                            "target_lang_translations": {
                                "en": "The child trips and <w>falls</w> straight to the ground."
                            }
                        },
                        {
                            "text": "Книга соскользнула со стола и <w>летела</w> на пол.",
                            "target_lang_translations": {
                                "en": "The book slid off the table and <w>fell</w> to the floor."
                            }

{
    "word_family": [
        "hourly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "81248385-bfed-3210-976a-8eca8c8bae49",
                    "sense_definition": "A unit of time equal to sixty minutes, or one twenty-fourth of a day.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I waited for an <w>hour</w> but nobody came.",
                            "target_lang_translations": {
                                "ru": "Я ждал целый <w>час</w>, но никто не пришел."
                            }
                        },
                        {
                            "text": "The train leaves in half an <w>hour</w>.",
                            "target_lang_translations": {
                                "ru": "Поезд отправляется через пол<w>часа</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "half an hour",
                        "an hour ago",
                        "for hours",
                        "per hour",
                        "miles per hour"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица времени, равная шестидесяти минутам, или одна двадцать четвертая часть суток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "час",
                                "target_lang_sense_clarification": "единица времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56b2b1f4-5df7-3ec3-9711-0c19bfb151da",
                    "sense_definition": "A distance that takes sixty minutes to travel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The beach is only an <w>hour</w> away from here.",
                            "target_lang_translations": {
                                "ru": "Пляж находится всего в <w>часе</w> езды отсюда."
                            }
                        },
                        {
                            "text": "We live about an <w>hour</w> from the city center.",
                            "target_lang_translations": {
                                "ru": "Мы живем примерно в <w>часе</w> пути от центра города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "an hour away",
                        "an hour from here"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние, которое можно преодолеть за шестьдесят минут."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "час",
                                "target_lang_sense_clarification": "расстояние (езды или ходьбы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf58fc02-40fa-31a3-906f-3e04d57f3ffe",
                    "sense_definition": "A specific period of time scheduled for work, business, or operation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scheduled_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shop has extended opening <w>hours</w> during the holidays.",
                            "target_lang_translations": {
                                "ru": "В праздники у магазина продленные <w>часы</w> работы."
                            }
                        },
                        {
                            "text": "I need to work more <w>hours</w> to save money for the trip.",
                            "target_lang_translations": {
                                "ru": "Мне нужно работать больше <w>часов</w>, чтобы накопить денег на поездку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "office hours",
                        "opening hours",
                        "working hours",
                        "after hours",
                        "long hours"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный период времени, отведенный для работы, дел или функционирования учреждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часы",
                                "target_lang_sense_clarification": "рабочее время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b7dd4ad-208b-3da1-8dd1-fbc86ca298bf",
                    "sense_definition": "A particular moment or time of day, often significant or characterized by a specific quality.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "specific_moment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Traffic is always terrible during rush <w>hour</w>.",
                            "target_lang_translations": {
                                "ru": "В <w>час</w> пик движение всегда ужасное."
                            }
                        },
                        {
                            "text": "He helped us in our darkest <w>hour</w>.",
                            "target_lang_translations": {
                                "ru": "Он помог нам в наш самый тяжелый <w>час</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stound"
                    ],
                    "common_phrases": [
                        "rush hour",
                        "lunch hour",
                        "darkest hour",
                        "eleventh hour",
                        "happy hour"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особый момент или время дня, часто значимое или характеризующееся определенным качеством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "час",
                                "target_lang_sense_clarification": "момент"
                            },
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "пора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "106ccbef-8050-3f32-bdb1-535f37661d81",
                    "sense_definition": "Used after a number to state the time on a twenty-four-hour clock.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mission starts at exactly 0600 <w>hours</w>.",
                            "target_lang_translations": {
                                "ru": "Миссия начинается ровно в 06:00 <w>часов</w>."
                            }
                        },
                        {
                            "text": "We expect the team to arrive by 1400 <w>hours</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ожидаем, что команда прибудет к 14:00 <w>часов</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется после числа для обозначения времени по двадцатичетырехчасовой шкале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часов",
                                "target_lang_sense_clarification": "указание времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f858cb0b-b92c-36aa-a7fc-3334ff32fecd",
                    "sense_definition": "Fixed times for prayer in certain Christian traditions, or the prayers said at these times.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The monks gathered in the chapel for the morning <w>hour</w>.",
                            "target_lang_translations": {
                                "ru": "Монахи собрались в часовне на утренний <w>час</w>."
                            }
                        },
                        {
                            "text": "She reads from the book of <w>hours</w> every day.",
                            "target_lang_translations": {
                                "ru": "Она читает книгу <w>часов</w> каждый день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "book of hours",
                        "liturgy of the hours"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установленное время для молитвы в некоторых христианских традициях или молитвы, читаемые в это время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часы",
                                "target_lang_sense_clarification": "церковная служба"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "7a0ac8b8-60de-3645-b591-80642fbc76aa",
                    "sense_definition": "Niet aanwezig zijn op een bepaalde plek.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben een weekje <w>weggeweest</w> naar Frankrijk.",
                            "target_lang_translations": {
                                "ru": "Я на недельку <w>уезжал</w> во Францию."
                            }
                        },
                        {
                            "text": "Het is soms fijn om even helemaal <w>weg</w> te <w>wezen</w> uit de drukte.",
                            "target_lang_translations": {
                                "ru": "Иногда приятно ненадолго полностью <w>уехать</w> от суеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "afwezig zijn",
                        "weg zijn",
                        "ontbreken"
                    ],
                    "antonyms": [
                        "aanwezig zijn",
                        "blijven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствовать в определенном месте или находиться в отъезде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть в отъезде",
                                "target_lang_sense_clarification": "находиться в другом месте, путешествовать"
                            },
                            {
                                "target_lang_word": "отсутствовать",
                                "target_lang_sense_clarification": "не быть на месте"
                            },
                            {
                                "target_lang_word": "уехать",
                                "target_lang_sense_clarification": "покинуть место пребывания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cf878df-5e34-3cd6-a990-195c8b561956",
                    "sense_definition": "Snel vertrekken of maken dat je wegkomt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie komt eraan, we moeten hier <w>wegwezen</w>!",
                            "target_lang_translations": {

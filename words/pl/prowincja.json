                    ],
                    "common_phrases": [
                        "prowincja kościelna",
                        "prowincja zakonna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Административная единица в структуре Католической церкви или ордена, объединяющая несколько епархий или монастырей."
                    },
                    "translations": {

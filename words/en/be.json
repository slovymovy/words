                },
                {
                    "sense_id": "f74412bf-7a62-343d-8125-8e76aea834f5",
                    "sense_definition": "Used in perfect tenses to mean 'gone to and returned from' or 'visited'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "existence_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have <w>been</w> to Spain many times."
                        },
                        {
                            "text": "Has the postman <w>been</w> today?"
                        }
                    ],
                    "common_phrases": [
                        "have been to"
                    ]
                },
                {
                    "sense_id": "cba84ab9-4cd6-3ca4-8427-8d8c815acaf8",
                    "sense_definition": "To occur or take place.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "existence_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When will the meeting <w>be</w>?"

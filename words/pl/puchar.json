{
    "word_family": [
        "pucharek",
        "pucharowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "979ade7e-48aa-370a-8768-7bae7797f2a4",
                    "sense_definition": "Trofeum w kształcie ozdobnego naczynia, przyznawane zwycięzcy zawodów sportowych lub konkursów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_trophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapitan drużyny z dumą podniósł <w>puchar</w> do góry.",
                            "target_lang_translations": {
                                "ru": "Капитан команды с гордостью поднял <w>кубок</w> вверх."
                            }
                        },
                        {
                            "text": "Zwycięzcy zrobili sobie pamiątkowe zdjęcie ze zdobytym <w>pucharem</w>.",
                            "target_lang_translations": {
                                "ru": "Победители сделали памятное фото с выигранным <w>кубком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "roztruchan"
                    ],
                    "common_phrases": [
                        "zdobyć puchar",
                        "wręczyć puchar",
                        "puchar przechodni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трофей в форме декоративного сосуда, вручаемый победителю спортивных соревнований или конкурсов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кубок",
                                "target_lang_sense_clarification": "спортивный трофей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abc95b06-b459-382b-a6cf-d2ecb5ac237b",
                    "sense_definition": "Rozgrywki sportowe, w których główną nagrodą jest puchar; turniej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_trophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza reprezentacja awansowała do finału <w>pucharu</w>.",
                            "target_lang_translations": {
                                "ru": "Наша сборная вышла в финал <w>кубка</w>."
                            }
                        },
                        {
                            "text": "Wczoraj oglądaliśmy emocjonujący mecz o <w>puchar</w> kraju.",
                            "target_lang_translations": {
                                "ru": "Вчера мы смотрели захватывающий матч за <w>кубок</w> страны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Puchar Świata",
                        "finał pucharu",
                        "mecz o puchar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивные соревнования, в которых главным призом является кубок; турнир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кубок",
                                "target_lang_sense_clarification": "соревнование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbc327c9-5e44-30f6-b1e8-6bc032705d6d",
                    "sense_definition": "Ozdobne naczynie do picia w kształcie dużego kielicha na nóżce, używane często podczas uroczystych uczt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vessel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król uniósł złoty <w>puchar</w> wina, aby wznieść toast za gości.",
                            "target_lang_translations": {
                                "ru": "Король поднял золотой <w>кубок</w> вина, чтобы произнести тост за гостей."
                            }
                        },
                        {
                            "text": "Na stole stał srebrny <w>puchar</w> wypełniony słodkim napojem.",
                            "target_lang_translations": {
                                "ru": "На столе стоял серебряный <w>кубок</w>, наполненный сладким напитком."
                            }
                        }
                    ],
                    "synonyms": [
                        "roztruchan"
                    ],
                    "common_phrases": [
                        "złoty puchar",
                        "puchar wina",
                        "wznieść puchar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный сосуд для питья в форме большой чаши на ножке, часто используемый во время торжественных пиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кубок",
                                "target_lang_sense_clarification": "сосуд для питья"
                            },
                            {
                                "target_lang_word": "чаша",
                                "target_lang_sense_clarification": "сосуд"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e13c924c-a109-377b-ae00-eac661504c39",
                    "sense_definition": "Jeden z gwiazdozbiorów nieba południowego, znany w astronomii jako Crater.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Astronom amator próbował odnaleźć <w>Puchar</w> na nocnym niebie.",
                            "target_lang_translations": {
                                "ru": "Астроном-любитель пытался найти <w>Чашу</w> на ночном небе."
                            }
                        },
                        {
                            "text": "Gwiazdozbiór ten jest znany jako <w>Puchar</w> ze względu na swój charakterystyczny kształt.",
                            "target_lang_translations": {
                                "ru": "Это созвездие известно как <w>Чаша</w> из-за своей характерной формы."
                            }
                        }
                    ],
                    "synonyms": [
                        "Gwiazdozbiór Pucharu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из созвездий южного полушария неба, известное в астрономии как Crater."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Чаша",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
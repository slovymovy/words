    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe029ed6-6759-35e5-9b79-d7826209d57b",
                    "sense_definition": "Een periode van 24 uur; de tijd die de aarde nodig heeft om één keer volledig om haar as te draaien.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zitten zeven <w>dagen</w> in een week.",
                            "target_lang_translations": {
                                "ru": "В неделе семь <w>дней</w>."
                            }
                        },
                        {
                            "text": "We gaan over twee <w>dagen</w> op vakantie naar Frankrijk.",
                            "target_lang_translations": {
                                "ru": "Мы едем в отпуск во Францию через два <w>дня</w>."
                            }
                        },
                        {
                            "text": "Hij werkt elke <w>dag</w> aan zijn nieuwe boek.",
                            "target_lang_translations": {
                                "ru": "Он работает над своей новой книгой каждый <w>день</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "etmaal"
                    ],
                    "common_phrases": [
                        "elke dag",
                        "de hele dag",
                        "dag in dag uit",
                        "van dag tot dag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени в 24 часа; время, за которое Земля совершает полный оборот вокруг своей оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день",
                                "target_lang_sense_clarification": "календарный день"
                            },
                            {
                                "target_lang_word": "сутки",
                                "target_lang_sense_clarification": "период 24 часа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7507e08-26f3-3cac-bb3e-aa5ed95556fd",
                    "sense_definition": "De periode tussen zonsopgang en zonsondergang, wanneer het buiten licht is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer is de <w>dag</w> veel langer dan in de winter.",
                            "target_lang_translations": {
                                "ru": "Летом <w>день</w> намного длиннее, чем зимой."
                            }
                        },
                        {
                            "text": "Tijdens de <w>dag</w> zijn de meeste winkels geopend.",
                            "target_lang_translations": {
                                "ru": "В течение <w>дня</w> большинство магазинов открыто."
                            }
                        }
                    ],
                    "synonyms": [
                        "licht"
                    ],
                    "antonyms": [
                        "nacht",
                        "duisternis",
                        "avond"
                    ],
                    "common_phrases": [
                        "bij dag",
                        "dag en nacht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период между восходом и закатом солнца, когда на улице светло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день",
                                "target_lang_sense_clarification": "светлое время суток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f002cf5-ade3-37db-a5e7-da98218036c9",
                    "sense_definition": "Een officiële bijeenkomst of vergadering van bestuurders, vaak in een historische context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vorst riep een <w>dag</w> bijeen om de politieke situatie te bespreken.",
                            "target_lang_translations": {
                                "ru": "Монарх созвал <w>сейм</w> для обсуждения политической ситуации."
                            }
                        },
                        {
                            "text": "Tijdens de <w>dag</w> werden belangrijke besluiten genomen over de wetgeving.",
                            "target_lang_translations": {
                                "ru": "Во время <w>сейма</w> были приняты важные решения по законодательству."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "verwijst vaak naar historische rijksdagen of vergaderingen"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное собрание или заседание правителей, часто в историческом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сейм",
                                "target_lang_sense_clarification": "историческое собрание"
                            },
                            {
                                "target_lang_word": "собрание",
                                "target_lang_sense_clarification": "встреча"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "014d61ea-53b5-3d93-9a84-f389b37682b6",
                    "sense_definition": "Een begroeting die men gebruikt bij een ontmoeting.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dag</w> meneer, weet u hoe laat de trein vertrekt?",
                            "target_lang_translations": {
                                "ru": "<w>Добрый день</w>, сэр, вы знаете, во сколько отправляется поезд?"
                            }
                        },
                        {
                            "text": "Ze zei vriendelijk <w>dag</w> toen ze de kamer binnenkwam.",
                            "target_lang_translations": {
                                "ru": "Она дружелюбно сказала «<w>здравствуйте</w>», когда вошла в комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "hallo",
                        "hoi",
                        "goedendag"
                    ],
                    "common_phrases": [
                        "goede dag",
                        "dag allemaal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствие, которое используют при встрече."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здравствуйте",
                                "target_lang_sense_clarification": "формальное/нейтральное"
                            },
                            {
                                "target_lang_word": "добрый день",
                                "target_lang_sense_clarification": "приветствие"
                            },
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "неформальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1daf2c50-8881-32e8-a13f-a689914d892c",
                    "sense_definition": "Een groet die men gebruikt bij het weggaan of afscheid nemen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet nu echt naar huis, <w>dag</w>!",
                            "target_lang_translations": {
                                "ru": "Мне правда пора домой, <w>пока</w>!"
                            }
                        },
                        {
                            "text": "<w>Dag</w> hoor, tot de volgende keer.",
                            "target_lang_translations": {
                                "ru": "<w>До свидания</w>, до следующего раза."
                            }
                        }
                    ],
                    "synonyms": [
                        "doei",
                        "tot ziens",
                        "daag",
                        "vaarwel"
                    ],
                    "common_phrases": [
                        "dag hoor",
                        "fijne dag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прощание, которое используют при уходе или расставании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до свидания",
                                "target_lang_sense_clarification": "формальное/нейтральное"
                            },
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "неформальное"
                            },
                            {
                                "target_lang_word": "счастливо",
                                "target_lang_sense_clarification": "при прощании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "50a75048-cec5-331f-9aa9-0faf41338a33",
                    "sense_definition": "Een stuk touw dat vroeger op schepen werd gebruikt om mee te straffen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_rope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matroos werd gestraft met de <w>dag</w> voor zijn wangedrag.",
                            "target_lang_translations": {
                                "ru": "Матрос был наказан <w>линем</w> за свое дурное поведение."
                            }
                        },
                        {
                            "text": "In het museum lag een oude <w>dag</w> die ooit op een VOC-schip was gebruikt.",
                            "target_lang_translations": {
                                "ru": "В музее лежал старый <w>линь</w>, который когда-то использовался на корабле VOC."
                            }
                        }
                    ],
                    "synonyms": [
                        "dagtouwtje"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historische scheepvaartterm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок веревки, который раньше использовали на кораблях для наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "линь",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0699176a-071d-3d81-8a58-bc0be6d840c2",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van daggen (iemand met een touweinde slaan).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>dag</w> de opstandige matroos als straf.",
                            "target_lang_translations": {
                                "ru": "Я <w>бью линем</w> мятежного матроса в наказание."
                            }
                        },
                        {
                            "text": "Als kapitein <w>dag</w> ik zelden, ik laat dat aan de kwartiermeester over.",
                            "target_lang_translations": {
                                "ru": "Как капитан, я редко <w>наказываю линем</w>, я оставляю это квартирмейстеру."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervoeging van verouderd werkwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола daggen (бить концом веревки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бью линем",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

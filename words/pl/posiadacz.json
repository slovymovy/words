    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f32b47b4-d6cf-3c83-957a-8d8884cfdca9",
                    "sense_definition": "Osoba, która jest właścicielem czegoś lub ma coś w swoim władaniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "posiadacz_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mógł sobie pozwolić na taki luksus, ponieważ był szczęśliwym <w>posiadaczem</w> magnetowidu i kilku kaset z filmami.",
                            "target_lang_translations": {
                                "ru": "Он мог позволить себе такую роскошь, поскольку был счастливым <w>обладателем</w> видеомагнитофона и нескольких кассет с фильмами."
                            }
                        },
                        {
                            "text": "Każdy <w>posiadacz</w> psa ma obowiązek sprzątać po swoim pupilu w miejscach publicznych.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>владелец</w> собаки обязан убирать за своим питомцем в общественных местах."
                            }
                        }
                    ],
                    "synonyms": [
                        "właściciel",
                        "posesor"
                    ],
                    "antonyms": [
                        "nieposiadacz"
                    ],
                    "common_phrases": [
                        "szczęśliwy posiadacz",
                        "posiadacz ziemski",
                        "prawny posiadacz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который является собственником чего-либо или имеет что-либо в своем владении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "владелец",
                                "target_lang_sense_clarification": "собственник имущества, животного"
                            },
                            {
                                "target_lang_word": "обладатель",
                                "target_lang_sense_clarification": "тот, кто имеет что-то, часто в выражении «счастливый обладатель»"
                            },
                            {
                                "target_lang_word": "собственник",
                                "target_lang_sense_clarification": "юридический термин, хозяин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a944d0c2-b324-3c8b-8298-fe88ddf75bf8",
                    "sense_definition": "Osoba, która ma przy sobie określony dokument, papier wartościowy lub bilet i może go okazać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "posiadacz_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako <w>posiadacz</w> biletu miesięcznego możesz podróżować komunikacją miejską bez ograniczeń.",
                            "target_lang_translations": {
                                "ru": "Как <w>владелец</w> проездного билета вы можете пользоваться общественным транспортом без ограничений."
                            }
                        },
                        {
                            "text": "W przypadku zgubienia dokumentu, jego <w>posiadacz</w> powinien niezwłocznie zgłosić ten fakt odpowiednim służbom.",
                            "target_lang_translations": {
                                "ru": "В случае утери документа его <w>владелец</w> должен немедленно сообщить об этом соответствующим службам."
                            }
                        }
                    ],
                    "synonyms": [
                        "okaziciel"
                    ],
                    "common_phrases": [
                        "posiadacz weksla",
                        "posiadacz biletu",
                        "posiadacz akcji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, имеющее при себе определенный документ, ценную бумагу или билет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держатель",
                                "target_lang_sense_clarification": "ценных бумаг, акций, карты"
                            },
                            {
                                "target_lang_word": "владелец",
                                "target_lang_sense_clarification": "документа, билета"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
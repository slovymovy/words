                {
                    "sense_id": "bdab5bce-a54e-3320-98b2-b99d3c7d00f2",
                    "sense_definition": "A device or object designed to support, contain, or secure something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "container_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please place your coffee cup in the <w>holder</w> to prevent spills.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставьте чашку с кофе в <w>держатель</w>, чтобы не пролить."
                            }
                        },
                        {
                            "text": "She bought a ceramic <w>holder</w> for her toothbrushes.",
                            "target_lang_translations": {
                                "ru": "Она купила керамическую <w>подставку</w> для зубных щеток."
                            }
                        }
                    ],
                    "synonyms": [
                        "container",
                        "stand",
                        "rack",
                        "case",
                        "mount"
                    ],
                    "common_phrases": [
                        "cup holder",
                        "candle holder",
                        "pen holder",
                        "key holder",
                        "card holder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или предмет, предназначенный для поддержки, хранения или фиксации чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держатель",
                                "target_lang_sense_clarification": "общее название приспособления"
                            },
                            {
                                "target_lang_word": "подставка",
                                "target_lang_sense_clarification": "опора для предмета"
                            },
                            {
                                "target_lang_word": "футляр",
                                "target_lang_sense_clarification": "для хранения (карт, ключей)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78cd15f7-a529-3ffb-afcf-51b1f08dd74b",
                    "sense_definition": "A person who has possession, ownership, or control of a specific document, right, or asset.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "possessor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As an account <w>holder</w>, you are entitled to certain banking privileges.",

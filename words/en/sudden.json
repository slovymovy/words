                    "sense_definition": "Suddenly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "unexpected_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wind <w>sudden</w> shifted direction during the storm.",
                            "target_lang_translations": {
                                "ru": "Ветер <w>внезапно</w> сменил направление во время бури."
                            }
                        },
                        {
                            "text": "A light <w>sudden</w> appeared in the distance.",
                            "target_lang_translations": {
                                "ru": "Вдали <w>вдруг</w> появился свет."
                            }
                        }

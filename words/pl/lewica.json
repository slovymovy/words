                    "frequency": "High",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ostatnich wyborach <lewica> zdobyła znaczącą liczbę miejsc w parlamencie.",
                            "target_lang_translations": {
                                "ru": "На последних выборах <левые> получили значительное количество мест в парламенте."
                            }
                        },
                        {
                            "text": "Skrajna <lewica> zorganizowała protest w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Крайне <левые> организовали протест в центре города."
                            }
                        }
                    ],
                    "antonyms": [
                        "prawica"
                    ],
                    "common_phrases": [
                        "skrajna lewica",
                        "nowa lewica",
                        "poglądy lewicy",
                        "lider lewicy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность политических группировок, стремящихся к социальным изменениям, равенству и социальной справедливости, часто выступающих против традиционного порядка; противоположность правым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левые",
                                "target_lang_sense_clarification": "политическое направление"
                            },
                            {
                                "target_lang_word": "левые силы",
                                "target_lang_sense_clarification": "политические группировки"
                            }
                        ]
                    }
                },
                {

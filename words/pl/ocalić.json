    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9d57215-6880-350f-abd5-87608885a036",
                    "sense_definition": "Uchronić kogoś przed śmiercią lub coś przed zniszczeniem, zagładą lub zapomnieniem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rescue_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarze przez wiele godzin walczyli, aby <ocalić> życie ciężko rannego pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врачи много часов боролись, чтобы <спасти> жизнь тяжелораненого пациента."
                            }
                        },
                        {
                            "text": "Dzięki szybkiej reakcji strażaków udało się <ocalić> zabytkowy kościół przed spaleniem.",
                            "target_lang_translations": {
                                "ru": "Благодаря быстрой реакции пожарных удалось <спасти> старинную церковь от пожара."
                            }
                        }
                    ],
                    "synonyms": [
                        "uratować"
                    ],
                    "antonyms": [
                        "zniszczyć",
                        "zabić",
                        "zgubić"
                    ],
                    "common_phrases": [
                        "ocalić życie",
                        "ocalić od zapomnienia",
                        "ocalić honor",
                        "ocalić świat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уберечь кого-то от смерти или что-то от уничтожения, гибели или забвения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спасти",
                                "target_lang_sense_clarification": "от гибели или разрушения"
                            },
                            {
                                "target_lang_word": "уберечь",
                                "target_lang_sense_clarification": "защитить от опасности"
                            },
                            {
                                "target_lang_word": "сохранить",
                                "target_lang_sense_clarification": "сберечь, напр. от забвения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76c22b6c-f4cb-3c9d-98d7-66dc2e626ad6",
                    "sense_definition": "Uniknąć śmierci lub niebezpieczeństwa; wyjść cało z groźnej sytuacji (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rescue_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tylko nielicznym pasażerom udało się <ocalić> z tonącego statku podczas sztormu.",
                            "target_lang_translations": {
                                "ru": "Лишь немногим пассажирам удалось <спастись> с тонущего корабля во время шторма."
                            }
                        },
                        {
                            "text": "Zwierzę zdołało <ocalić> się przed atakiem drapieżnika dzięki błyskawicznej ucieczce.",
                            "target_lang_translations": {
                                "ru": "Животному удалось <спастись> от нападения хищника благодаря молниеносному бегству."
                            }
                        }
                    ],
                    "synonyms": [
                        "uratować się",
                        "ocaleć",
                        "ocaleć"
                    ],
                    "antonyms": [
                        "zginąć"
                    ],
                    "common_phrases": [
                        "ocalić się ucieczką",
                        "cudem się ocalić",
                        "ocalić się z pożaru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избежать смерти или опасности; выйти невредимым из угрожающей ситуации (обычно с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спастись",
                                "target_lang_sense_clarification": "избежать опасности"
                            },
                            {
                                "target_lang_word": "уцелеть",
                                "target_lang_sense_clarification": "остаться в живых"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "common_phrases": [
                        "nakład pracy",
                        "nakład sił",
                        "dużym nakładem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вклад труда, энергии или сил, необходимый для реализации какого-либо предприятия."

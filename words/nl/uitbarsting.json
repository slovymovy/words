            "senses": [
                {
                    "sense_id": "eab3b104-5386-344c-a648-761ce5673b53",
                    "sense_definition": "Het met geweld uitstoten van lava, as en gassen door een vulkaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>uitbarsting</w> van de vulkaan zorgde voor veel as in de lucht.",
                            "target_lang_translations": {
                                "ru": "<w>Извержение</w> вулкана привело к появлению большого количества пепла в воздухе.",
                                "en": "The <w>eruption</w> of the volcano caused a lot of ash in the air."
                            }
                        },
                        {
                            "text": "Wetenschappers waarschuwden de bevolking voor een mogelijke <w>uitbarsting</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые предупредили население о возможном <w>извержении</w>.",
                                "en": "Scientists warned the population about a possible <w>eruption</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eruptie"
                    ],
                    "common_phrases": [
                        "vulkanische uitbarsting",
                        "zware uitbarsting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный выброс лавы, пепла и газов из вулкана.",
                        "en": "The violent ejection of lava, ash, and gases by a volcano."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извержение",
                                "target_lang_sense_clarification": "вулкана"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "eruption",
                                "target_lang_sense_clarification": "volcanic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c5193be-74c5-3b2f-b726-47aa86e2aa08",
                    "sense_definition": "Een plotselinge en felle uiting van hevige emoties, zoals woede of verdriet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn plotselinge <w>uitbarsting</w> van woede maakte iedereen bang.",
                            "target_lang_translations": {
                                "ru": "Его внезапная <w>вспышка</w> гнева всех напугала.",
                                "en": "His sudden <w>outburst</w> of anger scared everyone."
                            }
                        },
                        {
                            "text": "Een emotionele <w>uitbarsting</w> was te verwachten na al die spanning.",
                            "target_lang_translations": {
                                "ru": "После такого напряжения следовало ожидать эмоционального <w>взрыва</w>.",
                                "en": "An emotional <w>outburst</w> was to be expected after all that tension."
                            }
                        }
                    ],
                    "common_phrases": [
                        "emotionele uitbarsting",
                        "uitbarsting van woede"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное и бурное проявление сильных эмоций, таких как гнев или горе.",
                        "en": "A sudden and intense expression of strong emotions, such as anger or grief."
                    },

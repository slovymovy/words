            "senses": [
                {
                    "sense_id": "0581cf48-f70b-34f3-9f77-5df58afd1c93",
                    "sense_definition": "Een dik, veerkrachtig kussen dat het lichaam ondersteunt en waarop men kan liggen en slapen, meestal als onderdeel van een bed",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb gisteren een nieuw <matras> gekocht omdat ik last van mijn rug kreeg.",
                            "target_lang_translations": {
                                "ru": "Вчера я купил новый <матрас>, так как у меня начала болеть спина."
                            }
                        },
                        {
                            "text": "Ze verstopte haar spaargeld jarenlang veilig onder het <matras>.",
                            "target_lang_translations": {
                                "ru": "Она годами надежно прятала свои сбережения под <матрасом>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hard matras",
                        "zacht matras",
                        "op een matras"
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстая мягкая или пружинная подстилка для кровати, предназначенная для сна"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "матрас",
                                "target_lang_sense_clarification": "постельная принадлежность"
                            },
                            {
                                "target_lang_word": "матрац",
                                "target_lang_sense_clarification": "вариант написания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48b8b72a-8e3b-3d2b-966e-85942f5e05d2",
                    "sense_definition": "Een beschermende laag of constructie van vlechtwerk of ander materiaal, vaak gebruikt in de waterbouwkunde",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_padding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ingenieurs lieten een zinken <matras> naar de bodem zakken om de oever te beschermen.",
                            "target_lang_translations": {
                                "ru": "Инженеры опустили на дно погружной <тюфяк>, чтобы защитить берег."
                            }
                        },
                        {
                            "text": "Dit type <matras> voorkomt dat de stroming de bodem van de rivier wegspoelt.",
                            "target_lang_translations": {
                                "ru": "Этот тип <матраса> предотвращает размывание дна реки течением."
                            }
                        }
                    ],
                    "synonyms": [

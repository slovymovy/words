    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ee58692f-216b-3f97-aa05-36e734c4d669",
                    "sense_definition": "Отнимать имущество или деньги у кого-либо с применением силы или угрозами.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "theft_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Преступники планировали <грабить> банк поздно ночью.",
                            "target_lang_translations": {
                                "en": "The criminals planned to <rob> the bank late at night."
                            }
                        },
                        {
                            "text": "В тёмном переулке хулиганы пытались <грабить> прохожих.",
                            "target_lang_translations": {
                                "en": "In a dark alley, hooligans tried to <rob> passers-by."
                            }
                        }
                    ],
                    "synonyms": [
                        "ограблять",
                        "захватывать"
                    ],
                    "common_phrases": [
                        "грабить банк",
                        "грабить людей",
                        "грабить прохожих",
                        "грабить население"
                    ],
                    "target_lang_definitions": {
                        "en": "To take money or property from a person or place illegally, often using force or threats."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rob",
                                "target_lang_sense_clarification": "to steal from someone using force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bac567a7-40fc-3c4f-a545-4f8147ce63f8",
                    "sense_definition": "Разорять и опустошать города или поселения, массово захватывая чужое имущество (обычно во время войны или беспорядков).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theft_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вражеские солдаты начали <грабить> захваченный город сразу после штурма.",
                            "target_lang_translations": {
                                "en": "Enemy soldiers began to <loot> the captured city immediately after the assault."
                            }
                        },
                        {
                            "text": "Пираты любили <грабить> торговые караваны в открытом море.",
                            "target_lang_translations": {
                                "en": "Pirates loved to <plunder> merchant caravans on the open sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "разорять",
                        "опустошать"
                    ],
                    "antonyms": [
                        "обогащать"
                    ],
                    "common_phrases": [
                        "грабить деревни",
                        "грабить города",
                        "грабить караваны",
                        "грабить и убивать"
                    ],
                    "target_lang_definitions": {
                        "en": "To steal goods from a place or person, typically during a war, riot, or unrest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plunder",
                                "target_lang_sense_clarification": "to steal goods violently, often in war"
                            },
                            {
                                "target_lang_word": "loot",
                                "target_lang_sense_clarification": "to steal from a place during chaos"
                            },
                            {
                                "target_lang_word": "pillage",
                                "target_lang_sense_clarification": "to strip a place of goods using violence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfc76e83-b8be-3420-9ed3-e236d3d915cb",
                    "sense_definition": "Собирать или сгребать что-либо (обычно сено или листья) с помощью граблей.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка отправила нас <грабить> сухое сено на дальнем лугу.",
                            "target_lang_translations": {
                                "en": "Grandmother sent us to <rake> dry hay in the far meadow."
                            }
                        },
                        {
                            "text": "Осенью садовник выходит <грабить> опавшие листья в парке.",
                            "target_lang_translations": {
                                "en": "In autumn, the gardener goes out to <rake> fallen leaves in the park."
                            }
                        }
                    ],
                    "common_phrases": [
                        "грабить сено",
                        "грабить листья",
                        "грабить траву"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard Russian uses 'сгребать'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To gather leaves, hay, or other material using a rake."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rake",
                                "target_lang_sense_clarification": "to use a rake to gather material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78a114dd-e7f0-3ad3-9853-4d2479f4cf91",
                    "sense_definition": "Копировать цифровые данные (музыку, видео) с внешнего носителя на компьютер или делать снимок экрана.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг научил меня <грабить> музыку с компакт-дисков в хорошем качестве.",
                            "target_lang_translations": {
                                "en": "My friend taught me how to <rip> music from CDs in high quality."
                            }
                        },
                        {
                            "text": "Для создания этой инструкции мне пришлось <грабить> видео с экрана монитора.",
                            "target_lang_translations": {
                                "en": "To create this manual, I had to <capture> video from the monitor screen."
                            }
                        }
                    ],
                    "synonyms": [
                        "капчурить",
                        "захватывать"
                    ],
                    "common_phrases": [
                        "грабить диски",
                        "грабить видео",
                        "грабить экран"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computer slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To copy audio or video data from a storage medium (like a CD or DVD) to a computer hard drive."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rip",
                                "target_lang_sense_clarification": "to copy digital data from a disc"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "32d9dcc0-698e-3d40-8a46-8eeb34818555",
                    "sense_definition": "Met een zeer hoog volume; erg luid.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de <w>keiharde</w> muziek konden we elkaar niet verstaan.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>оглушительной</w> музыки мы не слышали друг друга.",
                                "en": "Because of the <w>deafening</w> music, we couldn't understand each other."
                            }
                        },
                        {
                            "text": "Er klonk een <w>keiharde</w> knal uit de keuken.",
                            "target_lang_translations": {
                                "ru": "Из кухни раздался <w>оглушительный</w> хлопок.",
                                "en": "A <w>deafening</w> bang sounded from the kitchen."
                            }
                        }
                    ],
                    "synonyms": [
                        "oorverdovend",
                        "luid"
                    ],
                    "antonyms": [
                        "zacht",
                        "stil"
                    ],
                    "common_phrases": [
                        "keiharde muziek",
                        "keiharde knal",
                        "keihard geluid"
                    ],
                    "target_lang_definitions": {
                        "ru": "С очень высокой громкостью; очень громкий.",
                        "en": "With a very high volume; very loud."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оглушительный",
                                "target_lang_sense_clarification": "настолько громкий, что закладывает уши"
                            },
                            {
                                "target_lang_word": "громкий",
                                "target_lang_sense_clarification": "общий термин для высокого уровня звука"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "deafening",
                                "target_lang_sense_clarification": "so loud that it is overwhelming"
                            },
                            {
                                "target_lang_word": "very loud",
                                "target_lang_sense_clarification": "general term for high volume"
                            },
                            {
                                "target_lang_word": "ear-splitting",
                                "target_lang_sense_clarification": "painfully loud"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9e72d42-5af1-3915-beef-caa01cc65acc",
                    "sense_definition": "Zeer hard van materiaal of structuur; niet in te drukken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_hardness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Diamant is een <w>keiharde</w> edelsteen.",

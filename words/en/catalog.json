            "senses": [
                {
                    "sense_id": "58cbfa73-3376-35cc-8299-49f2fdef0f8f",
                    "sense_definition": "To make a systematic list of items; to classify or record things in a list.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "list_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum staff worked for months to <w>catalog</w> every artifact in the collection.",

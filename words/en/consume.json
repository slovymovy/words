{
    "word_family": [
        "consumer",
        "consumption",
        "consumable",
        "consumerism"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dfe8e0cf-d15f-3652-b53c-a22a3bcac169",
                    "sense_definition": "To use a supply of something, such as fuel, energy, or time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "utilization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new factory <w>consumes</w> a vast amount of electricity every day.",
                            "target_lang_translations": {
                                "ru": "Новый завод <w>потребляет</w> огромное количество электроэнергии каждый день."
                            }
                        },
                        {
                            "text": "This complex project will <w>consumes</w> all of our available resources.",
                            "target_lang_translations": {
                                "ru": "Этот сложный проект <w>поглотит</w> все наши доступные ресурсы."
                            }
                        }
                    ],
                    "synonyms": [
                        "use",
                        "use up",
                        "expend",
                        "burn"
                    ],
                    "antonyms": [
                        "save",
                        "conserve"
                    ],
                    "common_phrases": [
                        "consume energy",
                        "consume time",
                        "consume resources",
                        "consume fuel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать запас чего-либо, например, топлива, энергии или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потреблять",
                                "target_lang_sense_clarification": "использовать энергию или ресурсы"
                            },
                            {
                                "target_lang_word": "расходовать",
                                "target_lang_sense_clarification": "тратить запас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88ba2f22-a601-3b03-8802-1d103d0a192b",
                    "sense_definition": "To eat or drink something, especially in large amounts.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "ingestion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The athletes need to <w>consume</w> plenty of protein after their workout.",
                            "target_lang_translations": {
                                "ru": "Спортсменам необходимо <w>употреблять</w> много белка после тренировки."
                            }
                        },
                        {
                            "text": "It is not safe to <w>consume</w> alcohol before driving a car.",
                            "target_lang_translations": {
                                "ru": "Небезопасно <w>употреблять</w> алкоголь перед вождением автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "eat",
                        "devour",
                        "swallow"
                    ],
                    "common_phrases": [
                        "consume alcohol",
                        "consume food",
                        "fit to consume"
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть или пить что-либо, особенно в больших количествах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "употреблять",
                                "target_lang_sense_clarification": "принимать пищу или напитки"
                            },
                            {
                                "target_lang_word": "съедать",
                                "target_lang_sense_clarification": "есть полностью"
                            },
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "есть жадно или много"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6f51a40-965d-3bd6-8d9c-df9b25afc0d3",
                    "sense_definition": "To buy and use goods or services.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "utilization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People tend to <w>consume</w> more goods during the holiday season.",
                            "target_lang_translations": {
                                "ru": "Люди склонны <w>потреблять</w> больше товаров в праздничный сезон."
                            }
                        },
                        {
                            "text": "We must learn to <w>consume</w> less if we want to protect the environment.",
                            "target_lang_translations": {
                                "ru": "Мы должны научиться <w>потреблять</w> меньше, если хотим защитить окружающую среду."
                            }
                        }
                    ],
                    "antonyms": [
                        "produce"
                    ],
                    "common_phrases": [
                        "consume goods",
                        "consume products"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покупать и использовать товары или услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потреблять",
                                "target_lang_sense_clarification": "экономический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76d212f2-f644-3586-809b-0af34111b894",
                    "sense_definition": "To destroy something completely, often by fire.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old wooden barn was quickly <w>consumed</w> by the flames.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный сарай был быстро <w>поглощен</w> пламенем."
                            }
                        },
                        {
                            "text": "A massive wildfire threatened to <w>consume</w> the entire forest.",
                            "target_lang_translations": {
                                "ru": "Масштабный лесной пожар угрожал <w>уничтожить</w> весь лес."
                            }
                        }
                    ],
                    "synonyms": [
                        "destroy",
                        "devastate",
                        "wipe out",
                        "annihilate",
                        "obliterate",
                        "raze"
                    ],
                    "common_phrases": [
                        "consumed by fire",
                        "consumed by flames"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью уничтожать что-либо, часто огнем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уничтожать",
                                "target_lang_sense_clarification": "разрушать полностью"
                            },
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "об огне или стихии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "761476f4-773a-33c9-ab20-d42c541e372b",
                    "sense_definition": "To fill someone's mind or attention completely.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was <w>consumed</w> with guilt after lying to her best friend.",
                            "target_lang_translations": {
                                "ru": "Она была <w>поглощена</w> чувством вины после того, как солгала лучшей подруге."
                            }
                        },
                        {
                            "text": "His desire to win the competition began to <w>consume</w> him entirely.",
                            "target_lang_translations": {
                                "ru": "Желание победить в соревновании начало полностью <w>поглощать</w> его."
                            }
                        }
                    ],
                    "synonyms": [
                        "occupy",
                        "overcome",
                        "take over"
                    ],
                    "common_phrases": [
                        "consumed by guilt",
                        "consumed with envy",
                        "consumed by rage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью занимать чьи-либо мысли или внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "всецело захватывать внимание"
                            },
                            {
                                "target_lang_word": "съедать",
                                "target_lang_sense_clarification": "мучить (о чувствах, перен.)"
                            },
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "овладевать (о чувствах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a68ff4f-2f9c-3b4f-a748-074ab339699b",
                    "sense_definition": "To absorb information, especially through mass media.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "utilization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Young people today <w>consume</w> news differently than their parents did.",
                            "target_lang_translations": {
                                "ru": "Молодые люди сегодня <w>потребляют</w> новости иначе, чем их родители."
                            }
                        },
                        {
                            "text": "It is easy to <w>consume</w> too much content on social media without realizing it.",
                            "target_lang_translations": {
                                "ru": "Легко <w>потребить</w> слишком много контента в социальных сетях, не осознавая этого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "consume news",
                        "consume media",
                        "consume content"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воспринимать информацию, особенно через средства массовой информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потреблять",
                                "target_lang_sense_clarification": "контент или новости"
                            },
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "впитывать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8389fd94-234f-30cc-be21-0e95a7bc902e",
                    "sense_definition": "To waste away physically or decay.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The terrible disease caused the patient to <w>consume</w> away slowly.",
                            "target_lang_translations": {
                                "ru": "Ужасная болезнь заставила пациента медленно <w>угасать</w>."
                            }
                        },
                        {
                            "text": "Without hope, his spirit began to <w>consume</w> in the dark prison cell.",
                            "target_lang_translations": {
                                "ru": "Без надежды его дух начал <w>чахнуть</w> в темной тюремной камере."
                            }
                        }
                    ],
                    "common_phrases": [
                        "consume away"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически истощаться или угасать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чахнуть",
                                "target_lang_sense_clarification": "увядать, болеть"
                            },
                            {
                                "target_lang_word": "угасать",
                                "target_lang_sense_clarification": "медленно умирать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "synonyms": [
                        "subsequent",
                        "derivative"
                    ],
                    "common_phrases": [
                        "secondary source",
                        "secondary effect",
                        "secondary stage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий за первым или основным по времени, месту или порядку; не оригинальный, а производный."

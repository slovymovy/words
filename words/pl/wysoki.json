    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "056cd15a-c142-3fb4-b2fa-28958d7f6961",
                    "sense_definition": "Mający duży wymiar pionowy; odnoszący się do wzrostu człowieka lub wysokości obiektu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest bardzo <wysoki> i świetnie gra w koszykówkę.",
                            "target_lang_translations": {
                                "ru": "Мой брат очень <высокий> и отлично играет в баскетбол."
                            }
                        },
                        {
                            "text": "W centrum miasta wybudowano nowy, <wysoki> biurowiec ze szkła.",
                            "target_lang_translations": {
                                "ru": "В центре города построили новое <высокое> офисное здание из стекла."
                            }
                        }
                    ],
                    "synonyms": [
                        "wielki",
                        "duży"
                    ],
                    "antonyms": [
                        "niski"
                    ],
                    "common_phrases": [
                        "wysoki mężczyzna",
                        "wysoki budynek",
                        "wysokie czoło",
                        "wysokie drzewo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большую протяженность по вертикали; относящийся к росту человека или высоте объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокий",
                                "target_lang_sense_clarification": "о росте или размере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e77e5e68-72d0-3d3f-891e-36eb8b4a050c",
                    "sense_definition": "O dużej wartości, natężeniu, poziomie lub liczbie (np. temperatura, cena, ciśnienie).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_magnitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem w tym regionie panuje bardzo <wysoka> temperatura powietrza.",
                            "target_lang_translations": {
                                "ru": "Летом в этом регионе стоит очень <высокая> температура воздуха."
                            }
                        },
                        {
                            "text": "Niestety <wysoka> cena tego samochodu przekracza nasze możliwości finansowe.",
                            "target_lang_translations": {
                                "ru": "К сожалению, <высокая> цена этого автомобиля превышает наши финансовые возможности."
                            }
                        }
                    ],
                    "synonyms": [
                        "duży"
                    ],
                    "antonyms": [
                        "niski"
                    ],
                    "common_phrases": [
                        "wysoka temperatura",
                        "wysoka cena",
                        "wysokie ciśnienie",
                        "wysokie ryzyko"
                    ],
                    "target_lang_definitions": {
                        "ru": "О большой величине, интенсивности, уровне или количестве (напр. температура, цена, давление)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокий",
                                "target_lang_sense_clarification": "о величине, уровне или интенсивности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bf33eb2-aba4-3339-a192-ff4e339f29ef",
                    "sense_definition": "Znajdujący się na dużej wysokości nad ziemią lub poziomem morza.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z naszego balkonu na <wysokim> piętrze widać całą panoramę miasta.",
                            "target_lang_translations": {
                                "ru": "С нашего балкона на <высоком> этаже видна вся панорама города."
                            }
                        },
                        {
                            "text": "W <wysokich> górach pogoda zmienia się bardzo gwałtownie.",
                            "target_lang_translations": {
                                "ru": "В <высоких> горах погода меняется очень резко."
                            }
                        }
                    ],
                    "antonyms": [
                        "niski"
                    ],
                    "common_phrases": [
                        "wysokie piętro",
                        "wysokie góry",
                        "wysoki parter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на большой высоте над землей или уровнем моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокий",
                                "target_lang_sense_clarification": "расположенный на высоте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a28dbe12-8c23-36eb-a791-1c59511058dd",
                    "sense_definition": "Mający duże znaczenie, prestiż, jakość lub zajmujący ważne miejsce w hierarchii.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki ciężkiej pracy awansował na bardzo <wysokie> stanowisko w firmie.",
                            "target_lang_translations": {
                                "ru": "Благодаря упорному труду он получил повышение на очень <высокую> должность в компании."
                            }
                        },
                        {
                            "text": "Ten producent słynie z oferowania towarów bardzo <wysokiej> jakości.",
                            "target_lang_translations": {
                                "ru": "Этот производитель славится товарами очень <высокого> качества."
                            }
                        }
                    ],
                    "synonyms": [
                        "ważny",
                        "wspaniały"
                    ],
                    "antonyms": [
                        "niski"
                    ],
                    "common_phrases": [
                        "wysokie stanowisko",
                        "wysoka jakość",
                        "wysoki poziom",
                        "wysoki urzędnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большое значение, престиж, качество или занимающий важное место в иерархии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокий",
                                "target_lang_sense_clarification": "о статусе, качестве или должности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56d61236-0357-35c6-8ff6-a72ba26bf4ff",
                    "sense_definition": "O dźwięku: mający dużą częstotliwość, brzmiący cienko lub piskliwie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "acoustic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Śpiewaczka operowa potrafi wydobyć z siebie niezwykle <wysokie> dźwięki.",
                            "target_lang_translations": {
                                "ru": "Оперная певица способна брать невероятно <высокие> ноты."
                            }
                        },
                        {
                            "text": "Ten instrument charakteryzuje się bardzo <wysokim> i czystym tonem.",
                            "target_lang_translations": {
                                "ru": "Этот инструмент отличается очень <высоким> и чистым тоном."
                            }
                        }
                    ],
                    "antonyms": [
                        "niski"
                    ],
                    "common_phrases": [
                        "wysoki głos",
                        "wysoki ton",
                        "wysoki dźwięk"
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке: имеющий большую частоту, звучащий тонко или пискляво."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокий",
                                "target_lang_sense_clarification": "о звуке или голосе"
                            },
                            {
                                "target_lang_word": "тонкий",
                                "target_lang_sense_clarification": "о голосе"
                            }
                        ]
                    }
                }
            ]
        }

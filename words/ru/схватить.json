    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b1177e13-dec5-30e4-83ac-53d3a2e07460",
                    "sense_definition": "Быстро и резко взять кого-либо или что-либо руками, зубами или инструментом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок попытался <w>схватить</w> кошку за хвост.",
                            "target_lang_translations": {
                                "en": "The child tried to <w>grab</w> the cat by the tail."
                            }
                        },
                        {
                            "text": "Он успел <w>схватить</w> падающую вазу.",
                            "target_lang_translations": {
                                "en": "He managed to <w>catch</w> the falling vase."
                            }
                        },
                        {
                            "text": "Собака <w>схватила</w> брошенную ей кость.",
                            "target_lang_translations": {
                                "en": "The dog <w>snatched</w> the bone thrown to it."
                            }
                        }
                    ],
                    "synonyms": [
                        "ухватить",
                        "захватить"
                    ],
                    "antonyms": [
                        "отпустить",
                        "выпустить"
                    ],
                    "common_phrases": [
                        "схватить за руку",
                        "схватить со стола",
                        "схватить в охапку"
                    ],
                    "target_lang_definitions": {
                        "en": "To take someone or something quickly and abruptly with hands, teeth, or a tool."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grab",
                                "target_lang_sense_clarification": "take quickly and roughly"
                            },
                            {
                                "target_lang_word": "seize",
                                "target_lang_sense_clarification": "take hold of suddenly or forcibly"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "intercept something falling or thrown"
                            },
                            {
                                "target_lang_word": "clutch",
                                "target_lang_sense_clarification": "grasp tightly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3de7754-b84a-303c-867d-859533734765",
                    "sense_definition": "Силой задержать, лишить свободы; арестовать.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "detention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция <w>схватила</w> преступника на месте кражи.",
                            "target_lang_translations": {
                                "en": "The police <w>caught</w> the criminal at the scene of the theft."
                            }
                        },
                        {
                            "text": "Охрана <w>схватила</w> нарушителя у входа.",
                            "target_lang_translations": {
                                "en": "Security <w>apprehended</w> the intruder at the entrance."
                            }
                        }
                    ],
                    "synonyms": [
                        "арестовать",
                        "скрутить"
                    ],
                    "antonyms": [
                        "отпустить",
                        "выпустить"
                    ],
                    "common_phrases": [
                        "схватить вора",
                        "схватить преступника",
                        "схватить за шиворот"
                    ],
                    "target_lang_definitions": {
                        "en": "To detain by force, deprive of liberty; to arrest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "capture",
                                "target_lang_sense_clarification": "take into custody"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "find and stop someone escaping"
                            },
                            {
                                "target_lang_word": "apprehend",
                                "target_lang_sense_clarification": "arrest (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d85c7c6-9ff0-30f5-ada7-3ccf4ddcb4da",
                    "sense_definition": "Заразиться болезнью или получить что-либо неприятное (например, плохую оценку).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "acquisition_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я вышел на улицу без шапки и <w>схватил</w> насморк.",
                            "target_lang_translations": {
                                "en": "I went outside without a hat and <w>caught</w> a cold."
                            }
                        },
                        {
                            "text": "Студент не подготовился и <w>схватил</w> двойку на экзамене.",
                            "target_lang_translations": {
                                "en": "The student didn't prepare and <w>got</w> an F on the exam."
                            }
                        }
                    ],
                    "synonyms": [
                        "подцепить",
                        "заполучить"
                    ],
                    "common_phrases": [
                        "схватить простуду",
                        "схватить двойку",
                        "схватить грипп"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become infected with a disease or receive something unpleasant (e.g., a bad grade)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "contract an illness"
                            },
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "acquire informally"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "receive (a grade or penalty)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61ad2eef-506a-316f-a119-8302ceb15c2b",
                    "sense_definition": "Быстро понять, воспринять или усвоить какую-либо мысль или информацию.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_grasp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученик буквально на лету <w>схватил</w> новую тему.",
                            "target_lang_translations": {
                                "en": "The student <w>grasped</w> the new topic literally on the fly."
                            }
                        },
                        {
                            "text": "Он мгновенно <w>схватил</w> суть проблемы.",
                            "target_lang_translations": {
                                "en": "He instantly <w>caught</w> the essence of the problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "понять",
                        "усвоить",
                        "ухватить"
                    ],
                    "common_phrases": [
                        "схватить на лету",
                        "схватить суть",
                        "схватить мысль"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "figurative usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To quickly understand, perceive, or assimilate a thought or information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grasp",
                                "target_lang_sense_clarification": "understand fully"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "perceive or hear"
                            },
                            {
                                "target_lang_word": "catch on",
                                "target_lang_sense_clarification": "understand quickly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bda94eb7-f563-3401-ba5f-05e98b2b339b",
                    "sense_definition": "Внезапно причинить острую боль или вызвать спазм (обычно безличная форма).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгого бега у спортсмена <w>схватило</w> бок.",
                            "target_lang_translations": {
                                "en": "After a long run, the athlete's side <w>cramped</w>."
                            }
                        },
                        {
                            "text": "Внезапно <w>схватило</w> живот, и пришлось идти к врачу.",
                            "target_lang_translations": {
                                "en": "Suddenly my stomach <w>seized up</w>, and I had to go to the doctor."
                            }
                        }
                    ],
                    "synonyms": [
                        "прихватить"
                    ],
                    "antonyms": [
                        "отпустить"
                    ],
                    "common_phrases": [
                        "живот схватило",
                        "сердце схватило",
                        "спину схватило"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often impersonal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly cause acute pain or cause a spasm (usually impersonal)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seize",
                                "target_lang_sense_clarification": "affect suddenly with pain"
                            },
                            {
                                "target_lang_word": "cramp",
                                "target_lang_sense_clarification": "suffer a muscle contraction"
                            },
                            {
                                "target_lang_word": "grip",
                                "target_lang_sense_clarification": "afflict strongly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29ce0442-7b4e-3d60-bb83-bc8c786cd78c",
                    "sense_definition": "Прочно соединить, скрепить или затвердеть (о клее, растворе, льде).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "binding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный мороз за ночь <w>схватил</w> лужи льдом.",
                            "target_lang_translations": {
                                "en": "A strong frost <w>froze</w> the puddles overnight."
                            }
                        },
                        {
                            "text": "Клей должен быстро <w>схватить</w> детали стула.",
                            "target_lang_translations": {
                                "en": "The glue must quickly <w>set</w> the chair parts."
                            }
                        }
                    ],
                    "synonyms": [
                        "сковать",
                        "прихватить"
                    ],
                    "common_phrases": [
                        "мороз схватил",
                        "клей схватил"
                    ],
                    "target_lang_definitions": {
                        "en": "To join firmly, fasten, or harden (about glue, mortar, ice)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "harden (glue, concrete)"
                            },
                            {
                                "target_lang_word": "freeze",
                                "target_lang_sense_clarification": "turn to ice"
                            },
                            {
                                "target_lang_word": "bond",
                                "target_lang_sense_clarification": "stick together"
                            }
                        ]
                    }
                }
            ]
        }
    ]

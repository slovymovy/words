{
    "word_family": [
        "gracieus",
        "disgratie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f377836e-be34-3bcd-9d6d-c82fc0d40bf3",
                    "sense_definition": "Natuurlijke sierlijkheid en elegantie in houding of beweging.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De danseres bewoog zich met veel <w>gratie</w> over het podium.",
                            "target_lang_translations": {
                                "ru": "Танцовщица двигалась по сцене с большой <w>грацией</w>.",
                                "en": "The dancer moved across the stage with great <w>grace</w>."
                            }
                        },
                        {
                            "text": "Zij accepteerde het compliment met een zekere <w>gratie</w> en glimlachte.",
                            "target_lang_translations": {
                                "ru": "Она приняла комплимент с некоторой <w>грацией</w> и улыбнулась.",
                                "en": "She accepted the compliment with a certain <w>grace</w> and smiled."
                            }
                        }
                    ],
                    "synonyms": [
                        "sierlijkheid",
                        "elegantie",
                        "bevalligheid"
                    ],
                    "antonyms": [
                        "lompheid",
                        "stijfheid"
                    ],
                    "common_phrases": [
                        "met gratie",
                        "natuurlijke gratie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественное изящество и элегантность в осанке или движениях.",
                        "en": "Natural gracefulness and elegance in posture or movement."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грация",
                                "target_lang_sense_clarification": "изящество, красота движений"
                            },
                            {
                                "target_lang_word": "изящество",
                                "target_lang_sense_clarification": "элегантность, утонченность"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "grace",
                                "target_lang_sense_clarification": "elegance of movement"
                            },
                            {
                                "target_lang_word": "gracefulness",
                                "target_lang_sense_clarification": "quality of being graceful"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aedbe578-65b5-3994-8903-f2511e8aa515",
                    "sense_definition": "Het besluit van een bevoegd gezag om een straf kwijt te schelden of te verminderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mercy_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De president besloot <w>gratie</w> te verlenen aan de gevangene.",
                            "target_lang_translations": {
                                "ru": "Президент решил даровать <w>помилование</w> заключенному.",
                                "en": "The president decided to grant the prisoner a <w>pardon</w>."
                            }
                        },
                        {
                            "text": "Zijn advocaat diende een officieel verzoek om <w>gratie</w> in.",
                            "target_lang_translations": {
                                "ru": "Его адвокат подал официальное прошение о <w>помиловании</w>.",
                                "en": "His lawyer submitted an official request for <w>clemency</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "strafkwijtschelding",
                        "pardon",
                        "kwijtschelding"
                    ],
                    "common_phrases": [
                        "gratie verlenen",
                        "verzoek om gratie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решение компетентного органа об отмене или смягчении наказания.",
                        "en": "The decision by a competent authority to remit or reduce a punishment."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помилование",
                                "target_lang_sense_clarification": "освобождение от наказания"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pardon",
                                "target_lang_sense_clarification": "official forgiveness of a crime"
                            },
                            {
                                "target_lang_word": "clemency",
                                "target_lang_sense_clarification": "mercy shown by an authority"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac6433b5-9b8e-357a-b05a-6e2ac9310561",
                    "sense_definition": "Een gunst of welwillendheid die iemand ontvangt, vaak van een hogere macht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mercy_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zijn excuses kwam hij weer in de <w>gratie</w> bij zijn werkgever.",
                            "target_lang_translations": {
                                "ru": "После извинений он снова вернул <w>благосклонность</w> своего работодателя.",
                                "en": "After his apology, he came back into <w>favor</w> with his employer."
                            }
                        },
                        {
                            "text": "De vorst regeert volgens de wet bij de <w>gratie</w> Gods.",
                            "target_lang_translations": {
                                "ru": "Монарх правит по закону Божьей <w>милостью</w>.",
                                "en": "The monarch rules according to the law by the <w>grace</w> of God."
                            }
                        }
                    ],
                    "synonyms": [
                        "gunst",
                        "genade"
                    ],
                    "antonyms": [
                        "ongenade"
                    ],
                    "common_phrases": [
                        "in gratie vallen",
                        "bij de gratie Gods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Благосклонность или милость, которую кто-то получает, часто от высшей силы.",
                        "en": "A favor or benevolence received by someone, often from a higher power."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "милость",
                                "target_lang_sense_clarification": "прощение, высшее покровительство"
                            },
                            {
                                "target_lang_word": "благосклонность",
                                "target_lang_sense_clarification": "доброжелательное отношение, расположение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "grace",
                                "target_lang_sense_clarification": "divine favor or mercy"
                            },
                            {
                                "target_lang_word": "favor",
                                "target_lang_sense_clarification": "approval or support"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
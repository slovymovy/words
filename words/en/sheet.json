{
    "word_family": [
        "sheeting"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6dc67238-59b6-39cb-a80d-6269566a0578",
                    "sense_definition": "A large rectangular piece of cotton or other fabric, used on a bed to cover the mattress or as a layer beneath the sleeper.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to wash the dirty <w>sheets</w> and put fresh ones on the bed.",
                            "target_lang_translations": {
                                "ru": "Мне нужно постирать грязные <w>простыни</w> и постелить свежие."
                            }
                        },
                        {
                            "text": "She pulled the <w>sheet</w> up to her chin because the room was cold.",
                            "target_lang_translations": {
                                "ru": "Она натянула <w>простыню</w> до подбородка, потому что в комнате было холодно."
                            }
                        }
                    ],
                    "synonyms": [
                        "blanket"
                    ],
                    "common_phrases": [
                        "clean sheets",
                        "fitted sheet",
                        "flat sheet",
                        "change the sheets",
                        "bed sheet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой прямоугольный кусок хлопчатобумажной или другой ткани, используемый на кровати для покрытия матраса или как слой под спящим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простыня",
                                "target_lang_sense_clarification": "постельное белье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f6be804-482f-316f-a95a-ce5a814d652e",
                    "sense_definition": "A rectangular piece of paper used for writing, printing, or drawing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "paper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please take a blank <w>sheet</w> of paper and write down your name.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, возьмите чистый <w>лист</w> бумаги и запишите свое имя."
                            }
                        },
                        {
                            "text": "The teacher handed out a <w>sheet</w> with all the math problems on it.",
                            "target_lang_translations": {
                                "ru": "Учитель раздал <w>листок</w> со всеми математическими задачами."
                            }
                        }
                    ],
                    "synonyms": [
                        "page",
                        "blanket"
                    ],
                    "common_phrases": [
                        "sheet of paper",
                        "answer sheet",
                        "cheat sheet",
                        "blank sheet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямоугольный кусок бумаги, используемый для письма, печати или рисования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лист",
                                "target_lang_sense_clarification": "бумаги"
                            },
                            {
                                "target_lang_word": "листок",
                                "target_lang_sense_clarification": "небольшой лист бумаги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d90fd40-5259-358c-aa80-5660ae0ff297",
                    "sense_definition": "A thin, flat, usually rectangular piece or layer of a solid material, such as metal, glass, or ice.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car body is made from a large <w>sheet</w> of steel.",
                            "target_lang_translations": {
                                "ru": "Кузов автомобиля сделан из большого стального <w>листа</w>."
                            }
                        },
                        {
                            "text": "Be careful walking on the sidewalk because it is covered in a <w>sheet</w> of ice.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны на тротуаре, потому что он покрыт <w>слоем</w> льда."
                            }
                        }
                    ],
                    "synonyms": [
                        "layer",
                        "coating",
                        "page"
                    ],
                    "common_phrases": [
                        "sheet metal",
                        "sheet of ice",
                        "sheet glass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий, плоский, обычно прямоугольный кусок или слой твердого материала, такого как металл, стекло или лед."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лист",
                                "target_lang_sense_clarification": "металла, стекла"
                            },
                            {
                                "target_lang_word": "слой",
                                "target_lang_sense_clarification": "льда, покрытия"
                            },
                            {
                                "target_lang_word": "пласт",
                                "target_lang_sense_clarification": "материала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25694a6c-de98-38f8-b5c1-0d5d3ef55581",
                    "sense_definition": "A flat metal tray used for baking food in an oven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Place the cookies on the baking <w>sheet</w> and put them in the oven.",
                            "target_lang_translations": {
                                "ru": "Положите печенье на <w>противень</w> и поставьте в духовку."
                            }
                        },
                        {
                            "text": "You should grease the <w>sheet</w> so the food does not stick to it.",
                            "target_lang_translations": {
                                "ru": "Вам следует смазать <w>противень</w> жиром, чтобы еда не прилипала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "baking sheet",
                        "cookie sheet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский металлический поднос, используемый для запекания еды в духовке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противень",
                                "target_lang_sense_clarification": "для выпечки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bee627a2-f0ed-3119-bb99-19d91a44de56",
                    "sense_definition": "A broad, moving expanse of something, such as rain, fire, or water.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expanse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rain came down in <w>sheets</w>, making it impossible to see the road.",
                            "target_lang_translations": {
                                "ru": "Дождь лил <w>стеной</w>, из-за чего невозможно было разглядеть дорогу."
                            }
                        },
                        {
                            "text": "A sudden <w>sheet</w> of flame rose from the burning building.",
                            "target_lang_translations": {
                                "ru": "Внезапная <w>стена</w> огня поднялась из горящего здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "blanket",
                        "layer",
                        "coat"
                    ],
                    "common_phrases": [
                        "sheets of rain",
                        "sheet of flame",
                        "sheet of water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкое движущееся пространство чего-либо, например дождя, огня или воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стена",
                                "target_lang_sense_clarification": "огня, дождя"
                            },
                            {
                                "target_lang_word": "пелена",
                                "target_lang_sense_clarification": "сплошной покров"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9fac112-4f16-32f6-9e55-1b84c4d037ea",
                    "sense_definition": "A polite or euphemistic way of saying the word 'shit'.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oh <w>sheet</w>, I forgot to bring my wallet!",
                            "target_lang_translations": {
                                "ru": "О <w>блин</w>, я забыл взять кошелек!"
                            }
                        },
                        {
                            "text": "This whole situation is a pile of <w>sheet</w>.",
                            "target_lang_translations": {
                                "ru": "Вся эта ситуация — полная <w>фигня</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "euphemism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливый или эвфемистический способ произнесения ругательства 'shit'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блин",
                                "target_lang_sense_clarification": "эвфемизм"
                            },
                            {
                                "target_lang_word": "чёрт",
                                "target_lang_sense_clarification": "ругательство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "341ad425-9c92-3497-8bb9-13b5c2497275",
                    "sense_definition": "A rope or line used to control the position of a sail on a boat.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor pulled the <w>sheet</w> tight to catch the wind.",
                            "target_lang_translations": {
                                "ru": "Матрос туго натянул <w>шкот</w>, чтобы поймать ветер."
                            }
                        },
                        {
                            "text": "If you let go of the <w>sheet</w>, the sail will flap loosely.",
                            "target_lang_translations": {
                                "ru": "Если вы отпустите <w>шкот</w>, парус будет свободно болтаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "rope"
                    ],
                    "common_phrases": [
                        "main sheet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "nautical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Веревка или трос, используемые для управления положением паруса на лодке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкот",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0f269a1a-f1d0-3fc7-8a77-898b73be1175",
                    "sense_definition": "To rain very heavily.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "weather_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stayed inside because the rain was <w>sheeting</w> down all afternoon.",
                            "target_lang_translations": {
                                "ru": "Мы остались внутри, потому что весь день <w>лил</w> дождь."
                            }
                        },
                        {
                            "text": "Water <w>sheeted</w> off the roof during the storm.",
                            "target_lang_translations": {
                                "ru": "Во время шторма вода <w>скатывалась</w> с крыши сплошным потоком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sheet down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти очень сильным потоком (о дожде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лить как из ведра",
                                "target_lang_sense_clarification": "о сильном дожде"
                            },
                            {
                                "target_lang_word": "хлестать",
                                "target_lang_sense_clarification": "о дожде или воде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea4546ac-81b0-322c-8003-c7697b89d8db",
                    "sense_definition": "To cover or wrap something with a large piece of cloth or similar material.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cover_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>sheeted</w> the furniture to protect it from dust while painting.",
                            "target_lang_translations": {
                                "ru": "Они <w>накрыли</w> мебель, чтобы защитить её от пыли во время покраски."
                            }
                        },
                        {
                            "text": "The hills were <w>sheeted</w> in a thick layer of snow.",
                            "target_lang_translations": {
                                "ru": "Холмы были <w>укрыты</w> толстым слоем снега."
                            }
                        }
                    ],
                    "synonyms": [
                        "coat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать или оборачивать что-либо большим куском ткани или подобного материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "накрывать тканью"
                            },
                            {
                                "target_lang_word": "укрывать",
                                "target_lang_sense_clarification": "защищать покрытием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c28e2695-dcb9-3ed7-88de-c6cb6df07f8d",
                    "sense_definition": "To adjust or trim a sail using a rope.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crew <w>sheeted</w> the sails to gain more speed.",
                            "target_lang_translations": {
                                "ru": "Экипаж <w>настроил</w> паруса, чтобы набрать скорость."
                            }
                        },
                        {
                            "text": "You need to <w>sheet</w> in the mainsail as we turn.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>подобрать</w> грот на повороте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sheet in",
                        "sheet home"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "nautical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулировать или настраивать парус с помощью троса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "шкот"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "парусом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "611ebadd-cb42-31a1-9921-3c008794d6c0",
                    "sense_definition": "A village in Hampshire, England.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited a small village called <w>Sheet</w> during their trip to England.",
                            "target_lang_translations": {
                                "ru": "Во время поездки в Англию они посетили небольшую деревню под названием <w>Шит</w>."
                            }
                        },
                        {
                            "text": "The road signs pointed towards <w>Sheet</w> and other local towns.",
                            "target_lang_translations": {
                                "ru": "Дорожные знаки указывали на <w>Шит</w> и другие местные города."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня в графстве Гэмпшир, Англия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Шит",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
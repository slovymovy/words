    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "289effa4-17a8-34a6-b424-5237eb73e1c4",
                    "sense_definition": "Długi, przeciągły i głośny dźwięk wydawany przez zwierzęta (np. wilki, psy), wiatr lub syreny alarmowe.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nocy obudziło nas przeraźliwe <w>wycie</w> wilków w pobliskim lesie.",
                            "target_lang_translations": {
                                "ru": "Ночью нас разбудил ужасающий <w>вой</w> волков в близлежащем лесу."
                            }
                        },
                        {
                            "text": "Przez nieszczelne okna słychać było głośne <w>wycie</w> wiatru.",
                            "target_lang_translations": {
                                "ru": "Сквозь щели в окнах был слышен громкий <w>вой</w> ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "skowyt"
                    ],
                    "antonyms": [
                        "niewycie",
                        "niewycie"
                    ],
                    "common_phrases": [
                        "wycie wiatru",
                        "wycie syren",
                        "wycie psa",
                        "wycie do księżyca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный, протяжный и громкий звук, издаваемый животными (например, волками, собаками), ветром или сиренами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вой",
                                "target_lang_sense_clarification": "звук животных, ветра или сирен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "472e93e2-d169-360b-aa0b-4cc8c35773f2",
                    "sense_definition": "Głośny, niekontrolowany płacz lub krzyk człowieka wyrażający silne emocje, takie jak ból, strach lub rozpacz.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej rozpaczliwe <w>wycie</w> było słychać na całym korytarzu szpitalnym.",
                            "target_lang_translations": {
                                "ru": "Её отчаянный <w>вой</w> был слышен во всём больничном коридоре."
                            }
                        },
                        {
                            "text": "Dziecko wpadło w histerię i zaczęło się głośne <w>wycie</w>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок впал в истерику, и начался громкий <w>вой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "płacz",
                        "szloch",
                        "wrzask"
                    ],
                    "common_phrases": [
                        "wycie z bólu",
                        "wycie z rozpaczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий, неконтролируемый плач или крик человека, выражающий сильные эмоции, такие как боль, страх или отчаяние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вой",
                                "target_lang_sense_clarification": "громкий плач человека"
                            },
                            {
                                "target_lang_word": "вытьё",
                                "target_lang_sense_clarification": "процесс плача, рыдания"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "694553ec-de8a-3621-9c01-2d4847bf7b6b",
                    "sense_definition": "Een systeem om elektronisch te betalen met een bankpas en een geheime code, of de code zelf.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "finance_pin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze winkel kun je alleen met de <w>pin</w> betalen.",
                            "target_lang_translations": {
                                "en": "In this shop you can only pay by <w>debit card</w>.",
                                "ru": "В этом магазине можно платить только <w>картой</w>."
                            }
                        },
                        {
                            "text": "Ik ben de code van mijn <w>pin</w> vergeten.",
                            "target_lang_translations": {
                                "en": "I have forgotten my <w>PIN</w> code.",
                                "ru": "Я забыл код от своей <w>карты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pincode"
                    ],
                    "common_phrases": [
                        "met de pin",
                        "aan de pin"
                    ],
                    "target_lang_definitions": {
                        "en": "A system for electronic payment using a bank card and a secret code, or the code itself.",
                        "ru": "Система электронных платежей с помощью банковской карты и секретного кода, или сам код."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "debit card",
                                "target_lang_sense_clarification": "payment method"
                            },
                            {
                                "target_lang_word": "PIN",
                                "target_lang_sense_clarification": "code"
                            },
                            {
                                "target_lang_word": "card",
                                "target_lang_sense_clarification": "payment method"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "банковская карта",
                                "target_lang_sense_clarification": "средство оплаты"
                            },
                            {
                                "target_lang_word": "пин-код",
                                "target_lang_sense_clarification": "секретный код"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbe6bda9-d948-39ae-9fbb-0e874eb29501",
                    "sense_definition": "Een dun, puntig staafje van metaal of ander materiaal waarmee iets wordt vastgezet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object_pin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De plank rust op een kleine metalen <w>pin</w> in de kast.",
                            "target_lang_translations": {
                                "en": "The shelf rests on a small metal <w>pin</w> in the cupboard.",
                                "ru": "Полка держится на маленьком металлическом <w>штифте</w> в шкафу."
                            }
                        },
                        {
                            "text": "Hij sloeg de <w>pin</w> met een hamer in de muur.",
                            "target_lang_translations": {
                                "en": "He hit the <w>pin</w> into the wall with a hammer.",
                                "ru": "Он забил <w>штырь</w> молотком в стену."
                            }
                        }
                    ],
                    "synonyms": [
                        "pen",
                        "stift"
                    ],
                    "target_lang_definitions": {
                        "en": "A thin, pointed rod of metal or other material used to fasten or support something.",
                        "ru": "Тонкий заостренный стержень из металла или другого материала, которым что-либо скрепляют."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pin",
                                "target_lang_sense_clarification": "fastener"
                            },
                            {
                                "target_lang_word": "peg",
                                "target_lang_sense_clarification": "support or fastener"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "штифт",

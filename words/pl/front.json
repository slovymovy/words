{
    "word_family": [
        "frontowy",
        "frontowiec"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb3e6b98-9cab-348b-8d65-5c79bbb90c7b",
                    "sense_definition": "Przednia część jakiegoś przedmiotu lub strona zwrócona w kierunku, w którym się on porusza.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napis na <froncie> pudełka był bardzo wyraźny.",
                            "target_lang_translations": {
                                "ru": "Надпись на <лицевой стороне> коробки была очень четкой."
                            }
                        },
                        {
                            "text": "Samochód uderzył <frontem> w drzewo.",
                            "target_lang_translations": {
                                "ru": "Автомобиль ударился <передней частью> в дерево."
                            }
                        }
                    ],
                    "synonyms": [
                        "przód"
                    ],
                    "antonyms": [
                        "tył"
                    ],
                    "common_phrases": [
                        "z frontu",
                        "od frontu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя часть предмета или сторона, обращенная в направлении движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передняя часть",
                                "target_lang_sense_clarification": "общая"
                            },
                            {
                                "target_lang_word": "лицевая сторона",
                                "target_lang_sense_clarification": "поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b48304e-fad6-32a7-bea7-51673e43a8ef",
                    "sense_definition": "Rejon, w którym toczą się bezpośrednie działania wojenne; linia zetknięcia walczących wojsk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego brat zginął na <froncie> podczas II wojny światowej.",
                            "target_lang_translations": {
                                "ru": "Его брат погиб на <фронте> во время Второй мировой войны."
                            }
                        },
                        {
                            "text": "Po bitwie moskiewskiej <front> przesunął się na zachód.",
                            "target_lang_translations": {
                                "ru": "После битвы под Москвой <фронт> сдвинулся на запад."
                            }
                        }
                    ],
                    "antonyms": [
                        "tył"
                    ],
                    "common_phrases": [
                        "na froncie",
                        "linia frontu",
                        "wyruszyć na front"
                    ],
                    "target_lang_definitions": {
                        "ru": "Район, где ведутся непосредственные военные действия; линия соприкосновения сражающихся войск."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фронт",
                                "target_lang_sense_clarification": "военный"
                            },
                            {
                                "target_lang_word": "передовая",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0cdfee6-7c1b-3a89-8655-5c4d99ada44e",
                    "sense_definition": "Przednia, reprezentacyjna ściana budynku, zazwyczaj mieszcząca główne wejście.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciel pomalował <front> kamienicy na różowo.",
                            "target_lang_translations": {
                                "ru": "Владелец покрасил <фасад> дома в розовый цвет."
                            }
                        },
                        {
                            "text": "Główne wejście do muzeum znajduje się od <frontu>.",
                            "target_lang_translations": {
                                "ru": "Главный вход в музей находится с <фасада>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przód"
                    ],
                    "antonyms": [
                        "tył"
                    ],
                    "common_phrases": [
                        "front budynku",
                        "wejście od frontu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя, парадная стена здания, где обычно находится главный вход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фасад",
                                "target_lang_sense_clarification": "архитектура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12fc6e29-0c6e-3e17-a577-0537fdcf58dd",
                    "sense_definition": "Strefa przejściowa oddzielająca dwie masy powietrza o różnych właściwościach fizycznych, często przynosząca zmianę pogody.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad wschodnią częścią kraju będzie jeszcze przemieszczał się zimny <front>.",
                            "target_lang_translations": {
                                "ru": "Над восточной частью страны еще будет перемещаться холодный <фронт>."
                            }
                        },
                        {
                            "text": "Prognoza pogody zapowiada ciepły <front> atmosferyczny w weekend.",
                            "target_lang_translations": {
                                "ru": "Прогноз погоды обещает теплый атмосферный <фронт> в выходные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "front atmosferyczny",
                        "zimny front",
                        "ciepły front"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "meteorology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переходная зона, разделяющая две воздушные массы с разными физическими свойствами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фронт",
                                "target_lang_sense_clarification": "метеорология"
                            },
                            {
                                "target_lang_word": "атмосферный фронт",
                                "target_lang_sense_clarification": "полный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d92c021-f82e-3bf8-b2f8-7c92599e7c10",
                    "sense_definition": "Porozumienie lub grupa różnych organizacji, partii lub osób działających wspólnie dla osiągnięcia określonego celu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj powołany został <front> jedności narodowej.",
                            "target_lang_translations": {
                                "ru": "Вчера был создан <фронт> национального единства."
                            }
                        },
                        {
                            "text": "Opozycja stworzyła wspólny <front> przeciwko nowym podatkom.",
                            "target_lang_translations": {
                                "ru": "Оппозиция создала единый <фронт> против новых налогов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wspólny front",
                        "jednolity front"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "politics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединение различных организаций, партий или лиц, действующих совместно для достижения определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фронт",
                                "target_lang_sense_clarification": "политика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27236a2f-ec7f-3635-a773-03fe4662515d",
                    "sense_definition": "Wyższy związek operacyjny kilku armii objętych wspólnym dowództwem, realizujący zadania strategiczne.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "military_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dowódca wydał rozkaz przegrupowania wojsk całego <frontu>.",
                            "target_lang_translations": {
                                "ru": "Командующий отдал приказ о перегруппировке войск всего <фронта>."
                            }
                        },
                        {
                            "text": "Ten <front> obejmował swoim zasięgiem setki kilometrów granicy.",
                            "target_lang_translations": {
                                "ru": "Этот <фронт> охватывал сотни километров границы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dowódca frontu"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшее оперативное объединение нескольких армий под общим командованием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фронт",
                                "target_lang_sense_clarification": "военное формирование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
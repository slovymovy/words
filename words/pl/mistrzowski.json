                    ],
                    "common_phrases": [
                        "tytuł mistrzowski",
                        "pas mistrzowski",
                        "korona mistrzowska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с титулом чемпиона в спорте или другом соревновании; принадлежащий победителю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чемпионский",
                                "target_lang_sense_clarification": "относящийся к чемпиону или титулу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53f59a08-9902-3616-b8f7-597f3b931d8f",
                    "sense_definition": "Odznaczający się najwyższym kunsztem, doskonałością i biegłością; wykonany w sposób perfekcyjny.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To było <w>mistrzowskie</w> posunięcie, które zapewniło nam szybkie zwycięstwo.",
                            "target_lang_translations": {
                                "ru": "Это был <w>мастерский</w> ход, который обеспечил нам быструю победу."
                            }
                        },
                        {
                            "text": "Pianista zaprezentował <w>mistrzowskie</w> wykonanie trudnego utworu Chopina.",
                            "target_lang_translations": {
                                "ru": "Пианист продемонстрировал <w>мастерское</w> исполнение сложного произведения Шопена."
                            }

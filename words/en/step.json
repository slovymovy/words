                {
                    "sense_id": "6228ae49-0c34-39f8-baec-1be8e9a7291f",
                    "sense_definition": "A constant difference between consecutive values in a mathematical or computational series.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "math_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The program loop increases the variable by a <w>step</w> of five each time.",
                            "target_lang_translations": {
                                "ru": "Цикл программы увеличивает переменную с <w>шагом</w> пять каждый раз."

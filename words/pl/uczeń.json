            "pos": "noun",
            "senses": [
                {
                    "sense_id": "40047286-44f8-318c-9fdc-b3614f0a8df8",
                    "sense_definition": "Osoba, która uczęszcza do szkoły lub pobiera naukę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>uczeń</w> w naszej klasie musi nosić mundurek.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>ученик</w> в нашем классе обязан носить форму."
                            }
                        },
                        {
                            "text": "Nauczyciel pochwalił zdolnego <w>ucznia</w> za doskonale napisany sprawdzian.",
                            "target_lang_translations": {
                                "ru": "Учитель похвалил способного <w>ученика</w> за отлично написанную контрольную работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "słuchacz",
                        "uczniak"
                    ],
                    "antonyms": [
                        "nauczyciel",
                        "wykładowca"
                    ],
                    "common_phrases": [
                        "pilny uczeń",
                        "zdolny uczeń",
                        "uczeń szkoły podstawowej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который посещает школу или учится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ученик",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "школьник",
                                "target_lang_sense_clarification": "учащийся школы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f95e6224-6835-3f2d-ab44-8366583f889c",
                    "sense_definition": "Kontynuator czyjejś teorii, nauki lub twórczości; osoba kształcąca się pod kierunkiem mistrza w dziedzinie nauki lub sztuki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Platon był najsłynniejszym <w>uczniem</w> Sokratesa i spisał jego dialogi.",
                            "target_lang_translations": {
                                "ru": "Платон был самым известным <w>учеником</w> Сократа и записал его диалоги."
                            }
                        },
                        {
                            "text": "Młody malarz okazał się godnym <w>uczniem</w> swojego mistrza.",
                            "target_lang_translations": {
                                "ru": "Молодой художник оказался достойным <w>учеником</w> своего мастера."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwolennik",
                        "stronnik",
                        "poplecznik",
                        "adept"
                    ],
                    "antonyms": [
                        "mistrz",
                        "luminarz"
                    ],
                    "common_phrases": [
                        "wierny uczeń",
                        "uczeń i mistrz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжатель чьей-либо теории, науки или творчества; человек, обучающийся под руководством мастера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ученик",
                                "target_lang_sense_clarification": "последователь мастера"
                            },
                            {
                                "target_lang_word": "последователь",
                                "target_lang_sense_clarification": "сторонник учения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3cc49fe-0311-3e94-a708-d38437ee2a1a",
                    "sense_definition": "Osoba pobierająca nauki u mistrza duchowego; wyznawca danej religii lub doktryny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jezus wędrował przez kraj wraz ze swoimi <w>uczniami</w>.",
                            "target_lang_translations": {
                                "ru": "Иисус странствовал по стране вместе со своими <w>учениками</w>."
                            }
                        },
                        {
                            "text": "Jako oddany <w>uczeń</w>, mnich spędzał godziny na medytacji.",
                            "target_lang_translations": {
                                "ru": "Как преданный <w>ученик</w>, монах проводил часы в медитации."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyznawca",
                        "towarzysz",
                        "akolita"
                    ],
                    "antonyms": [
                        "mistrz"
                    ],
                    "common_phrases": [
                        "uczniowie Jezusa",
                        "grono uczniów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обучающийся у духовного наставника; последователь религии или учения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ученик",
                                "target_lang_sense_clarification": "духовный последователь"
                            },
                            {
                                "target_lang_word": "последователь",
                                "target_lang_sense_clarification": "приверженец учения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f19e7881-9b85-3046-a4d2-d4a28b0f44c4",
                    "sense_definition": "Osoba przyuczająca się do zawodu rzemieślniczego; najniższy stopień kwalifikacji zawodowych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zakładzie fryzjerskim pracuje dwóch <w>uczniów</w>, którzy myją głowy klientom.",
                            "target_lang_translations": {
                                "ru": "В парикмахерской работают два <w>ученика</w>, которые моют головы клиентам."
                            }
                        },
                        {
                            "text": "Stary szewc przyjął nowego <w>ucznia</w> na przyuczenie do zawodu.",
                            "target_lang_translations": {
                                "ru": "Старый сапожник взял нового <w>ученика</w> для обучения ремеслу."
                            }
                        }
                    ],
                    "synonyms": [
                        "praktykant",
                        "terminator",
                        "czeladnik",
                        "pomocnik"
                    ],
                    "antonyms": [
                        "mistrz"
                    ],
                    "common_phrases": [
                        "uczeń w zawodzie",
                        "przyjąć ucznia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обучающийся ремеслу или профессии; начальная ступень квалификации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ученик",
                                "target_lang_sense_clarification": "обучающийся ремеслу"
                            },
                            {
                                "target_lang_word": "подмастерье",
                                "target_lang_sense_clarification": "помощник мастера"
                            }
                        ]
                    }
                }
            ]
        }

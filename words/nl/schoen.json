                {
                    "sense_id": "2e08fa57-4895-389e-9136-e5f9a1ad1894",
                    "sense_definition": "Een kledingstuk dat de voet bedekt en beschermt, meestal met een stevige zool.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik trek mijn nieuwe <w>schoenen</w> aan voordat ik naar buiten ga.",
                            "target_lang_translations": {
                                "ru": "Я надеваю свои новые <w>ботинки</w> перед тем, как выйти на улицу."
                            }
                        },
                        {
                            "text": "Met Sinterklaas mogen de kinderen hun <w>schoen</w> bij de open haard zetten.",
                            "target_lang_translations": {
                                "ru": "На праздник Синтерклаас дети могут поставить свой <w>ботинок</w> у камина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nieuwe schoenen",
                        "schoenen aantrekken",
                        "schoenen poetsen",
                        "stevige schoenen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет одежды, который закрывает и защищает ногу, обычно имеющий плотную подошву."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ботинок",
                                "target_lang_sense_clarification": "стандартная обувь"
                            },
                            {
                                "target_lang_word": "туфля",
                                "target_lang_sense_clarification": "легкая или выходная обувь"
                            },
                            {
                                "target_lang_word": "обувь",
                                "target_lang_sense_clarification": "собирательное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e887087-b22f-34bd-abe1-444680e9694c",
                    "sense_definition": "Een voorwerp of onderdeel dat qua vorm op een schoen lijkt, bijvoorbeeld in een remsysteem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monteur moest de <w>schoen</w> van de rem vervangen omdat deze versleten was.",
                            "target_lang_translations": {
                                "ru": "Механику пришлось заменить тормозную <w>колодку</w>, так как она износилась."
                            }
                        },
                        {
                            "text": "In deze machine zit een metalen onderdeel dat een <w>schoen</w> wordt genoemd.",
                            "target_lang_translations": {
                                "ru": "В этой машине есть металлическая деталь, которую называют <w>башмаком</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет или деталь, напоминающая по форме обувь, например, в тормозной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "башмак",

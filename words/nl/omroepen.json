    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "25720562-388c-3e00-b76d-0b8f94575c1e",
                    "sense_definition": "Een bericht luidkeels bekendmaken aan een groep mensen, meestal via een geluidsinstallatie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "announcing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De conducteur <w>riep om</w> dat we het volgende station naderden.",
                            "target_lang_translations": {
                                "en": "The conductor <w>announced</w> that we were approaching the next station.",
                                "ru": "Кондуктор <w>объявил</w>, что мы приближаемся к следующей станции."
                            }
                        },
                        {
                            "text": "In de supermarkt werd <w>omgeroepen</w> dat de winkel bijna ging sluiten.",
                            "target_lang_translations": {
                                "en": "In the supermarket, it was <w>announced</w> that the store was about to close.",
                                "ru": "В супермаркете <w>объявили</w>, что магазин скоро закрывается."
                            }
                        }
                    ],
                    "synonyms": [
                        "aankondigen",
                        "meedelen"
                    ],
                    "common_phrases": [
                        "een bericht omroepen",
                        "de vertrektijd omroepen"
                    ],
                    "target_lang_definitions": {
                        "en": "To announce a message loudly to a group of people, usually via a sound system.",
                        "ru": "Громко объявлять сообщение группе людей, обычно через систему громкой связи."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to announce",
                                "target_lang_sense_clarification": "via a PA system or loudspeaker"
                            },
                            {
                                "target_lang_word": "to make an announcement",
                                "target_lang_sense_clarification": "publicly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "объявлять",
                                "target_lang_sense_clarification": "по громкой связи"
                            },
                            {
                                "target_lang_word": "оповещать",
                                "target_lang_sense_clarification": "сообщать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b0e22e4-00fa-3ff2-9d0b-bed31eda6034",
                    "sense_definition": "Een programma of bericht uitzenden via radio of televisie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "broadcasting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuws werd gisteren op alle zenders <w>omgeroepen</w>.",
                            "target_lang_translations": {
                                "en": "The news was <w>broadcast</w> on all channels yesterday.",
                                "ru": "Вчера новости <w>передавали</w> по всем каналам."
                            }
                        },
                        {
                            "text": "De radiozender zal het speciale programma vanavond <w>omroepen</w>.",
                            "target_lang_translations": {
                                "en": "The radio station will <w>broadcast</w> the special program tonight.",
                                "ru": "Радиостанция будет <w>транслировать</w> специальную программу сегодня вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitzenden"
                    ],
                    "target_lang_definitions": {
                        "en": "To transmit a program or message via radio or television.",
                        "ru": "Передавать программу или сообщение по радио или телевидению."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to broadcast",
                                "target_lang_sense_clarification": "radio or TV"
                            },
                            {
                                "target_lang_word": "to air",
                                "target_lang_sense_clarification": "a program"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "транслировать",
                                "target_lang_sense_clarification": "передавать в эфир"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "по радио/ТВ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60658b4f-2383-360d-a9cb-8781009fc279",
                    "sense_definition": "In het openbaar luidkeels een officieel bericht verkondigen (historisch door een stadsomroeper).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "announcing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stadsbode liep over het plein om het nieuws <w>om</w> te <w>roepen</w>.",
                            "target_lang_translations": {
                                "en": "The town messenger walked across the square to <w>proclaim</w> the news.",
                                "ru": "Городской глашатай ходил по площади, чтобы <w>огласить</w> новости."
                            }
                        },
                        {
                            "text": "Vroeger werden nieuwe wetten door de stad <w>omgeroepen</w>.",
                            "target_lang_translations": {
                                "en": "In the past, new laws were <w>proclaimed</w> throughout the city.",
                                "ru": "Раньше новые законы <w>оглашались</w> по всему городу."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkondigen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "verwijst naar historische context van stadsomroepers"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To publicly and loudly proclaim an official message (historically by a town crier).",
                        "ru": "Публично и громко провозглашать официальное сообщение (исторически)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to proclaim",
                                "target_lang_sense_clarification": "publicly and officially"
                            },
                            {
                                "target_lang_word": "to cry out",
                                "target_lang_sense_clarification": "as a town crier"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оглашать",
                                "target_lang_sense_clarification": "публично выкрикивать"
                            },
                            {
                                "target_lang_word": "провозглашать",
                                "target_lang_sense_clarification": "торжественно объявлять"
                            }
                        ]
                    }
                }
            ]
        },

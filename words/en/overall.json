    "word_family": [],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5dbd30b8-64b0-3603-845a-bb2cb7c1ec66",
                    "sense_definition": "Including everything or everyone; regarding the total amount or entire situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>overall</w> cost of the project was higher than we expected.",
                            "target_lang_translations": {
                                "ru": "<w>Общая</w> стоимость проекта оказалась выше, чем мы ожидали."
                            }
                        },
                        {
                            "text": "Her <w>overall</w> performance in the competition was excellent.",
                            "target_lang_translations": {
                                "ru": "Её <w>общий</w> результат в соревновании был превосходным."
                            }
                        }
                    ],
                    "synonyms": [
                        "comprehensive",
                        "total",
                        "entire",
                        "whole",
                        "thorough"
                    ],
                    "common_phrases": [
                        "overall effect",
                        "overall view",
                        "overall picture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охватывающий всё или всех; касающийся общего итога или всей ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́бщий",
                                "target_lang_sense_clarification": "касающийся всего в целом"
                            },
                            {
                                "target_lang_word": "по́лный",
                                "target_lang_sense_clarification": "исчерпывающий, весь"
                            },
                            {
                                "target_lang_word": "всео́бщий",
                                "target_lang_sense_clarification": "относящийся ко всем"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4eec0409-e019-35b8-8ccc-206506564e73",
                    "sense_definition": "Generally; when everything is taken into account.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Overall</w>, the situation has improved significantly since last year.",
                            "target_lang_translations": {
                                "ru": "<w>В целом</w>, ситуация значительно улучшилась с прошлого года."
                            }
                        },
                        {
                            "text": "The team played well <w>overall</w> despite a few mistakes.",
                            "target_lang_translations": {
                                "ru": "Команда сыграла хорошо <w>в целом</w>, несмотря на несколько ошибок."
                            }
                        }
                    ],
                    "synonyms": [
                        "all things considered",
                        "mostly"
                    ],
                    "common_phrases": [
                        "taken overall"
                    ],
                    "target_lang_definitions": {
                        "ru": "В общем; если принимать во внимание всё; в итоге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в це́лом",
                                "target_lang_sense_clarification": "обобщая"
                            },
                            {
                                "target_lang_word": "в о́бщем и це́лом",
                                "target_lang_sense_clarification": "не касаясь частностей"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a85bbc4-66a7-3be4-a1d9-d2a30fbf16b8",
                    "sense_definition": "A loose piece of clothing worn over other clothes to protect them from dirt, often consisting of trousers with a piece of cloth covering the chest and straps over the shoulders.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic put on his <w>overalls</w> before fixing the car.",
                            "target_lang_translations": {
                                "ru": "Механик надел свой <w>комбинезон</w> перед починкой машины."
                            }
                        },
                        {
                            "text": "She wore protective <w>overalls</w> while painting the house.",
                            "target_lang_translations": {
                                "ru": "Она носила защитный <w>комбинезон</w>, пока красила дом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wear overalls",
                        "blue overalls"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рабочая одежда, надеваемая поверх обычной для защиты от грязи; обычно состоит из брюк с нагрудником и лямками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комбинезо́н",
                                "target_lang_sense_clarification": "рабочая одежда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
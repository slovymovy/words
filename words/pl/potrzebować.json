{
    "word_family": [
        "potrzeba",
        "potrzebny",
        "niepotrzebny",
        "zapotrzebowanie",
        "potrzebujący"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f1c5d507-9ade-3b51-aa51-49270b71feb4",
                    "sense_definition": "Odczuwać brak kogoś lub czegoś, co jest niezbędne do życia, funkcjonowania lub osiągnięcia określonego celu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "need_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo <w>potrzebuję</w> twojej pomocy przy tym trudnym zadaniu.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>нуждаюсь</w> в твоей помощи с этим трудным заданием."
                            }
                        },
                        {
                            "text": "Rośliny <w>potrzebują</w> dużo słońca, aby zdrowo rosnąć.",
                            "target_lang_translations": {
                                "ru": "Растения <w>нуждаются</w> в большом количестве солнца, чтобы расти здоровыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymagać"
                    ],
                    "common_phrases": [
                        "potrzebować pomocy",
                        "potrzebować czasu",
                        "potrzebować pieniędzy",
                        "pilnie potrzebować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать недостаток в ком-либо или чем-либо, необходимом для жизни, функционирования или достижения определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нуждаться",
                                "target_lang_sense_clarification": "испытывать потребность"
                            },
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "быть необходимым (безличная форма)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "302b5ad3-cdf1-30e8-acbd-8d3ea0467f7b",
                    "sense_definition": "Być zobowiązanym lub zmuszonym do wykonania jakiejś czynności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>potrzebujesz</w> nic więcej dodawać, bo wszystko zrozumiałem.",
                            "target_lang_translations": {
                                "ru": "Тебе не <w>нужно</w> ничего больше добавлять, потому что я всё понял."
                            }
                        },
                        {
                            "text": "Czy my naprawdę <w>potrzebujemy</w> to robić właśnie teraz?",
                            "target_lang_translations": {
                                "ru": "Нам действительно <w>нужно</w> делать это именно сейчас?"
                            }
                        }
                    ],
                    "synonyms": [
                        "musieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть обязанным или вынужденным выполнить какое-либо действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "быть должным сделать что-то"
                            },
                            {
                                "target_lang_word": "надо",
                                "target_lang_sense_clarification": "разговорный вариант необходимости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fd5a3b1-310c-3d16-a480-3eb882d16e2c",
                    "sense_definition": "Odczuwać wzajemną niezbędność lub zależność od siebie w relacji.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reciprocal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo wielu różnic, oni wciąż się <w>potrzebują</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на множество различий, они всё ещё <w>нуждаются</w> друг в друге."
                            }
                        },
                        {
                            "text": "Te dwa działy w firmie wzajemnie się <w>potrzebują</w>, by projekt się udał.",
                            "target_lang_translations": {
                                "ru": "Эти два отдела в компании взаимно <w>нуждаются</w> друг в друге, чтобы проект удался."
                            }
                        }
                    ],
                    "common_phrases": [
                        "potrzebować się nawzajem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать взаимную необходимость или зависимость друг от друга в отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нуждаться",
                                "target_lang_sense_clarification": "испытывать потребность друг в друге"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
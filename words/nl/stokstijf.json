{
    "word_family": [
        "stijf",
        "stijfheid",
        "verstijven"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "168ba352-489b-342d-9612-71284652fc38",
                    "sense_definition": "Helemaal onbeweeglijk, vaak als gevolg van schrik, verbazing of spanning.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motionless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bleef <w>stokstijf</w> staan toen de hond plotseling begon te blaffen.",
                            "target_lang_translations": {
                                "en": "He stood <w>stock-still</w> when the dog suddenly started barking."
                            }
                        },
                        {
                            "text": "Van angst stond ze <w>stokstijf</w> in de donkere gang en durfde niet te bewegen.",
                            "target_lang_translations": {
                                "en": "Out of fear, she stood <w>frozen</w> in the dark hallway and dared not move."
                            }
                        }
                    ],
                    "synonyms": [
                        "roerloos",
                        "onbeweeglijk",
                        "verstart"
                    ],
                    "antonyms": [
                        "beweeglijk"
                    ],
                    "common_phrases": [
                        "stokstijf blijven staan",
                        "stokstijf stilstaan"
                    ],
                    "target_lang_definitions": {
                        "en": "Completely motionless, often due to shock, surprise, or tension."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stock-still",
                                "target_lang_sense_clarification": "completely motionless"
                            },
                            {
                                "target_lang_word": "frozen",
                                "target_lang_sense_clarification": "unable to move due to fear or shock"
                            },
                            {
                                "target_lang_word": "rigid",
                                "target_lang_sense_clarification": "stiff and unmoving"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05ecae96-e1ff-3019-bb2b-0e935e4b02ca",
                    "sense_definition": "Heel erg stijf en niet soepel, vaak gezegd van lichaamsdelen of spieren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_stiffness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude pianiste kon niet meer spelen met haar <w>stokstijve</w> vingers.",
                            "target_lang_translations": {
                                "en": "The old pianist could no longer play with her <w>rigid</w> fingers."
                            }
                        },
                        {
                            "text": "Na de lange wandeling in de kou voelden zijn benen <w>stokstijf</w> aan.",
                            "target_lang_translations": {
                                "en": "After the long walk in the cold, his legs felt <w>stiff as a board</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stram",
                        "verstijfd"
                    ],
                    "antonyms": [
                        "soepel",
                        "lenig"
                    ],
                    "common_phrases": [
                        "stokstijve benen",
                        "stokstijve spieren"
                    ],
                    "target_lang_definitions": {
                        "en": "Extremely stiff and inflexible, often referring to body parts or muscles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stiff as a board",
                                "target_lang_sense_clarification": "idiomatic expression for extreme stiffness"
                            },
                            {
                                "target_lang_word": "rigid",
                                "target_lang_sense_clarification": "physically unbending"
                            },
                            {
                                "target_lang_word": "very stiff",
                                "target_lang_sense_clarification": "literal description"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f14fc7b-1991-3eea-a188-f2cb2c6b474b",
                    "sense_definition": "Onverzettelijk en koppig vasthoudend aan een mening, bewering of standpunt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stubborn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte bleef <w>stokstijf</w> volhouden dat hij onschuldig was.",
                            "target_lang_translations": {
                                "en": "The suspect <w>adamantly</w> maintained that he was innocent."
                            }
                        },
                        {
                            "text": "Ondanks het duidelijke bewijs hield zij <w>stokstijf</w> vast aan haar eigen gelijk.",
                            "target_lang_translations": {
                                "en": "Despite the clear evidence, she <w>stubbornly</w> stuck to her position."
                            }
                        }
                    ],
                    "synonyms": [
                        "onverzettelijk",
                        "halsstarrig",
                        "koppig"
                    ],
                    "antonyms": [
                        "meegaand",
                        "flexibel"
                    ],
                    "common_phrases": [
                        "stokstijf volhouden",
                        "stokstijf ontkennen"
                    ],
                    "target_lang_definitions": {
                        "en": "Unyielding and stubbornly holding to an opinion, claim, or viewpoint."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "adamantly",
                                "target_lang_sense_clarification": "often used with denying or insisting"
                            },
                            {
                                "target_lang_word": "stubbornly",
                                "target_lang_sense_clarification": "refusing to change one's mind"
                            },
                            {
                                "target_lang_word": "obstinately",
                                "target_lang_sense_clarification": "perversely adhering to an opinion"
                            }
                        ]
                    }
                }
            ]
        }
    ]

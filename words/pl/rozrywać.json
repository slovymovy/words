                    "target_lang_definitions": {
                        "ru": "Яростно разрушать что-то, вызывая распад на мелкие элементы, часто в результате взрыва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разносить",
                                "target_lang_sense_clarification": "разрушать взрывом или ударом"
                            },
                            {
                                "target_lang_word": "разрывать",
                                "target_lang_sense_clarification": "уничтожать силой взрыва"
                            }
                        ]
                    }
                }
            ]
        }
    ]

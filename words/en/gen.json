        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d9657dd4-c202-3493-a0d4-489789a4ea0c",
                    "sense_definition": "A female given name; short for <w>Genevieve</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I asked <w>Gen</w> if she wanted to come to the movies with us.",
                            "target_lang_translations": {
                                "ru": "Я спросил <w>Джен</w>, не хочет ли она пойти с нами в кино."
                            }
                        },
                        {
                            "text": "Everyone calls her <w>Gen</w>, even though her full name is on the roster.",
                            "target_lang_translations": {
                                "ru": "Все зовут её <w>Джен</w>, хотя в списке указано её полное имя."
                            }
                        }

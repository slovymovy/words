{
    "word_family": [
        "operatic",
        "operetta"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7fd2aee5-47d3-3f8f-b7a8-aa2e3e6ad6b9",
                    "sense_definition": "A theatrical work combining drama, music, song, and sometimes dance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "musical_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to see a famous <w>opera</w> by Mozart last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы ходили на известную <w>оперу</w> Моцарта."
                            }
                        },
                        {
                            "text": "The <w>opera</w> tells the tragic story of a prince and a princess.",
                            "target_lang_translations": {
                                "ru": "Эта <w>опера</w> рассказывает трагическую историю принца и принцессы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "soap opera",
                        "comic opera",
                        "grand opera",
                        "space opera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкально-драматическое произведение, сочетающее инструментальную музыку с вокальной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опера",
                                "target_lang_sense_clarification": "музыкальное произведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8181495f-24f0-33d9-8a27-75ad091328dc",
                    "sense_definition": "The art form or genre of composing and performing operas.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "musical_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a powerful voice that is perfect for <w>opera</w>.",
                            "target_lang_translations": {
                                "ru": "У неё сильный голос, который идеально подходит для <w>оперы</w>."
                            }
                        },
                        {
                            "text": "Italian <w>opera</w> is popular all over the world.",
                            "target_lang_translations": {
                                "ru": "Итальянская <w>опера</w> популярна во всём мире."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opera singer",
                        "opera glasses",
                        "at the opera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр музыкально-драматического искусства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опера",
                                "target_lang_sense_clarification": "жанр искусства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c3e8519-5b7d-3c11-bb31-eeb8f5c1e081",
                    "sense_definition": "A building designed for the performance of operas.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We agreed to meet in front of the <w>opera</w> at seven.",
                            "target_lang_translations": {
                                "ru": "Мы договорились встретиться перед <w>оперным театром</w> в семь часов."
                            }
                        },
                        {
                            "text": "The city built a magnificent new <w>opera</w> near the river.",
                            "target_lang_translations": {
                                "ru": "Город построил великолепный новый <w>оперный театр</w> у реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "opera house"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание, предназначенное для исполнения оперных спектаклей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оперный театр",
                                "target_lang_sense_clarification": "здание"
                            },
                            {
                                "target_lang_word": "опера",
                                "target_lang_sense_clarification": "разг. здание театра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2542b97-d1ac-3227-a7bf-cebd43997659",
                    "sense_definition": "The plural form of <w>opus</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>opera</w> of the philosopher were collected in a single volume.",
                            "target_lang_translations": {
                                "ru": "<w>Труды</w> философа были собраны в один том."
                            }
                        },
                        {
                            "text": "These <w>opera</w> represent the culmination of his life's work.",
                            "target_lang_translations": {
                                "ru": "Эти <w>сочинения</w> представляют собой кульминацию дела всей его жизни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "magna opera"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural of opus"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от латинского слова opus (труд, произведение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "труды",
                                "target_lang_sense_clarification": "научные или творческие работы"
                            },
                            {
                                "target_lang_word": "сочинения",
                                "target_lang_sense_clarification": "произведения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
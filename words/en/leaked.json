        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3e47d113-bbb0-3807-b633-ced2430a4100",
                    "sense_definition": "Describing a document or information that was intended to be confidential but has been released to the public or the press without authorization.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>leaked</w> memo revealed the company's secret plans to reduce its workforce.",
                            "target_lang_translations": {
                                "ru": "<w>Слитая</w> служебная записка раскрыла секретные планы компании по сокращению штата."
                            }
                        },
                        {
                            "text": "Journalists spent days analyzing the <w>leaked</w> documents to find evidence of corruption.",
                            "target_lang_translations": {
                                "ru": "Журналисты потратили дни на анализ <w>утекших</w> документов, чтобы найти доказательства коррупции."
                            }
                        }
                    ],
                    "synonyms": [
                        "disclosed",
                        "revealed"
                    ],
                    "antonyms": [
                        "confidential",
                        "secret",
                        "classified"
                    ],
                    "common_phrases": [
                        "leaked document",
                        "leaked information",
                        "leaked report",
                        "leaked photo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает документ или информацию, которые должны были оставаться конфиденциальными, но были обнародованы без разрешения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слитый",
                                "target_lang_sense_clarification": "о документах или информации (разг.)"
                            },
                            {
                                "target_lang_word": "утекший",
                                "target_lang_sense_clarification": "о данных, попавших в открытый доступ"
                            }
                        ]
                    }
                }
            ]
        }
    ]

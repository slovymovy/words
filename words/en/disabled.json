        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b4d638ac-3941-38c5-883f-a66c267b03a7",
                    "sense_definition": "Having a physical or mental condition that limits movements, senses, or activities.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "human_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum has installed a new ramp for <disabled> visitors.",
                            "target_lang_translations": {
                                "ru": "Музей установил новый пандус для посетителей <с ограниченными возможностями>."
                            }
                        },
                        {
                            "text": "She has dedicated her life to caring for <disabled> children.",
                            "target_lang_translations": {
                                "ru": "Она посвятила свою жизнь заботе о детях-<инвалидах>."
                            }
                        }
                    ],
                    "synonyms": [
                        "incapacitated"
                    ],
                    "antonyms": [
                        "abled"
                    ],
                    "common_phrases": [
                        "physically disabled",
                        "mentally disabled",
                        "severely disabled",
                        "learning disabled"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий физические или психические ограничения, затрудняющие деятельность или восприятие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с ограни́ченными возмо́жностями",
                                "target_lang_sense_clarification": "polite, standard term"
                            },
                            {
                                "target_lang_word": "инвали́д",
                                "target_lang_sense_clarification": "noun often used as an attribute"
                            },
                            {
                                "target_lang_word": "нетрудоспосо́бный",
                                "target_lang_sense_clarification": "incapacitated, unable to work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70a191f5-3ddd-3681-986f-dd83a33c9da6",
                    "sense_definition": "Made unable to work or function; switched off, broken, or deactivated.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "functional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <disabled> vehicle was towed to the nearest garage for repairs.",
                            "target_lang_translations": {
                                "ru": "<Неисправный> автомобиль отбуксировали в ближайшую мастерскую для ремонта."
                            }
                        },
                        {
                            "text": "This feature is currently <disabled> in the settings menu.",
                            "target_lang_translations": {
                                "ru": "Эта функция в данный момент <отключена> в меню настроек."
                            }
                        }

                {
                    "sense_id": "623c60e8-f96d-3a23-bcb7-c6d9a7c97f93",
                    "sense_definition": "Een gevoel van vreugde, vermaak of tevredenheid.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wens jullie heel veel <w>plezier</w> op vakantie.",

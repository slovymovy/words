                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в штате Луизиана или община в штате Огайо, США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уэлш",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                                "target_lang_sense_clarification": "to learn something new"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "узнавать",
                                "target_lang_sense_clarification": "получать новые сведения"
                            },
                            {
                                "target_lang_word": "выяснять",
                                "target_lang_sense_clarification": "устанавливать факты"
                            }
                        ]

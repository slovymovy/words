                    "frequency": "VeryLow",
                    "semantic_group_id": "proper_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Circle</w> is listed in the directory.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Серкл</w> есть в справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Серкл",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "38b0d4a9-bb43-3338-bfe1-b73ff65cbdb4",
                    "sense_definition": "A medical professional who specializes in the care, treatment, and repair of teeth and gums.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medical_profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have a painful tooth, so I need to make an appointment with the <w>dentist</w>.",
                            "target_lang_translations": {
                                "ru": "У меня болит зуб, поэтому мне нужно записаться к <w>стоматологу</w>."
                            }
                        },
                        {
                            "text": "The <w>dentist</w> examined her teeth and told her to floss more often.",
                            "target_lang_translations": {
                                "ru": "<w>Стоматолог</w> осмотрел её зубы и посоветовал чаще пользоваться зубной нитью."
                            }
                        }
                    ],
                    "synonyms": [
                        "dental surgeon"
                    ],
                    "common_phrases": [
                        "visit the dentist",
                        "dentist appointment",
                        "at the dentist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Врач, специализирующийся на уходе за зубами и деснами, их лечении и восстановлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоматолог",
                                "target_lang_sense_clarification": "специалист с высшим медицинским образованием"
                            },
                            {
                                "target_lang_word": "зубной врач",
                                "target_lang_sense_clarification": "общее название специалиста"
                            },
                            {
                                "target_lang_word": "дантист",
                                "target_lang_sense_clarification": "практикующий зубной врач"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96427021-b589-33a7-9cf5-eccaacaee8d5",
                    "sense_definition": "A deliberate misspelling of <w>Dengist</w>, used in internet political slang.",
                    "learner_level": "C2",

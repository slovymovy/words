{
    "word_family": [
        "decumuleren",
        "decumulatie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0707206-e4f5-3598-aa9d-a7fb711d3a1b",
                    "sense_definition": "Het verbod op of het beëindigen van het gelijktijdig uitoefenen van meerdere politieke of bestuurlijke functies door één persoon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De partij pleit voor een volledige <w>decumul</w> van politieke mandaten.",
                            "target_lang_translations": {
                                "en": "The party advocates for a complete <w>ban on dual</w> political mandates."
                            }
                        },
                        {
                            "text": "Door de regels rond <w>decumul</w> mag hij zijn ambt niet combineren met een zetel in het parlement.",
                            "target_lang_translations": {
                                "en": "Due to the rules regarding <w>multiple office-holding</w>, he may not combine his office with a seat in parliament."
                            }
                        }
                    ],
                    "synonyms": [
                        "decumulatie"
                    ],
                    "antonyms": [
                        "cumul"
                    ],
                    "common_phrases": [
                        "decumul van mandaten",
                        "volledige decumul"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Belgium"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The prohibition against or termination of holding multiple political or administrative positions simultaneously."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ban on dual mandates",
                                "target_lang_sense_clarification": "politics"
                            },
                            {
                                "target_lang_word": "prohibition of multiple office-holding",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9597290b-f1e0-397b-9e28-be1c77f4b8d8",
                    "sense_definition": "Het stelsel waarbij de inkomens van partners afzonderlijk worden belast in plaats van samen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fiscale <w>decumul</w> zorgt ervoor dat beide partners een apart aanslagbiljet ontvangen.",
                            "target_lang_translations": {
                                "en": "The fiscal <w>separate taxation</w> ensures that both partners receive a separate tax assessment."
                            }
                        },
                        {
                            "text": "Sinds de invoering van de <w>decumul</w> worden de inkomsten van gehuwden niet meer samengeteld.",
                            "target_lang_translations": {
                                "en": "Since the introduction of <w>separate taxation</w>, the incomes of married couples are no longer aggregated."
                            }
                        }
                    ],
                    "synonyms": [
                        "decumulatie",
                        "individualisering"
                    ],
                    "antonyms": [
                        "cumul"
                    ],
                    "common_phrases": [
                        "fiscale decumul"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Belgium"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The system where partners' incomes are taxed separately rather than jointly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "separate taxation",
                                "target_lang_sense_clarification": "finance"
                            },
                            {
                                "target_lang_word": "de-aggregation of income",
                                "target_lang_sense_clarification": "tax law"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
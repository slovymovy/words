                    ],
                    "target_lang_definitions": {
                        "ru": "Молодые животные как группа, особенно в контексте разведения или охоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодняк",
                                "target_lang_sense_clarification": "молодые животные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b114b1e1-cc0c-316a-8e15-930a8e953846",
                    "sense_definition": "Młode drzewa rosnące w skupisku, zwykle w lesie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leśnicy dbają o to, by leśna <w>młodzież</w> miała dostęp do światła.",
                            "target_lang_translations": {
                                "ru": "Лесники заботятся о том, чтобы лесной <w>молодняк</w> имел доступ к свету."
                            }
                        },
                        {
                            "text": "Gęsta sosnowa <w>młodzież</w> porastała całe zbocze góry.",
                            "target_lang_translations": {
                                "ru": "Густой сосновый <w>молодняк</w> покрывал весь склон горы."
                            }
                        }
                    ],
                    "synonyms": [

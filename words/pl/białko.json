        "białaczka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cab48085-7f9c-31bc-80ac-f2019a5411a0",
                    "sense_definition": "Przezroczysta, galaretowata substancja w jaju ptasim, otaczająca żółtko, która pod wpływem wysokiej temperatury ścina się i staje się biała.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do przygotowania bezy potrzebne jest tylko <w>białko</w>, żółtka możesz użyć do ciasta.",
                            "target_lang_translations": {
                                "ru": "Для приготовления безе нужен только <w>белок</w>, желтки можешь использовать для теста."
                            }
                        },
                        {
                            "text": "Musisz ostrożnie oddzielić <w>białko</w> od żółtka, aby piana dobrze się ubiła.",
                            "target_lang_translations": {
                                "ru": "Ты должен осторожно отделить <w>белок</w> от желтка, чтобы пена хорошо взбилась."
                            }
                        }
                    ],
                    "antonyms": [
                        "żółtko"
                    ],
                    "common_phrases": [
                        "ubić białka",
                        "oddzielić białko",
                        "ścięte białko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозрачное желеобразное вещество в птичьем яйце, окружающее желток, которое при нагревании сворачивается и становится белым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белок",
                                "target_lang_sense_clarification": "яйца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d499387-bdd5-38fb-a442-3bf0605270be",
                    "sense_definition": "Składnik odżywczy niezbędny do budowy i regeneracji organizmu, występujący w produktach takich jak mięso, ryby, jaja czy rośliny strączkowe.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protein_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sportowcy często stosują dietę bogatą w <w>białko</w>, aby zbudować masę mięśniową.",
                            "target_lang_translations": {
                                "ru": "Спортсмены часто придерживаются диеты, богатой <w>белком</w>, чтобы нарастить мышечную массу."
                            }
                        },
                        {
                            "text": "Rośliny strączkowe są doskonałym źródłem <w>białka</w> dla wegetarian.",
                            "target_lang_translations": {
                                "ru": "Бобовые являются отличным источником <w>белка</w> для вегетарианцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "proteina"
                    ],
                    "common_phrases": [
                        "źródło białka",
                        "dieta bogata w białko",
                        "zapotrzebowanie na białko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Питательное вещество, необходимое для строения и регенерации организма, содержащееся в таких продуктах, как мясо, рыба, яйца или бобовые."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белок",
                                "target_lang_sense_clarification": "питательное вещество"
                            },
                            {
                                "target_lang_word": "протеин",
                                "target_lang_sense_clarification": "спортивное питание/добавка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80edf13c-180b-30b7-be3a-7d887119b338",
                    "sense_definition": "Biała część gałki ocznej, widoczna wokół kolorowej tęczówki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zauważył, że <w>białka</w> pacjenta są lekko zażółcone.",
                            "target_lang_translations": {
                                "ru": "Врач заметил, что <w>белки</w> пациента слегка пожелтели."
                            }
                        },
                        {
                            "text": "Ze zmęczenia miał przekrwione <w>białka</w> oczu.",
                            "target_lang_translations": {
                                "ru": "От усталости у него были налитые кровью <w>белки</w> глаз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "białka oczu",
                        "przekrwione białka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Белая часть глазного яблока, видимая вокруг цветной радужки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белок",
                                "target_lang_sense_clarification": "глаза"
                            },
                            {
                                "target_lang_word": "склера",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21e0d75f-9e6b-3d74-92a6-cbb075e4a63e",
                    "sense_definition": "Wielocząsteczkowy związek organiczny zbudowany z aminokwasów, będący podstawowym elementem budulcowym żywych komórek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protein_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hemoglobina to <w>białko</w> odpowiedzialne za transport tlenu we krwi.",
                            "target_lang_translations": {
                                "ru": "Гемоглобин — это <w>белок</w>, отвечающий за транспорт кислорода в крови."
                            }
                        },
                        {
                            "text": "W wysokiej temperaturze <w>białko</w> ulega nieodwracalnemu procesowi denaturacji.",
                            "target_lang_translations": {
                                "ru": "При высокой температуре <w>белок</w> подвергается необратимому процессу денатурации."
                            }
                        }
                    ],
                    "synonyms": [
                        "proteina"
                    ],
                    "common_phrases": [
                        "synteza białka",
                        "struktura białka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "termin biologiczny/chemiczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокомолекулярное органическое соединение, состоящее из аминокислот; основной строительный элемент живых клеток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белок",
                                "target_lang_sense_clarification": "химия/биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f632be26-1b88-3076-b4cf-6362579d61ac",
                    "sense_definition": "Dokumenty, papiery urzędowe lub dowody tożsamości.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja sprawdziła jego <w>białko</w> i po chwili puściła go wolno.",
                            "target_lang_translations": {
                                "ru": "Полиция проверила его <w>документы</w> и через минуту отпустила его."
                            }
                        },
                        {
                            "text": "Nie miał przy sobie żadnego <w>białka</w>, więc nie mógł potwierdzić, kim jest.",
                            "target_lang_translations": {
                                "ru": "У него не было с собой никаких <w>документов</w>, поэтому он не мог подтвердить свою личность."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna lub przestępcza"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Документы, официальные бумаги или удостоверение личности (на сленге)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ксива",
                                "target_lang_sense_clarification": "сленг: документы/удостоверение"
                            },
                            {
                                "target_lang_word": "документы",
                                "target_lang_sense_clarification": "нейтральное значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "580ab52d-9f15-3588-952f-0fd2cc144bf2",
                    "sense_definition": "Past participle of <w>forget</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have completely <w>forgotten</w> what I wanted to say to you.",
                            "target_lang_translations": {
                                "ru": "Я совершенно <w>забыл</w>, что хотел тебе сказать."
                            }
                        },
                        {
                            "text": "She realized that she had <w>forgotten</w> to lock the front door.",
                            "target_lang_translations": {
                                "ru": "Она поняла, что <w>забыла</w> запереть входную дверь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have forgotten",
                        "had forgotten",
                        "be forgotten"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола <w>forget</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "глагол, действие"
                            },
                            {
                                "target_lang_word": "забывший",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e993654d-0afe-3225-b86a-6db971c8c660",
                    "sense_definition": "No longer remembered or kept in mind; having knowledge of something lost.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They discovered a <w>forgotten</w> city buried deep in the jungle.",
                            "target_lang_translations": {
                                "ru": "Они обнаружили <w>забытый</w> город, погребенный глубоко в джунглях."
                            }
                        },
                        {
                            "text": "This is a long <w>forgotten</w> tradition that we are trying to revive.",
                            "target_lang_translations": {
                                "ru": "Это давно <w>забытая</w> традиция, которую мы пытаемся возродить."
                            }
                        }
                    ],
                    "synonyms": [
                        "unremembered",
                        "lost",
                        "overlooked"
                    ],
                    "antonyms": [
                        "remembered",
                        "recalled",
                        "famous",
                        "cherished"
                    ],
                    "common_phrases": [
                        "long forgotten",
                        "easily forgotten",
                        "largely forgotten",
                        "almost forgotten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше не вспоминаемый; стершийся из памяти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забытый",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3b8ce741-79d1-3fc5-b63c-f53969cb583a",
                    "sense_definition": "A person or thing that is no longer remembered.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The monument stands as a tribute to the <w>forgotten</w> of the war.",
                            "target_lang_translations": {
                                "ru": "Этот памятник — дань уважения <w>забытым</w> жертвам войны."
                            }
                        },
                        {
                            "text": "Society often fails to care for the <w>forgotten</w> living on the streets.",
                            "target_lang_translations": {
                                "ru": "Общество часто не заботится о <w>забытых</w>, живущих на улицах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the forgotten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди или вещи, о которых больше не помнят."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забытые",
                                "target_lang_sense_clarification": "существительное, люди"
                            },
                            {
                                "target_lang_word": "забытое",
                                "target_lang_sense_clarification": "существительное, абстрактное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

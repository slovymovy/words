    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c3e93e85-31b3-3812-b8cc-0f68cea989fe",
                    "sense_definition": "Положить или нанести что-либо сверху на поверхность, закрывая её или защищая.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед началом ремонта нужно <покрыть> мебель плёнкой, чтобы не испачкать её.",
                            "target_lang_translations": {
                                "en": "Before starting repairs, you need to <w>cover</w> the furniture with film so as not to stain it."
                            }
                        },
                        {
                            "text": "Мастер посоветовал <покрыть> деревянный пол лаком для защиты от влаги.",
                            "target_lang_translations": {
                                "en": "The master advised to <w>coat</w> the wooden floor with varnish for protection against moisture."
                            }
                        }
                    ],
                    "synonyms": [
                        "накрыть",
                        "прикрыть",
                        "заполнить"
                    ],
                    "antonyms": [
                        "открыть",
                        "раскрыть",
                        "оголить",
                        "обнажить"
                    ],
                    "common_phrases": [
                        "покрыть лаком",
                        "покрыть крышу",
                        "покрыть стол"
                    ],
                    "target_lang_definitions": {
                        "en": "To put or apply something on top of a surface, closing or protecting it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "place something over"
                            },
                            {
                                "target_lang_word": "coat",
                                "target_lang_sense_clarification": "apply a layer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5df65330-d934-382f-910a-38c7c14790f9",
                    "sense_definition": "Распространиться по всей поверхности, занять собой пространство или территорию.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За ночь густой снег успел полностью <покрыть> поля и дороги.",
                            "target_lang_translations": {
                                "en": "Overnight, thick snow managed to completely <w>cover</w> the fields and roads."
                            }
                        },
                        {
                            "text": "Новая сеть мобильной связи позволит <покрыть> сигналом даже отдалённые деревни.",
                            "target_lang_translations": {
                                "en": "The new mobile network will allow to <w>cover</w> even remote villages with signal."
                            }
                        }
                    ],
                    "synonyms": [
                        "усеять",
                        "заполонить",
                        "охватить"
                    ],
                    "common_phrases": [
                        "покрыть снегом",
                        "покрыть территорию"
                    ],
                    "target_lang_definitions": {
                        "en": "To spread over the entire surface, occupying space or territory."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "extend over an area"
                            },
                            {

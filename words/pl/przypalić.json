    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cffb4e89-a0c8-3abe-a066-ee8334e834bd",
                    "sense_definition": "Spalić lekko lub częściowo powierzchnię czegoś, zwłaszcza potrawy w trakcie gotowania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scorching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz ciągle mieszać sos, żeby go nie <w>przypalić</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен постоянно помешивать соус, чтобы он не <w>пригорел</w>."
                            }
                        },
                        {
                            "text": "Przez chwilę nieuwagi <w>przypaliłam</w> mleko, które gotowałam na śniadanie.",
                            "target_lang_translations": {
                                "ru": "Из-за минутной невнимательности у меня <w>пригорело</w> молоко, которое я варила на завтрак."
                            }
                        }
                    ],
                    "synonyms": [
                        "przypiec",
                        "nadpalić"
                    ],
                    "common_phrases": [
                        "przypalić mleko",
                        "przypalić garnek",
                        "przypalić żelazkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слегка сжечь или подпалить поверхность чего-либо, особенно еды во время готовки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подпалить",
                                "target_lang_sense_clarification": "слегка обжечь поверхность"
                            },
                            {
                                "target_lang_word": "сжечь",
                                "target_lang_sense_clarification": "испортить еду огнем (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20e8fdfa-3be7-3a31-8b4c-ec6147787765",
                    "sense_definition": "Ulec lekkiemu spaleniu lub przywarciu do naczynia pod wpływem wysokiej temperatury (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scorching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zostawiłem zupę na gazie i niestety <w>przypaliła</w> się.",
                            "target_lang_translations": {
                                "ru": "Я оставил суп на плите, и, к сожалению, он <w>пригорел</w>."
                            }
                        },
                        {
                            "text": "Ciasto w piekarniku <w>przypaliło</w> się z wierzchu, ale w środku jest dobre.",
                            "target_lang_translations": {
                                "ru": "Пирог в духовке <w>подгорел</w> сверху, но внутри он хороший."
                            }
                        }
                    ],
                    "synonyms": [
                        "przypiec się"
                    ],
                    "common_phrases": [
                        "mleko się przypaliło",
                        "obiad się przypalił"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергнуться легкому сгоранию или прилипанию к посуде под воздействием высокой температуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пригореть",
                                "target_lang_sense_clarification": "прилипнуть ко дну, о еде"
                            },
                            {
                                "target_lang_word": "подгореть",
                                "target_lang_sense_clarification": "слегка обгореть снаружи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "072cdb07-aad0-381b-adde-7a619394ff0a",
                    "sense_definition": "Zapalić wyrób tytoniowy, taki jak papieros, często korzystając z ognia innej osoby.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "lighting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna zatrzymał się na chwilę, aby <w>przypalić</w> papierosa.",
                            "target_lang_translations": {
                                "ru": "Мужчина остановился на мгновение, чтобы <w>прикурить</w> сигарету."
                            }
                        },
                        {
                            "text": "Czy mógłby mi pan dać <w>przypalić</w>?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы дать мне <w>прикурить</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zapalić"
                    ],
                    "common_phrases": [
                        "dać przypalić",
                        "przypalić papierosa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зажечь табачное изделие (сигарету), часто от огня другого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикурить",
                                "target_lang_sense_clarification": "зажечь сигарету"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
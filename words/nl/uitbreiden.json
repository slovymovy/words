            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b800259-bd06-3bae-aec8-35b554e4fa8c",
                    "sense_definition": "Iets groter, ruimer of talrijker maken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expansion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kinderen kunnen hun woordenschat <w>uitbreiden</w> door liedjes en versjes te leren.",
                            "target_lang_translations": {
                                "en": "Children can <w>expand</w> their vocabulary by learning songs and rhymes.",
                                "ru": "Дети могут <w>расширять</w> свой словарный запас, разучивая песенки и стишки."
                            }
                        },
                        {
                            "text": "De boeren <w>breidden</w> hun stallen <w>uit</w> om meer dieren te kunnen houden.",
                            "target_lang_translations": {
                                "en": "The farmers <w>expanded</w> their stables to be able to keep more animals.",
                                "ru": "Фермеры <w>расширили</w> свои хлевы, чтобы иметь возможность содержать больше животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergroten",
                        "vermeerderen",
                        "verruimen"
                    ],
                    "antonyms": [
                        "verkleinen",
                        "beperken",
                        "inkrimpen"
                    ],
                    "common_phrases": [
                        "kennis uitbreiden",
                        "het assortiment uitbreiden",
                        "een collectie uitbreiden"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something larger, more spacious, or more numerous.",
                        "ru": "Делать что-либо больше, просторнее или многочисленнее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expand",
                                "target_lang_sense_clarification": "to increase in size, number, or scope"
                            },
                            {
                                "target_lang_word": "extend",
                                "target_lang_sense_clarification": "to make larger in area or scope"
                            },
                            {
                                "target_lang_word": "enlarge",
                                "target_lang_sense_clarification": "to make physically bigger"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расширять",
                                "target_lang_sense_clarification": "делать более обширным или вместительным"
                            },
                            {
                                "target_lang_word": "увеличивать",
                                "target_lang_sense_clarification": "делать больше по размеру или количеству"
                            },
                            {
                                "target_lang_word": "пополнять",
                                "target_lang_sense_clarification": "добавлять новые элементы (например, коллекцию или знания)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "903fcbdc-4192-3b51-9ab6-da0477b64d7b",
                    "sense_definition": "Zich over een groter gebied verspreiden of in omvang toenemen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expansion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn roem <w>breidde</w> zich <w>uit</w> van zijn geboortestad naar de rest van het land.",
                            "target_lang_translations": {
                                "en": "His fame <w>spread</w> from his hometown to the rest of the country.",
                                "ru": "Его слава <w>распространилась</w> из родного города на всю остальную страну."
                            }
                        },
                        {
                            "text": "De olievlek <w>breidde</w> zich snel <w>uit</w> over het wateroppervlak.",
                            "target_lang_translations": {
                                "en": "The oil slick quickly <w>spread</w> over the water surface.",
                                "ru": "Нефтяное пятно быстро <w>распространилось</w> по поверхности воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "verspreiden",
                        "groeien",
                        "toenemen"
                    ],
                    "antonyms": [
                        "krimpen",
                        "afnemen"
                    ],
                    "common_phrases": [
                        "zich snel uitbreiden",
                        "zich verder uitbreiden"
                    ],
                    "target_lang_definitions": {
                        "en": "To spread over a larger area or increase in size.",
                        "ru": "Распространяться на большую территорию или увеличиваться в объеме."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spread",
                                "target_lang_sense_clarification": "to cover a larger area"
                            },
                            {
                                "target_lang_word": "expand",
                                "target_lang_sense_clarification": "to grow larger or spread out"
                            }
                        ],
                        "ru": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1bfc2eb8-e2e8-3fb6-89cc-424884b63790",
                    "sense_definition": "To put a dead body into a grave, tomb, or the ground, usually with a ceremony.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "death_interment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>bury</w> him in the old churchyard near his home.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>похоронить</w> его на старом кладбище возле его дома."
                            }
                        },
                        {
                            "text": "Many cultures <w>bury</w> the dead with specific rituals and traditions.",
                            "target_lang_translations": {
                                "ru": "Многие культуры <w>хоронят</w> мертвых с особыми ритуалами и традициями."
                            }
                        }
                    ],
                    "synonyms": [
                        "inter",
                        "entomb",
                        "lay to rest"
                    ],
                    "antonyms": [
                        "exhume",
                        "disinter"
                    ],
                    "common_phrases": [
                        "bury the dead",
                        "buried alive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать мертвое тело в могилу, гробницу или землю, обычно с соблюдением церемонии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хорони́ть",
                                "target_lang_sense_clarification": "imperfective, general term"
                            },
                            {
                                "target_lang_word": "похорони́ть",
                                "target_lang_sense_clarification": "perfective, general term"
                            },
                            {
                                "target_lang_word": "погреба́ть",
                                "target_lang_sense_clarification": "formal or literary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d74e537-0462-3b0e-811c-62eeb0312b0e",
                    "sense_definition": "To put something under the ground or cover it completely with earth, leaves, or another substance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_hiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog ran to the garden to <w>bury</w> its bone.",
                            "target_lang_translations": {
                                "ru": "Собака побежала в сад, чтобы <w>зарыть</w> свою кость."
                            }
                        },
                        {
                            "text": "Pirates in stories often <w>bury</w> their treasure on deserted islands.",
                            "target_lang_translations": {
                                "ru": "Пираты в историях часто <w>зарывают</w> свои сокровища на необитаемых островах."
                            }
                        }
                    ],
                    "synonyms": [
                        "hide",
                        "cover"
                    ],
                    "antonyms": [
                        "unearth",
                        "dig up"
                    ],
                    "common_phrases": [
                        "bury treasure",
                        "bury a bone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать что-либо под землю или полностью покрывать землей, листьями или другим веществом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарыва́ть",
                                "target_lang_sense_clarification": "to dig something in"
                            },
                            {
                                "target_lang_word": "зака́пывать",
                                "target_lang_sense_clarification": "to cover with earth"
                            },
                            {
                                "target_lang_word": "пря́тать",
                                "target_lang_sense_clarification": "to hide"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b27a63c-4d89-3c5a-9911-99644433c8c6",
                    "sense_definition": "To cover or hide someone or something completely so they cannot be seen or found.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_hiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was so tired that she <w>buried</w> her face in the pillow and fell asleep.",
                            "target_lang_translations": {
                                "ru": "Она так устала, что <w>зарыла</w> лицо в подушку и уснула."
                            }
                        },
                        {
                            "text": "The important information was <w>buried</w> deep within the long report.",
                            "target_lang_translations": {
                                "ru": "Важная информация была <w>спрятана</w> глубоко в длинном отчете."
                            }
                        }
                    ],
                    "synonyms": [
                        "conceal",
                        "hide",
                        "cover up"
                    ],
                    "antonyms": [
                        "reveal",
                        "expose",
                        "uncover"
                    ],
                    "common_phrases": [
                        "bury one's face",
                        "buried in the sand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью укрывать или прятать кого-либо или что-либо, чтобы их нельзя было увидеть или найти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спря́тать",
                                "target_lang_sense_clarification": "to hide completely"
                            },
                            {
                                "target_lang_word": "зары́ть",
                                "target_lang_sense_clarification": "often used with 'face'"
                            },
                            {
                                "target_lang_word": "скры́ть",
                                "target_lang_sense_clarification": "to conceal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a470f26f-7752-3b45-b4df-30c4758cc522",
                    "sense_definition": "To give someone a very large amount of work or information to deal with.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_overwhelm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am currently <w>buried</w> in paperwork and cannot leave the office early.",
                            "target_lang_translations": {
                                "ru": "Я сейчас <w>завален</w> бумажной работой и не могу уйти из офиса рано."
                            }
                        },
                        {
                            "text": "The manager was <w>buried</w> under a mountain of complaints from customers.",
                            "target_lang_translations": {
                                "ru": "Менеджер был <w>погребен</w> под горой жалоб от клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "overwhelm",
                        "swamp",
                        "inundate"
                    ],
                    "common_phrases": [
                        "buried in work",
                        "buried in paperwork"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-либо очень большой объем работы или информации для выполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завали́ть",
                                "target_lang_sense_clarification": "colloquial, e.g., with work"
                            },
                            {
                                "target_lang_word": "погря́знуть",
                                "target_lang_sense_clarification": "to be bogged down in"
                            },
                            {
                                "target_lang_word": "погрести́",
                                "target_lang_sense_clarification": "metaphorical, under a pile"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7031d32f-b332-3b64-940c-83ba6ebee529",
                    "sense_definition": "To try to forget or ignore an unpleasant feeling, memory, or secret.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_suppress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>bury</w> his grief by working harder than ever before.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>заглушить</w> свое горе, работая усерднее, чем когда-либо прежде."
                            }
                        },
                        {
                            "text": "They kept the family secret <w>buried</w> for many years.",
                            "target_lang_translations": {
                                "ru": "Они хранили семейную тайну <w>скрытой</w> много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "suppress",
                        "repress",
                        "stifle"
                    ],
                    "antonyms": [
                        "express",
                        "vent"
                    ],
                    "common_phrases": [
                        "bury one's feelings",
                        "bury the past"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться забыть или игнорировать неприятное чувство, воспоминание или секрет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "похорони́ть",
                                "target_lang_sense_clarification": "metaphorical, to put to rest"
                            },
                            {
                                "target_lang_word": "заглуши́ть",
                                "target_lang_sense_clarification": "to stifle or suppress"
                            },
                            {
                                "target_lang_word": "скры́ть",
                                "target_lang_sense_clarification": "to hide or keep secret"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e512a05e-3a56-34a4-8ace-877c36dc52fe",
                    "sense_definition": "To put an end to a disagreement or conflict.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After years of fighting, they finally decided to <w>bury</w> the hatchet.",
                            "target_lang_translations": {
                                "ru": "После многих лет вражды они наконец решили <w>зарыть</w> топор войны."
                            }
                        },
                        {
                            "text": "We should <w>bury</w> our differences and work together for the common good.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>забыть</w> наши разногласия и работать вместе ради общего блага."
                            }
                        }
                    ],
                    "synonyms": [
                        "resolve",
                        "settle"
                    ],
                    "common_phrases": [
                        "bury the hatchet"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "336f2a49-a654-3953-a2f1-26e87180e5a7",
                    "sense_definition": "Decorated or edged with small loops of thread known as picots.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The seamstress admired the <w>picoted</w> edge of the delicate lace handkerchief.",
                            "target_lang_translations": {
                                "ru": "Швея любовалась <w>отделанным пико</w> краем тонкого кружевного платочка."
                            }
                        },
                        {
                            "text": "She chose a <w>picoted</w> ribbon to add a decorative finish to the garment.",
                            "target_lang_translations": {
                                "ru": "Она выбрала ленту, <w>украшенную пико</w>, чтобы добавить изделию декоративную отделку."
                            }
                        }
                    ],
                    "antonyms": [
                        "plain",
                        "unadorned"
                    ],
                    "common_phrases": [
                        "picoted edge",
                        "picoted ribbon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшенный или окаймленный маленькими петельками из ниток, известными как пико."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшенный пико",
                                "target_lang_sense_clarification": "имеющий декоративную отделку в виде петелек"
                            },
                            {
                                "target_lang_word": "окаймленный пико",
                                "target_lang_sense_clarification": "с краем, обработанным мелкими петельками"
                            }
                        ]
                    }
                }
            ]
        },
        {

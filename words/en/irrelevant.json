    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8e6515b9-4fc4-3f35-9ec6-f320f898508b",
                    "sense_definition": "Not connected with or relevant to the matter at hand; lacking importance or utility to the current context.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "not_connected",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His comments were completely <w>irrelevant</w> to the discussion we were having.",
                            "target_lang_translations": {
                                "ru": "Его комментарии совершенно <w>не относились</w> к нашей дискуссии."
                            }
                        },
                        {
                            "text": "The fact that she is his sister is <w>irrelevant</w> to the facts of the case.",
                            "target_lang_translations": {
                                "ru": "Тот факт, что она его сестра, <w>не имеет значения</w> для обстоятельств дела."
                            }
                        }
                    ],
                    "synonyms": [
                        "unrelated",
                        "immaterial",
                        "inapplicable",
                        "beside the point",
                        "off-topic",
                        "tangential",
                        "unassociated"
                    ],
                    "antonyms": [
                        "relevant",
                        "pertinent"
                    ],
                    "common_phrases": [
                        "completely irrelevant",
                        "irrelevant details",
                        "irrelevant to the discussion",
                        "largely irrelevant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не связанный с предметом обсуждения; не имеющий значения или пользы в текущем контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не относящийся к делу",
                                "target_lang_sense_clarification": "не связанный с обсуждаемым вопросом"
                            },
                            {
                                "target_lang_word": "несущественный",
                                "target_lang_sense_clarification": "не имеющий большого значения"
                            },
                            {
                                "target_lang_word": "неуместный",
                                "target_lang_sense_clarification": "не подходящий к ситуации или контексту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db6b1bb0-9a5d-3e4a-869f-50c9ae67f73b",
                    "sense_definition": "No longer famous, popular, or culturally significant.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The singer worried about becoming <w>irrelevant</w> after his last album failed to sell.",
                            "target_lang_translations": {
                                "ru": "Певец переживал, что станет <w>неактуальным</w> после того, как его последний альбом плохо продавался."
                            }
                        },
                        {
                            "text": "Trends change so fast that today's stars might be <w>irrelevant</w> by next year.",
                            "target_lang_translations": {
                                "ru": "Тренды меняются так быстро, что сегодняшние звёзды могут стать <w>никому не нужными</w> уже в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "unimportant"
                    ],
                    "antonyms": [
                        "relevant"
                    ],
                    "common_phrases": [
                        "become irrelevant",
                        "culturally irrelevant"
                    ],

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "85282715-e1ab-30b6-9f25-7c44592795e5",
                    "sense_definition": "To act or work together with another person or force.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two chemicals <w>coact</w> to produce a stronger reaction than either could alone.",
                            "target_lang_translations": {
                                "ru": "Эти два химиката <w>взаимодействуют</w>, вызывая более сильную реакцию, чем каждый из них по отдельности."
                            }
                        },
                        {
                            "text": "Different departments must <w>coact</w> efficiently to achieve the company's goals.",
                            "target_lang_translations": {
                                "ru": "Различные отделы должны эффективно <w>сотрудничать</w> для достижения целей компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "cooperate"
                    ],
                    "common_phrases": [
                        "coact with",
                        "coact to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действовать или работать совместно с другим человеком или силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взаимодействовать",
                                "target_lang_sense_clarification": "оказывать взаимное влияние или работать вместе"
                            },
                            {
                                "target_lang_word": "действовать сообща",
                                "target_lang_sense_clarification": "объединять усилия для достижения цели"
                            },
                            {
                                "target_lang_word": "сотрудничать",
                                "target_lang_sense_clarification": "работать вместе над общим делом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e045276-6802-3618-b4a4-b153cc4c8050",
                    "sense_definition": "To force or compel someone to do something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ruler sought to <w>coact</w> his subjects into total obedience.",
                            "target_lang_translations": {
                                "ru": "Правитель стремился <w>принудить</w> своих подданных к полному повиновению."
                            }
                        },
                        {
                            "text": "They were <w>coacted</w> by dire necessity to leave their ancestral homes.",
                            "target_lang_translations": {
                                "ru": "Они были <w>принуждены</w> крайней необходимостью покинуть свои родовые дома."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "usage is largely obsolete in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять или принуждать кого-либо сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принуждать",
                                "target_lang_sense_clarification": "заставлять делать что-то против воли"
                            },
                            {
                                "target_lang_word": "заставлять",
                                "target_lang_sense_clarification": "вынуждать совершить действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d4877bae-f769-3274-a3bf-ce7096f616de",
                    "sense_definition": "Forced or done under compulsion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His submission to the treaty was <w>coact</w> rather than voluntary.",
                            "target_lang_translations": {
                                "ru": "Его подчинение договору было <w>принудительным</w>, а не добровольным."
                            }
                        },
                        {
                            "text": "A <w>coact</w> confession is generally considered invalid in a court of law.",
                            "target_lang_translations": {
                                "ru": "<w>Вынужденное</w> признание, как правило, считается недействительным в суде."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "rarely used in modern English"
                        }
                    ],
                    "target_lang_definitions": {

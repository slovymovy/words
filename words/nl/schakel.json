            "senses": [
                {
                    "sense_id": "b1b0d0c7-960f-3c05-92f9-4e5f4b9cb56b",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>schakelen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",

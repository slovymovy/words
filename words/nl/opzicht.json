{
    "word_family": [
        "opzichter",
        "opzichtig"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc51278d-b5c9-3ca2-8d8c-0de4b926f51d",
                    "sense_definition": "Een specifiek punt van waaruit men iets bekijkt of beoordeelt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit <w>opzicht</w> ben ik het volledig met je eens.",
                            "target_lang_translations": {
                                "ru": "В этом <w>отношении</w> я полностью с тобой согласен.",
                                "en": "In this <w>respect</w>, I fully agree with you."
                            }
                        },
                        {
                            "text": "De twee voorstellen verschillen in veel <w>opzichten</w> van elkaar.",
                            "target_lang_translations": {
                                "ru": "Два предложения отличаются друг от друга во многих <w>отношениях</w>.",
                                "en": "The two proposals differ from each other in many <w>respects</w>."
                            }
                        },
                        {
                            "text": "Het was in dat <w>opzicht</w> opvallend fijn om alleen te zijn.",
                            "target_lang_translations": {
                                "ru": "В этом <w>смысле</w> было удивительно приятно побыть одному.",
                                "en": "It was remarkably nice to be alone in that <w>regard</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aspect",
                        "oogpunt",
                        "facet"
                    ],
                    "common_phrases": [
                        "in dit opzicht",
                        "in welk opzicht",
                        "in alle opzichten",
                        "ten opzichte van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная точка зрения или сторона, с которой что-либо рассматривается или оценивается.",
                        "en": "A specific point from which something is viewed or judged."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "точка зрения, контекст (часто во фразах 'в этом отношении')"
                            },
                            {
                                "target_lang_word": "аспект",
                                "target_lang_sense_clarification": "сторона, грань вопроса"
                            },
                            {
                                "target_lang_word": "смысл",
                                "target_lang_sense_clarification": "значение, понимание"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "respect",
                                "target_lang_sense_clarification": "a particular detail or point (often used in 'in this respect')"
                            },
                            {
                                "target_lang_word": "regard",
                                "target_lang_sense_clarification": "attention to or concern for something (often used in 'in that regard')"
                            },
                            {
                                "target_lang_word": "aspect",
                                "target_lang_sense_clarification": "a particular part or feature of something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "502c81cb-ec4f-3f29-b535-e60ff7dcf856",
                    "sense_definition": "Het houden van toezicht of de leiding over iets.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "supervision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De restauratie van het gebouw gebeurde onder <w>opzicht</w> van een deskundige.",
                            "target_lang_translations": {
                                "ru": "Реставрация здания проходила под <w>надзором</w> эксперта.",
                                "en": "The restoration of the building took place under the <w>supervision</w> of an expert."
                            }
                        },
                        {
                            "text": "Hij is belast met het <w>opzicht</w> over de openbare werken in de gemeente.",
                            "target_lang_translations": {
                                "ru": "Ему поручен <w>надзор</w> за общественными работами в муниципалитете.",
                                "en": "He is charged with the <w>oversight</w> of public works in the municipality."
                            }
                        }
                    ],
                    "synonyms": [
                        "toezicht",
                        "leiding",
                        "supervisie"
                    ],
                    "common_phrases": [
                        "onder opzicht van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществление надзора или руководство чем-либо.",
                        "en": "The act of keeping watch or having leadership over something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надзор",
                                "target_lang_sense_clarification": "наблюдение, контроль"
                            },
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "управление процессом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "supervision",
                                "target_lang_sense_clarification": "the action of supervising someone or something"
                            },
                            {
                                "target_lang_word": "oversight",
                                "target_lang_sense_clarification": "the action of overseeing something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
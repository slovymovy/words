    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "104687a3-5d42-33c5-bf00-83b16dd00932",
                    "sense_definition": "Helemaal opeten of opdrinken; zorgen dat er niets van de maaltijd overblijft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb zoveel gegeten dat ik mijn toetje niet meer <w>opkrijg</w>.",
                            "target_lang_translations": {
                                "en": "I have eaten so much that I can't <w>finish</w> my dessert anymore.",
                                "ru": "Я так много съел, что десерт уже не <w>осилю</w>."
                            }
                        },
                        {
                            "text": "Als je je bord <w>opkrijgt</w>, mag je van tafel.",
                            "target_lang_translations": {
                                "en": "If you <w>finish</w> your plate, you may leave the table.",
                                "ru": "Если ты <w>доешь</w> всё, что на тарелке, можешь выйти из-за стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "opeten",
                        "leegdrinken",
                        "verorberen"
                    ],
                    "antonyms": [
                        "laten staan"
                    ],
                    "common_phrases": [
                        "alles opkrijgen",
                        "zijn bord opkrijgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To eat or drink completely; to ensure nothing of the meal remains.",
                        "ru": "Полностью съесть или выпить; сделать так, чтобы от еды ничего не осталось."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finish",
                                "target_lang_sense_clarification": "food or drink"
                            },
                            {
                                "target_lang_word": "manage to eat",
                                "target_lang_sense_clarification": "capability"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осилить",
                                "target_lang_sense_clarification": "справиться с едой"
                            },
                            {
                                "target_lang_word": "доесть",
                                "target_lang_sense_clarification": "съесть до конца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4a8c40c-bb0f-3701-933d-68771eca64af",
                    "sense_definition": "Als taak of huiswerk opgegeven krijgen om te maken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leerlingen vonden het vervelend dat ze zoveel huiswerk <w>opkregen</w>.",
                            "target_lang_translations": {
                                "en": "The students found it annoying that they <w>were assigned</w> so much homework.",
                                "ru": "Ученикам не нравилось, что им <w>задавали</w> так много домашней работы."
                            }
                        },
                        {
                            "text": "We hebben voor morgen drie moeilijke sommen <w>opgekregen</w>.",
                            "target_lang_translations": {
                                "en": "We have <w>been assigned</w> three difficult math problems for tomorrow.",
                                "ru": "Нам <w>задали</w> на завтра три трудные задачи."
                            }
                        }
                    ],
                    "synonyms": [
                        "opgegeven krijgen"
                    ],
                    "common_phrases": [
                        "huiswerk opkrijgen",
                        "strafwerk opkrijgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be given a task or homework to complete.",
                        "ru": "Получить в качестве задания или домашней работы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be assigned",
                                "target_lang_sense_clarification": "homework or tasks"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "homework"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "задание"
                            },
                            {
                                "target_lang_word": "задать",
                                "target_lang_sense_clarification": "в контексте 'нам задали'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3248e8b-554a-302d-a1ef-6099b2214389",
                    "sense_definition": "Voor elkaar krijgen om iets open te maken of op het lichaam te plaatsen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "manipulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het deksel zit zo vast dat ik de pot niet <w>opkrijg</w>.",
                            "target_lang_translations": {
                                "en": "The lid is stuck so tight that I can't <w>get</w> the jar <w>open</w>.",
                                "ru": "Крышка сидит так плотно, что я не могу <w>открыть</w> банку."
                            }
                        },
                        {
                            "text": "Vanwege het lawaai moet je een koptelefoon <w>opkrijgen</w> om de muziek te horen.",
                            "target_lang_translations": {
                                "en": "Because of the noise, you have to <w>get</w> headphones <w>on</w> to hear the music.",
                                "ru": "Из-за шума нужно <w>надеть</w> наушники, чтобы услышать музыку."
                            }
                        }
                    ],
                    "synonyms": [
                        "openkrijgen",
                        "loskrijgen"
                    ],
                    "common_phrases": [
                        "de pot opkrijgen",
                        "de deur opkrijgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To manage to open something or place something on the body.",
                        "ru": "Суметь открыть что-либо или надеть что-либо на себя."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get open",
                                "target_lang_sense_clarification": "a container or door"
                            },
                            {
                                "target_lang_word": "get on",
                                "target_lang_sense_clarification": "clothing or accessories"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "банку, дверь"
                            },
                            {
                                "target_lang_word": "надеть",
                                "target_lang_sense_clarification": "одежду, аксессуары"
                            }
                        ]
                    }
                }
            ]
        }
    ]

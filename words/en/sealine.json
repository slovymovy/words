                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морская леска",
                                "target_lang_sense_clarification": "леска для морской рыбалки"
                            },
                            {
                                "target_lang_word": "рыболовный шнур",
                                "target_lang_sense_clarification": "прочная нить для ловли рыбы"
                            }
                        ]
                    }
                }
            ]

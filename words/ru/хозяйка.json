    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "501b5464-b01d-3062-8984-32a17b891835",
                    "sense_definition": "Женщина, которая владеет чем-либо на правах собственности.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ownership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полноправная <w>хозяйка</w> квартиры решила сделать капитальный ремонт.",
                            "target_lang_translations": {
                                "en": "The rightful <w>owner</w> of the apartment decided to do a major renovation."
                            }
                        },
                        {
                            "text": "Она чувствовала себя как <w>хозяйка</w> положения и уверенно отдавала приказы.",
                            "target_lang_translations": {
                                "en": "She felt like the <w>mistress</w> of the situation and confidently gave orders."
                            }
                        }
                    ],
                    "synonyms": [
                        "владелица",
                        "собственница"
                    ],
                    "common_phrases": [
                        "хозяйка квартиры",
                        "хозяйка машины",
                        "хозяйка положения"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who owns something by right of property."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "owner",
                                "target_lang_sense_clarification": "general ownership"
                            },
                            {
                                "target_lang_word": "proprietor",
                                "target_lang_sense_clarification": "business owner"
                            },
                            {
                                "target_lang_word": "mistress",
                                "target_lang_sense_clarification": "having control or authority over something (e.g., a situation)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f59cdc0e-1c14-3599-92ec-3bf265b860c7",
                    "sense_definition": "Женщина, принимающая гостей у себя дома или распоряжающаяся в доме.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "domestic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Приветливая <w>хозяйка</w> встретила нас на пороге и пригласила к столу.",
                            "target_lang_translations": {
                                "en": "A welcoming <w>hostess</w> met us at the threshold and invited us to the table."
                            }
                        },
                        {
                            "text": "Гости поблагодарили <w>хозяйку</w> за вкусный ужин и тёплый приём.",
                            "target_lang_translations": {
                                "en": "The guests thanked the <w>hostess</w> for the delicious dinner and warm welcome."
                            }
                        }
                    ],
                    "common_phrases": [
                        "гостеприимная хозяйка",
                        "радушная хозяйка",
                        "хозяйка дома"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who receives guests at her home or manages the household."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hostess",
                                "target_lang_sense_clarification": "receiving guests"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "480b2369-fe1f-31d4-80de-fb1e1de9912f",
                    "sense_definition": "Женщина, умело ведущая домашние дела и заботящаяся о быте.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "domestic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя бабушка была прекрасная <w>хозяйка</w> и всегда держала дом в чистоте.",
                            "target_lang_translations": {
                                "en": "My grandmother was a wonderful <w>housekeeper</w> and always kept the house clean."
                            }
                        },
                        {
                            "text": "Даже самая экономная <w>хозяйка</w> иногда позволяет себе купить что-то красивое для кухни.",
                            "target_lang_translations": {
                                "en": "Even the most thrifty <w>housewife</w> sometimes allows herself to buy something beautiful for the kitchen."
                            }
                        }
                    ],
                    "common_phrases": [
                        "хорошая хозяйка",
                        "экономная хозяйка",
                        "домашняя хозяйка"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who skillfully manages household affairs and domestic life."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "housewife",
                                "target_lang_sense_clarification": "manager of domestic affairs"
                            },
                            {
                                "target_lang_word": "homemaker",
                                "target_lang_sense_clarification": "one who manages a home"
                            },
                            {
                                "target_lang_word": "housekeeper",
                                "target_lang_sense_clarification": "referring to skill in maintaining a home"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28d9fb1e-c8ea-3de2-b1ef-473a8e7f27e9",
                    "sense_definition": "Владелица жилья, сдающая его внаём (по отношению к квартирантам).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ownership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенты каждый месяц отдавали деньги <w>хозяйке</w> за съёмную комнату.",
                            "target_lang_translations": {
                                "en": "Every month the students gave money to the <w>landlady</w> for the rented room."
                            }
                        },
                        {
                            "text": "Наша <w>хозяйка</w> оказалась строгой женщиной и запретила шуметь после десяти вечера.",
                            "target_lang_translations": {
                                "en": "Our <w>landlady</w> turned out to be a strict woman and forbade making noise after ten in the evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "владелица"
                    ],
                    "common_phrases": [
                        "хозяйка съёмной квартиры",
                        "платить хозяйке"
                    ],
                    "target_lang_definitions": {
                        "en": "A female property owner who rents it out to tenants."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "landlady",
                                "target_lang_sense_clarification": "lessor of property"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "786f581f-5b63-36fb-85eb-eb1abd89a1b6",
                    "sense_definition": "Женщина-нанимательница или начальница по отношению к работникам.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "ownership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Работники боялись гнева своей <w>хозяйки</w> и старались всё делать вовремя.",
                            "target_lang_translations": {
                                "en": "The workers feared the anger of their <w>mistress</w> and tried to do everything on time."
                            }
                        },
                        {
                            "text": "Новая <w>хозяйка</w> фермы решила полностью изменить график работы персонала.",
                            "target_lang_translations": {
                                "en": "The new <w>owner</w> of the farm decided to completely change the staff's work schedule."
                            }
                        }
                    ],
                    "synonyms": [
                        "начальница",
                        "работодательница"
                    ],
                    "common_phrases": [
                        "строгая хозяйка"
                    ],
                    "target_lang_definitions": {
                        "en": "A female employer or person in authority over workers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mistress",
                                "target_lang_sense_clarification": "female head of a household or estate (archaic/formal)"
                            },
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "employer or supervisor"
                            },
                            {
                                "target_lang_word": "owner",
                                "target_lang_sense_clarification": "of a business or farm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d911076-c14f-3a5f-a9af-19ffba2759e2",
                    "sense_definition": "Жена (обычно в речи мужа или близких знакомых).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно позвонить домой, а то моя <w>хозяйка</w> будет волноваться.",
                            "target_lang_translations": {
                                "en": "I need to call home, or my <w>missus</w> will worry."
                            }
                        },
                        {
                            "text": "Сосед сказал, что его <w>хозяйка</w> уехала отдыхать в санаторий.",
                            "target_lang_translations": {
                                "en": "The neighbor said that his <w>missus</w> went away to a sanatorium to rest."
                            }
                        }
                    ],
                    "common_phrases": [
                        "моя хозяйка"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "прост."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Wife (colloquial, usually spoken by the husband)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "missus",
                                "target_lang_sense_clarification": "colloquial/slang"
                            },
                            {
                                "target_lang_word": "wife",
                                "target_lang_sense_clarification": "standard meaning"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
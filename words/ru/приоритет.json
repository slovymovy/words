    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d25c4419-591d-327f-8d0c-27b3877a295c",
                    "sense_definition": "То, что считается наиболее важным, первоочередным и требующим особого внимания.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Безопасность пассажиров — это наш главный <w>приоритет</w>.",
                            "target_lang_translations": {
                                "en": "Passenger safety is our main <w>priority</w>."
                            }
                        },
                        {
                            "text": "Нам нужно правильно расставить <w>приоритеты</w>, чтобы успеть закончить проект.",
                            "target_lang_translations": {
                                "en": "We need to set <w>priorities</w> correctly to finish the project on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "цель"
                    ],
                    "common_phrases": [
                        "расставить приоритеты",
                        "главный приоритет",
                        "смена приоритетов",
                        "выбрать приоритет"
                    ],
                    "target_lang_definitions": {
                        "en": "Something considered the most important, urgent, and requiring special attention before other things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "priority",
                                "target_lang_sense_clarification": "most important thing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7746789d-4999-3fa3-82ff-4ab3b8775991",
                    "sense_definition": "Преимущественное право на совершение действия или использование ресурса по сравнению с другими.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На этом перекрёстке трамвай всегда имеет <w>приоритет</w> перед автомобилями.",
                            "target_lang_translations": {
                                "en": "At this intersection, the tram always has <w>priority</w> over cars."
                            }
                        },
                        {
                            "text": "Машины скорой помощи получают <w>приоритет</w> в движении по городу.",
                            "target_lang_translations": {
                                "en": "Ambulances get <w>priority</w> in city traffic."
                            }
                        }
                    ],
                    "synonyms": [
                        "преимущество"
                    ],
                    "common_phrases": [
                        "иметь приоритет",
                        "приоритет проезда",
                        "получить приоритет"
                    ],
                    "target_lang_definitions": {
                        "en": "A preferential right to perform an action or use a resource before others, often used in traffic contexts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "priority",
                                "target_lang_sense_clarification": "right to proceed first"
                            },
                            {
                                "target_lang_word": "right of way",
                                "target_lang_sense_clarification": "traffic precedence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcf0b38a-e4e3-3e70-90fe-7956d1b9ca52",
                    "sense_definition": "Официально признанное первенство по времени в научном открытии, изобретении или идее.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёные ведут долгий спор о <w>приоритете</w> в открытии этого химического элемента.",
                            "target_lang_translations": {
                                "en": "Scientists are having a long dispute about <w>priority</w> in the discovery of this chemical element."
                            }
                        },
                        {
                            "text": "Наш институт хочет закрепить за собой <w>приоритет</w> этой важной разработки.",
                            "target_lang_translations": {
                                "en": "Our institute wants to secure <w>priority</w> for this important development."
                            }
                        }
                    ],
                    "synonyms": [
                        "первенство"
                    ],
                    "common_phrases": [
                        "приоритет открытия",
                        "спор о приоритете",
                        "закрепить приоритет"
                    ],
                    "target_lang_definitions": {
                        "en": "Officially recognized primacy in time regarding a scientific discovery, invention, or idea."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "priority",
                                "target_lang_sense_clarification": "precedence in discovery"
                            },
                            {
                                "target_lang_word": "precedence",
                                "target_lang_sense_clarification": "state of coming earlier"
                            }

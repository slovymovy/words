    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1d8fc984-9a35-3421-b1c7-d9cf1d48d252",
                    "sense_definition": "Переместить кого-либо или что-либо через какое-либо препятствие (реку, границу, дорогу) на другую сторону.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лодочник согласился <переправить> нас на другой берег реки до заката.",
                            "target_lang_translations": {
                                "en": "The boatman agreed to <ferry> us to the other bank of the river before sunset."
                            }
                        },
                        {
                            "text": "Спасателям удалось быстро <переправить> пострадавших через разрушенный мост.",
                            "target_lang_translations": {
                                "en": "The rescuers managed to quickly <transport> the victims across the destroyed bridge."
                            }
                        }
                    ],
                    "synonyms": [
                        "перевезти",
                        "перенести"
                    ],
                    "common_phrases": [
                        "переправить через реку",
                        "переправить на другой берег",
                        "переправить груз",
                        "переправить через границу"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something across an obstacle (such as a river or border) to the other side."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "move across a barrier"
                            },
                            {
                                "target_lang_word": "ferry",
                                "target_lang_sense_clarification": "transport, especially by boat"
                            },
                            {
                                "target_lang_word": "get across",
                                "target_lang_sense_clarification": "cause to move to the other side"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdc91788-9b63-3d65-a59b-90db4d6341fa",
                    "sense_definition": "Внести изменения в написанное, чтобы устранить ошибку или поменять одну букву, цифру или слово на другое.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель попросил ученика <переправить> ошибку в слове прямо в тетради.",
                            "target_lang_translations": {
                                "en": "The teacher asked the student to <correct> the mistake in the word right in the notebook."
                            }
                        },
                        {
                            "text": "В ведомости нужно аккуратно <переправить> старую дату на новую.",
                            "target_lang_translations": {
                                "en": "In the register, one needs to carefully <change> the old date to the new one."
                            }
                        }
                    ],
                    "synonyms": [
                        "исправить",
                        "поправить"
                    ],
                    "common_phrases": [
                        "переправить ошибку",
                        "переправить дату",
                        "переправить текст",
                        "переправить оценку"
                    ],
                    "target_lang_definitions": {
                        "en": "To make changes to written text to fix an error or change a character or word."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "correct",
                                "target_lang_sense_clarification": "fix an error"
                            },
                            {
                                "target_lang_word": "alter",
                                "target_lang_sense_clarification": "change specific details"
                            },
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "replace one thing with another"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14ef8a45-64a0-305f-b716-a8eb8e612e80",
                    "sense_definition": "Отправить или передать что-либо (письмо, документы, посылку) по назначению, часто через посредника.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь обещал срочно <переправить> эти документы в главный офис компании.",
                            "target_lang_translations": {
                                "en": "The secretary promised to urgently <forward> these documents to the company's head office."
                            }
                        },
                        {
                            "text": "Мы решили <переправить> посылку с подарками через знакомых, которые едут в Москву.",
                            "target_lang_translations": {
                                "en": "We decided to <send> the parcel with gifts via acquaintances who are traveling to Moscow."
                            }
                        }
                    ],
                    "synonyms": [
                        "переслать",
                        "отправить",
                        "передать"
                    ],
                    "common_phrases": [
                        "переправить письмо",
                        "переправить документы",
                        "переправить сообщение",
                        "переправить посылку"
                    ],
                    "target_lang_definitions": {
                        "en": "To send or transmit something (like documents or a parcel) to a destination, often via an intermediary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forward",
                                "target_lang_sense_clarification": "send on to a further destination"
                            },
                            {
                                "target_lang_word": "transmit",
                                "target_lang_sense_clarification": "pass on documents or information"
                            },
                            {
                                "target_lang_word": "send",
                                "target_lang_sense_clarification": "cause to go to a destination"
                            }
                        ]
                    }
                }
            ]
        }
    ]

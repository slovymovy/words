            "senses": [
                {
                    "sense_id": "9f0b46b3-b7d1-384d-91ae-b21995a8e8d0",
                    "sense_definition": "Zrobiony z gruszek, zawierający gruszki lub mający ich smak.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "culinary_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na deser podano pyszne ciasto <w>gruszkowe</w> z kruszonką.",
                            "target_lang_translations": {
                                "ru": "На десерт подали вкусный <w>грушевый</w> пирог с посыпкой."
                            }
                        },
                        {
                            "text": "Zawsze rano piję świeży sok <w>gruszkowy</w> do śniadania.",
                            "target_lang_translations": {
                                "ru": "По утрам я всегда пью свежий <w>грушевый</w> сок на завтрак."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sok gruszkowy",
                        "ciasto gruszkowe",
                        "kompot gruszkowy",
                        "dżem gruszkowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приготовленный из груш, содержащий груши или имеющий их вкус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грушевый",
                                "target_lang_sense_clarification": "о еде и напитках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4390659-d5c0-3c04-868a-79720eb7f342",
                    "sense_definition": "Związany z gruszkami, dotyczący gruszy lub owoców gruszki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relational_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W powietrzu unosił się delikatny, <w>gruszkowy</w> zapach.",
                            "target_lang_translations": {
                                "ru": "В воздухе витал нежный <w>грушевый</w> аромат."
                            }
                        },
                        {
                            "text": "Mój dziadek posiada duży sad <w>gruszkowy</w> za miastem.",
                            "target_lang_translations": {
                                "ru": "У моего дедушки есть большой <w>грушевый</w> сад за городом."

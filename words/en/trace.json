{
    "word_family": [
        "traceable",
        "tracer",
        "tracing",
        "retrace",
        "untraceable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "62796785-a18c-3c30-968a-fec74acad476",
                    "sense_definition": "To find or discover someone or something by investigating evidence or following a trail.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police are trying to <w>trace</w> the owner of the abandoned car.",
                            "target_lang_translations": {
                                "ru": "Полиция пытается <w>разыскать</w> владельца брошенной машины."
                            }
                        },
                        {
                            "text": "They managed to <w>trace</w> the call to a phone booth in the city center.",
                            "target_lang_translations": {
                                "ru": "Им удалось <w>отследить</w> звонок до телефонной будки в центре города."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "locate",
                        "find",
                        "hunt down"
                    ],
                    "common_phrases": [
                        "trace the source",
                        "trace a call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Найти или обнаружить кого-то или что-то, исследуя улики или идя по следу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отслеживать",
                                "target_lang_sense_clarification": "процесс поиска по следам или данным"
                            },
                            {
                                "target_lang_word": "разыскивать",
                                "target_lang_sense_clarification": "искать местонахождение"
                            },
                            {
                                "target_lang_word": "находить",
                                "target_lang_sense_clarification": "обнаруживать в результате поиска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46291940-59d9-3edf-9853-9db6d434046a",
                    "sense_definition": "To discover the origin or development of something by investigating its history.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was able to <w>trace</w> her family history back to the seventeenth century.",
                            "target_lang_translations": {
                                "ru": "Она смогла <w>проследить</w> историю своей семьи вплоть до семнадцатого века."
                            }
                        },
                        {
                            "text": "The documentary <w>traces</w> the development of the internet from its early days.",
                            "target_lang_translations": {
                                "ru": "Документальный фильм <w>прослеживает</w> развитие интернета с первых дней его существования."
                            }
                        }
                    ],
                    "synonyms": [
                        "investigate",
                        "research",
                        "determine"
                    ],
                    "common_phrases": [
                        "trace back to",
                        "trace the origin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить происхождение или развитие чего-либо, изучая историю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прослеживать",
                                "target_lang_sense_clarification": "восстанавливать ход событий или историю"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "выяснять происхождение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6797dec-290b-3179-ada1-3c5aa3c0c4d2",
                    "sense_definition": "To copy an image or text by drawing over its lines on a transparent sheet placed on top of it.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_copying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child learned to draw by <w>tracing</w> pictures from a comic book.",
                            "target_lang_translations": {
                                "ru": "Ребенок учился рисовать, <w>срисовывая</w> картинки из комикса."
                            }
                        },
                        {
                            "text": "He <w>traced</w> the map onto a fresh piece of paper so he could mark the route.",
                            "target_lang_translations": {
                                "ru": "Он <w>перевел</w> карту на чистый лист бумаги, чтобы отметить маршрут."
                            }
                        }
                    ],
                    "synonyms": [
                        "copy",
                        "outline",
                        "draw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копировать изображение или текст, обводя линии на прозрачном листе, наложенном сверху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срисовывать",
                                "target_lang_sense_clarification": "копировать изображение"
                            },
                            {
                                "target_lang_word": "обводить",
                                "target_lang_sense_clarification": "проводить линии поверх контура"
                            },
                            {
                                "target_lang_word": "калькировать",
                                "target_lang_sense_clarification": "копировать через кальку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe2efbb2-acbd-3a74-9482-9b1435365a06",
                    "sense_definition": "To follow the execution of a computer program step by step to find errors.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The developer had to <w>trace</w> the code to understand why it was crashing.",
                            "target_lang_translations": {
                                "ru": "Разработчику пришлось <w>отследить</w> код, чтобы понять, почему он давал сбой."
                            }
                        },
                        {
                            "text": "We need to <w>trace</w> the transaction logs to see where the error occurred.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>протрассировать</w> журналы транзакций, чтобы увидеть, где произошла ошибка."
                            }
                        }
                    ],
                    "synonyms": [
                        "debug"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пошагово отслеживать выполнение компьютерной программы для поиска ошибок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отслеживать",
                                "target_lang_sense_clarification": "мониторить выполнение кода"
                            },
                            {
                                "target_lang_word": "трассировать",
                                "target_lang_sense_clarification": "технический термин в программировании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "027ea40c-fbba-3491-9030-77ea9c5dbf15",
                    "sense_definition": "A mark, object, or sign left behind by someone or something that shows they were there.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_residue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thieves vanished without a <w>trace</w> before the police arrived.",
                            "target_lang_translations": {
                                "ru": "Воры исчезли без <w>следа</w> до прибытия полиции."
                            }
                        },
                        {
                            "text": "We found <w>traces</w> of a campfire near the river.",
                            "target_lang_translations": {
                                "ru": "Мы нашли <w>следы</w> костра у реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "sign",
                        "mark",
                        "vestige",
                        "evidence",
                        "track"
                    ],
                    "common_phrases": [
                        "without a trace",
                        "leave no trace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отметка, предмет или знак, оставленный кем-то или чем-то, указывающий на их присутствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "след",
                                "target_lang_sense_clarification": "отпечаток или признак присутствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e061f7e4-1d2f-34b1-8567-e32339cb794e",
                    "sense_definition": "A very small amount of a substance or quality.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_residue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water was found to contain <w>traces</w> of dangerous chemicals.",
                            "target_lang_translations": {
                                "ru": "В воде были обнаружены <w>следы</w> опасных химикатов."
                            }
                        },
                        {
                            "text": "There was a <w>trace</w> of sadness in her voice when she spoke about her home.",
                            "target_lang_translations": {
                                "ru": "В ее голосе звучала <w>тень</w> грусти, когда она говорила о доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "hint",
                        "touch",
                        "bit",
                        "suggestion",
                        "speck",
                        "modicum"
                    ],
                    "common_phrases": [
                        "trace of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень малое количество вещества или качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "след",
                                "target_lang_sense_clarification": "малое количество вещества"
                            },
                            {
                                "target_lang_word": "примесь",
                                "target_lang_sense_clarification": "постороннее вещество"
                            },
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "сохранившаяся малая часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cc8ec5e-8a08-3673-92e0-aa0eeb11e3d6",
                    "sense_definition": "A search or investigation to locate something missing or identify a source.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police put a <w>trace</w> on the suspect's phone calls.",
                            "target_lang_translations": {
                                "ru": "Полиция установила <w>отслеживание</w> телефонных звонков подозреваемого."
                            }
                        },
                        {
                            "text": "We initiated a <w>trace</w> on the lost package to see where it stopped moving.",
                            "target_lang_translations": {
                                "ru": "Мы инициировали <w>розыск</w> потерянной посылки, чтобы узнать, где она остановилась."
                            }
                        }
                    ],
                    "synonyms": [
                        "search",
                        "investigation"
                    ],
                    "common_phrases": [
                        "put a trace on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поиск или расследование с целью обнаружения чего-то пропавшего или установления источника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отслеживание",
                                "target_lang_sense_clarification": "процесс слежения или поиска источника"
                            },
                            {
                                "target_lang_word": "розыск",
                                "target_lang_sense_clarification": "официальный поиск"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d963ac4f-440a-3b51-ab7c-7fbeb7bbe66c",
                    "sense_definition": "One of the two straps or chains connecting a horse's harness to the vehicle it pulls.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horse kicked over the <w>traces</w> and refused to move the cart.",
                            "target_lang_translations": {
                                "ru": "Лошадь перешагнула через <w>постромки</w> и отказалась везти телегу."
                            }
                        },
                        {
                            "text": "The leather <w>traces</w> were worn out from years of heavy pulling.",
                            "target_lang_translations": {
                                "ru": "Кожаные <w>постромки</w> износились от многолетней тяжелой тяги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kick over the traces"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "equestrian"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из двух ремней или цепей, соединяющих упряжь лошади с повозкой, которую она тянет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постромка",
                                "target_lang_sense_clarification": "часть конской упряжи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1944e214-8558-3bcb-8bc6-a52b581c21e4",
                    "sense_definition": "Existing in extremely small amounts.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_residue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The laboratory found <w>trace</w> amounts of poison in the sample.",
                            "target_lang_translations": {
                                "ru": "Лаборатория обнаружила <w>следовые</w> количества яда в образце."
                            }
                        },
                        {
                            "text": "Human bodies require certain <w>trace</w> elements to function correctly.",
                            "target_lang_translations": {
                                "ru": "Человеческому организму требуются определенные <w>микро</w>элементы для правильного функционирования."
                            }
                        }
                    ],
                    "synonyms": [
                        "minimal",
                        "negligible",
                        "minute"
                    ],
                    "antonyms": [
                        "significant",
                        "substantial"
                    ],
                    "common_phrases": [
                        "trace element",
                        "trace amount"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий в чрезвычайно малых количествах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовой",
                                "target_lang_sense_clarification": "относящийся к малым количествам"
                            },
                            {
                                "target_lang_word": "микро-",
                                "target_lang_sense_clarification": "приставка, обозначающая малый размер (в сложных словах)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4b848d53-8874-3f17-ad07-92e9103408fa",
                    "sense_definition": "A short form of the female given name Tracy.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My friend <w>Trace</w> is coming over for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Трейс</w> придет сегодня на ужин."
                            }
                        },
                        {
                            "text": "She prefers to be called <w>Trace</w> instead of her full name.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает, чтобы ее называли <w>Трейс</w>, а не полным именем."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткая форма женского имени Трейси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Трейс",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "478a7d79-525f-3066-b22c-2c3a17b45df5",
                    "sense_definition": "Present participle and gerund of <w>rock</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sat by the window, gently <w>rocking</w> the baby in her arms.",
                            "target_lang_translations": {
                                "ru": "Она сидела у окна, нежно <w>укачивая</w> ребенка на руках."
                            }
                        },
                        {
                            "text": "The boat was <w>rocking</w> violently in the stormy sea.",
                            "target_lang_translations": {
                                "ru": "Лодка сильно <w>раскачивалась</w> в штормовом море."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rocking the boat",
                        "keep rocking"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }

{
    "word_family": [
        "opgave"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c421b98a-8b1e-3ddd-8193-e4702ec47215",
                    "sense_definition": "Stoppen met proberen iets te bereiken, te doen of vol te houden; zich gewonnen geven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stoppen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na drie uur zoeken wilden we de hoop niet <w>opgeven</w>.",
                            "target_lang_translations": {
                                "en": "After three hours of searching, we didn't want to <w>give up</w> hope."
                            }
                        },
                        {
                            "text": "De wielrenner moest de wedstrijd <w>opgeven</w> vanwege een lekke band.",
                            "target_lang_translations": {
                                "en": "The cyclist had to <w>quit</w> the race because of a flat tire."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoppen",
                        "staken",
                        "capituleren"
                    ],
                    "antonyms": [
                        "volhouden",
                        "doorzetten"
                    ],
                    "common_phrases": [
                        "de moed opgeven",
                        "de strijd opgeven",
                        "nooit opgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop trying to do or achieve something; to admit defeat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give up",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "quit",
                                "target_lang_sense_clarification": "stop an activity or competition"
                            },
                            {
                                "target_lang_word": "surrender",
                                "target_lang_sense_clarification": "admit defeat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f3f5fe5-1a7b-3c70-823b-aa20d2a66813",
                    "sense_definition": "Zich aanmelden voor een activiteit of informatie officieel doorgeven (bijvoorbeeld aan een instantie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "melden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet al je inkomsten bij de belastingdienst <w>opgeven</w>.",
                            "target_lang_translations": {
                                "en": "You must <w>declare</w> all your income to the tax authorities."
                            }
                        },
                        {
                            "text": "Mijn broer heeft zich gisteren <w>opgegeven</w> voor de voetbalclub.",
                            "target_lang_translations": {
                                "en": "My brother <w>signed up</w> for the soccer club yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanmelden",
                        "doorgeven",
                        "vermelden"
                    ],
                    "antonyms": [
                        "verzwijgen",
                        "afmelden"
                    ],
                    "common_phrases": [
                        "zich opgeven voor",
                        "belasting opgeven",
                        "een wijziging opgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To register for an activity or to officially provide information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sign up",
                                "target_lang_sense_clarification": "register oneself"
                            },
                            {
                                "target_lang_word": "declare",
                                "target_lang_sense_clarification": "taxes or official details"
                            },
                            {
                                "target_lang_word": "report",
                                "target_lang_sense_clarification": "provide information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ee2f2dd-638f-3a62-887b-bbbe8d9faeff",
                    "sense_definition": "Een taak, opdracht of raadsel aan iemand geven om uit te voeren of op te lossen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "opdragen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar wiskunde gaat ons vandaag veel huiswerk <w>opgeven</w>.",
                            "target_lang_translations": {
                                "en": "The math teacher is going to <w>assign</w> us a lot of homework today."
                            }
                        },
                        {
                            "text": "Hij wist het antwoord niet toen ik hem een raadsel had <w>opgegeven</w>.",
                            "target_lang_translations": {
                                "en": "He didn't know the answer when I had <w>set</w> him a riddle."
                            }
                        }
                    ],
                    "synonyms": [
                        "opdragen"
                    ],
                    "common_phrases": [
                        "huiswerk opgeven",
                        "een raadsel opgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To give someone a task, assignment, or riddle to solve."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "assign",
                                "target_lang_sense_clarification": "homework or tasks"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "a riddle or task"
                            },
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e63f76b-0e15-3532-9f16-9d9baf3a1096",
                    "sense_definition": "Iets of iemand als verloren beschouwen en niet langer proberen te redden of te behouden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verliezen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De artsen hebben de ernstig zieke patiënt helaas moeten <w>opgeven</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, the doctors had to <w>give up on</w> the seriously ill patient."
                            }
                        },
                        {
                            "text": "Het leger moest de stad na een lange strijd <w>opgeven</w>.",
                            "target_lang_translations": {
                                "en": "The army had to <w>surrender</w> the city after a long battle."
                            }
                        }
                    ],
                    "synonyms": [
                        "prijsgeven",
                        "verliezen"
                    ],
                    "antonyms": [
                        "behouden",
                        "redden"
                    ],
                    "common_phrases": [
                        "een patiënt opgeven",
                        "grondgebied opgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To consider someone or something as lost and stop trying to save them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give up on",
                                "target_lang_sense_clarification": "lose hope for someone"
                            },
                            {
                                "target_lang_word": "surrender",
                                "target_lang_sense_clarification": "territory or control"
                            },
                            {
                                "target_lang_word": "abandon",
                                "target_lang_sense_clarification": "leave behind"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0b5166a-d6aa-308e-96d1-2df5ab429d67",
                    "sense_definition": "Zeer positief en met veel lof over iets of iemand spreken (vaak met enige overdrijving).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opscheppen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kan altijd erg hoog <w>opgeven</w> over zijn eigen kookkunsten.",
                            "target_lang_translations": {
                                "en": "He can always <w>boast</w> greatly about his own cooking skills."
                            }
                        },
                        {
                            "text": "Tijdens het sollicitatiegesprek gaf ze hoog <w>op</w> over haar ervaring.",
                            "target_lang_translations": {
                                "en": "During the job interview, she <w>spoke</w> highly of her experience."
                            }
                        }
                    ],
                    "synonyms": [
                        "opscheppen",
                        "prijzen",
                        "roemen"
                    ],
                    "antonyms": [
                        "kraken"
                    ],
                    "common_phrases": [
                        "hoog opgeven van",
                        "hoog opgeven over"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak very positively or boastfully about something or someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speak highly",
                                "target_lang_sense_clarification": "praise"
                            },
                            {
                                "target_lang_word": "boast",
                                "target_lang_sense_clarification": "brag"
                            },
                            {
                                "target_lang_word": "rave",
                                "target_lang_sense_clarification": "talk enthusiastically"
                            }
                        ]
                    }
                }
            ]
        }
    ]

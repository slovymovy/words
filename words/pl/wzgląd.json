            "senses": [
                {
                    "sense_id": "2627db73-e2a3-3d64-8f10-1fd9d1f6ccf1",
                    "sense_definition": "Przyczyna, powód lub motyw jakiegoś działania; okoliczność wpływająca na decyzję.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reason_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrezygnował z udziału w zawodach ze <względów> zdrowotnych.",
                            "target_lang_translations": {
                                "ru": "Он отказался от участия в соревнованиях по <соображениям> здоровья."
                            }
                        },
                        {
                            "text": "Ze <względu> na złe warunki pogodowe, lot został odwołany.",
                            "target_lang_translations": {
                                "ru": "<Из-за> плохих погодных условий рейс был отменён."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyczyna",
                        "pobudka",
                        "okoliczność"
                    ],
                    "common_phrases": [
                        "ze względu na",
                        "z tego względu",
                        "ze względów bezpieczeństwa",
                        "bez względu na"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причина, повод или мотив какого-либо действия; обстоятельство, влияющее на решение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соображение",
                                "target_lang_sense_clarification": "мотив, основание (часто во мн. ч.)"
                            },
                            {
                                "target_lang_word": "причина",
                                "target_lang_sense_clarification": "основание для действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b526415-ceb8-33b2-8c12-8f616869a8ba",
                    "sense_definition": "Punkt widzenia, określona cecha lub strona omawianego zagadnienia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reason_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod tym <względem> twój plan wydaje się o wiele lepszy.",
                            "target_lang_translations": {
                                "ru": "В этом <отношении> твой план кажется намного лучше."
                            }
                        },
                        {
                            "text": "Nowy pracownik jest doskonały pod każdym <względem>.",
                            "target_lang_translations": {
                                "ru": "Новый сотрудник идеален во всех <отношениях>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pod tym względem",
                        "pod każdym względem",
                        "w tym względzie",
                        "pod wieloma względami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка зрения, определенная черта или сторона обсуждаемого вопроса."

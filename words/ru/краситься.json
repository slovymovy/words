    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a2a77bae-38b9-3854-99d8-87f73264359d",
                    "sense_definition": "Наносить косметику на лицо или изменять цвет волос с помощью краски.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя сестра любит ярко <w>краситься</w> перед вечеринкой.",
                            "target_lang_translations": {
                                "en": "My sister likes to <w>put on bright makeup</w> before a party."
                            }
                        },
                        {
                            "text": "Она решила <w>краситься</w> в рыжий цвет, чтобы изменить имидж.",
                            "target_lang_translations": {
                                "en": "She decided to <w>dye her hair</w> red to change her image."
                            }
                        }
                    ],
                    "synonyms": [
                        "гримироваться"
                    ],
                    "antonyms": [
                        "умываться"
                    ],
                    "common_phrases": [
                        "краситься перед зеркалом",
                        "краситься в блондинку",
                        "ярко краситься",
                        "начать краситься"
                    ],
                    "target_lang_definitions": {
                        "en": "To apply cosmetics to one's face or change hair color using dye."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put on makeup",
                                "target_lang_sense_clarification": "apply cosmetics"
                            },
                            {
                                "target_lang_word": "dye one's hair",
                                "target_lang_sense_clarification": "change hair color"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f869cb7-1814-3e88-b0b0-c2b6f3a69d5e",
                    "sense_definition": "Оставлять цветные следы на других предметах или коже при прикосновении (обычно о ткани или свежеокрашенных поверхностях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эти новые джинсы сильно <w>красятся</w> при стирке.",
                            "target_lang_translations": {
                                "en": "The color of these new jeans really <w>runs</w> when washed."
                            }
                        },
                        {
                            "text": "Не садись на эту скамейку, она ещё <w>красится</w>.",
                            "target_lang_translations": {
                                "en": "Don't sit on this bench, the paint still <w>rubs off</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "пачкаться",
                        "линять"
                    ],
                    "common_phrases": [
                        "ткань красится",
                        "одежда красится",
                        "сильно краситься"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave colored marks on other objects or skin upon contact (usually regarding fabric or freshly painted surfaces)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "when color bleeds from fabric"
                            },
                            {
                                "target_lang_word": "rub off",
                                "target_lang_sense_clarification": "transfer color onto something else"
                            },
                            {
                                "target_lang_word": "stain",
                                "target_lang_sense_clarification": "leave a mark"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f10ca4a6-a53d-3462-9552-8887582e2d09",
                    "sense_definition": "Покрываться краской; быть объектом, который красят (страдательный залог к глаголу «красить»).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Фасад этого старого здания <w>красится</w> каждые пять лет.",
                            "target_lang_translations": {
                                "en": "The facade of this old building <w>is painted</w> every five years."
                            }
                        },
                        {
                            "text": "Натуральная шерсть <w>красится</w> намного лучше, чем синтетика.",
                            "target_lang_translations": {
                                "en": "Natural wool <w>dyes</w> much better than synthetics."
                            }
                        }
                    ],
                    "synonyms": [
                        "окрашиваться"
                    ],
                    "common_phrases": [
                        "легко краситься",
                        "плохо краситься",
                        "краситься кистью"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be covered with paint or dye; to undergo the process of painting (passive or reflexive use)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be painted",
                                "target_lang_sense_clarification": "passive voice for surfaces"
                            },
                            {
                                "target_lang_word": "dye",
                                "target_lang_sense_clarification": "absorb color (of materials)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

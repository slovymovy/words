            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3ff57aa6-9a03-3168-8180-2ddfa9317241",
                    "sense_definition": "Iemand of iets achternagaan; in dezelfde richting bewegen achter iemand of iets aan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trouwe hond <volgde> zijn baasje overal naartoe.",
                            "target_lang_translations": {
                                "en": "The faithful dog <w>followed</w> its owner everywhere.",
                                "ru": "Верный пес повсюду <w>следовал</w> за своим хозяином."
                            }
                        },
                        {
                            "text": "Zij <volgden> hem voorzichtig naar zijn huis.",
                            "target_lang_translations": {
                                "en": "They carefully <w>followed</w> him to his house.",
                                "ru": "Они осторожно <w>следовали</w> за ним до его дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "achternagaan",
                        "achternalopen"
                    ],
                    "antonyms": [
                        "voorgaan",
                        "leiden"
                    ],
                    "common_phrases": [
                        "iemand op de voet volgen",
                        "het spoor volgen",
                        "de weg volgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go after someone or something; to move in the same direction behind someone or something.",
                        "ru": "Идти или следовать за кем-то или чем-то; двигаться в том же направлении позади кого-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "physical movement behind someone"
                            },
                            {
                                "target_lang_word": "go after",
                                "target_lang_sense_clarification": "to pursue or chase"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "двигаться за кем-то"
                            },
                            {
                                "target_lang_word": "идти за",
                                "target_lang_sense_clarification": "физическое перемещение позади кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53270c12-5ebc-3446-aba6-1c2bc03670cd",
                    "sense_definition": "Na iets of iemand anders komen in tijd, rang of volgorde.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sequence_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de zware storm <volgde> er een periode van stilte.",
                            "target_lang_translations": {
                                "en": "After the heavy storm, a period of silence <w>followed</w>.",
                                "ru": "После сильного шторма <w>последовал</w> период тишины."
                            }
                        },
                        {
                            "text": "Er waren nog drie grote golven <gevolgd> na de eerste.",
                            "target_lang_translations": {
                                "en": "Three more large waves had <w>followed</w> after the first one.",
                                "ru": "За первой <w>последовали</w> еще три большие волны."
                            }
                        }
                    ],
                    "synonyms": [
                        "nakomen"
                    ],
                    "antonyms": [
                        "voorafgaan"
                    ],
                    "common_phrases": [
                        "hierna volgt",
                        "elkaar opvolgen",
                        "wordt vervolgd"
                    ],
                    "target_lang_definitions": {
                        "en": "To come after something or someone else in time, rank, or order.",
                        "ru": "Происходить или идти после чего-то или кого-то во времени, ранге или порядке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "to occur subsequently"
                            },
                            {
                                "target_lang_word": "come after",
                                "target_lang_sense_clarification": "sequence in time or order"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "происходить после чего-то"
                            },
                            {
                                "target_lang_word": "наступать",
                                "target_lang_sense_clarification": "о событиях или времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de954139-552e-3d05-b36d-c20c1573541c",
                    "sense_definition": "De berichten van een persoon of organisatie op sociale media in de gaten houden door je te abonneren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "digital_1",
                    "name_type": "no",

{
    "word_family": [
        "informatie",
        "informatief",
        "informant",
        "informateur"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f5c4f844-ee02-3bb2-bb0b-11f5438440cf",
                    "sense_definition": "Iemand kennis of inlichtingen geven over bepaalde feiten of gebeurtenissen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "giving_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente zal de bewoners tijdig <w>informeren</w> over de wegwerkzaamheden.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет своевременно <w>проинформирует</w> жителей о дорожных работах.",
                                "en": "The municipality will <w>inform</w> the residents about the roadworks in time."
                            }
                        },
                        {
                            "text": "Wij hebben alle klanten per e-mail <w>geïnformeerd</w> over de nieuwe openingstijden.",
                            "target_lang_translations": {
                                "ru": "Мы <w>проинформировали</w> всех клиентов о новых часах работы по электронной почте.",
                                "en": "We have <w>informed</w> all customers by email about the new opening hours."
                            }
                        }
                    ],
                    "synonyms": [
                        "inlichten",
                        "mededelen",
                        "verwittigen",
                        "op de hoogte stellen"
                    ],
                    "antonyms": [
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "iemand informeren over",
                        "het publiek informeren",
                        "volledig informeren",
                        "tijdig informeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщать кому-либо сведения или информацию об определенных фактах или событиях.",
                        "en": "To give someone knowledge or information about certain facts or events."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "информировать",
                                "target_lang_sense_clarification": "нейтральное, официальное"
                            },
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "передавать информацию"
                            },
                            {
                                "target_lang_word": "уведомлять",
                                "target_lang_sense_clarification": "официально извещать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "inform",
                                "target_lang_sense_clarification": "to communicate knowledge to someone"
                            },
                            {
                                "target_lang_word": "notify",
                                "target_lang_sense_clarification": "to give official notice"
                            },
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "general term for communicating information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42c75bf5-f668-3937-aba4-a87fcd79242c",
                    "sense_definition": "Vragen naar gegevens of inlichtingen om iets te weten te komen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "asking_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag even <w>informeren</w> naar de prijs van dit product.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>узнать</w> цену этого продукта.",
                                "en": "I would like to <w>inquire</w> about the price of this product."
                            }
                        },
                        {
                            "text": "Toen ik bij de receptie naar de vertrektijden <w>informeerde</w>, kreeg ik direct een folder.",
                            "target_lang_translations": {
                                "ru": "Когда я <w>спросил</w> на стойке регистрации о времени отправления, мне сразу дали брошюру.",
                                "en": "When I <w>inquired</w> about the departure times at the reception, I immediately received a brochure."
                            }
                        }
                    ],
                    "synonyms": [
                        "navragen",
                        "vragen"
                    ],
                    "common_phrases": [
                        "informeren naar",
                        "even informeren",
                        "telefonisch informeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запрашивать данные или сведения, чтобы узнать что-либо.",
                        "en": "To ask for data or information to find out something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узнавать",
                                "target_lang_sense_clarification": "выяснять информацию"
                            },
                            {
                                "target_lang_word": "осведомляться",
                                "target_lang_sense_clarification": "вежливо спрашивать"
                            },
                            {
                                "target_lang_word": "справляться",
                                "target_lang_sense_clarification": "наводить справки"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "inquire",
                                "target_lang_sense_clarification": "to ask for information (formal)"
                            },
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to request information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7071cbf-af2a-36c7-b527-4b8393fbf27a",
                    "sense_definition": "Zichzelf van kennis voorzien; zorgen dat men op de hoogte is van iets.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "giving_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je goed <w>informeren</w> voordat je zo'n belangrijke beslissing neemt.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно хорошо <w>собрать информацию</w>, прежде чем принимать такое важное решение.",
                                "en": "You need to <w>inform yourself</w> well before making such an important decision."
                            }
                        },
                        {
                            "text": "Hij had zich onvoldoende <w>geïnformeerd</w> over de risico's van de belegging.",
                            "target_lang_translations": {
                                "ru": "Он недостаточно <w>навел справки</w> о рисках инвестиций.",
                                "en": "He had insufficiently <w>informed himself</w> about the risks of the investment."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich inlezen"
                    ],
                    "common_phrases": [
                        "zich laten informeren",
                        "zich goed informeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать знания; обеспечивать свою осведомленность о чем-либо.",
                        "en": "To provide oneself with knowledge; to ensure one is aware of something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать информацию",
                                "target_lang_sense_clarification": "искать сведения"
                            },
                            {
                                "target_lang_word": "наводить справки",
                                "target_lang_sense_clarification": "выяснять детали"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "inform oneself",
                                "target_lang_sense_clarification": "to gather information for oneself"
                            },
                            {
                                "target_lang_word": "get informed",
                                "target_lang_sense_clarification": "to become knowledgeable about a topic"
                            },
                            {
                                "target_lang_word": "read up",
                                "target_lang_sense_clarification": "to acquire information by reading"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
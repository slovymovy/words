                        "bedaard"
                    ],
                    "antonyms": [
                        "wild",
                        "driftig"
                    ],
                    "common_phrases": [
                        "rustig aan doen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a calm or unhurried manner.",
                        "ru": "Спокойным образом; не торопясь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "calmly",
                                "target_lang_sense_clarification": "in a peaceful and composed way"
                            },
                            {
                                "target_lang_word": "quietly",
                                "target_lang_sense_clarification": "without making noise or drawing attention"
                            },
                            {
                                "target_lang_word": "gently",
                                "target_lang_sense_clarification": "carefully and without force"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спокойно",
                                "target_lang_sense_clarification": "без волнения"
                            },
                            {
                                "target_lang_word": "не спеша",
                                "target_lang_sense_clarification": "медленно, размеренно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08d6bbed-6f52-30f0-a92c-655ec43040c5",
                    "sense_definition": "Goed of prima; zonder bijzonderheden (als antwoord op de vraag hoe het gaat).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "greeting_response",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"Hoe gaat het met je?\" \"Gewoon <w>rustig</w>.\"",
                            "target_lang_translations": {
                                "en": "\"How are you doing?\" \"Just <w>quiet</w>.\"",
                                "ru": "\"Как дела?\" \"Всё <w>спокойно</w>.\""
                            }
                        },
                        {
                            "text": "Alles is <w>rustig</w> hier, geen nieuws.",
                            "target_lang_translations": {
                                "en": "Everything is <w>quiet</w> here, no news.",
                                "ru": "Здесь всё <w>спокойно</w>, новостей нет."

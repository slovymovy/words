                {
                    "sense_id": "eff0b84f-65b3-31ea-8e35-6a66cccaf965",
                    "sense_definition": "Osoba, która nie potrafi mówić, zazwyczaj z powodu wady wrodzonej lub choroby.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "disability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <niemowa> porozumiewa się z otoczeniem za pomocą języka migowego.",
                            "target_lang_translations": {
                                "ru": "Этот <немой> общается с окружающими с помощью языка жестов."
                            }
                        },
                        {
                            "text": "Od urodzenia jest <niemową>, ale świetnie radzi sobie w pisaniu.",
                            "target_lang_translations": {
                                "ru": "Он <немой> с рождения, но отлично справляется с письмом."
                            }
                        }
                    ],
                    "synonyms": [
                        "niemy",
                        "niema"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который не может говорить, обычно из-за врожденного дефекта или болезни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "немой",
                                "target_lang_sense_clarification": "человек, лишенный способности говорить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "944051c2-a2f8-3bbc-b02b-bba4621c30a5",
                    "sense_definition": "Osoba, która jest bardzo cicha, małomówna lub w danej chwili nic nie mówi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie siedź w kącie jak <niemowa>, tylko przyłącz się do rozmowy.",
                            "target_lang_translations": {
                                "ru": "Не сиди в углу как <немой>, а присоединяйся к разговору."
                            }
                        },
                        {
                            "text": "W towarzystwie obcych ludzi on często zamienia się w <niemowę>.",
                            "target_lang_translations": {
                                "ru": "В компании незнакомых людей он часто превращается в <немого>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "siedzieć jak niemowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который очень тих, молчалив или в данный момент ничего не говорит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молчун",
                                "target_lang_sense_clarification": "неразговорчивый человек"
                            },
                            {
                                "target_lang_word": "немой",
                                "target_lang_sense_clarification": "в переносном смысле: молчаливый"
                            }
                        ]
                    }
                }
            ]

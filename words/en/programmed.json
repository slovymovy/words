                    "sense_id": "787db4b9-e1c3-3e21-a96f-5ddde57aae95",
                    "sense_definition": "Simple past and past participle of <w>program</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>programmed</w> the alarm to wake her up at six.",
                            "target_lang_translations": {
                                "ru": "Она <w>запрограммировала</w> будильник, чтобы он разбудил её в шесть."
                            }
                        },
                        {
                            "text": "The software is <w>programmed</w> to detect viruses automatically.",
                            "target_lang_translations": {
                                "ru": "Это программное обеспечение <w>запрограммировано</w> на автоматическое обнаружение вирусов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "programmed to",
                        "pre-programmed"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",

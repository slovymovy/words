    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72fe78c3-5bad-34ab-80ae-c6e72091a36b",
                    "sense_definition": "Настраивать кого-либо враждебно против кого-либо или чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "incite_hostility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит намеренно <w>ополчать</w> коллектив против нового сотрудника.",
                            "target_lang_translations": {
                                "en": "You shouldn't intentionally <w>turn</w> the team against the new employee."
                            }
                        },
                        {
                            "text": "Своим высокомерием он начал <w>ополчать</w> против себя даже близких друзей.",
                            "target_lang_translations": {
                                "en": "With his arrogance, he began to <w>turn</w> even close friends against himself."
                            }
                        }
                    ],
                    "synonyms": [
                        "настраивать",
                        "восстанавливать"
                    ],
                    "antonyms": [
                        "мирить",
                        "примирять"
                    ],
                    "common_phrases": [
                        "ополчать против себя",
                        "ополчать общественное мнение"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "указано в исходных данных как 'разг.'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To turn someone hostile against someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn against",
                                "target_lang_sense_clarification": "to make someone hostile towards another"
                            },
                            {
                                "target_lang_word": "set against",
                                "target_lang_sense_clarification": "to cause conflict or hostility between people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43b9f69f-eb13-356a-8838-5f656e9a9757",
                    "sense_definition": "Вооружать людей для войны или собирать их в ополчение.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "arm_militia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Князь отдал приказ <w>ополчать</w> крестьян для защиты границ княжества.",
                            "target_lang_translations": {
                                "en": "The prince gave the order to <w>arm</w> the peasants to protect the principality's borders."
                            }
                        },
                        {
                            "text": "В случае опасности воеводы начинали спешно <w>ополчать</w> местных жителей.",
                            "target_lang_translations": {
                                "en": "In case of danger, the voivodes began to hastily <w>mobilize</w> the local residents."
                            }
                        }
                    ],
                    "synonyms": [
                        "вооружать",
                        "снаряжать"
                    ],
                    "antonyms": [
                        "разоружать"
                    ],
                    "common_phrases": [
                        "ополчать народ",
                        "ополчать войско"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "указано в исходных данных как 'устар.'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To arm people for war or gather them into a militia."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arm",
                                "target_lang_sense_clarification": "to equip with weapons for war"
                            },
                            {
                                "target_lang_word": "mobilize",
                                "target_lang_sense_clarification": "to gather people for military service"
                            }
                        ]
                    }
                }
            ]
        }
    ]

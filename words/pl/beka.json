            "senses": [
                {
                    "sense_id": "382479bb-914b-3fa9-a146-350ec78852fb",
                    "sense_definition": "Dawna osada rybacka oraz rezerwat przyrody w Polsce, w województwie pomorskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Rezerwat <Beka> jest domem dla wielu rzadkich ptaków.",
                            "target_lang_translations": {
                                "ru": "Заповедник <w>Бека</w> является домом для многих редких птиц."
                            }
                        },
                        {
                            "text": "Dawna osada <Beka> zniknęła pod wodami zatoki.",
                            "target_lang_translations": {
                                "ru": "Бывшее поселение <w>Бека</w> исчезло под водами залива."
                            }

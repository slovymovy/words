                {
                    "sense_id": "e5b85461-8425-3bd3-8511-dc010ae55627",
                    "sense_definition": "Tweede en derde persoon enkelvoud tegenwoordige tijd van <w>stellen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>stelt</w> hoge eisen aan de kwaliteit van het werk.",
                            "target_lang_translations": {
                                "en": "He <w>sets</w> high demands for the quality of the work.",
                                "ru": "Он <w>предъявляет</w> высокие требования к качеству работы."
                            }
                        },
                        {
                            "text": "Als je je zo <w>stelt</w>, bereik je helemaal niets.",
                            "target_lang_translations": {
                                "en": "If you <w>adopt</w> such an attitude, you won't achieve anything at all.",
                                "ru": "Если ты так себя <w>поставишь</w>, то ничего не добьешься."
                            }
                        },
                        {
                            "text": "Dat <w>stelt</w> in vergelijking met vroeger niet veel voor.",
                            "target_lang_translations": {
                                "en": "That doesn't <w>amount</w> to much compared to the past.",
                                "ru": "По сравнению с прошлым это мало что из себя <w>представляет</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stelt voor",
                        "stelt vast",
                        "stelt teleur",
                        "stelt gerust",
                        "stelt zich aan"
                    ],
                    "target_lang_definitions": {
                        "en": "Second and third person singular present tense of 'stellen'.",
                        "ru": "Форма второго и третьего лица единственного числа настоящего времени глагола <w>stellen</w>."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "d6605886-d72d-31d0-a2ce-0b0b194728d8",
                    "sense_definition": "Выражение несогласия с чем-либо; отсутствие возражений (в отрицании).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если ты не <w>против</w>, мы пойдём в кино.",
                            "target_lang_translations": {
                                "en": "If you don't <w>mind</w>, we will go to the cinema."
                            }
                        },
                        {
                            "text": "Все проголосовали за, и только он был <w>против</w>.",
                            "target_lang_translations": {
                                "en": "Everyone voted in favor, and only he was <w>against</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "за"
                    ],
                    "common_phrases": [
                        "я не против",
                        "никто не против",
                        "кто против"
                    ],
                    "target_lang_definitions": {
                        "en": "Expression of disagreement with something; absence of objections (in negation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "expressing disagreement"
                            },
                            {
                                "target_lang_word": "opposed to",
                                "target_lang_sense_clarification": "in opposition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44c3a970-9910-33a0-9430-7f39571147d1",
                    "sense_definition": "Предлог, указывающий на борьбу с кем-либо или чем-либо, противодействие или защиту.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны объединиться <w>против</w> общего врага.",
                            "target_lang_translations": {
                                "en": "We must unite <w>against</w> a common enemy."
                            }
                        },
                        {
                            "text": "Это лучшее лекарство <w>против</w> гриппа и простуды.",
                            "target_lang_translations": {
                                "en": "This is the best medicine <w>against</w> the flu and cold."
                            }
                        }
                    ],
                    "synonyms": [
                        "с"
                    ],
                    "antonyms": [
                        "за"
                    ],
                    "common_phrases": [
                        "средство против гриппа",
                        "играть против правил",
                        "бороться против врага"
                    ],
                    "target_lang_definitions": {
                        "en": "Preposition indicating a struggle against someone or something, opposition, or protection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "in opposition or defense"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f76c8159-a56d-3b4d-84ee-5ae3992f76ba",
                    "sense_definition": "Предлог, указывающий на нахождение прямо перед кем-либо или чем-либо, на противоположной стороне.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "loc_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш дом стоит прямо <w>против</w> торгового центра.",
                            "target_lang_translations": {
                                "en": "Our house stands directly <w>opposite</w> the shopping mall."
                            }
                        },
                        {
                            "text": "Она села <w>против</w> меня и улыбнулась.",
                            "target_lang_translations": {
                                "en": "She sat <w>opposite</w> me and smiled."
                            }
                        }
                    ],
                    "synonyms": [
                        "напротив",
                        "лицом к лицу"
                    ],
                    "antonyms": [
                        "рядом"
                    ],
                    "common_phrases": [
                        "жить против школы",
                        "сидеть против окна",
                        "стоять против входа"
                    ],
                    "target_lang_definitions": {
                        "en": "Preposition indicating location directly in front of someone or something, on the opposite side."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opposite",
                                "target_lang_sense_clarification": "location"
                            },
                            {
                                "target_lang_word": "across from",
                                "target_lang_sense_clarification": "location"
                            },
                            {
                                "target_lang_word": "facing",
                                "target_lang_sense_clarification": "positioning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfcec6d9-ddc0-3ea1-b6f2-36539df9f47e",
                    "sense_definition": "Предлог, указывающий на движение навстречу чему-либо, в обратном направлении.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Трудно плыть на лодке <w>против</w> сильного течения.",
                            "target_lang_translations": {
                                "en": "It is difficult to sail a boat <w>against</w> a strong current."
                            }
                        },
                        {
                            "text": "Мы шли <w>против</w> ветра и очень устали.",
                            "target_lang_translations": {
                                "en": "We walked <w>against</w> the wind and got very tired."
                            }
                        }
                    ],
                    "synonyms": [
                        "навстречу"
                    ],
                    "antonyms": [
                        "по",
                        "вдоль"
                    ],
                    "common_phrases": [
                        "плыть против течения",
                        "идти против ветра",
                        "гладить против шерсти"
                    ],
                    "target_lang_definitions": {
                        "en": "Preposition indicating movement towards something, in the reverse direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "counter to direction or force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28683437-3c72-340b-a712-942a745b96e5",
                    "sense_definition": "Предлог, означающий действие вопреки, наперекор чему-либо (желанию, правилам, совести).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он поступил <w>против</w> своей совести.",
                            "target_lang_translations": {
                                "en": "He acted <w>against</w> his conscience."
                            }
                        },
                        {
                            "text": "Я ничего не имею <w>против</w> вашего плана.",
                            "target_lang_translations": {
                                "en": "I have nothing <w>against</w> your plan."
                            }
                        }
                    ],
                    "synonyms": [
                        "вопреки",
                        "наперекор"
                    ],
                    "antonyms": [
                        "согласно",
                        "в соответствии с"
                    ],
                    "common_phrases": [
                        "против воли",
                        "против совести",
                        "против закона"
                    ],
                    "target_lang_definitions": {
                        "en": "Preposition meaning action contrary to or in defiance of something (desire, rules, conscience)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "contrary to"
                            },
                            {
                                "target_lang_word": "contrary to",
                                "target_lang_sense_clarification": "in conflict with"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdd56d23-6a5f-3c96-a6d6-421bcf1e1010",
                    "sense_definition": "Предлог, используемый для сравнения или сопоставления с кем-либо или чем-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Урожай в этом году богат <w>против</w> прошлого года.",
                            "target_lang_translations": {
                                "en": "The harvest this year is rich <w>compared to</w> last year."
                            }
                        },
                        {
                            "text": "Этот дом казался маленьким <w>против</w> огромного дворца.",
                            "target_lang_translations": {
                                "en": "This house seemed small <w>against</w> the huge palace."
                            }
                        }
                    ],
                    "synonyms": [
                        "по сравнению с",
                        "супротив"
                    ],
                    "target_lang_definitions": {
                        "en": "Preposition used for comparison or juxtaposition with someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "compared to",
                                "target_lang_sense_clarification": "comparison"
                            },
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "in contrast with"
                            }
                        ]
                    }
                }
            ]
        },

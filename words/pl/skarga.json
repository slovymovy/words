                {
                    "sense_id": "0e8fcf2a-5889-30ae-8e56-6b547b607e09",
                    "sense_definition": "Wypowiedź lub pismo, w którym ktoś wyraża niezadowolenie, krytykę lub żal do kogoś z powodu jakiegoś zdarzenia lub sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klient złożył oficjalną <w>skargę</w> na nieuprzejme zachowanie sprzedawcy.",
                            "target_lang_translations": {
                                "ru": "Клиент подал официальную <w>жалобу</w> на грубое поведение продавца."
                            }
                        },
                        {
                            "text": "Dyrektor nie miał czasu, aby wysłuchać mojej <w>skargi</w> na warunki pracy.",
                            "target_lang_translations": {
                                "ru": "У директора не было времени выслушать мою <w>жалобу</w> на условия труда."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzekanie",
                        "pretensja",
                        "zażalenie",
                        "utyskiwanie"
                    ],
                    "antonyms": [
                        "pochwała"
                    ],
                    "common_phrases": [
                        "złożyć skargę",
                        "wnieść skargę",
                        "księga skarg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высказывание или письмо, в котором выражается недовольство, критика или сожаление по поводу какого-либо события или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалоба",
                                "target_lang_sense_clarification": "выражение недовольства"
                            },
                            {
                                "target_lang_word": "претензия",
                                "target_lang_sense_clarification": "требование или выражение неудовлетворенности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85631fef-9144-3f86-a311-9cbac4b5828e",
                    "sense_definition": "Oficjalne pismo procesowe lub urzędowe, służące do zgłoszenia naruszenia prawa, interesu lub zaniedbania organów władzy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Adwokat przygotował <w>skargę</w> kasacyjną do Sądu Najwyższego.",
                            "target_lang_translations": {
                                "ru": "Адвокат подготовил кассационную <w>жалобу</w> в Верховный суд."
                            }
                        },
                        {
                            "text": "Urząd gminy ma miesiąc na rozpatrzenie twojej <w>skargi</w> administracyjnej.",
                            "target_lang_translations": {
                                "ru": "У администрации гмины есть месяц на рассмотрение твоей административной <w>жалобы</w>."
                            }
                        }
                    ],
                    "synonyms": [

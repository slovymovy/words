                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "July is my favourite <w>month</w>.",
                            "target_lang_translations": {
                                "ru": "Июль — мой любимый <w>месяц</w>."
                            }
                        },
                        {
                            "text": "We have to pay the rent every <w>month</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны платить за аренду каждый <w>месяц</w>."
                            }
                        }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "36fdb166-018d-3080-8e7d-8a83d2a9d820",
                    "sense_definition": "Mający umiarkowanie niską temperaturę, dający uczucie chłodu; nie ciepły, ale też nie mroźny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczór był <w>chłodny</w>, więc musiałem założyć grubszą kurtkę.",
                            "target_lang_translations": {
                                "ru": "Вечер был <w>прохладным</w>, поэтому мне пришлось надеть куртку потеплее."
                            }
                        },
                        {
                            "text": "Łasice żyją w <w>chłodnym</w> klimacie, gdzie zimy bywają długie.",
                            "target_lang_translations": {
                                "ru": "Ласки живут в <w>прохладном</w> климате, где зимы бывают долгими."
                            }
                        }
                    ],
                    "synonyms": [
                        "zimny"
                    ],
                    "antonyms": [
                        "ciepły",
                        "gorący"
                    ],
                    "common_phrases": [
                        "chłodny wiatr",
                        "chłodny wieczór",
                        "chłodny napój",
                        "chłodny prysznic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий умеренно низкую температуру, дающий ощущение прохлады; не теплый, но и не морозный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прохладный",
                                "target_lang_sense_clarification": "умеренно холодный, свежий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b87c0b97-d5ea-3e17-9421-35aa858e394b",
                    "sense_definition": "O człowieku lub jego zachowaniu: pozbawiony serdeczności, życzliwości i emocji; zdystansowany.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>chłodne</w> spojrzenie sprawiło, że wszyscy w pokoju zamilkli.",
                            "target_lang_translations": {
                                "ru": "Его <w>холодный</w> взгляд заставил всех в комнате замолчать."
                            }
                        },
                        {
                            "text": "Mimo długiej znajomości, ich relacje pozostają oficjalne i dość <w>chłodne</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на долгое знакомство, их отношения остаются официальными и довольно <w>прохладными</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieczuły",
                        "obojętny",
                        "beznamiętny",
                        "zimny",
                        "bezemocjonalny"
                    ],
                    "antonyms": [
                        "ciepły",
                        "gorący"
                    ],
                    "common_phrases": [
                        "chłodne powitanie",
                        "chłodne spojrzenie",
                        "chłodna kalkulacja",
                        "chłodny stosunek"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке или его поведении: лишенный сердечности, доброжелательности и эмоций; отстраненный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холодный",
                                "target_lang_sense_clarification": "неприветливый, бесчувственный"
                            },
                            {
                                "target_lang_word": "прохладный",
                                "target_lang_sense_clarification": "равнодушный, сдержанный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "bd7b7d26-bcf7-35c9-a3a2-a379eaeadc62",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C2",

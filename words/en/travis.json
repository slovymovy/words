        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "38ac08f2-e486-3caf-8bac-365ef6a18cd3",
                    "sense_definition": "A male given name.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Travis</w> is coming to visit us next week.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Трэвис</w> приедет навестить нас на следующей неделе."
                            }
                        },
                        {
                            "text": "We asked <w>Travis</w> to help us move the heavy furniture.",
                            "target_lang_translations": {
                                "ru": "Мы попросили <w>Трэвиса</w> помочь нам передвинуть тяжелую мебель."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское личное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Трэвис",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78e9e121-523e-3103-bf2f-37a8c3e8f442",
                    "sense_definition": "A surname originating from an occupation.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Travis</w> has been our neighbor for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Трэвис</w> был нашим соседом более двадцати лет."
                            }
                        },
                        {
                            "text": "The <w>Travis</w> family owns the bakery on the corner of the street.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Трэвис</w> владеет пекарней на углу улицы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от названия профессии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Трэвис",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d87a200b-f600-3898-8357-0aa219f8b71a",
                    "sense_definition": "A neighborhood of Staten Island, New York City.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "place_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They bought a house in the <w>Travis</w> neighborhood of Staten Island.",
                            "target_lang_translations": {
                                "ru": "Они купили дом в районе <w>Трэвис</w> на Статен-Айленде."
                            }
                        },
                        {
                            "text": "The bus route goes through <w>Travis</w> before reaching the ferry terminal.",
                            "target_lang_translations": {
                                "ru": "Автобусный маршрут проходит через <w>Трэвис</w> перед тем, как достичь паромного терминала."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Район Статен-Айленда, Нью-Йорк."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Трэвис",
                                "target_lang_sense_clarification": "район Нью-Йорка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1665282-2821-3f7f-b08f-2fb94b63331e",
                    "sense_definition": "A community in Falls County, Texas.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "He grew up in the small community of <w>Travis</w> in Texas.",
                            "target_lang_translations": {
                                "ru": "Он вырос в небольшом поселении <w>Трэвис</w> в Техасе."
                            }
                        },
                        {
                            "text": "The road map shows <w>Travis</w> located in the eastern part of the county.",
                            "target_lang_translations": {
                                "ru": "На дорожной карте <w>Трэвис</w> расположен в восточной части округа."

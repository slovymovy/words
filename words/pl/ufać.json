            "senses": [
                {
                    "sense_id": "231b14e1-fd9e-3d3b-b53f-70f2af94af7d",
                    "sense_definition": "Być przekonanym o czyjejś uczciwości, szczerości i dobrych intencjach; polegać na kimś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "trust_belief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice zawsze mnie wspierają, dlatego bardzo im <ufam>.",
                            "target_lang_translations": {
                                "ru": "Мои родители всегда меня поддерживают, поэтому я им очень <доверяю>."
                            }
                        },
                        {
                            "text": "Nie można bezgranicznie <ufać> obcym ludziom spotkanym na ulicy.",
                            "target_lang_translations": {
                                "ru": "Нельзя безгранично <доверять> незнакомым людям, встреченным на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "wierzyć"
                    ],
                    "common_phrases": [
                        "ufać bezgranicznie",
                        "ufać intuicji",
                        "ufać sobie",
                        "ufać lekarzowi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть уверенным в чьей-либо честности, искренности и добрых намерениях; полагаться на кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доверять",
                                "target_lang_sense_clarification": "полагаться на кого-либо"
                            },
                            {
                                "target_lang_word": "верить",
                                "target_lang_sense_clarification": "быть уверенным в честности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89dd46a3-6e45-39bf-b721-6c684c12f79e",
                    "sense_definition": "Wierzyć, że czyjeś słowa, informacje lub zapewnienia są zgodne z prawdą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "trust_belief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trudno jest <ufać> informacjom, które nie zostały oficjalnie potwierdzone.",
                            "target_lang_translations": {
                                "ru": "Трудно <доверять> информации, которая не была официально подтверждена."
                            }
                        },
                        {
                            "text": "Czy naprawdę <ufasz> temu, co politycy mówią w telewizji?",
                            "target_lang_translations": {
                                "ru": "Ты действительно <веришь> тому, что политики говорят по телевизору?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wierzyć"
                    ],
                    "antonyms": [
                        "wątpić"
                    ],
                    "common_phrases": [
                        "ufać plotkom",
                        "ufać statystykom",
                        "ufać słowom",
                        "ufać zapewnieniom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верить, что чьи-то слова, информация или заверения соответствуют истине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доверять",
                                "target_lang_sense_clarification": "относиться с доверием к источнику"
                            },
                            {
                                "target_lang_word": "верить",
                                "target_lang_sense_clarification": "принимать за истину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e1203e4-9b35-3f34-8092-7c6b4c4da737",
                    "sense_definition": "Mieć głęboką nadzieję i przekonanie, że wydarzy się coś pomyślnego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hope_expectation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy głęboko <ufać>, że nasze plany zakończą się sukcesem.",
                            "target_lang_translations": {
                                "ru": "Мы должны глубоко <верить>, что наши планы увенчаются успехом."
                            }
                        },
                        {
                            "text": "W trudnych chwilach warto <ufać> w lepszą i spokojną przyszłość.",
                            "target_lang_translations": {
                                "ru": "В трудные моменты стоит <верить> в лучшее и спокойное будущее."
                            }
                        }
                    ],
                    "antonyms": [
                        "wątpić"
                    ],
                    "common_phrases": [
                        "ufać w zwycięstwo",
                        "ufać w przyszłość",
                        "ufać Bogu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь глубокую надежду и убежденность в том, что произойдет что-то хорошее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верить",
                                "target_lang_sense_clarification": "иметь твердую уверенность"
                            },
                            {
                                "target_lang_word": "надеяться",
                                "target_lang_sense_clarification": "ожидать благополучного исхода"
                            },
                            {
                                "target_lang_word": "уповать",
                                "target_lang_sense_clarification": "возлагать надежды (книжн.)"
                            }
                        ]
                    }
                }
            ]
        }

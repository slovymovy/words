                                "target_lang_word": "плевать",
                                "target_lang_sense_clarification": "выбрасывать слюну"
                            },
                            {
                                "target_lang_word": "плюнуть",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "сплёвывать",
                                "target_lang_sense_clarification": "периодическое действие"
                            }
                        ]

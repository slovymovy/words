                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_concept",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Mercy</w> signed the documents at the front desk.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Мерси</w> подписал документы на стойке регистрации."
                            }
                        }
                    ],

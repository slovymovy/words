            "senses": [
                {
                    "sense_id": "1e058f86-0b46-3da0-9be1-2d0a0a925b20",
                    "sense_definition": "A unit of credit or scoring in a game or competition.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "score_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team with the most <w>points</w> wins the match.",
                            "target_lang_translations": {
                                "ru": "Команда, набравшая больше всего <w>очков</w>, побеждает в матче."
                            }
                        },
                        {
                            "text": "He scored three <w>points</w> in the last minute of the game.",
                            "target_lang_translations": {
                                "ru": "Он заработал три <w>очка</w> на последней минуте игры."
                            }

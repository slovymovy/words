    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c4a9fbcb-cc09-30e7-8865-5f0b8b64e623",
                    "sense_definition": "A unit for measuring liquids or dry goods, equal to one eighth of a gallon.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The recipe requires exactly one <w>pint</w> of heavy cream for the sauce.",
                            "target_lang_translations": {
                                "ru": "Для соуса по рецепту требуется ровно одна <w>пинта</w> жирных сливок."
                            }
                        },
                        {
                            "text": "Could you please pick up a <w>pint</w> of milk from the store?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты купить в магазине <w>пинту</w> молока?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "pint of milk",
                        "half a pint",
                        "pint of water",
                        "pint of cream"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения жидкостей или сыпучих продуктов, равная одной восьмой галлона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пинта",
                                "target_lang_sense_clarification": "мера объема"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eff18553-255f-3dc0-90c3-8cfba0888ad2",
                    "sense_definition": "A serving of beer or cider, typically in a glass holding one pint.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to go for a <w>pint</w> at the local pub after work.",
                            "target_lang_translations": {
                                "ru": "Мы решили зайти в местный паб выпить по <w>пинте</w> после работы."
                            }
                        },
                        {
                            "text": "He ordered a <w>pint</w> of lager and sat down by the window.",
                            "target_lang_translations": {
                                "ru": "Он заказал <w>пинту</w> лагера и сел у окна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go for a pint",
                        "couple of pints",
                        "pint of beer",
                        "pint of lager"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often refers to the social act of drinking beer"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Порция пива или сидра, обычно в стакане объемом в одну пинту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пинта",
                                "target_lang_sense_clarification": "порция пива"
                            },
                            {
                                "target_lang_word": "кружка",
                                "target_lang_sense_clarification": "разговорное, бокал пива"
                            }
                        ]
                    }
                }
            ]
        },

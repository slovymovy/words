            "senses": [
                {
                    "sense_id": "70e887aa-34d2-3e3d-9ec6-2193b49a31db",
                    "sense_definition": "Mężczyzna uprawiający kulturystykę; sportowiec dążący do uzyskania dużej masy mięśniowej i estetycznej sylwetki poprzez ćwiczenia siłowe i dietę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kilku bytomskich <w>kulturystów</w> wystąpiło w reklamie maszynki do golenia.",
                            "target_lang_translations": {
                                "ru": "Несколько <w>культуристов</w> из Бытома снялись в рекламе бритвенного станка."
                            }
                        },
                        {
                            "text": "Każdy zawodowy <w>kulturysta</w> spędza wiele godzin na siłowni, aby przygotować się do konkursu.",
                            "target_lang_translations": {
                                "ru": "Каждый профессиональный <w>культурист</w> проводит много часов в тренажерном зале, чтобы подготовиться к соревнованиям."
                            }
                        }
                    ],
                    "antonyms": [
                        "chuchro"
                    ],
                    "common_phrases": [
                        "zawodowy kulturysta",
                        "sylwetka kulturysty",
                        "trening kulturysty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, занимающийся культуризмом; спортсмен, стремящийся к наращиванию мышечной массы и созданию эстетичного телосложения с помощью силовых тренировок и диеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "культурист",
                                "target_lang_sense_clarification": "спортсмен"
                            },
                            {
                                "target_lang_word": "бодибилдер",
                                "target_lang_sense_clarification": "современный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04ccad81-877d-3073-98d1-f0173a89dc44",
                    "sense_definition": "Mężczyzna bardzo dobrze zbudowany, o widocznej i imponującej muskulaturze.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy ochroniarz to prawdziwy <w>kulturysta</w>, budzi respekt samym swoim wyglądem.",
                            "target_lang_translations": {
                                "ru": "Этот новый охранник — настоящий <w>культурист</w>, он внушает уважение одним своим видом."
                            }
                        },
                        {
                            "text": "Nie musisz być <w>kulturystą</w>, żeby pomóc mi wnieść te zakupy na piętro.",
                            "target_lang_translations": {
                                "ru": "Не обязательно быть <w>культуристом</w>, чтобы помочь мне занести эти покупки на этаж."
                            }
                        }
                    ],
                    "antonyms": [
                        "chuchro"
                    ],
                    "common_phrases": [
                        "wyglądać jak kulturysta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина с очень хорошим телосложением, с заметной и внушительной мускулатурой."
                    },

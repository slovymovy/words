                {
                    "sense_id": "75eeae6e-418f-30cf-8f82-f189bce8753e",
                    "sense_definition": "Wykonany z metalu szlachetnego – złota.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "literal_gold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na urodziny dostałam od męża piękny <w>złoty</w> pierścionek.",
                            "target_lang_translations": {
                                "ru": "На день рождения я получила от мужа красивое <w>золотое</w> кольцо."
                            }
                        },
                        {
                            "text": "W muzeum oglądaliśmy stary <w>złoty</w> kielich królów.",
                            "target_lang_translations": {
                                "ru": "В музее мы рассматривали старинный <w>золотой</w> кубок королей."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczerozłoty"
                    ],
                    "antonyms": [
                        "srebrny"
                    ],
                    "common_phrases": [
                        "złoty pierścionek",
                        "złoty łańcuszek",
                        "złoty zegarek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из благородного металла — золота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золотой",
                                "target_lang_sense_clarification": "сделанный из золота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6675492-d01c-38f0-947e-333cc25b2440",
                    "sense_definition": "Mający barwę lub połysk przypominający złoto.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_gold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zachodzące słońce zabarwiło liście drzew na <w>złoty</w> kolor.",
                            "target_lang_translations": {
                                "ru": "Заходящее солнце окрасило листья деревьев в <w>золотой</w> цвет."
                            }
                        },
                        {
                            "text": "Jej długie <w>złote</w> włosy lśniły w słońcu.",
                            "target_lang_translations": {
                                "ru": "Её длинные <w>золотые</w> волосы блестели на солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "złocisty",
                        "złotawy"
                    ],
                    "common_phrases": [
                        "złota jesień",
                        "złote włosy",
                        "złoty piasek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий цвет или блеск, напоминающий золото."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золотой",
                                "target_lang_sense_clarification": "цвета золота"
                            },
                            {
                                "target_lang_word": "золотистый",
                                "target_lang_sense_clarification": "с золотым отливом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bfa61bf-9713-377c-b4e4-b0069f7c8b55",
                    "sense_definition": "Związany z najwyższą nagrodą w konkursach, zwykle medalem za pierwsze miejsce.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz najlepszy skoczek zdobył <w>złoty</w> medal na igrzyskach.",
                            "target_lang_translations": {
                                "ru": "Наш лучший прыгун завоевал <w>золотую</w> медаль на Олимпийских играх."
                            }
                        },
                        {
                            "text": "Drużyna siatkarzy przywiozła do kraju <w>złoty</w> puchar.",
                            "target_lang_translations": {
                                "ru": "Волейбольная команда привезла в страну <w>золотой</w> кубок."
                            }
                        }
                    ],
                    "antonyms": [
                        "srebrny"
                    ],
                    "common_phrases": [
                        "złoty medal",
                        "złoty puchar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с высшей наградой в соревнованиях, обычно медалью за первое место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золотой",
                                "target_lang_sense_clarification": "о награде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fabeee7-91c7-3897-9d58-03fe4df67e8c",
                    "sense_definition": "Bardzo dobry, szlachetny i życzliwy (zwykle o człowieku lub jego cechach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_good",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój sąsiad to <w>złoty</w> człowiek, zawsze wszystkim pomaga.",
                            "target_lang_translations": {
                                "ru": "Твой сосед — <w>золотой</w> человек, он всегда всем помогает."
                            }
                        },
                        {
                            "text": "Moja babcia ma <w>złote</w> serce i nikogo nie ocenia.",
                            "target_lang_translations": {
                                "ru": "У моей бабушки <w>золотое</w> сердце, и она никого не осуждает."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspaniały",
                        "szlachetny",
                        "zacny",
                        "anielski"
                    ],
                    "common_phrases": [
                        "złoty człowiek",
                        "złote serce",
                        "złota rączka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень хороший, благородный и доброжелательный (обычно о человеке или его качествах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золотой",
                                "target_lang_sense_clarification": "перен. прекрасный, добрый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97f21d17-ddf3-34a2-9227-1cf5265b1c4b",
                    "sense_definition": "Dotyczący złota jako kruszcu, jego wydobycia lub występowania.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "literal_gold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cenny kruszec wydobywano tu kiedyś w starej <w>złotej</w> sztolni.",
                            "target_lang_translations": {
                                "ru": "Драгоценный металл добывали здесь когда-то в старой <w>золотой</w> штольне."
                            }
                        },
                        {
                            "text": "W XIX wieku w Ameryce wybuchła wielka gorączka <w>złota</w>.",
                            "target_lang_translations": {
                                "ru": "В XIX веке в Америке разразилась великая <w>золотая</w> лихорадка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gorączka złota",
                        "złoty wiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к золоту как к металлу, его добыче или месторождениям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золотой",
                                "target_lang_sense_clarification": "относящийся к добыче золота"
                            }
                        ]
                    }
                }
            ]

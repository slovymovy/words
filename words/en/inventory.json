            "senses": [
                {
                    "sense_id": "76078b01-dd62-3ea8-8393-3bf9deef384a",
                    "sense_definition": "The stock of an item on hand at a particular location or business.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store has a large <w>inventory</w> of winter coats.",
                            "target_lang_translations": {
                                "ru": "В магазине большие <w>запасы</w> зимних пальто."
                            }
                        },
                        {
                            "text": "We need to reduce our <w>inventory</w> to save space.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сократить <w>товарные запасы</w>, чтобы сэкономить место."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inventory control",
                        "low inventory",
                        "reduce inventory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запасы товара или ресурсов, имеющиеся в наличии в определенном месте или на предприятии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запасы",
                                "target_lang_sense_clarification": "reserves, stock"
                            },
                            {
                                "target_lang_word": "товарные запасы",
                                "target_lang_sense_clarification": "merchandise stock"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb99c8bf-c7e5-3ac0-9766-a3b80e22cc23",

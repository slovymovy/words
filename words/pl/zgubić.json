{
    "word_family": [
        "gubić",
        "zguba",
        "zgubny",
        "zagubić",
        "pogubić",
        "zgubiciel"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ef713e25-d009-3ef5-bb42-a07e29d655fd",
                    "sense_definition": "Przestać posiadać coś, nie wiedząc, gdzie to się znajduje; zapodziać.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj przez nieuwagę <zgubiłem> klucze do mieszkania.",
                            "target_lang_translations": {
                                "ru": "Вчера по невнимательности я <потерял> ключи от квартиры."
                            }
                        },
                        {
                            "text": "Musisz pilnować biletu, żeby go nie <zgubić> w tłumie.",
                            "target_lang_translations": {
                                "ru": "Ты должен следить за билетом, чтобы не <потерять> его в толпе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawieruszyć się",
                        "zawieruszyć się"
                    ],
                    "antonyms": [
                        "znaleźć",
                        "odszukać"
                    ],
                    "common_phrases": [
                        "zgubić klucze",
                        "zgubić portfel",
                        "zgubić telefon",
                        "zgubić pieniądze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать владеть чем-то, не зная, где оно находится; потерять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерять",
                                "target_lang_sense_clarification": "лишиться предмета по неосторожности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b15d22f-fa90-30b8-958b-7e2b9bbfcc5f",
                    "sense_definition": "Stracić orientację w terenie i nie wiedzieć, jak trafić do celu (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko <zgubiło> się w dużym centrum handlowym.",
                            "target_lang_translations": {
                                "ru": "Маленький ребёнок <потерялся> в большом торговом центре."
                            }
                        },
                        {
                            "text": "Bez mapy łatwo <zgubić> się w tych górach.",
                            "target_lang_translations": {
                                "ru": "Без карты в этих горах легко <заблудиться>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawieruszyć się",
                        "zawieruszyć się"
                    ],
                    "antonyms": [
                        "odnaleźć się"
                    ],
                    "common_phrases": [
                        "zgubić się w lesie",
                        "zgubić się w mieście",
                        "zgubić drogę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять ориентацию на местности и не знать, как добраться до цели (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заблудиться",
                                "target_lang_sense_clarification": "сбиться с пути"
                            },
                            {
                                "target_lang_word": "потеряться",
                                "target_lang_sense_clarification": "о человеке в незнакомом месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9871d068-61c9-388b-827f-fbd5e1feed92",
                    "sense_definition": "Stracić kontakt wzrokowy lub fizyczny z osobą, z którą się gdzieś szło.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tłumie turystów nagle <zgubiłem> moją wycieczkę.",
                            "target_lang_translations": {
                                "ru": "В толпе туристов я вдруг <потерял> свою экскурсионную группу."
                            }
                        },
                        {
                            "text": "Trzymaj mnie za rękę, żebyś mnie nie <zgubił> na targu.",
                            "target_lang_translations": {
                                "ru": "Держи меня за руку, чтобы не <потерять> меня на рынке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zgubić kogoś z oczu",
                        "zgubić dziecko",
                        "zgubić znajomych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять зрительный или физический контакт с человеком, с которым шли вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерять",
                                "target_lang_sense_clarification": "перестать видеть кого-то в толпе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dec2019-bdf8-365e-854a-c6009a4d783b",
                    "sense_definition": "Stracić wątek, przestać rozumieć tok rozmowy lub wywodu (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student <zgubił> się podczas skomplikowanego wykładu profesora.",
                            "target_lang_translations": {
                                "ru": "Студент <запутался> во время сложной лекции профессора."
                            }
                        },
                        {
                            "text": "Już na początku filmu <zgubiłam> się w zawiłej fabule.",
                            "target_lang_translations": {
                                "ru": "Уже в начале фильма я <запуталась> в запутанном сюжете."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawieruszyć się",
                        "zawieruszyć się"
                    ],
                    "common_phrases": [
                        "zgubić się w rachunkach",
                        "zgubić się w domysłach",
                        "zgubić wątek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять нить разговора, перестать понимать ход мыслей или рассуждений (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запутаться",
                                "target_lang_sense_clarification": "перестать понимать суть"
                            },
                            {
                                "target_lang_word": "сбиться",
                                "target_lang_sense_clarification": "потерять мысль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b63ea549-be72-3fa0-a931-6d296124d9c7",
                    "sense_definition": "Uciec komuś, kto nas śledzi lub goni; sprawić, że pościg traci trop.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej zdołał <zgubić> policję w wąskich uliczkach miasta.",
                            "target_lang_translations": {
                                "ru": "Вору удалось <оторваться> от полиции в узких улочках города."
                            }
                        },
                        {
                            "text": "Doświadczony szpieg szybko <zgubił> śledzącego go agenta.",
                            "target_lang_translations": {
                                "ru": "Опытный шпион быстро <оторвался> от следившего за ним агента."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawieruszyć się",
                        "zawieruszyć się"
                    ],
                    "common_phrases": [
                        "zgubić pościg",
                        "zgubić ogon",
                        "zgubić trop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уйти от преследования; сделать так, чтобы преследователь потерял след."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оторваться",
                                "target_lang_sense_clarification": "уйти от погони"
                            },
                            {
                                "target_lang_word": "скрыться",
                                "target_lang_sense_clarification": "исчезнуть из виду преследователя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc0bb03f-167e-309b-bff0-24192d52b55b",
                    "sense_definition": "Doprowadzić kogoś do nieszczęścia, klęski życiowej lub upadku moralnego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hazard i nadmierne picie alkoholu <zgubiły> wielu zdolnych ludzi.",
                            "target_lang_translations": {
                                "ru": "Азартные игры и чрезмерное употребление алкоголя <погубили> многих талантливых людей."
                            }
                        },
                        {
                            "text": "Miał szansę na sukces, ale <zgubiła> go własna pycha.",
                            "target_lang_translations": {
                                "ru": "У него был шанс на успех, но его <погубила> собственная гордыня."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawieruszyć się",
                        "zawieruszyć się"
                    ],
                    "antonyms": [
                        "ocalić",
                        "uratować"
                    ],
                    "common_phrases": [
                        "zgubić duszę",
                        "pycha go zgubiła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести кого-то к несчастью, жизненному краху или моральному падению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погубить",
                                "target_lang_sense_clarification": "привести к гибели или краху"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
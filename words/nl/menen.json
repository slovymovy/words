{
    "word_family": [
        "mening",
        "vermeend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "af7a2452-2565-3d48-b205-a0900dd34683",
                    "sense_definition": "Serieus zijn; iets werkelijk zo bedoelen en niet als grap.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intentie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Meen</w> je dat nou echt of maak je een grapje?",
                            "target_lang_translations": {
                                "en": "Do you really <w>mean</w> that or are you joking?",
                                "ru": "Ты действительно <w>говоришь серьезно</w> или шутишь?"
                            }
                        },
                        {
                            "text": "Haar excuses waren duidelijk niet <w>gemeend</w>.",
                            "target_lang_translations": {
                                "en": "Her apologies were clearly not <w>sincere</w>.",
                                "ru": "Ее извинения были явно не <w>искренними</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "serieus bedoelen"
                    ],
                    "common_phrases": [
                        "het goed menen",
                        "het serieus menen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be serious; to really mean something and not as a joke.",
                        "ru": "Быть серьезным; действительно иметь что-то в виду, а не шутить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mean",
                                "target_lang_sense_clarification": "to be sincere"
                            },
                            {
                                "target_lang_word": "be serious",
                                "target_lang_sense_clarification": "not joking"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "говорить серьезно",
                                "target_lang_sense_clarification": "не шутить"
                            },
                            {
                                "target_lang_word": "иметь в виду",
                                "target_lang_sense_clarification": "подразумевать всерьез"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b44eda2-8351-3ed7-b9c6-f0ee33d87c83",
                    "sense_definition": "Denken; een mening toegedaan zijn of veronderstellen dat iets zo is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "denken",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>meent</w> dat hij recht heeft op een hoger salaris.",
                            "target_lang_translations": {
                                "en": "He <w>thinks</w> he is entitled to a higher salary.",
                                "ru": "Он <w>считает</w>, что имеет право на более высокую зарплату."
                            }
                        },
                        {
                            "text": "Ik <w>meen</w> me te herinneren dat de winkel op zondag gesloten is.",
                            "target_lang_translations": {
                                "en": "I <w>think</w> I remember that the shop is closed on Sundays.",
                                "ru": "Мне <w>кажется</w>, я помню, что магазин закрыт по воскресеньям."
                            }
                        }
                    ],
                    "synonyms": [
                        "denken"
                    ],
                    "common_phrases": [
                        "ik meen dat",
                        "menen te weten"
                    ],
                    "target_lang_definitions": {
                        "en": "To think; to hold an opinion or suppose that something is the case.",
                        "ru": "Думать; придерживаться мнения или предполагать, что это так."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "think",
                                "target_lang_sense_clarification": "to have an opinion"
                            },
                            {
                                "target_lang_word": "believe",
                                "target_lang_sense_clarification": "to suppose"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "полагать",
                                "target_lang_sense_clarification": "предполагать"
                            },
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение"
                            },
                            {
                                "target_lang_word": "думать",
                                "target_lang_sense_clarification": "мыслить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea696ab4-f4dd-336c-a4ba-b557cdeddfec",
                    "sense_definition": "De intentie hebben; beogen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intentie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>meende</w> het goed, ook al kwam zijn opmerking verkeerd over.",
                            "target_lang_translations": {
                                "en": "He <w>meant</w> well, even though his remark came across wrong.",
                                "ru": "Он <w>хотел</w> как лучше, даже если его замечание было воспринято неправильно."
                            }
                        },
                        {
                            "text": "Ik <w>meende</w> jou niet met die kritische opmerking.",
                            "target_lang_translations": {
                                "en": "I didn't <w>mean</w> you with that critical remark.",
                                "ru": "Я не <w>имел в виду</w> тебя, делая это критическое замечание."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedoelen"
                    ],
                    "common_phrases": [
                        "het goed menen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the intention; to aim or intend.",
                        "ru": "Иметь намерение; подразумевать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mean",
                                "target_lang_sense_clarification": "to intend"
                            },
                            {
                                "target_lang_word": "intend",
                                "target_lang_sense_clarification": "to plan"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "иметь в виду",
                                "target_lang_sense_clarification": "подразумевать кого-то"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "иметь намерение (обычно доброе)"
                            }
                        ]
                    }
                }
            ]
        },
        {

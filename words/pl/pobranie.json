            "senses": [
                {
                    "sense_id": "b8e30799-bf77-3166-8b4a-75110045ba4d",
                    "sense_definition": "Transfer danych z serwera internetowego na komputer lub inne urządzenie użytkownika.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>pobranie</w> zajmie tylko kilka minut przy szybkim internecie.",
                            "target_lang_translations": {
                                "ru": "Это <w>скачивание</w> займет всего несколько минут при быстром интернете."
                            }
                        },
                        {
                            "text": "Kliknij ten przycisk, aby rozpocząć <w>pobranie</w> najnowszej wersji programu.",
                            "target_lang_translations": {
                                "ru": "Нажмите эту кнопку, чтобы начать <w>загрузку</w> последней версии программы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ściągnięcie"
                    ],
                    "antonyms": [
                        "wysłanie"
                    ],
                    "common_phrases": [
                        "plik do pobrania",
                        "pobranie danych",
                        "pobranie aplikacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передача данных с интернет-сервера на компьютер или другое устройство пользователя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скачивание",
                                "target_lang_sense_clarification": "файлов, данных"
                            },
                            {
                                "target_lang_word": "загрузка",
                                "target_lang_sense_clarification": "процесс получения данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d57a7024-2271-3780-944f-36ec90f0c016",
                    "sense_definition": "Czynność polegająca na wzięciu próbki substancji lub tkanki z organizmu, zazwyczaj w celach medycznych lub badawczych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka przygotowała pacjenta na <w>pobranie</w> krwi do analizy.",
                            "target_lang_translations": {
                                "ru": "Медсестра подготовила пациента к <w>забору</w> крови для анализа."
                            }
                        },
                        {
                            "text": "Bezbolesne <w>pobranie</w> materiału genetycznego jest kluczowe dla tego badania.",
                            "target_lang_translations": {
                                "ru": "Безболезненное <w>взятие</w> генетического материала является ключевым для этого исследования."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzięcie",
                        "pozyskanie"
                    ],
                    "common_phrases": [
                        "pobranie krwi",
                        "pobranie próbki",
                        "pobranie narządów",
                        "pobranie wymazu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся во взятии образца вещества или ткани из организма, обычно в медицинских или исследовательских целях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забор",
                                "target_lang_sense_clarification": "медицинский, анализов"
                            },
                            {
                                "target_lang_word": "взятие",
                                "target_lang_sense_clarification": "пробы, крови"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "475540a5-2570-3f76-97b0-cbf48c565185",
                    "sense_definition": "Sposób płatności i dostawy, w którym uiszczenie należności następuje w momencie odbioru towaru.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepie internetowym wybrałem szybką wysyłkę za <w>pobraniem</w>.",
                            "target_lang_translations": {
                                "ru": "В интернет-магазине я выбрал быструю доставку <w>наложенным платежом</w>."
                            }
                        },
                        {
                            "text": "Kurier wyda paczkę dopiero po uiszczeniu opłaty za <w>pobranie</w>.",
                            "target_lang_translations": {
                                "ru": "Курьер выдаст посылку только после оплаты <w>наложенного платежа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "inkaso"
                    ],
                    "antonyms": [
                        "przedpłata"
                    ],
                    "common_phrases": [
                        "przesyłka za pobraniem",
                        "płatność za pobraniem",
                        "wysyłka za pobraniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ оплаты и доставки, при котором оплата производится в момент получения товара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наложенный платёж",
                                "target_lang_sense_clarification": "способ оплаты при получении"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "patronage",
        "patronize",
        "patroness",
        "patronal"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb3dbee2-b64d-3133-aa37-008559e42f99",
                    "sense_definition": "A customer, especially a regular one, of a store, restaurant, or hotel.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "customer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant offers a special discount to its regular <w>patrons</w>.",
                            "target_lang_translations": {
                                "ru": "Ресторан предлагает специальную скидку своим постоянным <w>клиентам</w>."
                            }
                        },
                        {
                            "text": "Parking in this lot is reserved strictly for <w>patrons</w> of the theater.",
                            "target_lang_translations": {
                                "ru": "Парковка на этой стоянке предназначена исключительно для <w>посетителей</w> театра."
                            }
                        }
                    ],
                    "synonyms": [
                        "customer",
                        "client",
                        "regular"
                    ],
                    "antonyms": [
                        "owner",
                        "staff"
                    ],
                    "common_phrases": [
                        "restaurant patron",
                        "bar patron",
                        "patrons only"
                    ],
                    "target_lang_definitions": {
                        "ru": "Клиент, особенно постоянный, магазина, ресторана или отеля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоянный клиент",
                                "target_lang_sense_clarification": "regular customer"
                            },
                            {
                                "target_lang_word": "посетитель",
                                "target_lang_sense_clarification": "visitor/guest"
                            },
                            {
                                "target_lang_word": "клиент",
                                "target_lang_sense_clarification": "customer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9f35f8d-8850-36ff-9227-4cf9086a5591",
                    "sense_definition": "A wealthy or influential person who supports an artist, writer, museum, or charity financially.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "protector_supporter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a well-known <w>patron</w> of the arts who funds local galleries.",
                            "target_lang_translations": {
                                "ru": "Она известный <w>меценат</w>, финансирующий местные галереи."
                            }
                        },
                        {
                            "text": "The museum relies heavily on donations from generous <w>patrons</w> to stay open.",
                            "target_lang_translations": {
                                "ru": "Музей сильно зависит от пожертвований щедрых <w>покровителей</w>, чтобы продолжать работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "sponsor",
                        "benefactor",
                        "backer",
                        "donor"
                    ],
                    "common_phrases": [
                        "patron of the arts",
                        "wealthy patron",
                        "generous patron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Богатый или влиятельный человек, оказывающий финансовую поддержку художнику, писателю, музею или благотворительной организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меценат",
                                "target_lang_sense_clarification": "patron of arts"
                            },
                            {
                                "target_lang_word": "покровитель",
                                "target_lang_sense_clarification": "protector/supporter"
                            },
                            {
                                "target_lang_word": "спонсор",
                                "target_lang_sense_clarification": "financial supporter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51789e37-214a-3ecc-8b26-28d8495dbc42",
                    "sense_definition": "A saint or deity regarded as the special guardian of a person, group, trade, or country.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "protector_supporter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "St. Patrick is the <w>patron</w> of Ireland and is celebrated every March.",
                            "target_lang_translations": {
                                "ru": "Святой Патрик — <w>покровитель</w> Ирландии, его чествуют каждый март."
                            }
                        },
                        {
                            "text": "Travelers often pray to their <w>patron</w> saint for safety during long journeys.",
                            "target_lang_translations": {
                                "ru": "Путешественники часто молятся своему святому <w>заступнику</w> о безопасности во время долгих странствий."
                            }
                        }
                    ],
                    "synonyms": [
                        "guardian",
                        "protector"
                    ],
                    "common_phrases": [
                        "patron saint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Святой или божество, считающееся особым защитником человека, группы, профессии или страны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покровитель",
                                "target_lang_sense_clarification": "patron saint"
                            },
                            {
                                "target_lang_word": "заступник",
                                "target_lang_sense_clarification": "intercessor/defender"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80bf7841-f535-3444-b452-71afdf10568c",
                    "sense_definition": "A master in ancient Rome who protected and supported a dependent or freed slave.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "protector_supporter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>patron</w> provided legal protection to his clients in exchange for their loyalty.",
                            "target_lang_translations": {
                                "ru": "<w>Патрон</w> предоставлял правовую защиту своим клиентам в обмен на их верность."
                            }
                        },
                        {
                            "text": "In Roman society, a client was expected to support his <w>patron</w> politically.",
                            "target_lang_translations": {
                                "ru": "В римском обществе ожидалось, что клиент будет поддерживать своего <w>патрона</w> политически."
                            }
                        }
                    ],
                    "synonyms": [
                        "master",
                        "protector"
                    ],
                    "antonyms": [
                        "client",
                        "dependent"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В Древнем Риме — знатный гражданин, защищавший и поддерживавший зависимых от него людей или вольноотпущенников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патрон",
                                "target_lang_sense_clarification": "historical term"
                            },
                            {
                                "target_lang_word": "покровитель",
                                "target_lang_sense_clarification": "protector"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f7a5be9-8ffb-3aa1-b35c-2c5e34856592",
                    "sense_definition": "The owner or manager of an establishment, such as an inn or restaurant.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "owner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I asked the <w>patron</w> of the inn if there were any rooms available.",
                            "target_lang_translations": {
                                "ru": "Я спросил у <w>хозяина</w> гостиницы, есть ли свободные номера."
                            }
                        },
                        {
                            "text": "The <w>patron</w> sat behind the counter and greeted every guest personally.",
                            "target_lang_translations": {
                                "ru": "<w>Хозяин</w> сидел за стойкой и лично приветствовал каждого гостя."
                            }
                        }
                    ],
                    "synonyms": [
                        "proprietor",
                        "landlord",
                        "owner"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "often used in French or Spanish contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Владелец или управляющий заведением, например, гостиницей или рестораном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хозяин",
                                "target_lang_sense_clarification": "owner/host"
                            },
                            {
                                "target_lang_word": "владелец",
                                "target_lang_sense_clarification": "proprietor"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "27420a80-b0d9-38f9-8ed0-f7e056cfbe1e",
                    "sense_definition": "To act as a patron towards; to support or frequent.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the past, wealthy nobles <w>patroned</w> artists to help them create their masterpieces.",
                            "target_lang_translations": {
                                "ru": "В прошлом богатые дворяне <w>покровительствовали</w> художникам, помогая им создавать шедевры."
                            }
                        },
                        {
                            "text": "The local shop was <w>patroned</w> by the villagers for many generations.",
                            "target_lang_translations": {
                                "ru": "Местные жители <w>посещали</w> этот магазин на протяжении многих поколений."
                            }
                        }
                    ],
                    "synonyms": [
                        "patronize",
                        "support"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'patronize'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступать в роли покровителя; поддерживать или часто посещать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покровительствовать",
                                "target_lang_sense_clarification": "to protect/support"
                            },
                            {
                                "target_lang_word": "опекать",
                                "target_lang_sense_clarification": "to look after"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
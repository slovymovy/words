            "senses": [
                {
                    "sense_id": "dff7ab58-1a2e-3241-9bac-84d2185c6b17",
                    "sense_definition": "Столб у ворот, на который навешиваются их створки.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "gate_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лысый старик отворил ворота, прижавшись к <w>верее</w>, чтобы пропустить тройку.",
                            "target_lang_translations": {
                                "en": "The bald old man opened the gate, pressing himself against the <w>gatepost</w> to let the troika pass."
                            }
                        },
                        {
                            "text": "Телега неловко повернула и задела колесом дубовую <w>верею</w>.",
                            "target_lang_translations": {
                                "en": "The cart turned awkwardly and its wheel grazed the oak <w>gatepost</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "дубовая верея",
                        "стоять у вереи"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "в современном языке встречается редко, в основном в диалектах или литературе"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "слово характерно для старого быта"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A post at a gate on which the gate leaves or doors are hung."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gatepost",
                                "target_lang_sense_clarification": "structural support for a gate"
                            },
                            {
                                "target_lang_word": "post",
                                "target_lang_sense_clarification": "upright support"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66c74109-4283-3a2b-b1dc-e519ade10d96",
                    "sense_definition": "Возвышенная полоса земли, гряда или участок леса, луга (обычно на пойме).",

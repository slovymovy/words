            "senses": [
                {
                    "sense_id": "e6e4796d-322a-3ee4-b2ee-0a0bf2250c03",
                    "sense_definition": "An account of real or fictional people and events told for entertainment or information.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "narrative_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother used to tell me a <w>story</w> before I went to sleep.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка рассказывала мне <w>сказку</w> перед сном."
                            }
                        },
                        {
                            "text": "The film tells the true <w>story</w> of a famous musician who changed the world.",
                            "target_lang_translations": {
                                "ru": "Фильм рассказывает правдивую <w>историю</w> известного музыканта, который изменил мир."
                            }
                        }
                    ],
                    "synonyms": [
                        "tale",
                        "narrative",
                        "account"
                    ],
                    "common_phrases": [
                        "tell a story",
                        "short story",
                        "true story",
                        "love story",
                        "bedtime story"

                            {
                                "target_lang_word": "погаснуть",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "выключаться",
                                "target_lang_sense_clarification": "о приборах или электричестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7215d53b-0cf4-3b32-932d-1dbf9bad39af",

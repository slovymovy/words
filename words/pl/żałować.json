    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "782bff58-a055-3d86-bd12-68410069fbd6",
                    "sense_definition": "Odczuwać smutek, przygnębienie lub niezadowolenie z powodu tego, co się zrobiło lub czego się nie zrobiło w przeszłości",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotions_regret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo <żałuję>, że nie poszedłem wczoraj na to spotkanie.",
                            "target_lang_translations": {
                                "ru": "Я очень <жалею>, что не пошёл вчера на эту встречу."
                            }
                        },
                        {
                            "text": "Kiedyś będziesz <żałować> tej pochopnej decyzji.",
                            "target_lang_translations": {
                                "ru": "Однажды ты будешь <жалеть> об этом поспешном решении."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubolewać",
                        "pluć sobie w brodę",
                        "mieć wyrzuty sumienia",
                        "odczuwać skruchę"
                    ],
                    "common_phrases": [
                        "gorzko żałować",
                        "żałować za grzechy",
                        "nie ma czego żałować",
                        "żałować decyzji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать грусть, разочарование или недовольство из-за того, что было сделано или не сделано в прошлом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалеть",
                                "target_lang_sense_clarification": "испытывать грусть о прошлом событии или поступке"
                            },
                            {
                                "target_lang_word": "сожалеть",
                                "target_lang_sense_clarification": "официально или вежливо выражать скорбь о чем-либо"
                            },
                            {
                                "target_lang_word": "раскаиваться",
                                "target_lang_sense_clarification": "признавать свою вину или ошибку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c0d2b3a-e8c0-3083-8c21-527714a55bcb",
                    "sense_definition": "Odczuwać litość i współczucie wobec kogoś, kto cierpi lub znajduje się w trudnej sytuacji",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions_pity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <żałowała> bezdomnych zwierząt i przynosiła im jedzenie.",
                            "target_lang_translations": {
                                "ru": "Она всегда <жалела> бездомных животных и приносила им еду."
                            }
                        },
                        {
                            "text": "Nie musisz mnie <żałować>, poradzę sobie z tym problemem sam.",
                            "target_lang_translations": {
                                "ru": "Не нужно меня <жалеть>, я справлюсь с этой проблемой сам."
                            }
                        }
                    ],
                    "synonyms": [
                        "współczuć",
                        "litować się"
                    ],
                    "common_phrases": [
                        "żałować biednych",
                        "żałować kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать жалость и сострадание к кому-то, кто страдает или находится в трудной ситуации"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалеть",
                                "target_lang_sense_clarification": "испытывать чувство жалости к кому-либо"
                            },
                            {
                                "target_lang_word": "сочувствовать",
                                "target_lang_sense_clarification": "разделять чьи-то переживания, относиться с пониманием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0de3b42-d449-39a2-81a5-a6c987f105bb",
                    "sense_definition": "Być oszczędnym do przesady, niechętnie wydawać pieniądze lub dzielić się czymś z innymi",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior_stinginess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ojciec nigdy nie <żałował> nam pieniędzy na dobre książki.",
                            "target_lang_translations": {
                                "ru": "Отец никогда не <жалел> нам денег на хорошие книги."
                            }
                        },
                        {
                            "text": "Dlaczego <żałujesz> sobie cukru do herbaty, skoro go lubisz?",
                            "target_lang_translations": {
                                "ru": "Почему ты <жалеешь> себе сахара в чай, если ты его любишь?"
                            }
                        }
                    ],
                    "synonyms": [
                        "skąpić",
                        "szczędzić",
                        "sknerzyć",
                        "chytrzyć"
                    ],
                    "common_phrases": [
                        "nie żałować grosza",
                        "żałować sobie",
                        "żałować pieniędzy",
                        "żałować czasu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть чрезмерно экономным, неохотно тратить деньги или делиться чем-то с другими"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалеть",
                                "target_lang_sense_clarification": "скупиться, неохотно отдавать что-либо"
                            },
                            {
                                "target_lang_word": "скупиться",
                                "target_lang_sense_clarification": "проявлять чрезмерную бережливость"
                            },
                            {
                                "target_lang_word": "жадничать",
                                "target_lang_sense_clarification": "проявлять жадность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
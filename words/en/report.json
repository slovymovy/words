        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9eddd865-5750-3bc3-92d7-3758a446e76b",
                    "sense_definition": "To give a description or details of an event, situation, or news, often publicly.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journalist will <w>report</w> on the election results live from the capital."
                        },
                        {
                            "text": "Witnesses <w>reported</w> seeing a strange light in the sky just before the crash."
                        }
                    ],
                    "common_phrases": [
                        "report on",
                        "report that"
                    ]
                },
                {
                    "sense_id": "3303947d-f15e-348f-9c93-0e7d93f8a354",
                    "sense_definition": "To officially tell someone in authority about a crime, accident, or bad behavior.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>report</w> the theft to the police station immediately."
                        },
                        {
                            "text": "If he continues to be rude to the customers, I will have to <w>report</w> him."
                        }
                    ],
                    "common_phrases": [
                        "report a crime",
                        "report an accident",
                        "report someone"
                    ]
                },
                {
                    "sense_id": "e4a44daa-270d-3acd-bcaa-627d72d85984",

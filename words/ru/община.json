    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b316c96b-dff5-3fe2-acf2-a6b59ae6b883",
                    "sense_definition": "Группа людей, объединённых общими интересами, национальностью, местом жительства или другими социальными признаками",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Русская <w>община</w> за рубежом старается сохранять родной язык и культуру.",
                            "target_lang_translations": {
                                "en": "The Russian <w>community</w> abroad tries to preserve its native language and culture."
                            }
                        },
                        {
                            "text": "Местная <w>община</w> активно участвует в благоустройстве своего района.",
                            "target_lang_translations": {
                                "en": "The local <w>community</w> actively participates in the improvement of their neighborhood."
                            }
                        }
                    ],
                    "synonyms": [
                        "сообщество"
                    ],
                    "common_phrases": [
                        "русская община",
                        "местная община",
                        "еврейская община",
                        "городская община"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people united by common interests, nationality, place of residence, or other social characteristics"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "community",
                                "target_lang_sense_clarification": "social or local group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab7ede37-42cd-3015-a090-2cf0236c4083",
                    "sense_definition": "Объединение верующих одной конфессии, часто привязанное к определённому храму или приходу",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В воскресенье вся церковная <w>община</w> собралась на праздничную службу.",
                            "target_lang_translations": {
                                "en": "On Sunday, the whole church <w>community</w> gathered for the festive service."
                            }
                        },
                        {
                            "text": "Эта религиозная <w>община</w> помогает бедным и организует благотворительные обеды.",
                            "target_lang_translations": {
                                "en": "This religious <w>community</w> helps the poor and organizes charity dinners."
                            }
                        }
                    ],
                    "synonyms": [
                        "сообщество"
                    ],
                    "common_phrases": [
                        "церковная община",
                        "православная община",
                        "религиозная община",
                        "мусульманская община"
                    ],
                    "target_lang_definitions": {
                        "en": "An association of believers of the same denomination, often attached to a specific church or parish"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "community",
                                "target_lang_sense_clarification": "religious group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b774d5f-d313-3fbb-908c-dee4e67c2575",
                    "sense_definition": "Историческая форма социальной организации общества с коллективным владением средствами производства и самоуправлением",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Первобытная <w>община</w> основывалась на равенстве всех её членов.",
                            "target_lang_translations": {
                                "en": "The primitive <w>community</w> was based on the equality of all its members."
                            }
                        },
                        {
                            "text": "Крестьянская <w>община</w> решала вопросы распределения земли на общих сходах.",
                            "target_lang_translations": {
                                "en": "The peasant <w>commune</w> decided land distribution issues at general assemblies."
                            }
                        }
                    ],
                    "synonyms": [
                        "коммуна"
                    ],
                    "common_phrases": [
                        "первобытная община",
                        "крестьянская община",
                        "родовая община",
                        "соседская община"
                    ],
                    "target_lang_definitions": {
                        "en": "A historical form of social organization characterized by collective ownership of the means of production and self-governance"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commune",
                                "target_lang_sense_clarification": "collective organization"
                            },
                            {
                                "target_lang_word": "community",
                                "target_lang_sense_clarification": "historical social unit"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
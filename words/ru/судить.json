            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1d9974a6-e878-3102-89ea-f05f344fd24b",
                    "sense_definition": "Составлять мнение, суждение о ком-либо или чём-либо; делать вывод на основе наблюдений.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я стараюсь беспристрастно <w>судить</w> о людях и их поступках."
                        },
                        {
                            "text": "По его словам можно <w>судить</w> о его добром характере."
                        },
                        {
                            "text": "Мы не можем <w>судить</w> о достоверности этой информации без проверки."
                        }
                    ],
                    "synonyms": [
                        "обслуживать"
                    ],
                    "common_phrases": [
                        "судя по всему",
                        "судить по себе",
                        "судить по одежке"
                    ]
                },
                {
                    "sense_id": "0f72eb99-d3cd-3b7b-843c-48351bbdaa5e",
                    "sense_definition": "Оценивать чьи-либо дела или поступки, часто выражая неодобрение или критику.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит строго <w>судить</w> человека, если вы не знаете всех обстоятельств."
                        },
                        {
                            "text": "Окружающие часто любят <w>судить</w> других за малейшие ошибки."
                        }
                    ],
                    "synonyms": [
                        "обслуживать"
                    ],
                    "common_phrases": [
                        "не судите строго",
                        "Бог тебе судья"
                    ]
                },
                {
                    "sense_id": "81f949c7-9832-3109-9cad-6fa059a5e19c",
                    "sense_definition": "Рассматривать чьё-либо дело в судебном порядке для выяснения виновности и вынесения приговора.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Преступника будут <w>судить</w> по всей строгости закона."
                        },
                        {
                            "text": "Его <w>судили</w> за хулиганство и приговорили к штрафу."
                        }
                    ],
                    "common_phrases": [
                        "судить по закону",
                        "судить преступника"
                    ]
                },
                {
                    "sense_id": "3b73c108-0305-3538-bd8c-0f9df6e5241c",
                    "sense_definition": "Следить за соблюдением правил в спортивной игре или состязании в качестве арбитра.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный арбитр будет <w>судить</w> финальный матч чемпионата."
                        },
                        {
                            "text": "Мой брат начал <w>судить</w> футбольные игры ещё в университете."

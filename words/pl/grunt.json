    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0f11850-0681-3e25-af99-4b3963fc2154",
                    "sense_definition": "Wierzchnia warstwa ziemi, gleba, po której się chodzi lub na której rosną rośliny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_ground",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długich opadach deszczu <grunt> stał się bardzo błotnisty i śliski.",
                            "target_lang_translations": {
                                "ru": "После затяжных дождей <грунт> стал очень грязным и скользким."
                            }
                        },
                        {
                            "text": "Rolnik sprawdził, czy <grunt> jest już wystarczająco suchy, by rozpocząć siew.",
                            "target_lang_translations": {
                                "ru": "Фермер проверил, достаточно ли высохла <почва>, чтобы начать посев."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziemia",
                        "gleba",
                        "podstawa",
                        "podstawa"
                    ],
                    "common_phrases": [
                        "twardy grunt",
                        "grząski grunt",
                        "równy grunt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхний слой земли, почва, по которой ходят или на которой растут растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грунт",
                                "target_lang_sense_clarification": "геологический или строительный термин"
                            },
                            {
                                "target_lang_word": "почва",
                                "target_lang_sense_clarification": "плодородный слой земли"
                            },
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "общее название поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27625716-7675-37cd-81ca-9315f2a36f94",
                    "sense_definition": "Dno zbiornika wodnego (rzeki, jeziora, morza); stałe podłoże pod wodą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_ground",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wypłynąłem daleko od brzegu i przestałem czuć <grunt> pod nogami.",
                            "target_lang_translations": {
                                "ru": "Я отплыл далеко от берега и перестал чувствовать <дно> под ногами."
                            }
                        },
                        {
                            "text": "Woda w jeziorze była tak czysta, że widzieliśmy piaszczysty <grunt>.",
                            "target_lang_translations": {
                                "ru": "Вода в озере была такой чистой, что мы видели песчаное <дно>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziemia",
                        "gleba",
                        "podstawa",
                        "podstawa"
                    ],
                    "common_phrases": [
                        "czuć grunt",
                        "stracić grunt",
                        "dotknąć gruntu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дно водоема (реки, озера, моря); твердая опора под водой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дно",
                                "target_lang_sense_clarification": "нижняя поверхность водоема"
                            },
                            {
                                "target_lang_word": "грунт",
                                "target_lang_sense_clarification": "почва дна водоема"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d1d7419-7e86-3127-9597-2d352455c81d",
                    "sense_definition": "Najważniejsza rzecz, istota sprawy lub podstawa dająca pewność i stabilizację (często używane w wyrażeniach idiomatycznych).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie przejmuj się drobnymi problemami, <grunt> to zdrowie i rodzina.",
                            "target_lang_translations": {
                                "ru": "Не переживай из-за мелких проблем, <главное> — это здоровье и семья."
                            }
                        },
                        {
                            "text": "Kiedy zadano mu trudne pytanie, poczuł, że traci <grunt> pod nogami.",
                            "target_lang_translations": {
                                "ru": "Когда ему задали трудный вопрос, он почувствовал, что теряет <почву> под ногами."
                            }
                        },
                        {
                            "text": "Musimy najpierw zbadać <grunt>, zanim podejmiemy ostateczną decyzję o inwestycji.",
                            "target_lang_translations": {
                                "ru": "Мы должны сначала прощупать <почву>, прежде чем принимать окончательное решение об инвестициях."
                            }
                        }
                    ],
                    "synonyms": [
                        "podstawa",
                        "fundament",
                        "zaprawa",
                        "podstawa",
                        "fundament",
                        "świat",
                        "tło",
                        "źródłosłów",
                        "oryginał"
                    ],
                    "common_phrases": [
                        "grunt to zdrowie",
                        "tracić grunt pod nogami",
                        "badać grunt",
                        "na gruncie prawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суть дела, основа или самое главное (часто используется в идиомах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "главное",
                                "target_lang_sense_clarification": "суть, в выражении 'grunt to...'"
                            },
                            {
                                "target_lang_word": "почва",
                                "target_lang_sense_clarification": "в переносном смысле (терять почву под ногами)"
                            },
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "фундамент, база"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d3a659c-a76c-3652-8837-e38b714a4590",
                    "sense_definition": "Wydzielony obszar ziemi stanowiący czyjąś własność; działka budowlana lub rolna.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "real_estate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inwestor wykupił wszystkie <grunty> w okolicy pod budowę nowego osiedla.",
                            "target_lang_translations": {
                                "ru": "Инвестор выкупил все <земли> в округе под строительство нового жилого комплекса."
                            }
                        },
                        {
                            "text": "Podatek od tego <gruntu> jest wyższy, ponieważ znajduje się on w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Налог на этот <земельный участок> выше, так как он находится в центре города."
                            }
                        }
                    ],
                    "synonyms": [
                        "teren",
                        "ziemia",
                        "gleba",
                        "podstawa",
                        "podstawa"
                    ],
                    "common_phrases": [
                        "kupno gruntu",
                        "grunty orne",
                        "właściciel gruntu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Земельный участок, территория, находящаяся в чьей-либо собственности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "территория, владения"
                            },
                            {
                                "target_lang_word": "земельный участок",
                                "target_lang_sense_clarification": "выделенная часть земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d10dc0a2-e780-3520-a3f0-75c21680d86c",
                    "sense_definition": "Specjalny preparat nakładany na powierzchnię (np. ścianę) w celu jej wzmocnienia przed malowaniem lub tynkowaniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "construction_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed położeniem płytek musisz nałożyć na ścianę odpowiedni <grunt>.",
                            "target_lang_translations": {
                                "ru": "Перед укладкой плитки нужно нанести на стену соответствующую <грунтовку>."
                            }
                        },
                        {
                            "text": "Malarz czekał, aż <grunt> wyschnie, zanim zaczął malować sufit.",
                            "target_lang_translations": {
                                "ru": "Маляр ждал, пока <грунтовка> высохнет, прежде чем начать красить потолок."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaprawa",
                        "podstawa",
                        "fundament",
                        "świat",
                        "tło",
                        "źródłosłów",
                        "oryginał"
                    ],
                    "common_phrases": [
                        "nałożyć grunt",
                        "grunt głęboko penetrujący"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "budownictwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальный состав, наносимый на поверхность для её укрепления перед покраской."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грунтовка",
                                "target_lang_sense_clarification": "жидкий состав"
                            },
                            {
                                "target_lang_word": "грунт",
                                "target_lang_sense_clarification": "профессиональный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

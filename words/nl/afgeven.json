{
    "word_family": [
        "afgifte"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "974f9d8f-770b-3198-902c-df0456bcd0d0",
                    "sense_definition": "Iets overhandigen aan iemand of achterlaten op een specifieke plaats.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "delivery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt je jas bij de garderobe <w>afgeven</w>.",
                            "target_lang_translations": {
                                "en": "You can <w>leave</w> your coat at the cloakroom.",
                                "ru": "Вы можете <w>сдать</w> пальто в гардероб."
                            }
                        },
                        {
                            "text": "Ik moet even een pakketje <w>afgeven</w> bij de buren.",
                            "target_lang_translations": {
                                "en": "I just need to <w>drop</w> a package <w>off</w> at the neighbors'.",
                                "ru": "Мне нужно <w>оставить</w> посылку у соседей."
                            }
                        }
                    ],
                    "synonyms": [
                        "afleveren",
                        "opgeven",
                        "afleveren"
                    ],
                    "antonyms": [
                        "aannemen",
                        "ontvangen"
                    ],
                    "common_phrases": [
                        "een pakketje afgeven",
                        "bij de balie afgeven",
                        "jas afgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To hand something over to someone or leave it at a specific place.",
                        "ru": "Передавать что-либо кому-либо или оставлять в определенном месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drop off",
                                "target_lang_sense_clarification": "leave an item somewhere"
                            },
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "transport to a destination"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "deposit for safekeeping"
                            },
                            {
                                "target_lang_word": "hand in",
                                "target_lang_sense_clarification": "submit or give to a person in charge"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "передавать на хранение"
                            },
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "помещать в определенное место"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "вручать кому-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad43ca96-1c17-3418-b257-1672749f7587",
                    "sense_definition": "Kleur of materiaal verliezen en vlekken maken op iets anders bij aanraking.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op met die nieuwe spijkerbroek, want hij kan <w>afgeven</w> op de bank.",
                            "target_lang_translations": {
                                "en": "Watch out with those new jeans, because the color might <w>rub off</w> on the couch.",
                                "ru": "Осторожнее с этими новыми джинсами, они могут <w>окрасить</w> диван."
                            }
                        },
                        {
                            "text": "De verf op de muur is nog nat en <w>geeft</w> <w>af</w>.",
                            "target_lang_translations": {
                                "en": "The paint on the wall is still wet and <w>comes off</w>.",
                                "ru": "Краска на стене еще сырая и <w>мажется</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kleur afgeven",
                        "niet afgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To lose color or material and stain something else upon contact.",
                        "ru": "Терять цвет или частицы материала и пачкать что-либо при соприкосновении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rub off",
                                "target_lang_sense_clarification": "transfer color or material by contact"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "of dye or color spreading when wet"
                            },
                            {
                                "target_lang_word": "stain",
                                "target_lang_sense_clarification": "discolor something else"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "краситься",
                                "target_lang_sense_clarification": "оставлять следы краски"
                            },
                            {
                                "target_lang_word": "пачкать",
                                "target_lang_sense_clarification": "оставлять грязные следы"
                            },
                            {
                                "target_lang_word": "линять",
                                "target_lang_sense_clarification": "терять цвет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a748a520-e350-3412-a4ce-3023405f6e55",
                    "sense_definition": "Scherpe kritiek uiten op iemand of iets.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "criticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij blijft maar <w>afgeven</w> op het beleid van de regering.",
                            "target_lang_translations": {
                                "en": "He keeps <w>railing against</w> the government's policy.",
                                "ru": "Он продолжает <w>нападать</w> на политику правительства."
                            }
                        },
                        {
                            "text": "Waarom moet je altijd zo <w>afgeven</w> op je collega's?",
                            "target_lang_translations": {
                                "en": "Why do you always have to <w>run down</w> your colleagues like that?",
                                "ru": "Почему ты вечно так <w>нападаешь</w> на коллег?"
                            }
                        }
                    ],
                    "synonyms": [
                        "opgeven"
                    ],
                    "antonyms": [
                        "prijzen"
                    ],
                    "common_phrases": [
                        "afgeven op iemand",
                        "flink afgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To express sharp criticism of someone or something.",
                        "ru": "Высказывать резкую критику в адрес кого-либо или чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rail against",
                                "target_lang_sense_clarification": "criticize harshly and persistently"
                            },
                            {
                                "target_lang_word": "run down",
                                "target_lang_sense_clarification": "speak ill of or disparage"
                            },
                            {
                                "target_lang_word": "criticize",
                                "target_lang_sense_clarification": "indicate faults"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нападать",
                                "target_lang_sense_clarification": "резко критиковать"
                            },
                            {
                                "target_lang_word": "критиковать",
                                "target_lang_sense_clarification": "указывать на недостатки"
                            },
                            {
                                "target_lang_word": "ругать",
                                "target_lang_sense_clarification": "бранить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9e38af3-0c05-34cf-bd23-f6773ca04be2",
                    "sense_definition": "Een stof, geur of energie naar buiten brengen of uitstoten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De radiator begon eindelijk warmte <w>af</w> te <w>geven</w>.",
                            "target_lang_translations": {
                                "en": "The radiator finally started to <w>give off</w> heat.",
                                "ru": "Радиатор наконец начал <w>отдавать</w> тепло."
                            }
                        },
                        {
                            "text": "Planten <w>geven</w> zuurstof <w>af</w> aan de omgeving.",
                            "target_lang_translations": {
                                "en": "Plants <w>release</w> oxygen into the environment.",
                                "ru": "Растения <w>выделяют</w> кислород в окружающую среду."
                            }
                        }
                    ],
                    "synonyms": [
                        "afleveren"
                    ],
                    "antonyms": [
                        "opnemen"
                    ],
                    "common_phrases": [
                        "warmte afgeven",
                        "signaal afgeven",
                        "geur afgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring out or emit a substance, scent, or energy.",
                        "ru": "Выделять или испускать вещество, запах или энергию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give off",
                                "target_lang_sense_clarification": "emit heat, light, or smell"
                            },
                            {
                                "target_lang_word": "emit",
                                "target_lang_sense_clarification": "discharge something"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "let go of a substance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выделять",
                                "target_lang_sense_clarification": "испускать вещество"
                            },
                            {
                                "target_lang_word": "излучать",
                                "target_lang_sense_clarification": "испускать энергию или свет"
                            },
                            {
                                "target_lang_word": "испускать",
                                "target_lang_sense_clarification": "издавать запах или звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01ff423d-78d8-3202-b870-2b48706965e4",
                    "sense_definition": "Afstand doen van iets dat je bezit of waar je recht op hebt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "forfeit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kampioen moest zijn titel <w>afgeven</w> na de verloren wedstrijd.",
                            "target_lang_translations": {
                                "en": "The champion had to <w>relinquish</w> his title after the lost match.",
                                "ru": "Чемпион был вынужден <w>уступить</w> свой титул после проигранного матча."
                            }
                        },
                        {
                            "text": "Zij wilde haar rechten niet zomaar <w>afgeven</w>.",
                            "target_lang_translations": {
                                "en": "She didn't want to just <w>give up</w> her rights.",
                                "ru": "Она не хотела просто так <w>отдавать</w> свои права."
                            }
                        }
                    ],
                    "synonyms": [
                        "opgeven"
                    ],
                    "antonyms": [
                        "behouden"
                    ],
                    "common_phrases": [
                        "een titel afgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To give up something one possesses or has a right to.",
                        "ru": "Отказываться от чего-либо, чем владеешь или на что имеешь право."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relinquish",
                                "target_lang_sense_clarification": "formally surrender a claim or title"
                            },
                            {
                                "target_lang_word": "give up",
                                "target_lang_sense_clarification": "stop having or holding"
                            },
                            {
                                "target_lang_word": "surrender",
                                "target_lang_sense_clarification": "yield possession"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "передавать право или титул"
                            },
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "расставаться с чем-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "2599485b-00d9-3ad9-a845-2fea6f787788",
                    "sense_definition": "Każda osoba z danej grupy lub ogół ludzi; zaimek zastępujący rzeczowniki określające osoby.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_everyone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na przyjęcie urodzinowe przyszli <wszyscy> i świetnie się bawili.",
                            "target_lang_translations": {
                                "ru": "На день рождения пришли <все> и отлично повеселились."
                            }
                        },
                        {
                            "text": "To była niespodzianka dla nas <wszystkich>.",
                            "target_lang_translations": {
                                "ru": "Это был сюрприз для нас <всех>."
                            }
                        },
                        {
                            "text": "<Wszyscy> chcą być szczęśliwi i zdrowi w życiu.",
                            "target_lang_translations": {
                                "ru": "<Все> хотят быть счастливыми и здоровыми в жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "każdy z …"
                    ],
                    "antonyms": [
                        "nikt"
                    ],
                    "common_phrases": [
                        "wszyscy razem",
                        "my wszyscy",
                        "wy wszyscy",
                        "oni wszyscy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Каждое лицо из данной группы или люди в целом; местоимение, заменяющее существительные, обозначающие людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "все",
                                "target_lang_sense_clarification": "местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c7ab9ec-453f-33e2-8788-450faf8f1a17",
                    "sense_definition": "Słowo określające, że następujący po nim rzeczownik męskoosobowy dotyczy każdej osoby z wymienionej grupy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_all_determiner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Wszyscy> ludzie rodzą się wolni i równi.",
                            "target_lang_translations": {
                                "ru": "<Все> люди рождаются свободными и равными."
                            }
                        },
                        {
                            "text": "Nauczyciel poprosił, aby <wszyscy> uczniowie usiedli na miejscach.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил, чтобы <все> ученики сели на свои места."
                            }
                        },
                        {
                            "text": "Znam <wszystkich> sąsiadów w moim bloku.",
                            "target_lang_translations": {
                                "ru": "Я знаю <всех> соседей в моем доме."
                            }
                        }
                    ],
                    "antonyms": [
                        "żaden"
                    ],
                    "common_phrases": [
                        "wszyscy ludzie",
                        "wszyscy święci",
                        "wszyscy wierni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, указывающее, что следующее за ним существительное мужского рода относится к каждому лицу из упомянутой группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "все",
                                "target_lang_sense_clarification": "определение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
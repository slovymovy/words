    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f60ad52b-1dcb-36c7-ab2d-df4e79999cd6",
                    "sense_definition": "Pocisk wyrzucany z łuku, składający się z cienkiego drzewca zakończonego ostrym grotem i lotkami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "projectile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Łucznik napiął cięciwę i wypuścił <strzałę> w kierunku tarczy.",
                            "target_lang_translations": {
                                "ru": "Лучник натянул тетиву и выпустил <стрелу> в сторону мишени."
                            }
                        },
                        {
                            "text": "Ten samochód jest szybki jak <strzała>.",
                            "target_lang_translations": {
                                "ru": "Эта машина быстрая как <стрела>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypuścić strzałę",
                        "szybki jak strzała",
                        "prosty jak strzała"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снаряд, выпускаемый из лука, состоящий из тонкого древка с острым наконечником и оперением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стрела",
                                "target_lang_sense_clarification": "оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dab71c07-4faf-3c37-a7bd-7d67922eb696",
                    "sense_definition": "Główny, prosty pień drzewa, pozbawiony gałęzi w dolnej części, biegnący od podstawy do wierzchołka.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leśniczy ocenił jakość <strzały> sosny przeznaczonej na wycinkę.",
                            "target_lang_translations": {
                                "ru": "Лесничий оценил качество <ствола> сосны, предназначенной для вырубки."
                            }
                        },
                        {
                            "text": "Wysoka i prosta <strzała> tego dębu robiła ogromne wrażenie.",
                            "target_lang_translations": {
                                "ru": "Высокий и прямой <ствол> этого дуба производил огромное впечатление."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "leśnictwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный, прямой ствол дерева, лишенный ветвей в нижней части, идущий от основания до вершины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ствол",
                                "target_lang_sense_clarification": "дерева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "101e7ff5-0681-32ca-83e7-3d96fc4e6d59",
                    "sense_definition": "Dolna część korpusu pługa, do której przymocowany jest lemiesz.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik musiał wymienić pękniętą <strzałę> w swoim pługu.",
                            "target_lang_translations": {
                                "ru": "Фермеру пришлось заменить треснувший <башмак> в своем плуге."
                            }
                        },
                        {
                            "text": "Nowoczesna <strzała> pługa zapewnia mniejszy opór gleby.",
                            "target_lang_translations": {
                                "ru": "Современный <башмак> плуга обеспечивает меньшее сопротивление почвы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "rolnictwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя часть корпуса плуга, к которой крепится лемех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "башмак",
                                "target_lang_sense_clarification": "плуга"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "32e47e3a-edf0-3c35-9b87-f5a85ef8518c",
                    "sense_definition": "Niewielki gwiazdozbiór nieba północnego, leżący wewnątrz Trójkąta Letniego.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Podczas nocnych obserwacji udało nam się dostrzec gwiazdozbiór <Strzały>.",
                            "target_lang_translations": {
                                "ru": "Во время ночных наблюдений нам удалось разглядеть созвездие <Стрелы>."
                            }
                        },
                        {
                            "text": "<Strzała> jest jednym z mniejszych gwiazdozbiorów na naszym niebie.",
                            "target_lang_translations": {
                                "ru": "<Стрела> — одно из самых маленьких созвездий на нашем небе."
                            }
                        }
                    ],
                    "synonyms": [
                        "Sge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое созвездие северного полушария, находящееся внутри Летнего треугольника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Стрела",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45b07b05-183a-3899-8d09-1a1ce538f414",
                    "sense_definition": "Polskie nazwisko.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Strzała> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <Стшала> был избран председателем совета микрорайона."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <Strzała> w tej okolicy.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого с фамилией <Стшала> в этом районе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Стшала",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "2dbee54b-a948-341c-b2e8-09b4956d5665",
                    "sense_definition": "Potoczne, nieformalne powitanie, używane często w języku młodzieżowym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Strzała> stary, kopę lat cię nie widziałem!",
                            "target_lang_translations": {
                                "ru": "<Салют>, старик, сто лет тебя не видел!"
                            }
                        },
                        {
                            "text": "Wchodząc do pokoju, rzucił krótkie <strzała> do zgromadzonych.",
                            "target_lang_translations": {
                                "ru": "Входя в комнату, он бросил собравшимся короткое «<салют>»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strzała ziomek"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, неформальное приветствие, часто используемое в молодежном сленге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "салют",
                                "target_lang_sense_clarification": "приветствие"
                            },
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "приветствие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "dopóki",
        "póty"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "09cd12a7-15a1-336e-96f1-7f88a12f8902",
                    "sense_definition": "W czasie, gdy trwa jakaś czynność lub stan; tak długo, jak.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy naprawić dach, <w>póki</w> pogoda jest słoneczna i nie pada deszcz.",
                            "target_lang_translations": {
                                "ru": "Мы должны починить крышу, <w>пока</w> погода солнечная и не идет дождь."
                            }
                        },
                        {
                            "text": "Korzystaj z życia i podróżuj, <w>póki</w> jesteś młody i masz siłę.",
                            "target_lang_translations": {
                                "ru": "Наслаждайся жизнью и путешествуй, <w>пока</w> ты молод и у тебя есть силы."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopóki",
                        "tak długo jak"
                    ],
                    "common_phrases": [
                        "póki co",
                        "póki czas",
                        "póki życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "В то время, когда длится какое-либо действие или состояние; пока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "в значении «в то время как»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcef6278-93f6-36bc-880e-c7124fe404d0",
                    "sense_definition": "Do momentu, w którym coś się wydarzy; zanim.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wychodź z domu, <w>póki</w> nie skończysz odrabiać wszystkich lekcji.",
                            "target_lang_translations": {
                                "ru": "Не выходи из дома, <w>пока</w> не закончишь делать все уроки."
                            }
                        },
                        {
                            "text": "Będę tu na ciebie czekać, <w>póki</w> nie wrócisz ze sklepu.",
                            "target_lang_translations": {
                                "ru": "Я буду ждать тебя здесь, <w>пока</w> ты не вернешься из магазина."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopóki",
                        "aż"
                    ],
                    "common_phrases": [
                        "póki nie",
                        "póki śmierć nie rozłączy"
                    ],
                    "target_lang_definitions": {
                        "ru": "До того момента, как что-то произойдет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "часто с частицей «не» в значении «до тех пор, пока»"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
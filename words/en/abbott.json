                    "sense_id": "cc21b4ce-d21d-3dcc-b4d5-ae6ade12b1b8",
                    "sense_definition": "A surname originating from the occupation of an abbot.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Abbott</w> has been our neighbor for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Эббот</w> — наш сосед уже двадцать лет."
                            }
                        },
                        {
                            "text": "Please send the documents to Sarah <w>Abbott</w> in the accounting department.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте документы Саре <w>Эббот</w> в бухгалтерию."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "originating as an occupation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от названия церковного сана аббата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Эббот",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d10e262c-d516-337b-a25f-d49638a3af80",
                    "sense_definition": "A male given name transferred from the surname.",

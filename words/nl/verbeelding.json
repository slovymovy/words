        "inbeelding"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5716a5ce-6619-386a-8b75-2715003c0454",
                    "sense_definition": "Het vermogen van de geest om beelden, ideeën of mogelijkheden te vormen die niet direct waarneembaar zijn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit prachtige schilderij spreekt enorm tot de <w>verbeelding</w>.",
                            "target_lang_translations": {
                                "ru": "Эта прекрасная картина сильно будоражит <w>воображение</w>."
                            }
                        },
                        {
                            "text": "Je hebt een grote <w>verbeelding</w> nodig om dit verhaal te schrijven.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно богатое <w>воображение</w>, чтобы написать эту историю."
                            }
                        }
                    ],
                    "synonyms": [
                        "fantasie",
                        "verbeeldingskracht"
                    ],
                    "common_phrases": [
                        "tot de verbeelding spreken",
                        "een levendige verbeelding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность ума формировать образы, идеи или возможности, которые не воспринимаются непосредственно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воображение",
                                "target_lang_sense_clarification": "способность создавать образы"
                            },
                            {
                                "target_lang_word": "фантазия",
                                "target_lang_sense_clarification": "творческая способность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fddf54e6-05b4-3155-8faa-f8b5c5a108ea",
                    "sense_definition": "Iets dat men meent waar te nemen, maar dat in werkelijkheid niet bestaat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "illusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Was dat geluid echt of was het louter <w>verbeelding</w>?",
                            "target_lang_translations": {
                                "ru": "Был ли этот звук настоящим или это была лишь <w>игра воображения</w>?"
                            }
                        },
                        {
                            "text": "Zijn angst voor spoken is puur <w>verbeelding</w> en nergens op gebaseerd.",
                            "target_lang_translations": {
                                "ru": "Его страх перед привидениями — это чистый <w>вымысел</w> и ни на чем не основан."
                            }
                        }
                    ],
                    "synonyms": [
                        "fantasie"
                    ],
                    "common_phrases": [
                        "louter verbeelding",
                        "geen verbeelding"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что человек якобы воспринимает, но чего на самом деле не существует."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вымысел",
                                "target_lang_sense_clarification": "нечто придуманное, нереальное"
                            },
                            {
                                "target_lang_word": "игра воображения",
                                "target_lang_sense_clarification": "иллюзия восприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c320813e-a499-330e-843a-de2e00a6e8c7",
                    "sense_definition": "Het proces van zich iets visueel voor de geest halen of ergens een mentaal beeld van vormen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verbeelding</w> van de architectuur in zijn hoofd was zeer gedetailleerd.",
                            "target_lang_translations": {
                                "ru": "<w>Представление</w> архитектуры в его голове было очень детальным."
                            }
                        },
                        {
                            "text": "Door middel van <w>verbeelding</w> probeerde de atleet zijn prestatie te verbeteren.",
                            "target_lang_translations": {
                                "ru": "С помощью <w>визуализации</w> атлет пытался улучшить свои результаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "visualisatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс визуального представления чего-либо или формирования мысленного образа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "визуализация",
                                "target_lang_sense_clarification": "создание зрительного образа"
                            },
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "мысленное воссоздание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
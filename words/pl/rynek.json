    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df3e6707-fa6c-39ad-8513-e2321afafc77",
                    "sense_definition": "Plac w mieście lub wyznaczone miejsce, gdzie kupcy sprzedają towary, zazwyczaj na straganach.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "place_trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiliśmy na <w>rynku</w> świeżą kapustę i mięso na obiad.",
                            "target_lang_translations": {
                                "ru": "Мы купили на <w>рынке</w> свежую капусту и мясо на обед."
                            }
                        },
                        {
                            "text": "W każdą sobotę rano chodzę na <w>rynek</w> po owoce.",
                            "target_lang_translations": {
                                "ru": "Каждую субботу утром я хожу на <w>рынок</w> за фруктами."
                            }
                        }
                    ],
                    "synonyms": [
                        "targowisko",
                        "targ",
                        "bazar",
                        "ryneczek"
                    ],
                    "common_phrases": [
                        "iść na rynek",
                        "świeże warzywa z rynku",
                        "sprzedawać na rynku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место в городе или специально отведенная территория, где торговцы продают товары, обычно с прилавков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "место торговли"
                            },
                            {
                                "target_lang_word": "базар",
                                "target_lang_sense_clarification": "торговые ряды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be71d57b-901c-3118-a5bf-bf8e824ff0e9",
                    "sense_definition": "Główny, centralny plac w mieście, przy którym często znajdują się ważne budynki i który pełni funkcje reprezentacyjne.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_city",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przy naszym <w>rynku</w> znajduje się poczta, kościół i urząd miasta.",
                            "target_lang_translations": {
                                "ru": "На нашей <w>рыночной площади</w> находятся почта, церковь и мэрия."
                            }
                        },
                        {
                            "text": "Wieczorem turyści lubią spacerować po starym <w>rynku</w>.",
                            "target_lang_translations": {
                                "ru": "Вечером туристы любят гулять по старой <w>рыночной площади</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "plac",
                        "centrum"
                    ],
                    "common_phrases": [
                        "rynek główny",
                        "rynek starego miasta",
                        "mieszkać przy rynku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главная, центральная площадь в городе, на которой часто находятся важные здания и которая выполняет представительские функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыночная площадь",
                                "target_lang_sense_clarification": "центральная площадь"
                            },
                            {
                                "target_lang_word": "площадь",
                                "target_lang_sense_clarification": "городское пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33d5dae4-756f-383e-b117-bf529aaf49cb",
                    "sense_definition": "Całokształt stosunków handlowych i gospodarczych dotyczących sprzedaży i kupna towarów lub usług.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "economics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na polskim <w>rynku</w> stali zanotowano ostatnio znaczny wzrost cen.",
                            "target_lang_translations": {
                                "ru": "На польском <w>рынке</w> стали в последнее время отмечен значительный рост цен."
                            }
                        },
                        {
                            "text": "Firma planuje wprowadzić ten nowy produkt na <w>rynek</w>.",
                            "target_lang_translations": {
                                "ru": "Фирма планирует вывести этот новый продукт на <w>рынок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "handel"
                    ],
                    "common_phrases": [
                        "rynek pracy",
                        "rynek nieruchomości",
                        "gospodarka rynkowa",
                        "wejść na rynek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность торговых и экономических отношений, касающихся купли-продажи товаров или услуг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "экономика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2c186ee8-bfcf-3d44-9c66-49d69f702d41",
                    "sense_definition": "To eject saliva or another substance from the mouth.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sem_saliva",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dentist asked him to <w>spit</w> into the small basin.",
                            "target_lang_translations": {
                                "ru": "Стоматолог попросил его <w>сплюнуть</w> в маленькую миску."
                            }
                        },
                        {
                            "text": "She tasted the sour milk and immediately <w>spat</w> it out.",
                            "target_lang_translations": {
                                "ru": "Она попробовала кислое молоко и тут же <w>выплюнула</w> его."
                            }
                        }
                    ],
                    "synonyms": [
                        "expectorate"
                    ],
                    "common_phrases": [
                        "spit it out",
                        "spit up"
                    ],
                    "target_lang_definitions": {

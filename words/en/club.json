    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "639ed399-458c-3280-83bf-bb70ed2635f0",
                    "sense_definition": "An association of members joining together for some common purpose, especially sports or recreation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I decided to join the chess <w>club</w> at school to improve my skills.",
                            "target_lang_translations": {
                                "ru": "Я решил вступить в школьный шахматный <w>клуб</w>, чтобы улучшить свои навыки."
                            }
                        },
                        {
                            "text": "The local football <w>club</w> holds practice every Tuesday evening.",
                            "target_lang_translations": {
                                "ru": "Местный футбольный <w>клуб</w> проводит тренировки каждый вторник вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "team",
                        "confraternity"
                    ],
                    "common_phrases": [
                        "join the club",
                        "sports club",
                        "book club",
                        "youth club"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединение людей с общими интересами, особенно для занятий спортом или отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клуб",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df4cf8b9-6078-39f3-a6f6-72daec921963",
                    "sense_definition": "An establishment that provides staged entertainment, often with food and drink, such as a nightclub.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to a jazz <w>club</w> to listen to live music and relax.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в джазовый <w>клуб</w>, чтобы послушать живую музыку и расслабиться."
                            }
                        },
                        {
                            "text": "The new dance <w>club</w> downtown is always crowded on weekends.",
                            "target_lang_translations": {
                                "ru": "В новом танцевальном <w>клубе</w> в центре города по выходным всегда полно народу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "night club",
                        "dance club",
                        "comedy club",
                        "jazz club"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заведение для развлечений, часто с музыкой, танцами и напитками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клуб",
                                "target_lang_sense_clarification": "заведение"
                            },
                            {
                                "target_lang_word": "ночной клуб",
                                "target_lang_sense_clarification": "развлекательное заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6df2c10c-5469-3584-8bce-c1be74085a75",
                    "sense_definition": "An implement used to hit the ball in golf, consisting of a shaft and a head.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He selected a different <w>club</w> to make the difficult shot onto the green.",
                            "target_lang_translations": {
                                "ru": "Он выбрал другую <w>клюшку</w>, чтобы выполнить сложный удар на грин."
                            }
                        },
                        {
                            "text": "A standard set of golf <w>clubs</w> includes irons, woods, and a putter.",
                            "target_lang_translations": {
                                "ru": "Стандартный набор <w>клюшек</w> для гольфа включает айроны, вуды и паттер."
                            }
                        }
                    ],
                    "common_phrases": [
                        "golf club",
                        "set of clubs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивный снаряд, состоящий из рукоятки и головки, используемый для удара по мячу в гольфе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клюшка",
                                "target_lang_sense_clarification": "спортивный инвентарь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fdfcd92-95c6-3248-b939-9ee33fb412a5",
                    "sense_definition": "A heavy object, often a kind of stick, intended for use as a bludgeoning weapon.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guard carried a heavy wooden <w>club</w> for protection.",
                            "target_lang_translations": {
                                "ru": "Охранник носил с собой тяжёлую деревянную <w>дубинку</w> для защиты."
                            }
                        },
                        {
                            "text": "In ancient times, a simple <w>club</w> was a common tool for hunting and defense.",
                            "target_lang_translations": {
                                "ru": "В древние времена простая <w>дубина</w> была обычным орудием для охоты и защиты."
                            }
                        }
                    ],
                    "synonyms": [
                        "cudgel"
                    ],
                    "common_phrases": [
                        "wooden club"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжёлый предмет, обычно в виде палки, предназначенный для использования в качестве ударного оружия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дубина",
                                "target_lang_sense_clarification": "оружие"
                            },
                            {
                                "target_lang_word": "дубинка",
                                "target_lang_sense_clarification": "полицейская или небольшая"
                            },
                            {
                                "target_lang_word": "палица",
                                "target_lang_sense_clarification": "старинное оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c7ca4fe-1aad-3c82-a115-8fd131dc237c",
                    "sense_definition": "A black clover shape (♣), one of the four symbols used to mark the suits of playing cards.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ace of <w>clubs</w> was the final card dealt on the table.",
                            "target_lang_translations": {
                                "ru": "Туз <w>треф</w> был последней картой, выложенной на стол."
                            }
                        },
                        {
                            "text": "You must follow suit if you have a <w>club</w> in your hand.",
                            "target_lang_translations": {
                                "ru": "Вы должны ходить в масть, если у вас на руках есть <w>крести</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ace of clubs",
                        "king of clubs",
                        "queen of clubs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чёрный знак в форме клевера (♣), одна из четырёх карточных мастей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трефы",
                                "target_lang_sense_clarification": "масть"
                            },
                            {
                                "target_lang_word": "крести",
                                "target_lang_sense_clarification": "масть (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b28e5fbd-b26d-3418-8832-a0e9944e8aae",
                    "sense_definition": "A sandwich typically consisting of three slices of bread and two layers of filling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I ordered a turkey <w>club</w> with a side of fries for lunch.",
                            "target_lang_translations": {
                                "ru": "Я заказал <w>клаб-сэндвич</w> с индейкой и картошкой фри на обед."
                            }
                        },
                        {
                            "text": "The restaurant is famous for its classic chicken <w>club</w> sandwich.",
                            "target_lang_translations": {
                                "ru": "Этот ресторан славится своим классическим <w>клаб-сэндвичем</w> с курицей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "club sandwich",
                        "chicken club",
                        "turkey club"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сэндвич, обычно состоящий из трёх ломтиков хлеба и двух слоёв начинки."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1da314cd-8d5b-3b46-9a43-eaa4439382b8",

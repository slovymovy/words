                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстёгивать",
                                "target_lang_sense_clarification": "долю денег (сленг)"
                            },
                            {
                                "target_lang_word": "делиться",
                                "target_lang_sense_clarification": "общий смысл"
                            }

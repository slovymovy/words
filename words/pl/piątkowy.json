    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "da4d78fd-7b79-3c11-92d4-4d11b1c4f838",
                    "sense_definition": "Związany z piątkiem, odbywający się w ten dzień tygodnia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>piątkowy</w> wieczór zazwyczaj spędzam z przyjaciółmi w kinie.",
                            "target_lang_translations": {
                                "ru": "Мой <w>пятничный</w> вечер я обычно провожу с друзьями в кино."
                            }
                        },
                        {
                            "text": "Czy czytałeś już <w>piątkowy</w> numer naszej lokalnej gazety?",
                            "target_lang_translations": {
                                "ru": "Ты уже читал <w>пятничный</w> номер нашей местной газеты?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "piątkowy wieczór",
                        "piątkowe popołudnie",
                        "piątkowy obiad",
                        "piątkowe wydanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с пятницей, происходящий в этот день недели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятничный",
                                "target_lang_sense_clarification": "день недели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c84fbb6-0918-3eb9-8abb-e18a0959a827",
                    "sense_definition": "Dotyczący oceny bardzo dobrej (piątki) w szkole; o uczniu: taki, który otrzymuje piątki.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat to typowy <w>piątkowy</w> uczeń, który zawsze odrabia prace domowe.",
                            "target_lang_translations": {
                                "ru": "Мой брат — типичный ученик-<w>пятёрочник</w>, который всегда делает домашнее задание."
                            }
                        },
                        {
                            "text": "Nie byłem nigdy uczniem <w>piątkowym</w>, ale radziłem sobie w szkole całkiem nieźle.",
                            "target_lang_translations": {
                                "ru": "Я никогда не был учеником-<w>пятёрочником</w>, но справлялся в школе довольно неплохо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uczeń piątkowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к школьной оценке «пять» (отлично); об ученике: такой, который получает пятёрки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятёрочный",
                                "target_lang_sense_clarification": "об оценке"
                            },
                            {
                                "target_lang_word": "пятёрочник",
                                "target_lang_sense_clarification": "об ученике (сущ.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd01a837-7ace-3a93-9827-ecaeb1b075c6",
                    "sense_definition": "Związany z systemem liczbowym o podstawie pięć.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Piątkowy</w> system liczbowy jest rzadziej stosowany niż dziesiętny.",
                            "target_lang_translations": {
                                "ru": "<w>Пятеричная</w> система счисления используется реже, чем десятичная."
                            }
                        },
                        {
                            "text": "Na lekcji matematyki omawialiśmy pozycyjny system <w>piątkowy</w>.",
                            "target_lang_translations": {
                                "ru": "На уроке математики мы обсуждали позиционную <w>пятеричную</w> систему."
                            }
                        }
                    ],
                    "common_phrases": [
                        "system piątkowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с системой счисления с основанием пять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятеричный",
                                "target_lang_sense_clarification": "математика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрный",
                                "target_lang_sense_clarification": "нелегальный рынок"
                            },
                            {
                                "target_lang_word": "теневой",
                                "target_lang_sense_clarification": "об экономике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7500026-23da-3bd9-af5a-78a87ffaf31c",
                    "sense_definition": "Bad, evil, or ill-omened; associated with tragic or disastrous events.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>black</w> day in the history of the city.",
                            "target_lang_translations": {
                                "ru": "Это был <w>чёрный</w> день в истории города."
                            }
                        },
                        {
                            "text": "He is considered the <w>black</w> sheep of the family.",
                            "target_lang_translations": {
                                "ru": "Его считают <w>паршивой овцой</w> в семье."
                            }
                        }
                    ],
                    "antonyms": [
                        "bright"
                    ],
                    "common_phrases": [
                        "black magic",
                        "black sheep",
                        "black day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плохой, злой или предвещающий несчастье; связанный с трагическими событиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрный",
                                "target_lang_sense_clarification": "трагический"
                            },
                            {
                                "target_lang_word": "мрачный",
                                "target_lang_sense_clarification": "зловещий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6915fe6-7194-32b1-b677-9b546f0d7b22",
                    "sense_definition": "Expressing menace, discontent, or sullen anger.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave him a <w>black</w> look across the table.",
                            "target_lang_translations": {
                                "ru": "Она бросила на него <w>мрачный</w> взгляд через стол."
                            }
                        },
                        {
                            "text": "He has been in a <w>black</w> mood all morning.",
                            "target_lang_translations": {
                                "ru": "У него всё утро было <w>мрачное</w> настроение."
                            }
                        }
                    ],
                    "synonyms": [
                        "gloomy",
                        "sullen"
                    ],
                    "common_phrases": [
                        "black look",
                        "black mood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий угрозу, недовольство или угрюмый гнев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мрачный",
                                "target_lang_sense_clarification": "о взгляде или настроении"
                            },
                            {
                                "target_lang_word": "злобный",
                                "target_lang_sense_clarification": "враждебный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5477e79c-81c0-3aeb-9e0a-ec9ceaf42f34",
                    "sense_definition": "The color perceived in the absence of light, absorbing all wavelengths.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "color_noun_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She dresses in <w>black</w> almost every day.",
                            "target_lang_translations": {
                                "ru": "Она одевается в <w>чёрное</w> почти каждый день."
                            }
                        },
                        {
                            "text": "<w>Black</w> is my favorite color.",
                            "target_lang_translations": {
                                "ru": "<w>Чёрный</w> — мой любимый цвет."
                            }
                        }
                    ],
                    "synonyms": [
                        "blackness"
                    ],
                    "antonyms": [
                        "white",
                        "whiteness"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e85d25a7-fb93-358b-82a7-638886e269ab",
                    "sense_definition": "Sprawiać, że ktoś nie może się poruszyć, mówić lub działać, zazwyczaj pod wpływem silnych emocji, takich jak strach czy stres",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagły strach <paraliżował> ją tak bardzo, że nie mogła wydobyć z siebie głosu.",
                            "target_lang_translations": {
                                "ru": "Внезапный страх <парализовал> её настолько, что она не могла произнести ни звука."
                            }
                        },
                        {
                            "text": "Niepewność często <paraliżuje> nasze działania i powstrzymuje przed podjęciem ryzyka.",
                            "target_lang_translations": {
                                "ru": "Неуверенность часто <парализует> наши действия и удерживает от риска."
                            }
                        }
                    ],
                    "synonyms": [
                        "obezwładniać"
                    ],
                    "antonyms": [
                        "mobilizować",
                        "pobudzać",
                        "aktywizować"
                    ],
                    "common_phrases": [
                        "strach paraliżuje",
                        "paraliżować wolę",
                        "paraliżować myśli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать способности двигаться, говорить или действовать, обычно под влиянием сильных эмоций, таких как страх или стресс"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парализовать",
                                "target_lang_sense_clarification": "лишать способности действовать"
                            },
                            {
                                "target_lang_word": "сковывать",
                                "target_lang_sense_clarification": "о страхе или сильных эмоциях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f007e947-ef61-3354-8dde-39ee63398a2a",
                    "sense_definition": "Uniemożliwiać lub znacznie utrudniać normalne funkcjonowanie czegoś, na przykład instytucji, systemu lub ruchu ulicznego",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "systemic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strajk pracowników kolei <paraliżuje> transport publiczny w całym kraju.",
                            "target_lang_translations": {
                                "ru": "Забастовка железнодорожников <парализует> общественный транспорт по всей стране."
                            }
                        },
                        {
                            "text": "Gwałtowne opady śniegu mogą całkowicie <paraliżować> ruch w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Сильные снегопады могут полностью <парализовать> движение в центре города."
                            }
                        }
                    ],
                    "antonyms": [
                        "usprawniać",
                        "ułatwiać"
                    ],
                    "common_phrases": [
                        "paraliżować ruch",
                        "paraliżować pracę",
                        "paraliżować miasto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать невозможным или значительно затруднять нормальное функционирование чего-либо, например, учреждения, системы или дорожного движения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парализовать",
                                "target_lang_sense_clarification": "останавливать работу системы или движения"
                            },
                            {
                                "target_lang_word": "блокировать",
                                "target_lang_sense_clarification": "препятствовать процессу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a78bebc-7787-3b43-aa80-a5dc85d12632",
                    "sense_definition": "Powodować utratę czucia lub zdolności poruszania się w części ciała na skutek uszkodzenia układu nerwowego",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jad niektórych węży może błyskawicznie <paraliżować> układ nerwowy ofiary.",
                            "target_lang_translations": {
                                "ru": "Яд некоторых змей может мгновенно <парализовать> нервную систему жертвы."
                            }
                        },
                        {
                            "text": "Postępująca choroba zaczęła powoli <paraliżować> jego mięśnie nóg.",
                            "target_lang_translations": {
                                "ru": "Прогрессирующая болезнь начала медленно <парализовать> мышцы его ног."
                            }
                        }
                    ],
                    "antonyms": [
                        "leczyć",
                        "uzdrawiać"
                    ],
                    "common_phrases": [
                        "paraliżować ciało",
                        "paraliżować mięśnie",
                        "paraliżować układ nerwowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать потерю чувствительности или способности двигаться в части тела вследствие повреждения нервной системы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парализовать",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                }
            ]
        }

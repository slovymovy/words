                {
                    "sense_id": "f0e92a8b-b2d4-3b0c-a188-4749cbae6e25",
                    "sense_definition": "Związany z autyzmem, dotyczący autyzmu jako zaburzenia rozwojowego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zdiagnozował u pacjenta zaburzenia <w>autystyczne</w>.",
                            "target_lang_translations": {
                                "ru": "Врач диагностировал у пациента <w>аутистические</w> расстройства."
                            }
                        },
                        {
                            "text": "Wczesne rozpoznanie cech <w>autystycznych</w> jest kluczowe dla dalszej terapii.",
                            "target_lang_translations": {
                                "ru": "Раннее выявление <w>аутистических</w> черт имеет ключевое значение для дальнейшей терапии."
                            }
                        }
                    ],
                    "synonyms": [
                        "autyczny"
                    ],
                    "common_phrases": [
                        "spektrum autystyczne",
                        "zaburzenia autystyczne",
                        "cechy autystyczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с аутизмом, относящийся к аутизму как к расстройству развития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аутистический",
                                "target_lang_sense_clarification": "относящийся к клинической картине, напр. спектр"
                            },
                            {
                                "target_lang_word": "аутичный",
                                "target_lang_sense_clarification": "характеризующийся признаками аутизма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fbc8833-a027-36b3-a11d-bbf9627bca3c",
                    "sense_definition": "Będący w spektrum autyzmu (o osobie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kuba nie jest niegrzeczny, tylko lekko <w>autystyczny</w>.",
                            "target_lang_translations": {
                                "ru": "Куба не непослушный, он просто слегка <w>аутичный</w>."
                            }
                        },
                        {
                            "text": "Nauczyciel musi wiedzieć, jak wspierać ucznia <w>autystycznego</w> w nauce.",
                            "target_lang_translations": {
                                "ru": "Учитель должен знать, как поддерживать <w>аутичного</w> ученика в учебе."
                            }
                        }
                    ],
                    "synonyms": [
                        "autyczny"
                    ],
                    "antonyms": [
                        "neurotypowy"
                    ],
                    "common_phrases": [
                        "dziecko autystyczne",
                        "osoba autystyczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в спектре аутизма (о человеке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аутичный",
                                "target_lang_sense_clarification": "о человеке"

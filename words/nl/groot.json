                {
                    "sense_id": "a294ee92-2709-3d3d-a7ff-23869a0eb9e8",
                    "sense_definition": "Meer dan normaal in formaat of omvang.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk, wat een <w>groot</w> gebouw staat daar aan de overkant van de straat!",
                            "target_lang_translations": {
                                "en": "Look, what a <w>big</w> building stands there across the street!",
                                "ru": "Смотри, какое <w>большое</w> здание стоит там через дорогу!"
                            }
                        },
                        {
                            "text": "De bergpaden waren steiler en de slangen <w>groter</w> dan ik me had voorgesteld.",
                            "target_lang_translations": {
                                "en": "The mountain paths were steeper and the snakes <w>bigger</w> than I had imagined.",
                                "ru": "Горные тропы были круче, а змеи — <w>крупнее</w>, чем я себе представлял."
                            }
                        }
                    ],
                    "antonyms": [
                        "klein"
                    ],
                    "common_phrases": [
                        "een groot huis",
                        "grote ogen",
                        "een grote stad"
                    ],
                    "target_lang_definitions": {
                        "en": "More than normal in size or extent.",
                        "ru": "Больше обычного по размеру или объему."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "big",
                                "target_lang_sense_clarification": "general term for size"
                            },
                            {
                                "target_lang_word": "large",
                                "target_lang_sense_clarification": "often implies dimensions or quantity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "большой",
                                "target_lang_sense_clarification": "о размере"
                            },
                            {
                                "target_lang_word": "крупный",
                                "target_lang_sense_clarification": "значительный по величине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eba918e7-2413-3de4-acea-30a95adcb61e",
                    "sense_definition": "Lang van gestalte (vooral gezegd van mensen).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "height",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is vijf centimeter <w>groter</w> dan zijn jongere broer.",
                            "target_lang_translations": {
                                "en": "He is five centimeters <w>taller</w> than his younger brother.",
                                "ru": "Он на пять сантиметров <w>выше</w> своего младшего брата."
                            }
                        },
                        {
                            "text": "Mijn vader is erg <w>groot</w>, dus hij kan overal gemakkelijk bij.",
                            "target_lang_translations": {
                                "en": "My father is very <w>tall</w>, so he can reach everything easily.",
                                "ru": "Мой отец очень <w>высокий</w>, поэтому он может легко до всего дотянуться."
                            }
                        }
                    ],
                    "synonyms": [
                        "lang"
                    ],
                    "antonyms": [
                        "klein"
                    ],
                    "common_phrases": [
                        "groot zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Tall in stature (especially said of people).",
                        "ru": "Высокого роста (обычно о людях)."
                    },

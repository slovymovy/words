    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8984e89a-655c-3239-b861-b4040d2e3105",
                    "sense_definition": "Włożyć na siebie ubranie, buty, okulary lub inny element garderoby.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <założyć> ciepłą czapkę, ponieważ na zewnątrz jest bardzo zimno.",
                            "target_lang_translations": {
                                "ru": "Ты должен <надеть> теплую шапку, так как на улице очень холодно."
                            }
                        },
                        {
                            "text": "Ona lubi <zakładać> eleganckie sukienki na rodzinne uroczystości.",
                            "target_lang_translations": {
                                "ru": "Она любит <надевать> элегантные платья на семейные торжества."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubrać",
                        "włożyć",
                        "przywdziać",
                        "wdziać"
                    ],
                    "antonyms": [
                        "zdjąć"
                    ],
                    "common_phrases": [
                        "założyć buty",
                        "założyć okulary",
                        "założyć kurtkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеть на себя одежду, обувь, очки или другой предмет гардероба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надеть",
                                "target_lang_sense_clarification": "одежду, обувь, аксессуары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3e05ca1-4674-3c8a-bbdf-52a6d64dafb5",
                    "sense_definition": "Utworzyć coś nowego, np. organizację, firmę, rodzinę lub konto w serwisie; zapoczątkować istnienie czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek postanowił <założyć> własną firmę budowlaną wiele lat temu.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка решил <основать> собственную строительную фирму много лет назад."
                            }
                        },
                        {
                            "text": "Aby korzystać z tego serwisu, musisz najpierw <założyć> darmowe konto.",
                            "target_lang_translations": {
                                "ru": "Чтобы пользоваться этим сервисом, ты должен сначала <создать> бесплатный аккаунт."
                            }
                        }
                    ],
                    "synonyms": [
                        "ufundować",
                        "ustanowić"
                    ],
                    "common_phrases": [
                        "założyć rodzinę",
                        "założyć firmę",
                        "założyć konto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создать что-то новое, например, организацию, компанию, семью или учетную запись; положить начало существованию чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основать",
                                "target_lang_sense_clarification": "компанию, организацию"
                            },
                            {
                                "target_lang_word": "создать",
                                "target_lang_sense_clarification": "семью, аккаунт"
                            },
                            {
                                "target_lang_word": "завести",
                                "target_lang_sense_clarification": "аккаунт, семью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c45b0c8b-1caf-3bb1-a7c3-f36d2ea722eb",
                    "sense_definition": "Zamontować urządzenie techniczne, instalację lub umieścić element medyczny w odpowiednim miejscu, aby spełniał swoją funkcję.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik przyszedł dzisiaj, aby <założyć> nową uszczelkę pod zlewem.",
                            "target_lang_translations": {
                                "ru": "Сантехник пришел сегодня, чтобы <установить> новую прокладку под раковиной."
                            }
                        },
                        {
                            "text": "Lekarz musiał <założyć> pacjentowi gips na złamaną nogę.",
                            "target_lang_translations": {
                                "ru": "Врачу пришлось <наложить> пациенту гипс на сломанную ногу."
                            }
                        }
                    ],
                    "antonyms": [
                        "zdjąć"
                    ],
                    "common_phrases": [
                        "założyć alarm",
                        "założyć gips",
                        "założyć szwy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить техническое устройство или наложить медицинское средство (гипс, швы) для выполнения определенной функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установить",
                                "target_lang_sense_clarification": "оборудование, детали"
                            },
                            {
                                "target_lang_word": "наложить",
                                "target_lang_sense_clarification": "гипс, швы"
                            },
                            {
                                "target_lang_word": "поставить",
                                "target_lang_sense_clarification": "пломбу, устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87ce1b49-b37c-355b-ac3e-50051ed73632",
                    "sense_definition": "Przyjąć coś za pewnik lub punkt wyjścia do dalszych rozważań; uznać coś za prawdopodobne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <założyć>, że ten plan może się nie powieść i przygotować alternatywę.",
                            "target_lang_translations": {
                                "ru": "Мы должны <предположить>, что этот план может провалиться, и подготовить альтернативу."
                            }
                        },
                        {
                            "text": "Autor książki zdaje się <zakładać>, że czytelnik zna historię sztuki.",
                            "target_lang_translations": {
                                "ru": "Автор книги, кажется, <предполагает>, что читатель знает историю искусств."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjąć",
                        "przewidzieć"
                    ],
                    "common_phrases": [
                        "założyć z góry",
                        "założyć teoretycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять что-то за истину или исходную точку для дальнейших рассуждений; счесть вероятным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предположить",
                                "target_lang_sense_clarification": "сделать допущение"
                            },
                            {
                                "target_lang_word": "допустить",
                                "target_lang_sense_clarification": "принять как возможность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e104534b-1477-3067-822f-c51b7696bdb5",
                    "sense_definition": "Zrobić z kimś zakład o coś; postawić coś jako stawkę w zakładzie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chcę się z tobą <założyć>, że nasza drużyna wygra ten mecz.",
                            "target_lang_translations": {
                                "ru": "Я хочу с тобой <поспорить>, что наша команда выиграет этот матч."
                            }
                        },
                        {
                            "text": "Możemy <założyć> się o tabliczkę czekolady, kto pierwszy dobiegnie do celu.",
                            "target_lang_translations": {
                                "ru": "Мы можем <поспорить> на шоколадку, кто первым добежит до финиша."
                            }
                        }
                    ],
                    "common_phrases": [
                        "założyć się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заключить с кем-то пари; сделать ставку на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поспорить",
                                "target_lang_sense_clarification": "заключить пари"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71824d73-a34f-3953-9155-1c05d45c15a2",
                    "sense_definition": "Umieścić coś w określonej pozycji lub miejscu, np. spleść ręce, założyć nogę na nogę lub włożyć zakładkę do książki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas rozmowy kwalifikacyjnej nie wypada <zakładać> nogi na nogę.",
                            "target_lang_translations": {
                                "ru": "Во время собеседования не стоит <класть> ногу на ногу."
                            }
                        },
                        {
                            "text": "Proszę <założyć> ręce i słuchać uważnie tego, co mówi nauczyciel.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <скрестите> руки и слушайте внимательно то, что говорит учитель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "założyć ręce",
                        "założyć nogę na nogę",
                        "założyć włosy za ucho"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить конечности в определенное положение (скрестить руки, положить ногу на ногу) или вложить закладку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "ногу на ногу"
                            },
                            {
                                "target_lang_word": "скрестить",
                                "target_lang_sense_clarification": "руки"
                            },
                            {
                                "target_lang_word": "заложить",
                                "target_lang_sense_clarification": "руки за спину, закладку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c03a37e3-4f33-3dd4-a4af-aa576e0987c6",
                    "sense_definition": "Zapłacić za kogoś własnymi pieniędzmi, oczekując ich późniejszego zwrotu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "6",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możesz <założyć> za mnie pieniądze na bilet do kina, bo zapomniałem portfela?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <заплатить> за меня за билет в кино, так как я забыл кошелек?"
                            }
                        },
                        {
                            "text": "Kolega musiał <założyć> za mój obiad, ale oddałem mu dług następnego dnia.",
                            "target_lang_translations": {
                                "ru": "Коллеге пришлось <заплатить> за мой обед, но я вернул ему долг на следующий день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "założyć za kogoś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts regarding money"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заплатить за кого-то свои деньги с ожиданием их возврата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заплатить",
                                "target_lang_sense_clarification": "за кого-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
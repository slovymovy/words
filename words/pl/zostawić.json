                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "след, отпечаток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17a63dd0-eec9-3213-839f-48e19757ae0f",
                    "sense_definition": "Przekazać majątek lub przedmioty komuś w spadku po swojej śmierci",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "inheritance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziadek <w>zostawił</w> nam w spadku piękny dom na wsi oraz sad.",
                            "target_lang_translations": {
                                "ru": "Дедушка <w>оставил</w> нам в наследство красивый дом в деревне и сад."
                            }

                },
                {
                    "sense_id": "554cc3f7-1a81-3a75-84b1-e5bfa976040f",
                    "sense_definition": "Nazwa geograficzna noszona przez różne miasta i miejscowości na świecie",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Przenosimy się z <w>Edenu</w> do San Francisco.",

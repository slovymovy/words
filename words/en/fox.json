            "senses": [
                {
                    "sense_id": "058748f7-ad3b-3b0e-a38c-ae87b0fa76b8",
                    "sense_definition": "A common surname or male given name.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Fox</w> will be available to meet with you in five minutes.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Фокс</w> сможет встретиться с вами через пять минут."
                            }
                        },
                        {
                            "text": "My neighbor's last name is <w>Fox</w>, and he is very friendly.",
                            "target_lang_translations": {
                                "ru": "Фамилия моего соседа — <w>Фокс</w>, и он очень дружелюбен."
                            }

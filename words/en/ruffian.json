{
    "word_family": [
        "ruffianism",
        "ruffianly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4545a38d-970b-3dfb-98d7-a5de8ee4ea31",
                    "sense_definition": "A violent, lawless, and brutal person, often involved in crime or intimidation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy merchant was attacked by a <w>ruffian</w> while walking through the dark alley.",
                            "target_lang_translations": {
                                "ru": "Богатый купец подвергся нападению <w>бандита</w>, когда шел по темному переулку."
                            }
                        },
                        {
                            "text": "A hired <w>ruffian</w> was sent to intimidate the witnesses before the trial began.",
                            "target_lang_translations": {
                                "ru": "Наемный <w>бандит</w> был послан запугать свидетелей до начала судебного процесса."
                            }
                        }
                    ],
                    "synonyms": [
                        "thug",
                        "hooligan",
                        "hoodlum",
                        "bully",
                        "brute",
                        "rowdy",
                        "scoundrel",
                        "miscreant"
                    ],
                    "antonyms": [
                        "gentleman"
                    ],
                    "common_phrases": [
                        "hired ruffian",
                        "street ruffian"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестокий, агрессивный человек, часто вовлеченный в преступную деятельность; бандит или хулиган."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хулиган",
                                "target_lang_sense_clarification": "человек, грубо нарушающий общественный порядок"
                            },
                            {
                                "target_lang_word": "бандит",
                                "target_lang_sense_clarification": "вооруженный грабитель или преступник"
                            },
                            {
                                "target_lang_word": "буян",
                                "target_lang_sense_clarification": "человек, склонный к буйству и дракам"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3ed765fb-7dbc-3755-8706-29998df147ed",
                    "sense_definition": "Involving or displaying brutality, cruelty, or violence.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>ruffian</w> behavior shocked the polite guests at the dinner party.",
                            "target_lang_translations": {
                                "ru": "Его <w>хулиганское</w> поведение шокировало вежливых гостей на званом ужине."
                            }
                        },
                        {
                            "text": "The villagers feared the <w>ruffian</w> rage of the invading soldiers.",
                            "target_lang_translations": {
                                "ru": "Жители деревни боялись <w>бандитской</w> ярости захватчиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "brutal",
                        "cruel",
                        "violent",
                        "savage"
                    ],
                    "antonyms": [
                        "gentle",
                        "civilized"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубый, жестокий, свойственный хулигану или бандиту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хулиганский",
                                "target_lang_sense_clarification": "свойственный хулигану, грубый"
                            },
                            {
                                "target_lang_word": "бандитский",
                                "target_lang_sense_clarification": "свойственный бандиту, преступный"
                            },
                            {
                                "target_lang_word": "грубый",
                                "target_lang_sense_clarification": "невежливый, резкий, применяющий физическую силу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "edc701d8-2f18-36be-95cc-a1e5dd89a214",
                    "sense_definition": "To act in a rough, violent, or disorderly way.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strong wind <w>ruffianed</w> through the trees, breaking several large branches.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер <w>бушевал</w> среди деревьев, ломая большие ветки."
                            }
                        },
                        {
                            "text": "He <w>ruffianed</w> his way through the crowded tavern, shoving people aside without apology.",
                            "target_lang_translations": {
                                "ru": "Он <w>грубо прокладывал</w> себе путь через переполненную таверну, расталкивая людей без извинений."
                            }
                        }
                    ],
                    "synonyms": [
                        "rage",
                        "brawl"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя грубо, агрессивно или вызывающе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буянить",
                                "target_lang_sense_clarification": "вести себя шумно, затевать ссоры и драки"
                            },
                            {
                                "target_lang_word": "бесчинствовать",
                                "target_lang_sense_clarification": "совершать грубые нарушения порядка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
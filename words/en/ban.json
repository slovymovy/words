                {
                    "sense_id": "b75966a7-3ac5-3e8f-ba9e-39e887656c5f",
                    "sense_definition": "A curse or a formal religious condemnation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "curse_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The priest pronounced a <w>ban</w> upon the heretics.",
                            "target_lang_translations": {
                                "ru": "Священник предал еретиков <w>анафеме</w>."
                            }

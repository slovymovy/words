    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5e3dc7f-2e72-3fbb-a4b7-20b8051d0814",
                    "sense_definition": "Być w odpowiedniej ilości lub liczbie, aby zaspokoić jakąś potrzebę; nie zabraknąć.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te pieniądze powinny spokojnie <w>wystarczyć</w> na codzienne zakupy.",
                            "target_lang_translations": {
                                "ru": "Этих денег должно спокойно <w>хватить</w> на повседневные покупки."
                            }
                        },
                        {
                            "text": "Jedna butelka wody nie <w>wystarczy</w> nam na całą wycieczkę w góry.",
                            "target_lang_translations": {
                                "ru": "Одной бутылки воды нам не <w>хватит</w> на весь поход в горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "starczyć"
                    ],
                    "antonyms": [
                        "zabraknąć",
                        "braknąć"
                    ],
                    "common_phrases": [
                        "wystarczyć do pierwszego",
                        "wystarczyć na życie",
                        "wystarczyć dla wszystkich",
                        "to mi wystarczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть в достаточном количестве или числе, чтобы удовлетворить какую-либо потребность; не закончиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хватить",
                                "target_lang_sense_clarification": "оказаться в достаточном количестве"
                            },
                            {
                                "target_lang_word": "быть достаточным",
                                "target_lang_sense_clarification": "иметься в нужном объеме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93861f14-1464-3c44-ad4a-be5c367b8637",
                    "sense_definition": "Stanowić wystarczający powód lub przyczynę, aby coś się stało lub zostało zrobione.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czasami <w>wystarczy</w> jeden uśmiech, by poprawić komuś humor na cały dzień.",
                            "target_lang_translations": {
                                "ru": "Иногда <w>достаточно</w> одной улыбки, чтобы поднять кому-то настроение на весь день."
                            }
                        },
                        {
                            "text": "<w>Wystarczy</w> nacisnąć ten czerwony przycisk, aby natychmiast zatrzymać maszynę.",
                            "target_lang_translations": {
                                "ru": "<w>Достаточно</w> нажать эту красную кнопку, чтобы немедленно остановить машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "starczyć"
                    ],
                    "common_phrases": [
                        "wystarczy tylko chcieć",
                        "wystarczy powiedzieć słowo",
                        "to by wystarczyło",
                        "wystarczy że"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являться достаточным основанием или причиной для того, чтобы что-то произошло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть достаточным",
                                "target_lang_sense_clarification": "являться необходимым условием"
                            },
                            {
                                "target_lang_word": "хватить",
                                "target_lang_sense_clarification": "послужить достаточным поводом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e36e9c5-c197-3ed3-9320-dc1ed8b61dcc",
                    "sense_definition": "Zastąpić kogoś lub coś, pełniąc tę samą funkcję lub rolę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary, wełniany płaszcz musiał mi <w>wystarczyć</w> za kołdrę podczas zimnej nocy.",
                            "target_lang_translations": {
                                "ru": "Это старое шерстяное пальто должно было <w>заменить</w> мне одеяло холодной ночью."
                            }
                        },
                        {
                            "text": "Jego obecność w zupełności <w>wystarczyła</w> nam za całe towarzystwo.",
                            "target_lang_translations": {
                                "ru": "Его присутствие вполне <w>заменило</w> нам целую компанию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wystarczyć za dwóch",
                        "wystarczyć za odpowiedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заменить кого-то или что-то, выполняя ту же функцию или роль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заменить",
                                "target_lang_sense_clarification": "выполнить функцию чего-либо другого"
                            },
                            {
                                "target_lang_word": "сойти",
                                "target_lang_sense_clarification": "быть принятым вместо чего-то другого"
                            },
                            {
                                "target_lang_word": "послужить",
                                "target_lang_sense_clarification": "использоваться в качестве чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            {
                                "target_lang_word": "выйти сухим из воды",
                                "target_lang_sense_clarification": "идиома: избежать наказания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04164755-e672-3b36-8cce-4097e525c46a",
                    "sense_definition": "To go missing or be stolen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stolen_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you leave your phone on the table, it might <w>walk</w>.",
                            "target_lang_translations": {
                                "ru": "Если оставишь телефон на столе, он может <w>испариться</w>."
                            }
                        },
                        {
                            "text": "Office supplies tend to <w>walk</w> if they are not locked up.",
                            "target_lang_translations": {
                                "ru": "Канцелярские товары имеют обыкновение <w>пропадать</w>, если их не запирать."
                            }
                        }
                    ],
                    "synonyms": [
                        "get stolen",
                        "get nick",
                        "get pinch"

        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "907c8967-e184-38d3-baff-0f86249b85fa",
                    "sense_definition": "Accepted as normal, average, or usual; falling within a common range of quality, size, or amount.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "normal_avg",
                    "name_type": "no",

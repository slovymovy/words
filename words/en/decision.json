    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88a0e303-7bb9-3320-9598-6deb65ca2e13",
                    "sense_definition": "A choice or judgment that you make after thinking about several possibilities.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a tough <w>decision</w>, but I will choose the vanilla ice cream.",
                            "target_lang_translations": {
                                "ru": "Это трудное <w>решение</w>, но я выберу ванильное мороженое."
                            }
                        },
                        {
                            "text": "I told him about my <w>decision</w> to leave the company forever.",
                            "target_lang_translations": {
                                "ru": "Я сказал ему о моем <w>решении</w> покинуть компанию навсегда."
                            }
                        }
                    ],
                    "synonyms": [
                        "choice",
                        "resolution",
                        "conclusion",
                        "judgment"
                    ],
                    "antonyms": [
                        "indecision"
                    ],
                    "common_phrases": [
                        "make a decision",
                        "reach a decision",
                        "final decision",
                        "tough decision",
                        "informed decision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбор или суждение, к которому вы приходите после обдумывания нескольких возможностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решение",
                                "target_lang_sense_clarification": "сделанный выбор, результат обдумывания"
                            },
                            {
                                "target_lang_word": "постановление",
                                "target_lang_sense_clarification": "официальное решение суда или органа власти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06b97490-7ed0-3af5-aab9-fe6c46cdd5d3",
                    "sense_definition": "The ability to decide quickly and with confidence; firmness of conviction.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After agonizing deliberations, they finally proceeded with <w>decision</w>.",
                            "target_lang_translations": {
                                "ru": "После мучительных раздумий они наконец начали действовать с <w>решительностью</w>."
                            }
                        },
                        {
                            "text": "She handled the sudden crisis with great <w>decision</w> and courage.",
                            "target_lang_translations": {
                                "ru": "Она справилась с внезапным кризисом с большой <w>решительностью</w> и мужеством."
                            }
                        }
                    ],
                    "synonyms": [
                        "determination",
                        "decisiveness",
                        "firmness",
                        "resolve"
                    ],
                    "antonyms": [
                        "hesitation",
                        "indecision"
                    ],
                    "common_phrases": [
                        "act with decision",
                        "lack of decision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность принимать решения быстро и уверенно; твердость убеждений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решительность",
                                "target_lang_sense_clarification": "смелость и быстрота в принятии решений"
                            },
                            {
                                "target_lang_word": "решимость",
                                "target_lang_sense_clarification": "отсутствие боязни, готовность к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "007f6e73-60ad-346d-81ec-969e562c26cd",
                    "sense_definition": "A result in a boxing match arrived at by the judges when there is no knockout.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has won twice by knockout and once by <w>decision</w>.",
                            "target_lang_translations": {
                                "ru": "Он дважды побеждал нокаутом и один раз <w>решением судей</w>."
                            }
                        },
                        {
                            "text": "The crowd booed the judges' controversial <w>decision</w> at the end of the fight.",
                            "target_lang_translations": {
                                "ru": "Толпа освистала спорное <w>решение</w> судей в конце боя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "win by decision",
                        "lose by decision",
                        "split decision",
                        "unanimous decision"
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат боксерского поединка, определяемый судьями при отсутствии нокаута."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решение судей",
                                "target_lang_sense_clarification": "вердикт в спорте"
                            },
                            {
                                "target_lang_word": "победа по очкам",
                                "target_lang_sense_clarification": "спортивный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "70692299-a9e8-30d3-b5db-f0f56d0622a7",
                    "sense_definition": "To defeat an opponent in a fight based on points awarded by judges rather than a knockout.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young fighter <w>decisioned</w> the veteran in a grueling twelve-round match.",
                            "target_lang_translations": {
                                "ru": "Молодой боец <w>победил по очкам</w> ветерана в изнурительном двенадцатираундовом поединке."
                            }
                        },
                        {
                            "text": "He was <w>decisioned</w> in his last fight but hopes to win by knockout today.",
                            "target_lang_translations": {
                                "ru": "Он <w>проиграл по очкам</w> в своем последнем бою, но надеется победить нокаутом сегодня."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Победить противника в бою на основании очков, присужденных судьями, а не нокаутом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "победить по очкам",
                                "target_lang_sense_clarification": "выиграть решением судей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
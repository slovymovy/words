                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "туристическая, отдыха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a805d441-41f4-3c46-b9d0-8dd60a8e58e6",
                    "sense_definition": "Preparat kosmetyczny nakładany na skórę przed użyciem właściwego kosmetyku w celu jego utrwalenia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "substance_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dobra <w>baza</w> pod makijaż sprawi, że podkład utrzyma się przez cały dzień.",
                            "target_lang_translations": {
                                "ru": "Хорошая <w>база</w> под макияж обеспечит стойкость тонального крема на весь день."
                            }
                        },
                        {
                            "text": "Kosmetyczka nałożyła na twarz klientki wygładzającą <w>bazę</w> silikonową.",
                            "target_lang_translations": {
                                "ru": "Косметолог нанесла на лицо клиентки разглаживающую силиконовую <w>базу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "baza pod makijaż",
                        "baza hybrydowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство, наносимое на кожу перед использованием основной косметики для её закрепления."

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0128a20-3091-3f20-884b-b8c4573b7059",
                    "sense_definition": "Een nomadisch volk in Oost-Afrika, voornamelijk in Kenia en Tanzania, dat traditioneel leeft van veeteelt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "volk",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "De <w>Masai</w> staan bekend om hun kleurrijke kleding en springdansen.",
                            "target_lang_translations": {
                                "en": "The <w>Maasai</w> are known for their colorful clothing and jumping dances.",
                                "ru": "<w>Масаи</w> известны своей яркой одеждой и прыжковыми танцами."
                            }
                        },
                        {
                            "text": "Veel toeristen bezoeken de dorpen van de <w>Masai</w> tijdens een safari.",
                            "target_lang_translations": {
                                "en": "Many tourists visit the villages of the <w>Maasai</w> during a safari.",
                                "ru": "Многие туристы посещают деревни <w>масаи</w> во время сафари."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de cultuur van de <w>Masai</w>",
                        "het volk de <w>Masai</w>"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Kenia en Tanzania"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A nomadic people in East Africa, primarily in Kenya and Tanzania, who traditionally live by cattle herding.",
                        "ru": "Кочевой народ в Восточной Африке, проживающий в основном в Кении и Танзании и традиционно занимающийся скотоводством."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Maasai",
                                "target_lang_sense_clarification": "ethnic group"
                            },
                            {
                                "target_lang_word": "Masai",
                                "target_lang_sense_clarification": "ethnic group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "масаи",

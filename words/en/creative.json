    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7342b70e-9e1a-3499-84a0-ea0eff3a809b",
                    "sense_definition": "Having the ability or power to create things, especially in an original or novel way.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ability_to_create",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a <w>creative</w> dramatist who avoids cliche in her plays.",
                            "target_lang_translations": {
                                "ru": "Она <w>творческий</w> драматург, избегающий клише в своих пьесах."
                            }
                        },
                        {
                            "text": "The future belongs to those <w>creative</w> enough to build something better.",
                            "target_lang_translations": {
                                "ru": "Будущее принадлежит тем, кто достаточно <w>созидателен</w>, чтобы построить что-то лучшее."
                            }
                        }
                    ],
                    "synonyms": [
                        "inventive"
                    ],
                    "antonyms": [
                        "imitative"
                    ],
                    "common_phrases": [
                        "creative mind",
                        "creative thinking",
                        "creative team"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий способностью создавать что-то новое; созидательный, творческий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "творческий",
                                "target_lang_sense_clarification": "связанный с творчеством, созданием нового"
                            },
                            {
                                "target_lang_word": "созидательный",
                                "target_lang_sense_clarification": "направленный на создание, конструктивный"
                            },
                            {
                                "target_lang_word": "креативный",
                                "target_lang_sense_clarification": "современный термин, часто о нестандартном мышлении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa56819b-db5b-3bcb-b7d4-30b43f908e0c",
                    "sense_definition": "Involving the use of imagination or original ideas to produce something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ability_to_create",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need a <w>creative</w> new solution to solve this old problem.",
                            "target_lang_translations": {
                                "ru": "Нам нужно новое <w>креативное</w> решение, чтобы справиться с этой старой проблемой."
                            }
                        },
                        {
                            "text": "The students enjoyed the <w>creative</w> writing exercise in class.",
                            "target_lang_translations": {
                                "ru": "Студентам понравилось упражнение по <w>творческому</w> письму на уроке."
                            }
                        }
                    ],
                    "synonyms": [
                        "original",
                        "inventive"
                    ],
                    "antonyms": [
                        "imitative"
                    ],
                    "common_phrases": [
                        "creative writing",
                        "creative process",
                        "creative arts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с использованием воображения или оригинальных идей для создания чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "творческий",
                                "target_lang_sense_clarification": "относящийся к искусству или процессу создания"
                            },
                            {
                                "target_lang_word": "креативный",
                                "target_lang_sense_clarification": "оригинальный, изобретательный (об идеях, решениях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aea914c5-6287-37ed-a936-03c5e19fa6e3",
                    "sense_definition": "Designed or executed to deceive or mislead, often used to describe manipulative financial practices.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The director was fired for using <w>creative</w> accounting to hide the company's losses.",
                            "target_lang_translations": {
                                "ru": "Директора уволили за использование <w>креативной</w> бухгалтерии для сокрытия убытков компании."
                            }
                        },
                        {
                            "text": "His <w>creative</w> interpretation of the rules got him into trouble with the committee.",
                            "target_lang_translations": {
                                "ru": "Его <w>творческая</w> трактовка правил привела к проблемам с комитетом."
                            }
                        }

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa0178ea-df0d-3528-82af-4f19ce2ba6fa",
                    "sense_definition": "To prefer or choose someone or something because you like them better.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "verb_prefer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These seaside resorts are mostly <w>favoured</w> by families looking for a quiet holiday.",
                            "target_lang_translations": {
                                "ru": "Эти морские курорты в основном <w>предпочитают</w> семьи, ищущие спокойного отдыха."
                            }
                        },
                        {
                            "text": "He generally <w>favours</w> dark chocolate over milk chocolate for dessert.",
                            "target_lang_translations": {
                                "ru": "На десерт он обычно <w>предпочитает</w> темный шоколад молочному."
                            }
                        }
                    ],
                    "synonyms": [
                        "prefer",
                        "choose",
                        "lean toward",
                        "opt for"
                    ],
                    "antonyms": [
                        "reject",
                        "dislike"
                    ],
                    "common_phrases": [
                        "favour one over another",
                        "highly favoured"
                    ],
                    "traits": [

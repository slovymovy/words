{
    "word_family": [
        "poddać",
        "poddanie",
        "poddany",
        "poddaństwo"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9182495-380d-36d7-9eee-92cb657477fe",
                    "sense_definition": "(jako <w>poddawać się</w>) Rezygnować z walki, oporu lub dążenia do celu, uznając swoją porażkę lub niemożność osiągnięcia sukcesu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "submission_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawdziwy sportowiec nigdy się nie <poddaje>, nawet gdy przegrywa.",
                            "target_lang_translations": {
                                "ru": "Настоящий спортсмен никогда не <сдается>, даже когда проигрывает."
                            }
                        },
                        {
                            "text": "Musisz być silny i nie <poddawać> się w trudnych chwilach.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть сильным и не <сдаваться> в трудные моменты."
                            }
                        }
                    ],
                    "synonyms": [
                        "rezygnować",
                        "kapitulować",
                        "ustępować"
                    ],
                    "antonyms": [
                        "walczyć",
                        "opierać się"
                    ],
                    "common_phrases": [
                        "nie poddawać się",
                        "poddawać się bez walki",
                        "łatwo się poddawać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сдаваться, прекращать сопротивление или попытки достичь цели, признавая свое поражение или невозможность успеха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдаваться",
                                "target_lang_sense_clarification": "прекращать борьбу"
                            },
                            {
                                "target_lang_word": "опускать руки",
                                "target_lang_sense_clarification": "терять надежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cff8fdc-ef93-3f07-a93d-1aae42e76f01",
                    "sense_definition": "(jako <w>poddawać się</w>) Ulegać czyjemuś wpływowi, godzić się na jakieś działanie lub pozwalać, by coś nami kierowało.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "submission_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent zgodził się <poddawać> leczeniu w specjalistycznej klinice.",
                            "target_lang_translations": {
                                "ru": "Пациент согласился <подвергнуться> лечению в специализированной клинике."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak łatwo <poddawać> się złemu nastrojowi.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует так легко <поддаваться> плохому настроению."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulegać"
                    ],
                    "common_phrases": [
                        "poddawać się leczeniu",
                        "poddawać się nastrojowi",
                        "poddawać się emocjom",
                        "poddawać się sugestii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддаваться чьему-либо влиянию, соглашаться на какое-либо действие или позволять чему-то управлять собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддаваться",
                                "target_lang_sense_clarification": "уступать воздействию или эмоциям"
                            },
                            {
                                "target_lang_word": "подвергаться",
                                "target_lang_sense_clarification": "проходить процедуру или лечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef217735-4719-3870-8bdb-ded4727b43ef",
                    "sense_definition": "Wystawiać kogoś lub coś na działanie jakiegoś czynnika, procesu, badania lub próby.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W laboratorium musimy <poddawać> próbki działaniu wysokich temperatur.",
                            "target_lang_translations": {
                                "ru": "В лаборатории мы должны <подвергать> образцы воздействию высоких температур."
                            }
                        },
                        {
                            "text": "Każdy nowy pomysł warto <poddawać> konstruktywnej krytyce zespołu.",
                            "target_lang_translations": {
                                "ru": "Каждую новую идею стоит <подвергать> конструктивной критике команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystawiać",
                        "eksponować"
                    ],
                    "common_phrases": [
                        "poddawać próbie",
                        "poddawać analizie",
                        "poddawać krytyce",
                        "poddawać obróbce",
                        "poddawać weryfikacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать кого-либо или что-либо воздействию какого-либо фактора, процесса, исследования или испытания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвергать",
                                "target_lang_sense_clarification": "ставить под воздействие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68528e3b-abba-3550-90bc-66b4c4a5e119",
                    "sense_definition": "Oddawać coś, np. terytorium lub partię gry, przeciwnikowi, rezygnując z dalszej obrony lub walki.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "submission_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Generał zdecydował się <poddawać> kolejne miasta, aby ocalić ludność cywilną.",
                            "target_lang_translations": {
                                "ru": "Генерал решил <сдавать> один город за другим, чтобы спасти мирное население."
                            }
                        },
                        {
                            "text": "Widząc przewagę mistrza, zawodnik wolał <poddawać> partię niż grać do końca.",
                            "target_lang_translations": {
                                "ru": "Видя преимущество мастера, игрок предпочел <сдать> партию, чем играть до конца."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddawać"
                    ],
                    "common_phrases": [
                        "poddawać mecz",
                        "poddawać miasto",
                        "poddawać partię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдавать что-либо, например территорию или партию в игре, противнику, отказываясь от дальнейшей защиты или борьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "отдавать врагу или сопернику"
                            },
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "отдавать без боя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01465787-adf5-316a-b94a-eac6a73c0a40",
                    "sense_definition": "Podsuwać komuś myśl, pomysł lub temat do rozważenia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "suggestion_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym <poddawać> pod rozwagę kwestię ochrony środowiska w naszej gminie.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <вынести> на рассмотрение вопрос охраны окружающей среды в нашей гмине."
                            }
                        },
                        {
                            "text": "Nauczyciel lubił <poddawać> uczniom trudne zagadnienia do samodzielnego przemyślenia.",
                            "target_lang_translations": {
                                "ru": "Учитель любил <предлагать> ученикам трудные вопросы для самостоятельного обдумывания."
                            }
                        }
                    ],
                    "synonyms": [
                        "podsuwać",
                        "sugerować",
                        "podpowiadać"
                    ],
                    "common_phrases": [
                        "poddawać myśl",
                        "poddawać pod rozwagę",
                        "poddawać pod dyskusję",
                        "poddawać pomysł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлагать кому-либо мысль, идею или тему для размышления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "на обсуждение или рассмотрение"
                            },
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "давать идею"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
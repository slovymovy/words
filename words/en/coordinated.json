    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ff5cc1d4-7e80-35bf-8fc0-e1938409e9cb",
                    "sense_definition": "Planned or organized so that different parts or people work together effectively.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "organized_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police launched a <w>coordinated</w> operation to catch the group of thieves.",
                            "target_lang_translations": {
                                "ru": "Полиция начала <w>скоординированную</w> операцию по поимке группы воров."
                            }
                        },
                        {
                            "text": "We need a <w>coordinated</w> effort from all departments to finish the project on time.",
                            "target_lang_translations": {
                                "ru": "Нам нужны <w>согласованные</w> усилия всех отделов, чтобы завершить проект вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "organized",
                        "integrated",
                        "synchronized",
                        "unified"
                    ],
                    "antonyms": [
                        "uncoordinated"
                    ],
                    "common_phrases": [
                        "coordinated effort",
                        "coordinated attack",
                        "coordinated approach",
                        "coordinated response"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спланированный или организованный таким образом, чтобы различные части или люди эффективно работали вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скоординированный",
                                "target_lang_sense_clarification": "об организованных действиях"
                            },
                            {
                                "target_lang_word": "согласованный",
                                "target_lang_sense_clarification": "о совместных усилиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5533a11-d6cc-374e-a379-9e52c1eb305f",
                    "sense_definition": "Able to move different parts of the body together well or gracefully.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a very <w>coordinated</w> dancer who learns complex steps quickly.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>координированная</w> танцовщица, которая быстро разучивает сложные движения."
                            }
                        },
                        {
                            "text": "I am not <w>coordinated</w> enough to play tennis very well.",
                            "target_lang_translations": {
                                "ru": "Я недостаточно <w>координирован</w>, чтобы хорошо играть в теннис."
                            }
                        }
                    ],
                    "synonyms": [
                        "graceful",
                        "dexterous",
                        "agile"
                    ],
                    "antonyms": [
                        "uncoordinated"
                    ],
                    "common_phrases": [
                        "highly coordinated",
                        "physically coordinated",
                        "not very coordinated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный хорошо и грациозно управлять движениями своего тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "координированный",
                                "target_lang_sense_clarification": "о человеке или движениях"

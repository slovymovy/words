        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad63ae19-11f0-3933-bdac-c39c7f0c878c",
                    "sense_definition": "To make something dirty, untidy, or disordered.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "disorder_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>mess</w> the floor with your muddy boots.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>пачкай</w> пол своими грязными ботинками."
                            }
                        },
                        {
                            "text": "The wind will <w>mess</w> your hair if you do not wear a hat outside.",
                            "target_lang_translations": {
                                "ru": "Ветер <w>растреплет</w> твои волосы, если ты не наденешь шапку на улице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mess up",
                        "mess about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо грязным, неопрятным или приводить в беспорядок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пачкать",
                                "target_lang_sense_clarification": "делать грязным"
                            },
                            {
                                "target_lang_word": "приводить в беспорядок",
                                "target_lang_sense_clarification": "нарушать порядок"
                            },
                            {
                                "target_lang_word": "растрепать",
                                "target_lang_sense_clarification": "о волосах"
                            }
                        ]
                    }
                },
                {

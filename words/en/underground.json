            "senses": [
                {
                    "sense_id": "47fdeecc-f578-3f82-9261-4b4a861c6f59",
                    "sense_definition": "Below the ground; below the surface of the Earth.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is an <w>underground</w> tunnel that takes you across the river.",

{
    "word_family": [
        "hoop",
        "hoopvol",
        "hopeloos",
        "wanhopen",
        "ophopen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ea8fa51d-34eb-3040-a31e-37aae8afd602",
                    "sense_definition": "Wensen of graag zien dat er iets wel of niet gebeurt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>hoopte</w> dat hij zijn proefwerk had gehaald.",
                            "target_lang_translations": {
                                "en": "He <w>hoped</w> that he had passed his test.",
                                "ru": "Он <w>надеялся</w>, что сдал контрольную."
                            }
                        },
                        {
                            "text": "We <w>hoopten</w> op goed weer tijdens onze vakantie.",
                            "target_lang_translations": {
                                "en": "We <w>hoped</w> for good weather during our vacation.",
                                "ru": "Мы <w>надеялись</w> на хорошую погоду во время нашего отпуска."
                            }
                        },
                        {
                            "text": "Ik <w>hoop</w> dat je snel weer beter wordt.",
                            "target_lang_translations": {
                                "en": "I <w>hope</w> you get better soon.",
                                "ru": "Я <w>надеюсь</w>, что ты скоро поправишься."
                            }
                        }
                    ],
                    "synonyms": [
                        "wensen",
                        "verlangen"
                    ],
                    "antonyms": [
                        "vrezen",
                        "wanhopen"
                    ],
                    "common_phrases": [
                        "hopen op",
                        "het is te hopen",
                        "blijven hopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To wish or desire for something to happen or not happen.",
                        "ru": "Желать или хотеть, чтобы что-то произошло или не произошло."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hope",
                                "target_lang_sense_clarification": "to want something to happen"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "надеяться",
                                "target_lang_sense_clarification": "ожидать исполнения желаемого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e0a1687-ad41-380d-8f4b-0fa7ed7a9ac7",
                    "sense_definition": "Opstapelen of op een hoop gooien.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer <w>hoopte</w> het hooi op de wagen.",
                            "target_lang_translations": {
                                "en": "The farmer <w>piled</w> the hay onto the wagon.",
                                "ru": "Фермер <w>сгребал</w> сено на повозку."
                            }
                        },
                        {
                            "text": "Ze <w>hopen</w> de stenen op elkaar om een muurtje te bouwen.",
                            "target_lang_translations": {
                                "en": "They <w>pile</w> the stones on top of each other to build a small wall.",
                                "ru": "Они <w>складывают</w> камни друг на друга, чтобы построить стену."
                            }
                        }
                    ],
                    "synonyms": [
                        "opstapelen",
                        "ophopen"
                    ],
                    "antonyms": [
                        "verspreiden"
                    ],
                    "target_lang_definitions": {
                        "en": "To stack or throw onto a pile.",
                        "ru": "Складывать в кучу или нагромождать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pile up",
                                "target_lang_sense_clarification": "to accumulate or stack"
                            },
                            {
                                "target_lang_word": "heap",
                                "target_lang_sense_clarification": "to put in a pile"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сгребать",
                                "target_lang_sense_clarification": "собирать в кучу"
                            },
                            {
                                "target_lang_word": "нагромождать",
                                "target_lang_sense_clarification": "беспорядочно складывать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a34d7c53-a9e2-342a-b006-ac81e041817b",
                    "sense_definition": "Meervoud van <w>hoop</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de herfst liggen er overal <w>hopen</w> bladeren op de stoep.",
                            "target_lang_translations": {
                                "en": "In autumn, there are <w>piles</w> of leaves everywhere on the sidewalk.",
                                "ru": "Осенью повсюду на тротуаре лежат <w>кучи</w> листьев."
                            }
                        },
                        {
                            "text": "Er lagen grote <w>hopen</w> zand klaar voor de verbouwing.",
                            "target_lang_translations": {
                                "en": "Large <w>heaps</w> of sand lay ready for the renovation.",
                                "ru": "Для ремонта были заготовлены большие <w>кучи</w> песка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hopen werk",
                        "hopen geld"
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of <w>hoop</w> (heap).",
                        "ru": "Множественное число слова 'hoop' (куча)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heaps",
                                "target_lang_sense_clarification": "large piles or amounts"
                            },
                            {
                                "target_lang_word": "piles",
                                "target_lang_sense_clarification": "stacks or mounds"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кучи",
                                "target_lang_sense_clarification": "скопления предметов"
                            },
                            {
                                "target_lang_word": "груды",
                                "target_lang_sense_clarification": "большие кучи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
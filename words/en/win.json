                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My friend <w>Win</w> is coming to visit us tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мой друг <w>Уин</w> приедет навестить нас завтра."
                            }
                        },
                        {
                            "text": "He prefers to be called <w>Win</w> instead of his full name.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает, чтобы его звали <w>Уин</w>, а не полным именем."
                            }
                        }
                    ],
                    "target_lang_definitions": {

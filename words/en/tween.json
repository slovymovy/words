                        "tween fashion",
                        "tween audience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к детям в возрасте примерно от 8 до 12 лет или подходящий для них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для младших подростков",
                                "target_lang_sense_clarification": "о товарах или контенте"
                            },
                            {
                                "target_lang_word": "подростковый",
                                "target_lang_sense_clarification": "общий термин"
                            }

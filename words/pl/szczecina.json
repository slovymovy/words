        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "85e9b4ab-6d72-3bbb-a3ca-e0c6dbc37be3",
                    "sense_definition": "twarda, sztywna sierść zwierząt, zwłaszcza świń i dzików, używana często do produkcji szczotek",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hair_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najlepsze pędzle malarskie wykonuje się z naturalnej <w>szczeciny</w>.",
                            "target_lang_translations": {
                                "ru": "Лучшие малярные кисти делают из натуральной <w>щетины</w>."
                            }
                        },
                        {
                            "text": "Dzik posiadał gęstą i ciemną <w>szczecinę</w> na grzbiecie.",
                            "target_lang_translations": {
                                "ru": "У кабана была густая и тёмная <w>щетина</w> на спине."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczeć"
                    ],
                    "common_phrases": [
                        "szczecina dzika",
                        "naturalna szczecina",
                        "pędzel ze szczeciny"
                    ],
                    "target_lang_definitions": {
                        "ru": "жёсткая шерсть животных, особенно свиней и кабанов, часто используемая для производства щёток"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щетина",
                                "target_lang_sense_clarification": "шерсть животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e635c94b-1eeb-3b75-a2cc-9a401546583e",
                    "sense_definition": "krótki, twardy i kłujący zarost na twarzy mężczyzny",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hair_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego twarz pokrywała kilkudniowa, szorstka <w>szczecina</w>.",
                            "target_lang_translations": {
                                "ru": "Его лицо покрывала многодневная жёсткая <w>щетина</w>."
                            }
                        },
                        {
                            "text": "Poczuła na policzku jego kłującą <w>szczecinę</w> podczas powitania.",
                            "target_lang_translations": {
                                "ru": "При приветствии она почувствовала на щеке его колючую <w>щетину</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarost kilkudniowy"
                    ],
                    "common_phrases": [
                        "twarda szczecina",
                        "kłująca szczecina",
                        "siwa szczecina"
                    ],
                    "target_lang_definitions": {
                        "ru": "короткая, жёсткая и колючая растительность на лице мужчины"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щетина",
                                "target_lang_sense_clarification": "на лице"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "553f6116-459a-3477-ba29-ff0ded0a3089",
                    "sense_definition": "sztywny włosek lub wyrostek występujący na ciele niektórych bezkręgowców",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod mikroskopem widać wyraźnie każdą pojedynczą <w>szczecinę</w> dżdżownicy.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом отчётливо видна каждая отдельная <w>щетинка</w> дождевого червя."
                            }
                        },
                        {
                            "text": "Biolog badał funkcje czuciowe, jakie pełni <w>szczecina</w> u tego owada.",
                            "target_lang_translations": {
                                "ru": "Биолог изучал сенсорные функции, которые выполняет <w>щетинка</w> у этого насекомого."
                            }
                        }
                    ],
                    "target_lang_definitions": {

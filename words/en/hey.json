{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f78780db-1e08-3da0-9fbd-efbd8073322f",
                    "sense_definition": "An informal greeting, similar to hi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hey</w>, are you coming to the cinema with us?",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>, ты идешь с нами в кино?"
                            }
                        },
                        {
                            "text": "<w>Hey</w> there, it is really good to see you again.",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>, очень рад тебя снова видеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "hi",
                        "hello",
                        "heyo"
                    ],
                    "common_phrases": [
                        "hey there",
                        "hey man",
                        "hey guys",
                        "hey you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное приветствие, похожее на «привет»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "стандартное неформальное приветствие"
                            },
                            {
                                "target_lang_word": "здорово",
                                "target_lang_sense_clarification": "разговорное приветствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00391d3f-c721-3d5d-acd4-4528aeca94d3",
                    "sense_definition": "An exclamation to get attention.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attention_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hey</w>, you dropped your wallet on the floor.",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, вы уронили кошелек на пол."
                            }
                        },
                        {
                            "text": "<w>Hey</w>, listen to what I am trying to tell you.",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, послушай, что я пытаюсь тебе сказать."
                            }
                        }
                    ],
                    "synonyms": [
                        "oi",
                        "yo",
                        "ahem"
                    ],
                    "common_phrases": [
                        "hey wait",
                        "hey look",
                        "hey listen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас для привлечения внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fa41de4-359a-3552-852a-2d362400af4d",
                    "sense_definition": "A protest or reprimand.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "protest_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hey</w>! You cannot park your car in front of the exit.",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>! Вы не можете парковать машину перед выездом."
                            }
                        },
                        {
                            "text": "<w>Hey</w>, watch where you are going with that heavy box!",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, смотри, куда идешь с этой тяжелой коробкой!"
                            }
                        }
                    ],
                    "synonyms": [
                        "oi"
                    ],
                    "common_phrases": [
                        "hey stop that",
                        "hey watch it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас протеста или порицания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "выражение недовольства"
                            },
                            {
                                "target_lang_word": "э",
                                "target_lang_sense_clarification": "резкий протест"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82612c3a-0dc0-3343-9e9a-09ea4a3d7b6a",
                    "sense_definition": "An expression of surprise.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "surprise_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hey</w>, I actually won the first prize in the lottery!",
                            "target_lang_translations": {
                                "ru": "<w>Ого</w>, я действительно выиграл первый приз в лотерею!"
                            }
                        },
                        {
                            "text": "<w>Hey</w>! This new restaurant is much better than I expected.",
                            "target_lang_translations": {
                                "ru": "<w>Ух ты</w>! Этот новый ресторан гораздо лучше, чем я ожидал."
                            }
                        }
                    ],
                    "synonyms": [
                        "wow",
                        "blimey",
                        "gee whiz"
                    ],
                    "common_phrases": [
                        "hey look at that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение удивления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ого",
                                "target_lang_sense_clarification": "сильное удивление"
                            },
                            {
                                "target_lang_word": "ух ты",
                                "target_lang_sense_clarification": "удивление или восхищение"
                            },
                            {
                                "target_lang_word": "о",
                                "target_lang_sense_clarification": "внезапная реакция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "feae8a53-0f52-3585-88e6-8e6cdc2b0552",
                    "sense_definition": "A request for repetition or explanation; an expression of confusion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "confusion_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hey</w>? I did not hear what you just said.",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>? Я не расслышал, что ты только что сказал."
                            }
                        },
                        {
                            "text": "<w>Hey</w>? How is that even possible given the circumstances?",
                            "target_lang_translations": {
                                "ru": "<w>А</w>? Как это вообще возможно при таких обстоятельствах?"
                            }
                        }
                    ],
                    "synonyms": [
                        "eh",
                        "huh"
                    ],
                    "target_lang_definitions": {
                        "ru": "Просьба повторить или объяснить; выражение замешательства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "вопрос при непонимании"
                            },
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "разговорный переспрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48d02858-b506-35ac-b79d-ba3a32a31c45",
                    "sense_definition": "Used as a tag question, to emphasise what goes before or to request that the listener express an opinion about what has been said.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "tag_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is getting quite cold out here, <w>hey</w>?",
                            "target_lang_translations": {
                                "ru": "Здесь становится довольно холодно, <w>да</w>?"
                            }
                        },
                        {
                            "text": "You are coming to the game with us, <w>hey</w>?",
                            "target_lang_translations": {
                                "ru": "Ты идешь с нами на игру, <w>правда</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "eh"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "common in South African, Canadian, and some US dialects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в конце предложения для усиления сказанного или вопроса к слушателю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "вопрос-подтверждение"
                            },
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "запрос согласия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f018713b-20aa-36fd-b2c5-2d39a29c3567",
                    "sense_definition": "A meaningless beat marker or extra, filler syllable in song lyrics.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd sang along with the chorus, shouting \"<w>hey</w> <w>hey</w> <w>hey</w>\".",
                            "target_lang_translations": {
                                "ru": "Толпа подпевала припеву, выкрикивая «<w>хей</w> <w>хей</w> <w>хей</w>»."
                            }
                        },
                        {
                            "text": "The song includes a catchy \"na na na <w>hey</w> goodbye\" line.",
                            "target_lang_translations": {
                                "ru": "В песне есть запоминающаяся строчка «на-на-на <w>хей</w> гудбай»."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бессмысленный слог для ритма в песнях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хей",
                                "target_lang_sense_clarification": "в текстах песен"
                            },
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "в текстах песен"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "87a8ccef-3525-3956-9487-82cc33f4221e",
                    "sense_definition": "To greet with a \"hey\".",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She walked into the room and <w>heyed</w> everyone she knew.",
                            "target_lang_translations": {
                                "ru": "Она вошла в комнату и <w>поздоровалась</w> со всеми знакомыми."
                            }
                        },
                        {
                            "text": "I <w>heyed</w> him from across the street, but he did not notice.",
                            "target_lang_translations": {
                                "ru": "Я <w>окликнул</w> его с другой стороны улицы, но он не заметил."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствовать словом «эй» или «привет»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздороваться",
                                "target_lang_sense_clarification": "сказать привет"
                            },
                            {
                                "target_lang_word": "окликнуть",
                                "target_lang_sense_clarification": "позвать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

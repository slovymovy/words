{
    "word_family": [
        "rest",
        "resteren",
        "restering"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b88c4fbf-02f9-30b2-89f1-db239b3da5a2",
                    "sense_definition": "Wat overgebleven is van een groter geheel nadat de rest is gebruikt, weggehaald of verwerkt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben het <w>restant</w> van de taart mee naar huis genomen.",
                            "target_lang_translations": {
                                "ru": "Мы забрали <w>остаток</w> торта домой.",
                                "en": "We took the <w>remainder</w> of the cake home."
                            }
                        },
                        {
                            "text": "In de hoek van de kamer lag nog een klein <w>restant</w> bouwafval.",
                            "target_lang_translations": {
                                "ru": "В углу комнаты лежал небольшой <w>остаток</w> строительного мусора.",
                                "en": "In the corner of the room lay a small <w>remnant</w> of construction waste."
                            }
                        }
                    ],
                    "synonyms": [
                        "overschot",
                        "rest",
                        "overblijfsel"
                    ],
                    "antonyms": [
                        "geheel",
                        "totaal"
                    ],
                    "common_phrases": [
                        "een klein restant",
                        "het restant van"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что осталось от целого после того, как остальная часть была использована, убрана или переработана.",
                        "en": "That which remains of a larger whole after the rest has been used, removed, or processed."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "остатки",
                                "target_lang_sense_clarification": "часто во мн. ч. для еды или материалов"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "remainder",
                                "target_lang_sense_clarification": "general sense"
                            },
                            {
                                "target_lang_word": "remnant",
                                "target_lang_sense_clarification": "often for materials or physical traces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fae7937-05d3-3817-b6ba-e1340d33a3e0",
                    "sense_definition": "Het deel van een bedrag, levering of taak dat nog voldaan of uitgevoerd moet worden.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U moet het <w>restant</w> van de rekening binnen twee weken betalen.",
                            "target_lang_translations": {
                                "ru": "Вы должны оплатить <w>остаток</w> по счету в течение двух недель.",
                                "en": "You must pay the <w>balance</w> of the bill within two weeks."
                            }
                        },
                        {
                            "text": "Het <w>restant</w> van de bestelling wordt morgen geleverd.",
                            "target_lang_translations": {
                                "ru": "<w>Остаток</w> заказа будет доставлен завтра.",
                                "en": "The <w>remainder</w> of the order will be delivered tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "restbedrag",
                        "saldo"
                    ],
                    "common_phrases": [
                        "het verschuldigde restant",
                        "restant betalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть суммы, поставки или задачи, которую еще предстоит оплатить, доставить или выполнить.",
                        "en": "The part of an amount, delivery, or task that still needs to be paid, delivered, or performed."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "суммы, счета или заказа"
                            },
                            {
                                "target_lang_word": "оставшаяся часть",
                                "target_lang_sense_clarification": "работы или поставки"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "balance",
                                "target_lang_sense_clarification": "financial context"
                            },
                            {
                                "target_lang_word": "remainder",
                                "target_lang_sense_clarification": "of a task or delivery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e4dfe23-b574-3936-846b-331bc6461c2c",
                    "sense_definition": "Een openstaande schuld waarvan men verwacht dat deze niet meer betaald zal worden.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "financial_obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bank heeft het <w>restant</w> uiteindelijk als verlies afgeboekt.",
                            "target_lang_translations": {
                                "ru": "В итоге банк списал этот <w>остаток</w> в убыток.",
                                "en": "The bank finally wrote off the <w>balance</w> as a loss."
                            }
                        },
                        {
                            "text": "Dit <w>restant</w> is al jaren oud en zal waarschijnlijk nooit worden geïnd.",
                            "target_lang_translations": {
                                "ru": "Этому <w>остатку</w> уже много лет, и он, вероятно, никогда не будет взыскан.",
                                "en": "This <w>outstanding balance</w> is years old and will probably never be collected."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Непогашенная часть долга, возврат которой считается маловероятным.",
                        "en": "An outstanding debt that is expected never to be paid."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "долга"
                            },
                            {
                                "target_lang_word": "непогашенный остаток",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "outstanding balance",
                                "target_lang_sense_clarification": "unpaid debt"
                            },
                            {
                                "target_lang_word": "residual",
                                "target_lang_sense_clarification": "financial/technical"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "6dbf3ebf-b92f-334f-b3f3-e67ab3360108",
                    "sense_definition": "An acronym for <w>real</w> <w>estate</w> <w>investment</w> <w>trust</w>, a company that owns, operates, or finances income-generating real estate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Investors often choose a <w>REIT</w> to gain exposure to the property market without buying physical buildings.",
                            "target_lang_translations": {
                                "ru": "Инвесторы часто выбирают <w>инвестиционный траст недвижимости</w>, чтобы получить доступ к рынку недвижимости без покупки физических зданий."
                            }
                        },
                        {
                            "text": "The company operates as a <w>REIT</w> and distributes most of its taxable income to shareholders.",
                            "target_lang_translations": {
                                "ru": "Компания работает как <w>инвестиционный траст недвижимости</w> и распределяет большую часть своего налогооблагаемого дохода между акционерами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "invest in a REIT",
                        "publicly traded REIT",
                        "commercial REIT"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инвестиционный траст недвижимости — компания, которая владеет, управляет или финансирует приносящую доход недвижимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инвестиционный траст недвижимости",
                                "target_lang_sense_clarification": "финансовая структура"
                            },
                            {
                                "target_lang_word": "инвестиционный фонд недвижимости",
                                "target_lang_sense_clarification": "тип организации"
                            }
                        ]
                    }
                },

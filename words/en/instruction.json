{
    "word_family": [
        "instruct",
        "instructor",
        "instructional",
        "instructive"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9cc7fbb-1cd6-3a11-9689-2a40f318e7a8",
                    "sense_definition": "A set of directions provided by a manufacturer for the users of a product or service.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please read the <w>instructions</w> carefully before assembling the furniture.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, внимательно прочитайте <w>инструкцию</w> перед сборкой мебели."
                            }
                        },
                        {
                            "text": "The box contains full <w>instructions</w> on how to operate the machine.",
                            "target_lang_translations": {
                                "ru": "В коробке находится полная <w>инструкция</w> по эксплуатации машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "teaching",
                        "education"
                    ],
                    "common_phrases": [
                        "follow instructions",
                        "read the instructions",
                        "operating instructions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор указаний от производителя для пользователей продукта или услуги"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инструкция",
                                "target_lang_sense_clarification": "документ с правилами использования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ad274ad-95b5-3e25-a9cf-5f82c1393b02",
                    "sense_definition": "An authoritative order or command.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers received strict <w>instructions</w> to stay at their posts.",
                            "target_lang_translations": {
                                "ru": "Солдаты получили строгие <w>указания</w> оставаться на своих постах."
                            }
                        },
                        {
                            "text": "I have <w>instructions</w> to call the manager immediately if there is a problem.",
                            "target_lang_translations": {
                                "ru": "У меня есть <w>указания</w> немедленно позвонить менеджеру в случае возникновения проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "advice",
                        "counsel"
                    ],
                    "common_phrases": [
                        "strict instructions",
                        "await instructions",
                        "specific instructions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Авторитетный приказ или распоряжение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указание",
                                "target_lang_sense_clarification": "распоряжение или совет"
                            },
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "официальный приказ"
                            },
                            {
                                "target_lang_word": "предписание",
                                "target_lang_sense_clarification": "официальное требование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bda2d9e-053d-3fec-8749-9c619ca9c091",
                    "sense_definition": "The act of instructing, teaching, or providing with information or knowledge.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university provides excellent <w>instruction</w> in modern languages.",
                            "target_lang_translations": {
                                "ru": "Университет обеспечивает отличное <w>обучение</w> современным языкам."
                            }
                        },
                        {
                            "text": "She gives private <w>instruction</w> to students who need extra help.",
                            "target_lang_translations": {
                                "ru": "Она дает частные <w>уроки</w> студентам, которым нужна дополнительная помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "teaching",
                        "education"
                    ],
                    "common_phrases": [
                        "give instruction",
                        "receive instruction",
                        "medium of instruction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс обучения, преподавания или передачи знаний"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обучение",
                                "target_lang_sense_clarification": "процесс передачи знаний"
                            },
                            {
                                "target_lang_word": "инструктаж",
                                "target_lang_sense_clarification": "краткое обучение конкретным правилам"
                            },
                            {
                                "target_lang_word": "наставление",
                                "target_lang_sense_clarification": "поучение или совет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61647c10-289e-3055-92db-f2d1462f018b",
                    "sense_definition": "A single operation of a processor defined by an instruction set architecture.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The computer processor executes millions of <w>instructions</w> every second.",
                            "target_lang_translations": {
                                "ru": "Компьютерный процессор выполняет миллионы <w>инструкций</w> каждую секунду."
                            }
                        },
                        {
                            "text": "A bug in the software caused an illegal <w>instruction</w> error.",
                            "target_lang_translations": {
                                "ru": "Ошибка в программном обеспечении вызвала ошибку недопустимой <w>инструкции</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "instruction set",
                        "execute an instruction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единичная операция процессора, определенная архитектурой набора команд"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инструкция",
                                "target_lang_sense_clarification": "команда процессора"
                            },
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "вычислительная операция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02a4c346-afd9-3cfe-ac49-24dc0e8cebe8",
                    "sense_definition": "The directions given by a client to their lawyer in relation to a particular legal matter.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The solicitor took <w>instructions</w> from his client before proceeding with the case.",
                            "target_lang_translations": {
                                "ru": "Адвокат получил <w>указания</w> от своего клиента, прежде чем приступить к делу."
                            }
                        },
                        {
                            "text": "He acted on the <w>instructions</w> of the defense attorney.",
                            "target_lang_translations": {
                                "ru": "Он действовал по <w>указаниям</w> адвоката защиты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take instructions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указания, даваемые клиентом своему юристу в отношении конкретного дела"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указание",
                                "target_lang_sense_clarification": "поручение клиентом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

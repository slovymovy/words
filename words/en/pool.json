{
    "word_family": [
        "carpool",
        "whirlpool",
        "cesspool"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "593a6b40-c4b3-35da-b84c-88ae3a27baeb",
                    "sense_definition": "An artificial area of water built for swimming, diving, or bathing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "water_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children spent the whole afternoon playing in the <w>pool</w>.",
                            "target_lang_translations": {
                                "ru": "Дети провели весь день, играя в <w>бассейне</w>."
                            }
                        },
                        {
                            "text": "Does this hotel have an indoor swimming <w>pool</w>?",
                            "target_lang_translations": {
                                "ru": "В этом отеле есть крытый плавательный <w>бассейн</w>?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "swimming pool",
                        "indoor pool",
                        "outdoor pool"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственный водоем, построенный для плавания, ныряния или купания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бассейн",
                                "target_lang_sense_clarification": "для плавания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c965e954-34a7-3dcd-b518-51f3bed91389",
                    "sense_definition": "A game played on a cloth-covered table with pockets, where players use a cue stick to hit balls into the pockets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "game_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to the bar to play <w>pool</w> and relax.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в бар, чтобы сыграть в <w>пул</w> и расслабиться."
                            }
                        },
                        {
                            "text": "He is very good at shooting <w>pool</w>.",
                            "target_lang_translations": {
                                "ru": "Он очень хорошо играет в <w>пул</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play pool",
                        "shoot pool",
                        "pool table",
                        "pool cue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игра на покрытом сукном столе с лузами, где игроки используют кий, чтобы забивать шары в лузы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пул",
                                "target_lang_sense_clarification": "вид бильярда"
                            },
                            {
                                "target_lang_word": "бильярд",
                                "target_lang_sense_clarification": "игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f62834e-6921-3f9d-ac43-417f09d5b00f",
                    "sense_definition": "A small area of still water, often naturally formed or part of a river.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "water_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We found a small crab in a rock <w>pool</w> at the beach.",
                            "target_lang_translations": {
                                "ru": "Мы нашли маленького краба в скалистой <w>луже</w> на пляже."
                            }
                        },
                        {
                            "text": "The river slowed down and formed a deep, dark <w>pool</w>.",
                            "target_lang_translations": {
                                "ru": "Река замедлилась и образовала глубокий темный <w>омут</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "poule",
                        "group"
                    ],
                    "common_phrases": [
                        "rock pool",
                        "tide pool",
                        "deep pool"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой участок стоячей воды, часто образованный естественным путем или являющийся частью реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заводь",
                                "target_lang_sense_clarification": "в реке"
                            },
                            {
                                "target_lang_word": "омут",
                                "target_lang_sense_clarification": "глубокое место в реке"
                            },
                            {
                                "target_lang_word": "пруд",
                                "target_lang_sense_clarification": "водоем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b36388c6-b634-310f-9211-454db55138e6",
                    "sense_definition": "A supply of people, money, or things available for use or shared by a group.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "collective_resource",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company has a large <w>pool</w> of talented applicants to choose from.",
                            "target_lang_translations": {
                                "ru": "У компании есть большой <w>резерв</w> талантливых кандидатов на выбор."
                            }
                        },
                        {
                            "text": "We need to maintain a <w>pool</w> of resources for emergencies.",
                            "target_lang_translations": {
                                "ru": "Нам нужно поддерживать <w>фонд</w> ресурсов на случай чрезвычайных ситуаций."
                            }
                        }
                    ],
                    "synonyms": [
                        "group"
                    ],
                    "common_phrases": [
                        "talent pool",
                        "gene pool",
                        "pool of resources",
                        "car pool"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запас людей, денег или вещей, доступных для использования или совместного использования группой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонд",
                                "target_lang_sense_clarification": "денег или ресурсов"
                            },
                            {
                                "target_lang_word": "резерв",
                                "target_lang_sense_clarification": "запас"
                            },
                            {
                                "target_lang_word": "пул",
                                "target_lang_sense_clarification": "совокупность ресурсов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b50b3c5-f78d-3541-85a0-16d2a0bdb6e5",
                    "sense_definition": "A small quantity of liquid that has spilled or gathered on a surface.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "water_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He slipped in a <w>pool</w> of water on the kitchen floor.",
                            "target_lang_translations": {
                                "ru": "Он поскользнулся в <w>луже</w> воды на кухонном полу."
                            }
                        },
                        {
                            "text": "There was a <w>pool</w> of oil underneath the old car.",
                            "target_lang_translations": {
                                "ru": "Под старой машиной была <w>лужа</w> масла."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pool of blood",
                        "pool of water",
                        "pool of light"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое количество жидкости, пролитой или скопившейся на поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лужа",
                                "target_lang_sense_clarification": "жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "799c6bbc-5048-310b-8d1d-8be2e4341d3c",
                    "sense_definition": "A group of teams or competitors who play against each other in the early stage of a tournament.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "collective_resource",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our team finished at the top of their <w>pool</w> and advanced to the finals.",
                            "target_lang_translations": {
                                "ru": "Наша команда финишировала на вершине своей <w>группы</w> и вышла в финал."
                            }
                        },
                        {
                            "text": "It is going to be difficult to qualify from such a tough <w>pool</w>.",
                            "target_lang_translations": {
                                "ru": "Будет сложно квалифицироваться из такой сильной <w>группы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "group"
                    ],
                    "common_phrases": [
                        "pool stage"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа команд или участников, которые играют друг против друга на ранней стадии турнира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "в турнире"
                            },
                            {
                                "target_lang_word": "подгруппа",
                                "target_lang_sense_clarification": "часть участников"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4467457e-e450-3a58-ab6a-be59dfc482b7",
                    "sense_definition": "To put money, information, or resources together with others for shared use or benefit.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "collective_resource",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>pool</w> our money to buy a nice wedding gift.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>объединить</w> наши деньги, чтобы купить хороший свадебный подарок."
                            }
                        },
                        {
                            "text": "The scientists <w>pooled</w> their knowledge to find a solution to the problem.",
                            "target_lang_translations": {
                                "ru": "Ученые <w>объединили</w> свои знания, чтобы найти решение проблемы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pool resources",
                        "pool money",
                        "pool efforts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединять деньги, информацию или ресурсы с другими для совместного использования или выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "ресурсы или усилия"
                            },
                            {
                                "target_lang_word": "складывать",
                                "target_lang_sense_clarification": "деньги в общую сумму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27c14a47-a31f-3673-927f-103c438c9a17",
                    "sense_definition": "To collect or form into a puddle or small body of liquid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "water_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Blood began to <w>pool</w> on the floor where he had fallen.",
                            "target_lang_translations": {
                                "ru": "Кровь начала <w>скапливаться</w> на полу там, где он упал."
                            }
                        },
                        {
                            "text": "Rainwater tended to <w>pool</w> on the flat roof after a storm.",
                            "target_lang_translations": {
                                "ru": "Дождевая вода имела обыкновение <w>скапливаться</w> на плоской крыше после шторма."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Собираться или образовывать лужу или небольшое скопление жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скапливаться",
                                "target_lang_sense_clarification": "о жидкости"
                            },
                            {
                                "target_lang_word": "застаиваться",
                                "target_lang_sense_clarification": "о воде"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "84401e38-fd79-3579-a386-ba88bb43e8f6",
                    "sense_definition": "A village in Cornwall, England.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We visited the old mines near <w>Pool</w> during our trip to Cornwall.",
                            "target_lang_translations": {
                                "ru": "Мы посетили старые шахты возле <w>Пула</w> во время нашей поездки в Корнуолл."
                            }
                        },
                        {
                            "text": "The village of <w>Pool</w> has a long history related to mining.",
                            "target_lang_translations": {
                                "ru": "Деревня <w>Пул</w> имеет долгую историю, связанную с горным делом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня в Корнуолле, Англия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пул",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "80400ff7-3502-3ff7-a025-6804735cde9e",
                    "sense_definition": "Gerelateerd aan het voormalige Nederlands-Indië (het huidige Indonesië) of de daaruit ontstane mengcultuur en keuken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_east_indies",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij halen vanavond eten bij het Chinees-<w>Indisch</w> restaurant.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером мы возьмем еду в китайско-<w>индонезийском</w> ресторане."
                            }
                        },
                        {
                            "text": "Mijn oma vertelt vaak verhalen over haar <w>Indische</w> jeugd in de tropen.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка часто рассказывает истории о своей <w>ост-индской</w> молодости в тропиках."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Indische rijsttafel",
                        "Indische buurt",
                        "Chinees-Indisch restaurant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к бывшей Голландской Ост-Индии (нынешняя Индонезия) или возникшей там смешанной культуре и кухне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "индонезийский",
                                "target_lang_sense_clarification": "относящийся к Индонезии или её кухне"
                            },
                            {
                                "target_lang_word": "ост-индский",
                                "target_lang_sense_clarification": "исторический термин, относящийся к колониальному периоду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e39382d9-5f61-3727-bf89-aacb10d4c649",
                    "sense_definition": "Gerelateerd aan het land India.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_india",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>Indische</w> Oceaan is de op twee na grootste oceaan ter wereld.",
                            "target_lang_translations": {
                                "ru": "<w>Индийский</w> океан — третий по величине океан в мире."
                            }
                        },
                        {
                            "text": "In oudere literatuur wordt de bevolking van India soms <w>Indisch</w> genoemd.",
                            "target_lang_translations": {
                                "ru": "В старой литературе население Индии иногда называют <w>индийским</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Indiaas"
                    ],
                    "common_phrases": [
                        "Indische Oceaan"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "tegenwoordig is 'Indiaas' gebruikelijker, behalve in vaste namen"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к стране Индия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "индийский",
                                "target_lang_sense_clarification": "относящийся к государству Индия"
                            }
                        ]
                    }
                }
            ]
        },

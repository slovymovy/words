                            {
                                "target_lang_word": "предательский",
                                "target_lang_sense_clarification": "перен. ненадёжный, опасный"
                            },
                            {
                                "target_lang_word": "обманчивый",
                                "target_lang_sense_clarification": "создающий ложное впечатление безопасности"

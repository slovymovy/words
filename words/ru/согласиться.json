            "senses": [
                {
                    "sense_id": "4e9e723a-968a-39ff-b7f2-2704a64e51f8",
                    "sense_definition": "Принять предложение, ответить утвердительно на просьбу или выразить готовность сделать что-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <w>согласиться</w> на это выгодное предложение.",
                            "target_lang_translations": {
                                "en": "We decided to <w>agree</w> to this profitable offer."
                            }
                        },
                        {
                            "text": "Она не могла не <w>согласиться</w> помочь своему старому другу.",
                            "target_lang_translations": {
                                "en": "She couldn't help but <w>agree</w> to help her old friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "дать согласие",
                        "выразить согласие",
                        "пойти навстречу"
                    ],
                    "antonyms": [
                        "отказаться",
                        "отказать",
                        "заартачиться"
                    ],
                    "common_phrases": [
                        "согласиться на предложение",
                        "согласиться помочь",
                        "сразу согласиться",
                        "согласиться на условия"
                    ],
                    "target_lang_definitions": {
                        "en": "To accept a proposal, answer affirmatively to a request, or express readiness to do something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agree",
                                "target_lang_sense_clarification": "to accept a proposal or request"
                            },
                            {
                                "target_lang_word": "consent",
                                "target_lang_sense_clarification": "to give permission or express willingness"
                            },
                            {
                                "target_lang_word": "assent",
                                "target_lang_sense_clarification": "to agree to a proposal (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bd040d1-40ef-353b-af0c-cb9fce719ba1",
                    "sense_definition": "Признать правильность чьих-либо слов, разделить чьё-то мнение или подтвердить факт.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "opinion_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я вынужден полностью <w>согласиться</w> с вашими доводами.",
                            "target_lang_translations": {
                                "en": "I am forced to fully <w>agree</w> with your arguments."
                            }
                        },
                        {
                            "text": "Трудно не <w>согласиться</w>, что погода сегодня просто замечательная.",
                            "target_lang_translations": {
                                "en": "It is hard not to <w>agree</w> that the weather today is simply wonderful."
                            }
                        }
                    ],
                    "synonyms": [
                        "уступить"
                    ],
                    "common_phrases": [
                        "согласиться с мнением",
                        "полностью согласиться",
                        "вынужден согласиться",
                        "согласиться с автором"
                    ],
                    "target_lang_definitions": {
                        "en": "To acknowledge the correctness of someone's words, share someone's opinion, or confirm a fact."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agree",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "53c0ed0c-a041-320e-a58f-490c67444080",
                    "sense_definition": "Совершить гигиеническую процедуру (о душе или ванне).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "hygiene",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро я люблю <принять> холодный душ, чтобы проснуться.",
                            "target_lang_translations": {
                                "en": "Every morning I like to <take> a cold shower to wake up."
                            }
                        },
                        {
                            "text": "После долгой прогулки по лесу мы решили <принять> горячую ванну.",
                            "target_lang_translations": {
                                "en": "After a long walk in the forest, we decided to <take> a hot bath."
                            }
                        }
                    ],
                    "common_phrases": [
                        "принять душ",
                        "принять ванну"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a hygienic procedure (regarding a shower or bath)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "a shower or bath"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0df2174-3b19-342f-8a7d-5f9af1252a46",
                    "sense_definition": "Взять в руки или получить то, что дают, посылают или вручают.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Курьер попросил меня <принять> заказ и расписаться в документах.",
                            "target_lang_translations": {
                                "en": "The courier asked me to <accept> the order and sign the documents."
                            }
                        },
                        {
                            "text": "Она с благодарностью <приняла> подарок от своих коллег на день рождения.",
                            "target_lang_translations": {
                                "en": "She gratefully <accepted> the birthday gift from her colleagues."
                            }
                        }
                    ],
                    "synonyms": [
                        "взять",
                        "получить"
                    ],
                    "antonyms": [
                        "передать",
                        "вручить",
                        "послать",
                        "дать"
                    ],
                    "common_phrases": [
                        "принять подарок",
                        "принять мяч",
                        "принять посылку"
                    ],
                    "target_lang_definitions": {
                        "en": "To take into one's hands or receive something given, sent, or delivered."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accept",
                                "target_lang_sense_clarification": "a gift or delivery"
                            },
                            {
                                "target_lang_word": "receive",
                                "target_lang_sense_clarification": "something sent"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "something offered"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13cabc59-1429-3525-af61-fff18142cd2e",
                    "sense_definition": "Проглотить лекарство или выпить какую-либо жидкость.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ingest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач сказал больному <принять> это лекарство сразу после еды.",
                            "target_lang_translations": {
                                "en": "The doctor told the patient to <take> this medicine immediately after eating."
                            }
                        },
                        {
                            "text": "Чтобы успокоиться перед экзаменом, студент решил <принять> валерьянку.",
                            "target_lang_translations": {
                                "en": "To calm down before the exam, the student decided to <take> valerian."
                            }
                        }
                    ],
                    "common_phrases": [
                        "принять лекарство",
                        "принять таблетку",
                        "принять яд"
                    ],
                    "target_lang_definitions": {
                        "en": "To swallow medicine or drink a liquid."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "medicine"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e35fbba1-c037-3790-be1f-5d91833d5eff",
                    "sense_definition": "Согласиться с чем-либо, утвердить или признать что-либо (предложение, закон, условия).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Парламент должен <принять> новый закон о налогах до конца года.",
                            "target_lang_translations": {
                                "en": "Parliament must <adopt> the new tax law before the end of the year."
                            }
                        },
                        {
                            "text": "Мы с радостью <примем> ваше приглашение на свадьбу.",
                            "target_lang_translations": {
                                "en": "We will gladly <accept> your invitation to the wedding."
                            }
                        }
                    ],
                    "common_phrases": [
                        "принять решение",
                        "принять закон",
                        "принять предложение",
                        "принять участие"
                    ],
                    "target_lang_definitions": {
                        "en": "To agree to something, approve, or recognize something (a proposal, law, conditions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accept",
                                "target_lang_sense_clarification": "a proposal or invitation"
                            },
                            {
                                "target_lang_word": "adopt",
                                "target_lang_sense_clarification": "a law or resolution"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "a law"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "678160ee-a6e2-3cc0-a182-703d92bbb239",
                    "sense_definition": "Встретить гостей или посетителей, допустить их к себе для беседы или обслуживания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хозяева дома готовы радушно <принять> всех приехавших родственников.",
                            "target_lang_translations": {
                                "en": "The hosts are ready to warmly <receive> all the arriving relatives."
                            }
                        },
                        {
                            "text": "Директор не сможет <принять> вас сегодня из-за важного совещания.",
                            "target_lang_translations": {
                                "en": "The director will not be able to <see> you today because of an important meeting."
                            }
                        }
                    ],
                    "common_phrases": [
                        "принять гостей",
                        "принять посетителя",
                        "принять пациента"
                    ],
                    "target_lang_definitions": {
                        "en": "To meet guests or visitors, allowing them in for conversation or service."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "receive",
                                "target_lang_sense_clarification": "guests"
                            },
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "visitors or patients"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bab8887-2a29-3bf0-83b4-bf180f453274",
                    "sense_definition": "Зачислить или включить кого-либо в состав организации, учебного заведения или группы.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Университет готов <принять> новых студентов в этом семестре.",
                            "target_lang_translations": {
                                "en": "The university is ready to <accept> new students this semester."
                            }
                        },
                        {
                            "text": "Начальник согласился <принять> его на должность менеджера по продажам.",
                            "target_lang_translations": {
                                "en": "The boss agreed to <take> him on as a sales manager."
                            }
                        }
                    ],
                    "synonyms": [
                        "взять"
                    ],
                    "common_phrases": [
                        "принять на работу",
                        "принять в партию",
                        "принять в школу"
                    ],
                    "target_lang_definitions": {
                        "en": "To enroll or include someone in an organization, educational institution, or group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accept",
                                "target_lang_sense_clarification": "into a group or school"
                            },
                            {
                                "target_lang_word": "admit",
                                "target_lang_sense_clarification": "into an institution"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "on staff or as a student"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6d4c238-188b-37b0-a839-2f3bac8490ad",
                    "sense_definition": "Взять на себя (обязанность, ответственность) или приобрести какой-либо вид.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто из вас готов <принять> на себя ответственность за этот сложный проект?",
                            "target_lang_translations": {
                                "en": "Which of you is ready to <take> responsibility for this complex project?"
                            }
                        },
                        {
                            "text": "Под воздействием тепла вещество может <принять> жидкую форму.",
                            "target_lang_translations": {
                                "en": "Under the influence of heat, the substance can <take> a liquid form."
                            }
                        }
                    ],
                    "synonyms": [
                        "взять"
                    ],
                    "common_phrases": [
                        "принять ответственность",
                        "принять вид",
                        "принять форму",
                        "принять командование"
                    ],
                    "target_lang_definitions": {
                        "en": "To take upon oneself (duty, responsibility) or to acquire a certain appearance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "responsibility or shape"
                            },
                            {
                                "target_lang_word": "assume",
                                "target_lang_sense_clarification": "responsibility or form"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f1bb28d-6759-3df1-8376-ff3fd927db05",
                    "sense_definition": "Отреагировать на что-либо, воспринять информацию или событие определенным образом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители спокойно <приняли> новость о скором переезде сына.",
                            "target_lang_translations": {
                                "en": "The parents calmly <took> the news of their son's impending move."
                            }
                        },
                        {
                            "text": "Он слишком близко к сердцу <принял> критику своего начальника.",
                            "target_lang_translations": {
                                "en": "He <took> his boss's criticism too much to heart."
                            }
                        }
                    ],
                    "common_phrases": [
                        "принять к сведению",
                        "принять близко к сердцу",
                        "принять всерьез"
                    ],
                    "target_lang_definitions": {
                        "en": "To react to something or perceive information or an event in a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "news or criticism"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "841a2538-2fdf-3f15-bc3f-b969f6d0eaa0",
                    "sense_definition": "Ошибочно посчитать кого-либо или что-либо кем-то другим (обычно с предлогом «за»).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В темноте я случайно <принял> незнакомца за своего старого друга.",
                            "target_lang_translations": {
                                "en": "In the dark, I accidentally <took> a stranger for my old friend."
                            }
                        },
                        {
                            "text": "Извините, я <принял> вас за другого человека.",
                            "target_lang_translations": {
                                "en": "Excuse me, I <mistook> you for someone else."
                            }
                        }
                    ],
                    "common_phrases": [
                        "принять за другого",
                        "принять за чистую монету"
                    ],
                    "target_lang_definitions": {
                        "en": "To mistakenly consider someone or something to be someone else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "for someone else"
                            },
                            {
                                "target_lang_word": "mistake",
                                "target_lang_sense_clarification": "for someone else"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d6b619e-804c-3b7b-bfd6-b875bf786714",
                    "sense_definition": "Арестовать или задержать полицией.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_arrest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция <приняла> преступника прямо на месте кражи.",
                            "target_lang_translations": {
                                "en": "The police <arrested> the criminal right at the scene of the theft."
                            }

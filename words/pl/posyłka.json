    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "70c8902c-81d4-3153-9b33-2ad179f7019c",
                    "sense_definition": "Drobna sprawa do załatwienia wymagająca wyjścia z domu lub biura; polecenie wykonania takiej czynności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "task",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nowej firmie traktowano go początkowo jak chłopca na <w>posyłki</w>.",
                            "target_lang_translations": {
                                "ru": "В новой фирме к нему поначалу относились как к мальчику на <w>побегушках</w>."
                            }
                        },
                        {
                            "text": "Mam dzisiaj na mieście dwie pilne <w>posyłki</w> do załatwienia.",
                            "target_lang_translations": {
                                "ru": "У меня сегодня в городе два срочных <w>поручения</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chłopiec na posyłki",
                        "biegać na posyłki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкое дело или поручение, требующее выхода из дома или офиса; приказание выполнить такое действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поручение",
                                "target_lang_sense_clarification": "задание, дело"
                            },
                            {
                                "target_lang_word": "побегушки",
                                "target_lang_sense_clarification": "в выражении «на побегушках»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e928c201-d7a9-3a9d-ae8c-9041329f30fb",
                    "sense_definition": "Przedmiot, paczka lub towar, który został do kogoś wysłany.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "item",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Listonosz zostawił awizo, bo nie mógł doręczyć <w>posyłki</w>.",
                            "target_lang_translations": {
                                "ru": "Почтальон оставил извещение, так как не смог доставить <w>посылку</w>."
                            }
                        },
                        {
                            "text": "Rodzice przygotowali dla syna dużą <w>posyłkę</w> z żywnością.",
                            "target_lang_translations": {
                                "ru": "Родители подготовили для сына большую <w>посылку</w> с продуктами."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesyłka",
                        "paczka"
                    ],
                    "common_phrases": [
                        "odebrać posyłkę",
                        "czekać na posyłkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie częściej używa się słowa 'przesyłka'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, пакет или товар, отправленный кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посылка",
                                "target_lang_sense_clarification": "почтовое отправление"
                            },
                            {
                                "target_lang_word": "отправление",
                                "target_lang_sense_clarification": "пересылаемый предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d20d6808-0c8a-336c-abeb-628d128439d8",
                    "sense_definition": "Historyczny obowiązek chłopów pańszczyźnianych polegający na oddawaniu dzieci do pracy we dworze.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopi często buntowali się przeciwko przymusowej <w>posyłce</w> dzieci.",
                            "target_lang_translations": {
                                "ru": "Крестьяне часто бунтовали против принудительной <w>отправки</w> детей."
                            }
                        },
                        {
                            "text": "W dawnym systemie feudalnym <w>posyłka</w> była uciążliwą powinnością.",
                            "target_lang_translations": {
                                "ru": "В старой феодальной системе <w>посылка</w> была обременительной повинностью."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "termin historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческая повинность крепостных крестьян, заключающаяся в отправке детей на работу в поместье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправка",
                                "target_lang_sense_clarification": "действие по значению глагола"
                            },
                            {
                                "target_lang_word": "посылка",
                                "target_lang_sense_clarification": "устар. действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5cba5a6-7da5-3fe0-9002-fb661da5c743",
                    "sense_definition": "Osoba wyznaczona do biegania z wiadomościami lub załatwiania drobnych spraw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do sąsiedniej wioski wyruszyła piesza <w>posyłka</w> z listem.",
                            "target_lang_translations": {
                                "ru": "В соседнюю деревню отправился пеший <w>посыльный</w> с письмом."
                            }
                        },
                        {
                            "text": "Hrabia wysłał zaufaną <w>posyłkę</w>, aby sprawdzić sytuację na granicy.",
                            "target_lang_translations": {
                                "ru": "Граф отправил надежного <w>гонца</w>, чтобы проверить ситуацию на границе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "obecnie rzadko używane w odniesieniu do osoby"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, назначенный для передачи сообщений или выполнения мелких поручений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посыльный",
                                "target_lang_sense_clarification": "курьер, вестник"
                            },
                            {
                                "target_lang_word": "гонец",
                                "target_lang_sense_clarification": "устар. вестник"
                            }
                        ]
                    }
                }
            ]
        }
    ]

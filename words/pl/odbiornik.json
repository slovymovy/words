            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4e4297d0-1866-3222-86e6-2fcd7067859c",
                    "sense_definition": "Urządzenie elektroniczne służące do odbierania sygnałów (np. radiowych, telewizyjnych) i przetwarzania ich na dźwięk lub obraz.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "electronic_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary <odbiornik> radiowy wciąż działa bez zarzutu.",
                            "target_lang_translations": {
                                "ru": "Мой старый радио<w>приёмник</w> всё ещё работает безупречно."
                            }
                        },
                        {
                            "text": "Ten nowoczesny <odbiornik> pozwala na oglądanie programów w wysokiej jakości.",
                            "target_lang_translations": {
                                "ru": "Этот современный <w>приёмник</w> позволяет смотреть программы в высоком качестве."
                            }
                        }
                    ],
                    "synonyms": [
                        "radio",
                        "telewizor"
                    ],
                    "antonyms": [
                        "nadajnik"
                    ],
                    "common_phrases": [
                        "odbiornik radiowy",
                        "odbiornik telewizyjny",
                        "odbiornik GPS"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронное устройство, предназначенное для приема сигналов (например, радио, телевидения) и их преобразования в звук или изображение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приёмник",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "радиоприёмник",
                                "target_lang_sense_clarification": "радио"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1fe5fa1-3350-3912-9ddc-ff7512e725ff",
                    "sense_definition": "Urządzenie lub element obwodu, który pobiera energię elektryczną i przetwarza ją na inną formę energii.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "electrical_component",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <odbiornik> włączony do sieci zużywa określoną ilość prądu.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>приёмник</w>, включенный в сеть, потребляет определенное количество тока."
                            }
                        },
                        {
                            "text": "Żarówka jest prostym <odbiornikiem> energii elektrycznej w domowej instalacji.",
                            "target_lang_translations": {
                                "ru": "Лампочка является простым <w>приёмником</w> электрической энергии в домашней проводке."
                            }
                        }
                    ],
                    "antonyms": [
                        "źródło",
                        "generator"
                    ],
                    "common_phrases": [
                        "odbiornik energii",
                        "odbiornik prądu",
                        "moc odbiornika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или элемент цепи, который потребляет электрическую энергию и преобразует её в другую форму энергии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приёмник",
                                "target_lang_sense_clarification": "энергии"
                            },
                            {
                                "target_lang_word": "потребитель",
                                "target_lang_sense_clarification": "электроэнергии"
                            },
                            {
                                "target_lang_word": "электроприёмник",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "090d2cbb-6977-3d72-8801-e5aa14012329",
                    "sense_definition": "Zbiornik lub miejsce, do którego spływa ciecz, gaz lub ścieki w instalacji technicznej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda z rur odpływowych trafia bezpośrednio do głównego <odbiornika>.",
                            "target_lang_translations": {
                                "ru": "Вода из сливных труб попадает непосредственно в главный <w>приёмник</w>."
                            }
                        },
                        {
                            "text": "Rzeka stała się naturalnym <odbiornikiem> zanieczyszczeń z pobliskiej fabryki.",
                            "target_lang_translations": {
                                "ru": "Река стала естественным <w>приёмником</w> загрязнений с близлежащей фабрики."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbiornik"
                    ],
                    "common_phrases": [
                        "odbiornik ścieków",
                        "naturalny odbiornik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резервуар или место, куда стекает жидкость, газ или сточные воды в технической системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приёмник",
                                "target_lang_sense_clarification": "стоков, вод"
                            },
                            {
                                "target_lang_word": "водоприёмник",
                                "target_lang_sense_clarification": "гидротехника"
                            }
                        ]
                    }
                }
            ]
        }

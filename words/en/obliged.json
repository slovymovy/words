{
    "word_family": [
        "oblige",
        "obligation",
        "obligatory",
        "obliging"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e76d8edf-4729-3cf5-a227-d3c5f5edaa9a",
                    "sense_definition": "Under an obligation to do something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "duty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All employees are <w>obliged</w> to complete a tax return every year.",
                            "target_lang_translations": {
                                "ru": "Все сотрудники <w>обязаны</w> заполнять налоговую декларацию каждый год."
                            }
                        },
                        {
                            "text": "She felt <w>obliged</w> to tell the truth about what happened.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя <w>обязанной</w> рассказать правду о том, что случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "obligated"
                    ],
                    "common_phrases": [
                        "feel obliged",
                        "legally obliged",
                        "obliged to do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий обязательство сделать что-либо в силу закона, правил или морального долга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обязанный",
                                "target_lang_sense_clarification": "имеющий долг или обязательство"
                            },
                            {
                                "target_lang_word": "вынужденный",
                                "target_lang_sense_clarification": "в силу обстоятельств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "caa58287-a205-3bd5-9a7d-78634972dc88",
                    "sense_definition": "Grateful or indebted because of a favor done.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "gratitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am greatly <w>obliged</w> for your help with this problem.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>признателен</w> за вашу помощь с этой проблемой."
                            }
                        },
                        {
                            "text": "We are much <w>obliged</w> to you for the timely information.",
                            "target_lang_translations": {
                                "ru": "Мы весьма <w>благодарны</w> вам за своевременную информацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "obligated",
                        "obligated"
                    ],
                    "common_phrases": [
                        "much obliged",
                        "greatly obliged"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий благодарность или признательность за оказанную услугу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признательный",
                                "target_lang_sense_clarification": "выражающий признательность"
                            },
                            {
                                "target_lang_word": "благодарный",
                                "target_lang_sense_clarification": "чувствующий благодарность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a764243b-a8be-32c2-9d29-9da7a5c26e38",
                    "sense_definition": "Simple past and past participle of <w>oblige</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rain <w>obliged</w> us to cancel the picnic.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь <w>вынудил</w> нас отменить пикник."
                            }
                        },
                        {
                            "text": "He <w>obliged</w> the audience with a beautiful piano performance.",
                            "target_lang_translations": {
                                "ru": "Он <w>порадовал</w> публику прекрасной игрой на фортепиано."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past of oblige"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола oblige (обязывать, делать одолжение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынудил",
                                "target_lang_sense_clarification": "заставил в силу обстоятельств"
                            },
                            {
                                "target_lang_word": "обязал",
                                "target_lang_sense_clarification": "наложил обязательство"
                            },
                            {
                                "target_lang_word": "угодил",
                                "target_lang_sense_clarification": "сделал одолжение, выполнил просьбу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
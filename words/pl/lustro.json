    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "909804f4-efa0-3ad3-9ddf-c7534f76f072",
                    "sense_definition": "Gładka powierzchnia, najczęściej szklana, odbijająca obraz przedmiotów znajdujących się przed nią.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjściem z domu zawsze sprawdzam swój wygląd w <w>lustrze</w>.",
                            "target_lang_translations": {
                                "ru": "Перед выходом из дома я всегда проверяю свой внешний вид в <w>зеркале</w>."
                            }
                        },
                        {
                            "text": "W łazience powiesiliśmy duże, okrągłe <w>lustro</w> nad umywalką.",
                            "target_lang_translations": {
                                "ru": "В ванной мы повесили большое круглое <w>зеркало</w> над раковиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwierciadło"
                    ],
                    "common_phrases": [
                        "patrzeć w lustro",
                        "stawać przed lustrem",
                        "rozbić lustro",
                        "weneckie lustro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гладкая поверхность, чаще всего стеклянная, отражающая изображение предметов, находящихся перед ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зеркало",
                                "target_lang_sense_clarification": "предмет интерьера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fd92d97-2e64-357b-adf9-3b81d84d530e",
                    "sense_definition": "Niezmącona, gładka i spokojna powierzchnia wody.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorem <w>lustro</w> jeziora było idealnie gładkie i odbijało zachodzące słońce.",
                            "target_lang_translations": {
                                "ru": "Вечером <w>зеркало</w> озера было идеально гладким и отражало заходящее солнце."
                            }
                        },
                        {
                            "text": "Rzucony kamień zburzył spokojne <w>lustro</w> wody.",
                            "target_lang_translations": {
                                "ru": "Брошенный камень нарушил спокойное <w>зеркало</w> воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "tafla",
                        "zwierciadło"
                    ],
                    "common_phrases": [
                        "lustro wody",
                        "gładki jak lustro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спокойная, гладкая поверхность воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зеркало",
                                "target_lang_sense_clarification": "поверхность воды"
                            },
                            {
                                "target_lang_word": "гладь",
                                "target_lang_sense_clarification": "водная поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "571f720d-a00c-36af-8e96-6f0006d7c6c9",
                    "sense_definition": "Potoczne określenie aparatu fotograficznego typu lustrzanka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Profesjonalne <w>lustro</w> pozwala na robienie zdjęć o znacznie wyższej jakości niż telefon.",
                            "target_lang_translations": {
                                "ru": "Профессиональная <w>зеркалка</w> позволяет делать фотографии значительно более высокого качества, чем телефон."
                            }
                        },
                        {
                            "text": "Mój stary aparat to cyfrowe <w>lustro</w>, które służy mi już od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Мой старый фотоаппарат — это цифровая <w>зеркалка</w>, которая служит мне уже много лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cyfrowe lustro",
                        "pełne lustro"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane przez fotografów i entuzjastów"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название зеркального фотоаппарата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зеркалка",
                                "target_lang_sense_clarification": "фотоаппарат (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c60318bd-41e2-342d-b693-cdf8011d4bab",
                    "sense_definition": "Jasna plama sierści na tylnej części ciała zwierząt łownych, takich jak sarny czy jelenie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy zauważył białe <w>lustro</w> sarny uciekającej w gęste zarośla.",
                            "target_lang_translations": {
                                "ru": "Охотник заметил белое <w>зеркало</w> косули, убегающей в густые заросли."
                            }
                        },
                        {
                            "text": "Charakterystyczne <w>lustro</w> pomaga w identyfikacji gatunku zwierzęcia z dużej odległości.",
                            "target_lang_translations": {
                                "ru": "Характерное <w>зеркало</w> помогает определить вид животного с большого расстояния."
                            }
                        }
                    ],
                    "synonyms": [
                        "talerz"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "terminologia łowiecka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Светлое пятно шерсти на задней части тела охотничьих животных, таких как косули или олени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зеркало",
                                "target_lang_sense_clarification": "у животных (охотн.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

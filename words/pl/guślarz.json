            "senses": [
                {
                    "sense_id": "46a66418-c199-302b-83be-c7782431efdd",
                    "sense_definition": "Mężczyzna zajmujący się ludową magią, rzucaniem uroków, leczeniem i odprawianiem tajemnych obrzędów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "magic_practitioner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do ciężko chorego dziecka wzywano często miejscowego <w>guślarza</w>, aby odpędził złe duchy.",
                            "target_lang_translations": {
                                "ru": "К тяжело больному ребенку часто звали местного <w>колдуна</w>, чтобы он отогнал злых духов."
                            }
                        },
                        {
                            "text": "Stary <w>guślarz</w> znał wiele zaklęć, których obawiali się wszyscy mieszkańcy wioski.",
                            "target_lang_translations": {
                                "ru": "Старый <w>колдун</w> знал много заклинаний, которых боялись все жители деревни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stary guślarz",
                        "wioskowy guślarz",
                        "obrzędy guślarza"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to historical folk practices"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, занимающийся народной магией, наложением чар, лечением и совершением тайных обрядов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колдун",
                                "target_lang_sense_clarification": "человек, практикующий магию"
                            },
                            {
                                "target_lang_word": "знахарь",
                                "target_lang_sense_clarification": "народный целитель"
                            },
                            {
                                "target_lang_word": "чародей",
                                "target_lang_sense_clarification": "волшебник, маг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e8c3dc3-64e4-36d8-926f-b8accfdd5d73",
                    "sense_definition": "Kapłan lub szaman w dawnych wierzeniach słowiańskich, pełniący funkcje religijne i wieszcze.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "magic_practitioner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pogański <w>guślarz</w> rozpalił święty ogień i rozpoczął modlitwy do dawnych bóstw.",
                            "target_lang_translations": {
                                "ru": "Языческий <w>жрец</w> разжег священный огонь и начал молиться древним божествам."
                            }
                        },
                        {
                            "text": "Ludzie wierzyli, że doświadczony <w>guślarz</w> potrafi przepowiedzieć przyszłość z lotu ptaków.",
                            "target_lang_translations": {
                                "ru": "Люди верили, что опытный <w>волхв</w> может предсказать будущее по полету птиц."
                            }
                        }
                    ],
                    "synonyms": [
                        "wołchw"
                    ],
                    "common_phrases": [
                        "słowiański guślarz",
                        "pogański guślarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жрец или шаман в древних славянских верованиях, выполняющий религиозные и пророческие функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волхв",
                                "target_lang_sense_clarification": "древнерусский жрец, прорицатель"
                            },
                            {
                                "target_lang_word": "жрец",
                                "target_lang_sense_clarification": "языческий священнослужитель"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "резко",
        "резкость",
        "резать"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b7945acb-ed56-3b03-acfa-a744c581d40e",
                    "sense_definition": "Проявляющийся с большой силой и остротой; пронизывающий (о ветре, холоде, боли).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На улице дул <резкий> холодный ветер, пробирающий до костей.",
                            "target_lang_translations": {
                                "en": "A <w>piercing</w> cold wind was blowing outside, chilling to the bone."
                            }
                        },
                        {
                            "text": "Внезапно спортсмен почувствовал <резкую> боль в колене.",
                            "target_lang_translations": {
                                "en": "Suddenly, the athlete felt a <w>sharp</w> pain in his knee."
                            }
                        }
                    ],
                    "synonyms": [
                        "сильный",
                        "пронзительный",
                        "мощный"
                    ],
                    "antonyms": [
                        "слабый",
                        "лёгкий",
                        "несильный"
                    ],
                    "common_phrases": [
                        "резкая боль",
                        "резкий ветер",
                        "резкий холод",
                        "резкое похолодание"
                    ],
                    "target_lang_definitions": {
                        "en": "Manifesting with great force; piercing (of wind, cold, pain)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sharp",
                                "target_lang_sense_clarification": "severe, painful, or biting"
                            },
                            {
                                "target_lang_word": "piercing",
                                "target_lang_sense_clarification": "penetrating, especially of wind or cold"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bef3bef9-9c78-3695-b579-67b6fa4323aa",
                    "sense_definition": "Слишком сильный, неприятно действующий на органы чувств (о запахе, свете, звуке).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sensory_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнате стоял <резкий> запах краски после ремонта.",
                            "target_lang_translations": {
                                "en": "There was a <w>pungent</w> smell of paint in the room after the renovation."
                            }
                        },
                        {
                            "text": "<Резкий> свет фар ослепил водителя на ночной дороге.",
                            "target_lang_translations": {
                                "en": "The <w>harsh</w> light of headlights blinded the driver on the night road."
                            }
                        }
                    ],
                    "synonyms": [
                        "сильный",
                        "пронзительный",
                        "интенсивный"
                    ],
                    "antonyms": [
                        "слабый",
                        "мягкий",
                        "нерезкий"
                    ],
                    "common_phrases": [
                        "резкий запах",
                        "резкий свет",
                        "резкий звук",
                        "резкий голос"
                    ],
                    "target_lang_definitions": {
                        "en": "Overpowering and unpleasant to the senses (of smell, light, sound)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pungent",
                                "target_lang_sense_clarification": "having a strong, stinging odor"
                            },
                            {
                                "target_lang_word": "sharp",
                                "target_lang_sense_clarification": "intense and irritating to the senses"
                            },
                            {
                                "target_lang_word": "harsh",
                                "target_lang_sense_clarification": "unpleasantly rough or glaring"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4e5a851-772a-3dc8-b086-bc289e4b5676",
                    "sense_definition": "Внезапный и очень значительный (об изменениях, движениях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sudden_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Экономисты отметили <резкое> повышение цен на продукты.",
                            "target_lang_translations": {
                                "en": "Economists noted a <w>sharp</w> increase in food prices."
                            }
                        },
                        {
                            "text": "Машина сделала <резкий> поворот, чтобы избежать столкновения.",
                            "target_lang_translations": {
                                "en": "The car made a <w>sharp</w> turn to avoid a collision."
                            }
                        }
                    ],
                    "synonyms": [
                        "внезапный",
                        "неожиданный",
                        "крутой"
                    ],
                    "antonyms": [
                        "постепенный",
                        "плавный"
                    ],
                    "common_phrases": [
                        "резкое изменение",
                        "резкий поворот",
                        "резкий скачок",
                        "резкий переход"
                    ],
                    "target_lang_definitions": {
                        "en": "Sudden and very significant (of changes or movements)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sharp",
                                "target_lang_sense_clarification": "sudden, intense, or forming a tight angle"
                            },
                            {
                                "target_lang_word": "abrupt",
                                "target_lang_sense_clarification": "sudden and unexpected"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab8a7bad-af82-31d5-8c4d-f4bc7ec75e32",
                    "sense_definition": "Грубый, дерзкий, лишённый мягкости в обращении.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <резкий> ответ обидел всех присутствующих на собрании.",
                            "target_lang_translations": {
                                "en": "His <w>sharp</w> reply offended everyone present at the meeting."
                            }
                        },
                        {
                            "text": "Она не любит слушать <резкую> критику в свой адрес.",
                            "target_lang_translations": {
                                "en": "She does not like hearing <w>sharp</w> criticism directed at her."
                            }
                        }
                    ],
                    "synonyms": [
                        "грубый",
                        "дерзкий",
                        "обидный",
                        "беспощадный"
                    ],
                    "antonyms": [
                        "тактичный",
                        "мягкий"
                    ],
                    "common_phrases": [
                        "резкий ответ",
                        "резкая критика",
                        "резкий тон",
                        "резкое замечание"
                    ],
                    "target_lang_definitions": {
                        "en": "Rude, blunt, or lacking gentleness in manner"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sharp",
                                "target_lang_sense_clarification": "critical, severe, or angry"
                            },
                            {
                                "target_lang_word": "abrupt",
                                "target_lang_sense_clarification": "curt and unceremonious"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f3de287-c253-3b74-8835-5768cf20f8df",
                    "sense_definition": "Отчётливый, ясно очерченный, не расплывчатый (о контурах, тенях, чертах лица).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На фотографии были видны <резкие> тени от деревьев.",
                            "target_lang_translations": {
                                "en": "The photograph showed <w>sharp</w> shadows from the trees."
                            }
                        },
                        {
                            "text": "У этого актёра очень <резкие>, запоминающиеся черты лица.",
                            "target_lang_translations": {
                                "en": "This actor has very <w>sharp</w>, memorable facial features."
                            }
                        }
                    ],
                    "synonyms": [
                        "чёткий",
                        "отрывистый",
                        "отрывистый"
                    ],
                    "antonyms": [
                        "расплывчатый",
                        "размытый",
                        "нерезкий"
                    ],
                    "common_phrases": [
                        "резкие черты лица",
                        "резкие тени",
                        "резкий контраст",
                        "резкая линия"
                    ],
                    "target_lang_definitions": {
                        "en": "Distinct and clearly defined (of outlines, shadows, features)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sharp",
                                "target_lang_sense_clarification": "clearly defined, not blurred"
                            },
                            {
                                "target_lang_word": "distinct",
                                "target_lang_sense_clarification": "clear and unmistakable"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "3abec7e3-577d-3364-8584-e0795e962354",
                    "sense_definition": "Beklagenswaardige omstandigheden die zorgen voor lijden, armoede en verdriet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "misery_trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware aardbeving veroorzaakte veel <w>ellende</w> in de getroffen regio.",
                            "target_lang_translations": {
                                "en": "The severe earthquake caused a lot of <w>misery</w> in the affected region.",
                                "ru": "Сильное землетрясение принесло много <w>бед</w> в пострадавший регион."
                            }
                        },
                        {
                            "text": "Hij wil politieke munt slaan uit andermans <w>ellende</w>.",
                            "target_lang_translations": {
                                "en": "He wants to make political capital out of other people's <w>misery</w>.",
                                "ru": "Он хочет извлечь политическую выгоду из чужого <w>несчастья</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "misère",
                        "rampspoed",
                        "nood",
                        "narigheid"
                    ],
                    "antonyms": [
                        "geluk",
                        "voorspoed",
                        "welvaart"
                    ],
                    "common_phrases": [
                        "in de ellende zitten",
                        "diepe ellende",
                        "alle ellende van dien"
                    ],
                    "target_lang_definitions": {
                        "en": "Deplorable circumstances causing suffering, poverty, and grief.",
                        "ru": "Плачевные обстоятельства, вызывающие страдания, бедность и горе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "misery",

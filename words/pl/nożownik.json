                {
                    "sense_id": "3cd04415-c06e-316e-a679-a79ca54cb897",
                    "sense_definition": "Rzemieślnik zajmujący się zawodowo wyrabianiem noży.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "craftsman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony <w>nożownik</w> potrafi wykonać ostrze, które będzie służyć przez całe życie.",

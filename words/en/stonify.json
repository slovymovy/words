    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "67d77e9f-7523-3725-bca9-2c247713edee",
                    "sense_definition": "To turn into stone or a substance resembling stone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "literal_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the myth, the sorcerer used a spell to <w>stonify</w> his enemies instantly.",
                            "target_lang_translations": {
                                "ru": "В мифе колдун использовал заклинание, чтобы мгновенно <w>окаменить</w> своих врагов."
                            }
                        },
                        {
                            "text": "Over millions of years, the minerals in the water helped <w>stonify</w> the submerged wood.",
                            "target_lang_translations": {
                                "ru": "За миллионы лет минералы в воде помогли <w>превратить в камень</w> затонувшую древесину."
                            }
                        }
                    ],
                    "synonyms": [
                        "petrify",
                        "lapidify"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превращать в камень или вещество, напоминающее камень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окаменять",
                                "target_lang_sense_clarification": "превращать в камень"
                            },
                            {
                                "target_lang_word": "превращать в камень",
                                "target_lang_sense_clarification": "буквальное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bcb99df-c951-39c8-9429-5ec6cc534ba8",
                    "sense_definition": "To make someone unable to move or react, typically due to extreme fear or shock.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_immobilize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden, deafening noise seemed to <w>stonify</w> the deer standing on the road.",
                            "target_lang_translations": {
                                "ru": "Внезапный оглушительный шум, казалось, <w>окаменил</w> оленя, стоящего на дороге."
                            }
                        },
                        {
                            "text": "She was <w>stonified</w> by fear when she saw the shadow move across the wall.",
                            "target_lang_translations": {
                                "ru": "Она <w>оцепенела</w> от страха, когда увидела, как тень движется по стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "immobilize",
                        "paralyze",
                        "petrify",
                        "freeze"
                    ],
                    "antonyms": [
                        "mobilize",
                        "animate"
                    ],
                    "common_phrases": [
                        "stonified by fear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать способности двигаться или реагировать, обычно из-за сильного страха или шока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парализовать",
                                "target_lang_sense_clarification": "лишать способности действовать от страха"
                            },
                            {
                                "target_lang_word": "вводить в оцепенение",
                                "target_lang_sense_clarification": "заставлять застыть"
                            },
                            {
                                "target_lang_word": "окаменять",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
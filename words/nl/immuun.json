    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c24c44dd-84ab-3518-824a-e5b3dfba4c84",
                    "sense_definition": "Niet vatbaar voor een bepaalde ziekte of infectie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de vaccinatie ben je hopelijk <w>immuun</w> voor het virus.",
                            "target_lang_translations": {
                                "en": "After the vaccination, you will hopefully be <w>immune</w> to the virus.",
                                "ru": "После вакцинации вы, будем надеяться, станете <w>невосприимчивы</w> к вирусу."
                            }
                        },
                        {
                            "text": "Sommige mensen blijken van nature <w>immuun</w> te zijn voor deze aandoening.",
                            "target_lang_translations": {
                                "en": "Some people appear to be naturally <w>immune</w> to this condition.",
                                "ru": "Некоторые люди, как выясняется, от природы <w>невосприимчивы</w> к этому заболеванию."
                            }
                        }
                    ],
                    "synonyms": [
                        "onvatbaar",
                        "resistent"
                    ],
                    "antonyms": [
                        "vatbaar"
                    ],
                    "common_phrases": [
                        "immuun zijn voor",
                        "immuun maken"
                    ],
                    "target_lang_definitions": {
                        "en": "Not susceptible to a specific disease or infection.",
                        "ru": "Невосприимчивый к определенной болезни или инфекции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "immune",
                                "target_lang_sense_clarification": "protected against disease"
                            },
                            {
                                "target_lang_word": "resistant",
                                "target_lang_sense_clarification": "able to withstand"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "невосприимчивый",
                                "target_lang_sense_clarification": "устойчивый к заболеванию"
                            },
                            {
                                "target_lang_word": "иммунный",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "715a62ab-e319-34ae-b0af-4fea838e19fa",
                    "sense_definition": "Niet gevoelig voor invloeden van buitenaf (zoals kritiek) of vrijgesteld van bepaalde verplichtingen of straffen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politicus leek volledig <w>immuun</w> voor de harde kritiek van de pers.",
                            "target_lang_translations": {
                                "en": "The politician seemed completely <w>immune</w> to the harsh criticism from the press.",
                                "ru": "Политик казался совершенно <w>невосприимчивым</w> к жесткой критике со стороны прессы."
                            }
                        },
                        {
                            "text": "Buitenlandse diplomaten zijn in dit land <w>immuun</w> voor rechtsvervolging.",
                            "target_lang_translations": {
                                "en": "Foreign diplomats are <w>immune</w> from prosecution in this country.",
                                "ru": "Иностранные дипломаты в этой стране <w>обладают иммунитетом</w> от судебного преследования."
                            }
                        }
                    ],
                    "synonyms": [
                        "onschendbaar",
                        "ongevoelig"
                    ],
                    "antonyms": [
                        "vatbaar",
                        "gevoelig"
                    ],
                    "common_phrases": [
                        "immuun voor kritiek"
                    ],
                    "target_lang_definitions": {
                        "en": "Not sensitive to external influences (like criticism) or exempt from certain obligations or punishments.",
                        "ru": "Нечувствительный к внешним воздействиям (например, критике) или освобожденный от определенных обязательств или наказаний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "immune",
                                "target_lang_sense_clarification": "unaffected or exempt"
                            },
                            {
                                "target_lang_word": "exempt",
                                "target_lang_sense_clarification": "free from obligation"
                            },
                            {
                                "target_lang_word": "impervious",
                                "target_lang_sense_clarification": "not affected by"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "невосприимчивый",
                                "target_lang_sense_clarification": "не реагирующий на влияние"

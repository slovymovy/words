    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1439782-447d-3b94-9b38-da3d65bb0c16",
                    "sense_definition": "Tylna część tułowia zwierzęcia czworonożnego, zwłaszcza konia lub bydła.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten wyścigowy koń ma bardzo silny i umięśniony <w>zad</w>.",
                            "target_lang_translations": {
                                "ru": "У этой скаковой лошади очень сильный и мускулистый <w>круп</w>."
                            }
                        },
                        {
                            "text": "Rolnik klepnął krowę w <w>zad</w>, aby weszła do obory.",
                            "target_lang_translations": {
                                "ru": "Фермер шлепнул корову по <w>заду</w>, чтобы она зашла в хлев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koński zad",
                        "klepnąć w zad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя часть туловища животного, особенно лошади или крупного рогатого скота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круп",
                                "target_lang_sense_clarification": "анатомический термин, особенно у лошадей"
                            },
                            {
                                "target_lang_word": "зад",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcf560ba-ee43-354d-af4c-78a3536d89af",
                    "sense_definition": "Szerokie, duże pośladki człowieka (określenie potoczne lub zgrubiałe).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rusz wreszcie swój leniwy <w>zad</w> z kanapy i pomóż mi!",
                            "target_lang_translations": {
                                "ru": "Подними наконец свой ленивый <w>зад</w> с дивана и помоги мне!"
                            }
                        },
                        {
                            "text": "Nie zmieścisz swojego wielkiego <w>zadu</w> na tym małym krzesełku.",
                            "target_lang_translations": {
                                "ru": "Ты не уместишь свой огромный <w>зад</w> на этом маленьком стульчике."
                            }
                        }
                    ],
                    "synonyms": [
                        "dupsko"
                    ],
                    "common_phrases": [
                        "ruszyć zad",
                        "ciężki zad"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often augmentative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большие, широкие ягодицы человека (разговорное или грубое выражение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задница",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "зад",
                                "target_lang_sense_clarification": "разговорное, фамильярное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a879f30-84b2-3e43-8949-5dfde1a048ab",
                    "sense_definition": "Tylna część ugrupowania, oddziału wojskowego lub grupy ludzi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieprzyjaciel uderzył niespodziewanie w <w>zad</w> naszej armii.",
                            "target_lang_translations": {
                                "ru": "Неприятель неожиданно ударил в <w>тыл</w> нашей армии."
                            }
                        },
                        {
                            "text": "Wozy z zaopatrzeniem wlokły się na samym <w>zadzie</w> kolumny marszowej.",
                            "target_lang_translations": {
                                "ru": "Обозы с припасами плелись в самом <w>хвосте</w> походной колонны."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "military or literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя часть формирования, военного отряда или группы людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыл",
                                "target_lang_sense_clarification": "армии, отряда"
                            },
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "колонны, группы"
                            },
                            {
                                "target_lang_word": "арьергард",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            },
                            {
                                "target_lang_word": "establishment",
                                "target_lang_sense_clarification": "the act of founding or starting a business or organization"
                            },
                            {
                                "target_lang_word": "starting",
                                "target_lang_sense_clarification": "the action of beginning something, like a business or family"
                            }
                        ]

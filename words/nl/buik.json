                    ],
                    "common_phrases": [
                        "pijn in de buik",
                        "op de buik",
                        "platte buik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя часть туловища человека или животного, где находятся желудок и кишечник.",
                        "en": "The lower part of the torso of a human or animal, where the stomach and intestines are located."
                    },

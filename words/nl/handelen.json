                {
                    "sense_id": "68f89b46-5c91-3877-8f1d-271a55fcfad6",
                    "sense_definition": "Goederen kopen en verkopen; handel drijven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "trade_business",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is al jaren bezig met <w>handelen</w> in antiek en kunst.",
                            "target_lang_translations": {
                                "en": "He has been <w>trading</w> in antiques and art for years.",
                                "ru": "Он уже много лет <w>торгует</w> антиквариатом и предметами искусства."
                            }
                        },
                        {
                            "text": "De Nederlanders <w>handelden</w> vroeger met vele landen over de hele wereld.",
                            "target_lang_translations": {
                                "en": "The Dutch used to <w>trade</w> with many countries all over the world.",
                                "ru": "Раньше голландцы <w>торговали</w> со многими странами по всему миру."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakendoen",
                        "verhandelen",
                        "verkopen"
                    ],
                    "common_phrases": [
                        "handelen in",
                        "met iemand handelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To buy and sell goods; to conduct trade.",
                        "ru": "Покупать и продавать товары; заниматься торговлей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trade",
                                "target_lang_sense_clarification": "to buy and sell goods and services"
                            },
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "to do business, especially in specific goods"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "торговать",
                                "target_lang_sense_clarification": "заниматься куплей-продажей"
                            },
                            {
                                "target_lang_word": "вести торговлю",
                                "target_lang_sense_clarification": "осуществлять торговые операции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c28d6dba-c2b0-3cfb-a6da-a86fa23f90a2",
                    "sense_definition": "Iets doen of ondernemen; in actie komen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "act_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet in noodsituaties altijd snel en doeltreffend <w>handelen</w>.",
                            "target_lang_translations": {
                                "en": "You must always <w>act</w> quickly and effectively in emergency situations.",
                                "ru": "В чрезвычайных ситуациях всегда нужно <w>действовать</w> быстро и эффективно."
                            }
                        },
                        {
                            "text": "De politieagent <w>handelde</w> onmiddellijk toen hij het gevaar zag.",
                            "target_lang_translations": {
                                "en": "The police officer <w>acted</w> immediately when he saw the danger.",
                                "ru": "Полицейский немедленно <w>начал действовать</w>, когда увидел опасность."
                            }
                        }

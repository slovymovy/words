    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a3598fc9-8993-33f4-8c0a-55131349a748",
                    "sense_definition": "Исполняться голосом старательно, выразительно и протяжно (о песне, мелодии или звуках).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над широкой рекой протяжно и грустно <w>выпеваются</w> старинные народные песни.",
                            "target_lang_translations": {
                                "en": "Over the wide river, old folk songs <w>are sung out</w> lingeringly and sadly."
                            }
                        },
                        {
                            "text": "Каждая фраза в его торжественной речи не просто произносилась, а словно <w>выпевалась</w>.",
                            "target_lang_translations": {
                                "en": "Every phrase in his solemn speech was not merely spoken, but as if <w>sung out</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "исполняться",
                        "звучать",
                        "петься"
                    ],
                    "antonyms": [
                        "умолкать",
                        "затихать"
                    ],
                    "common_phrases": [
                        "мелодия выпевается",
                        "песня выпевается",
                        "слова выпеваются"
                    ],
                    "target_lang_definitions": {
                        "en": "To be performed vocally with diligence, expression, and duration (regarding a song, melody, or sounds)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be sung out",
                                "target_lang_sense_clarification": "to be performed with full voice and expression"
                            },
                            {
                                "target_lang_word": "ring out",
                                "target_lang_sense_clarification": "to sound clearly and resonantly (of a song)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63a96287-76c2-32da-a195-57048ac46789",
                    "sense_definition": "Исчерпывающе выражать свои чувства, душу и настроение через пение или творчество.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "self_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Настоящий артист должен уметь <w>выпеваться</w> на сцене, отдавая публике все свои эмоции.",
                            "target_lang_translations": {
                                "en": "A real artist must be able to <w>sing their heart out</w> on stage, giving all their emotions to the audience."
                            }
                        },
                        {
                            "text": "В своих лирических стихах поэт <w>выпевался</w> до конца, не скрывая душевной боли.",
                            "target_lang_translations": {
                                "en": "In his lyric poems, the poet <w>poured his soul out</w> completely, hiding no spiritual pain."
                            }
                        }
                    ],
                    "synonyms": [
                        "изливаться",
                        "раскрываться"
                    ],
                    "antonyms": [
                        "сдерживаться",
                        "замыкаться"
                    ],
                    "common_phrases": [
                        "выпеваться в песне",
                        "весь выпевайся"
                    ],
                    "target_lang_definitions": {
                        "en": "To exhaustively express one's feelings, soul, and mood through singing or creativity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sing one's heart out",
                                "target_lang_sense_clarification": "idiomatic; to express deep emotion through singing"
                            },
                            {
                                "target_lang_word": "pour one's soul out",
                                "target_lang_sense_clarification": "to express oneself fully, often through art"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
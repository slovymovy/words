{
    "word_family": [
        "prezentacja",
        "prezenter",
        "prezenterka",
        "prezentować"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ea0ba82b-0ff2-3eef-8c31-d9bb1d7a9458",
                    "sense_definition": "Pokazać coś publicznie, zademonstrować działanie lub wygląd czegoś (np. produktu, projektu, dzieła) szerszemu gronu odbiorców.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "demonstration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma planuje <zaprezentować> swój najnowszy model samochodu na międzynarodowych targach.",
                            "target_lang_translations": {
                                "ru": "Компания планирует <представить> свою новейшую модель автомобиля на международной выставке."
                            }
                        },
                        {
                            "text": "Uczniowie musieli <zaprezentować> wyniki swojej pracy grupowej przed całą klasą.",
                            "target_lang_translations": {
                                "ru": "Ученики должны были <презентовать> результаты своей групповой работы перед всем классом."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokazać",
                        "przedstawić"
                    ],
                    "common_phrases": [
                        "zaprezentować nowy produkt",
                        "zaprezentować wyniki",
                        "zaprezentować ofertę",
                        "zaprezentować swoje umiejętności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публично показать что-либо, продемонстрировать работу или внешний вид чего-либо (например, продукта, проекта) широкой аудитории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представить",
                                "target_lang_sense_clarification": "показать публике, ознакомить"
                            },
                            {
                                "target_lang_word": "презентовать",
                                "target_lang_sense_clarification": "провести презентацию нового продукта или проекта"
                            },
                            {
                                "target_lang_word": "продемонстрировать",
                                "target_lang_sense_clarification": "показать в действии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88a415d0-4e98-3653-91cf-99f5f4a7c5d4",
                    "sense_definition": "Oficjalnie przedstawić kogoś innej osobie lub grupie osób, często w sposób formalny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "introduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor chciał osobiście <zaprezentować> nowego kierownika wszystkim pracownikom biura.",
                            "target_lang_translations": {
                                "ru": "Директор хотел лично <представить> нового руководителя всем сотрудникам офиса."
                            }
                        },
                        {
                            "text": "Gospodarz uroczystości postanowił <zaprezentować> zaproszonych gości honorowych zebranym na sali.",
                            "target_lang_translations": {
                                "ru": "Хозяин торжества решил <представить> приглашенных почетных гостей собравшимся в зале."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawić"
                    ],
                    "common_phrases": [
                        "zaprezentować gościa",
                        "zaprezentować kandydata",
                        "zaprezentować zawodnika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально познакомить кого-либо с другим лицом или группой лиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представить",
                                "target_lang_sense_clarification": "познакомить кого-либо официально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65559d3e-5119-3fd5-9fb3-db22b57f5db4",
                    "sense_definition": "Wystąpić publicznie, pokazać się z określonej strony lub zrobić wrażenie na odbiorcach (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody pianista zdołał wspaniale <zaprezentować> się podczas finałowego koncertu w filharmonii.",
                            "target_lang_translations": {
                                "ru": "Молодому пианисту удалось великолепно <выступить> во время финального концерта в филармонии."
                            }
                        },
                        {
                            "text": "Musisz dobrze <zaprezentować> się na rozmowie kwalifikacyjnej, aby otrzymać to stanowisko.",
                            "target_lang_translations": {
                                "ru": "Ты должен хорошо <показать себя> на собеседовании, чтобы получить эту должность."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystąpić",
                        "pokazać się"
                    ],
                    "common_phrases": [
                        "zaprezentować się z dobrej strony",
                        "zaprezentować się na scenie",
                        "godnie się zaprezentować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступить публично, показать себя с определенной стороны или произвести впечатление (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показать себя",
                                "target_lang_sense_clarification": "произвести впечатление, проявить свои качества"
                            },
                            {
                                "target_lang_word": "выступить",
                                "target_lang_sense_clarification": "на сцене или перед аудиторией"
                            },
                            {
                                "target_lang_word": "проявить себя",
                                "target_lang_sense_clarification": "продемонстрировать свои способности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
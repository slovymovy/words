            "senses": [
                {
                    "sense_id": "a22651f0-9399-3622-bc94-0e1b3b3a2743",
                    "sense_definition": "De bijbelse aankondiging van de engel Gabriël aan Maria dat zij de moeder van Jezus zou worden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op het beroemde schilderij is de <w>annunciatie</w> in prachtige kleuren afgebeeld.",
                            "target_lang_translations": {
                                "ru": "На знаменитой картине <w>Благовещение</w> изображено в великолепных красках."
                            }
                        },
                        {
                            "text": "De kerk viert het feest van de <w>annunciatie</w> negen maanden voor Kerstmis.",
                            "target_lang_translations": {
                                "ru": "Церковь отмечает праздник <w>Благовещения</w> за девять месяцев до Рождества."
                            }
                        }
                    ],
                    "synonyms": [
                        "Maria-Boodschap"
                    ],
                    "common_phrases": [
                        "feest van de annunciatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейское возвещение архангела Гавриила Марии о том, что она станет матерью Иисуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Благовещение",
                                "target_lang_sense_clarification": "религиозное событие и праздник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b38941c-edea-3e2a-8161-f3ba6847fdf0",
                    "sense_definition": "Een plechtige aankondiging of bekendmaking.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "general_announcement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>annunciatie</w> van zijn vertrek kwam voor velen als een verrassing.",
                            "target_lang_translations": {
                                "ru": "<w>Возвещение</w> о его отъезде стало для многих неожиданностью."
                            }
                        },
                        {
                            "text": "Na de officiële <w>annunciatie</w> begon het publiek luid te applaudisseren.",
                            "target_lang_translations": {
                                "ru": "После официального <w>объявления</w> публика начала громко аплодировать."
                            }
                        }
                    ],
                    "synonyms": [
                        "afkondiging",
                        "aanzegging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественное объявление или оповещение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвещение",
                                "target_lang_sense_clarification": "торжественное сообщение"
                            },
                            {
                                "target_lang_word": "объявление",
                                "target_lang_sense_clarification": "официальное сообщение"
                            }
                        ]
                    }
                }
            ]

{
    "word_family": [
        "bouncy",
        "bouncer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a9beb44b-565e-3250-af98-864be79d6f79",
                    "sense_definition": "To change the direction of motion after hitting an obstacle or surface.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tennis ball <w>bounced</w> off the wall and rolled away.",
                            "target_lang_translations": {
                                "ru": "Теннисный мяч <w>отскочил</w> от стены и откатился."
                            }
                        },
                        {
                            "text": "If you drop the rubber ball, it will <w>bounce</w> back up.",
                            "target_lang_translations": {
                                "ru": "Если бросить резиновый мяч, он <w>отскочит</w> обратно вверх."
                            }
                        }
                    ],
                    "synonyms": [
                        "rebound",
                        "bounce back"
                    ],
                    "common_phrases": [
                        "bounce off",
                        "bounce back"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить направление движения после удара о препятствие или поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отскакивать",
                                "target_lang_sense_clarification": "отлетать после удара"
                            },
                            {
                                "target_lang_word": "отскочить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eabf4e4f-eab9-3c4f-8733-2f30b23ba6d2",
                    "sense_definition": "To move quickly up and then down (or vice versa), once or repeatedly.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The excited child <w>bounces</w> on the bed every morning.",
                            "target_lang_translations": {
                                "ru": "Взволнованный ребенок каждое утро <w>прыгает</w> на кровати."
                            }
                        },
                        {
                            "text": "He <w>bounced</w> nervously in his chair while waiting for the interview.",
                            "target_lang_translations": {
                                "ru": "Он нервно <w>подпрыгивал</w> на стуле в ожидании собеседования."
                            }
                        }
                    ],
                    "synonyms": [
                        "bob"
                    ],
                    "common_phrases": [
                        "bounce up and down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро двигаться вверх и вниз (или наоборот), один раз или многократно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подпрыгивать",
                                "target_lang_sense_clarification": "делать небольшие прыжки"
                            },
                            {
                                "target_lang_word": "скакать",
                                "target_lang_sense_clarification": "резво прыгать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40496235-7097-3296-8007-969b65a8e628",
                    "sense_definition": "To cause an object to move quickly up and down, or to hit a surface and rebound.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player <w>bounced</w> the basketball on the court before shooting.",
                            "target_lang_translations": {
                                "ru": "Игрок <w>постучал</w> баскетбольным мячом об пол перед броском."
                            }
                        },
                        {
                            "text": "She gently <w>bounced</w> the baby on her knee to soothe him.",
                            "target_lang_translations": {
                                "ru": "Она мягко <w>качала</w> ребенка на колене, чтобы успокоить его."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bounce a ball"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять предмет быстро двигаться вверх-вниз или ударяться о поверхность и отскакивать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "мячом об пол"
                            },
                            {
                                "target_lang_word": "качать",
                                "target_lang_sense_clarification": "ребенка на коленях"
                            },
                            {
                                "target_lang_word": "чеканить",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdad870a-0a02-3663-ac03-4aaa0fedad2e",
                    "sense_definition": "To suggest or introduce an idea to someone in order to gain feedback.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would like to <w>bounce</w> some ideas off you regarding the new project.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>обсудить</w> с тобой некоторые идеи касательно нового проекта."
                            }
                        },
                        {
                            "text": "They met for coffee to <w>bounce</w> thoughts off each other.",
                            "target_lang_translations": {
                                "ru": "Они встретились за кофе, чтобы <w>обменяться</w> мыслями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bounce ideas off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предложить или высказать идею кому-либо, чтобы узнать мнение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обсуждать",
                                "target_lang_sense_clarification": "идеи"
                            },
                            {
                                "target_lang_word": "делиться",
                                "target_lang_sense_clarification": "мыслями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e084dd3-0d98-3d71-833f-695324ce98d5",
                    "sense_definition": "To return undelivered (of an email or message).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "failure_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My email to her <w>bounced</w> because the address was incorrect.",
                            "target_lang_translations": {
                                "ru": "Мое письмо к ней <w>вернулось</w>, потому что адрес был неверен."
                            }
                        },
                        {
                            "text": "If the mailbox is full, the message will <w>bounce</w> back to the sender.",
                            "target_lang_translations": {
                                "ru": "Если почтовый ящик переполнен, сообщение <w>вернется</w> отправителю."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвращаться отправителю (об электронном письме или сообщении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "о письме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "492707ca-6daa-3c15-9c7d-0a24ddd0591c",
                    "sense_definition": "To be refused by a bank because there are insufficient funds in the account.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "failure_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was embarrassed when his check <w>bounced</w> at the grocery store.",
                            "target_lang_translations": {
                                "ru": "Ему было стыдно, когда его чек <w>не прошел</w> в продуктовом магазине."
                            }
                        },
                        {
                            "text": "The bank will charge a fee if your payment <w>bounces</w>.",
                            "target_lang_translations": {
                                "ru": "Банк возьмет комиссию, если ваш платеж <w>будет отклонен</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "check bounced"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть отклоненным банком из-за недостатка средств на счете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть отклоненным",
                                "target_lang_sense_clarification": "о платеже"
                            },
                            {
                                "target_lang_word": "не проходить",
                                "target_lang_sense_clarification": "об оплате"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7240bac4-1e61-388d-8754-ed6e69b705ec",
                    "sense_definition": "To leave a place.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "departure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is getting late, so I gotta <w>bounce</w>.",
                            "target_lang_translations": {
                                "ru": "Уже поздно, так что мне пора <w>сваливать</w>."
                            }
                        },
                        {
                            "text": "We stayed at the party for an hour and then <w>bounced</w>.",
                            "target_lang_translations": {
                                "ru": "Мы побыли на вечеринке час, а потом <w>свалили</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gotta bounce"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уходить, покидать место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сваливать",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a2d8727d-58de-3046-8b21-5375420aa68b",
                    "sense_definition": "A change of direction of motion after hitting the ground or an obstacle.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ball took a strange <w>bounce</w> and hit the goalpost.",
                            "target_lang_translations": {
                                "ru": "Мяч сделал странный <w>отскок</w> и ударился о штангу ворот."
                            }
                        },
                        {
                            "text": "With one lucky <w>bounce</w>, the coin landed in the cup.",
                            "target_lang_translations": {
                                "ru": "Благодаря одному удачному <w>отскоку</w> монета приземлилась в чашку."
                            }
                        }
                    ],
                    "synonyms": [
                        "rebound"
                    ],
                    "common_phrases": [
                        "lucky bounce",
                        "bad bounce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменение направления движения после удара о землю или препятствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отскок",
                                "target_lang_sense_clarification": "движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a0b0021-9b35-3aff-bb2a-353b993deb24",
                    "sense_definition": "An increase in popularity or success.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "popularity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate received a significant <w>bounce</w> in the polls after the convention.",
                            "target_lang_translations": {
                                "ru": "Кандидат получил значительный <w>скачок</w> в опросах после съезда."
                            }
                        },
                        {
                            "text": "The new product gave the company a temporary <w>bounce</w> in sales.",
                            "target_lang_translations": {
                                "ru": "Новый продукт обеспечил компании временный <w>подъем</w> продаж."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рост популярности или успеха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скачок",
                                "target_lang_sense_clarification": "резкий рост"
                            },
                            {
                                "target_lang_word": "подъем",
                                "target_lang_sense_clarification": "увеличение показателей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
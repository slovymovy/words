{
    "word_family": [
        "schilderen",
        "schilder",
        "schilderij",
        "beschilderen",
        "herschilderen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4556cbdb-53b2-31ac-938d-2b35635e1e19",
                    "sense_definition": "Een nieuwe laag verf aanbrengen op een oppervlak dat al eerder geschilderd is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan de muren in de woonkamer wit <w>overschilderen</w>.",
                            "target_lang_translations": {
                                "ru": "Мы собираемся <w>перекрасить</w> стены в гостиной в белый цвет."
                            }
                        },
                        {
                            "text": "Hij <w>schilderde</w> de beschadigde plekken op de deur zorgvuldig <w>over</w>.",
                            "target_lang_translations": {
                                "ru": "Он тщательно <w>закрасил</w> поврежденные места на двери."
                            }
                        }
                    ],
                    "synonyms": [
                        "oververven",
                        "herschilderen"
                    ],
                    "common_phrases": [
                        "de kozijnen overschilderen",
                        "in een andere kleur overschilderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести новый слой краски на поверхность, которая уже была окрашена ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекрашивать",
                                "target_lang_sense_clarification": "нанести новый цвет или обновить старый"
                            },
                            {
                                "target_lang_word": "красить заново",
                                "target_lang_sense_clarification": "покрыть краской еще раз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed380c25-5afc-38dd-a428-84e6bf6b4b9b",
                    "sense_definition": "Iets volledig met verf bedekken zodat de ondergrond of de vorige afbeelding niet meer zichtbaar is.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kunstenaar heeft het oorspronkelijke werk volledig <w>overschilderd</w> met een nieuw tafereel.",
                            "target_lang_translations": {
                                "ru": "Художник полностью <w>закрасил</w> первоначальную работу новой картиной."
                            }
                        },
                        {
                            "text": "Zij <w>overschilderde</w> de oude muurschildering om deze te verbergen.",
                            "target_lang_translations": {
                                "ru": "Она <w>закрасила</w> старую роспись, чтобы скрыть её."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegverven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью покрыть что-либо краской так, чтобы основа или предыдущее изображение больше не были видны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрашивать",
                                "target_lang_sense_clarification": "скрыть под слоем краски"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
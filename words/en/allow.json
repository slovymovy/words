{
    "word_family": [
        "allowance",
        "allowable",
        "disallow"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1b83a3d2-bff6-3f64-816c-8db3afe29703",
                    "sense_definition": "To give permission to someone to do something, or to not prevent something from happening.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents do not <w>allow</w> me to stay out late on school nights.",
                            "target_lang_translations": {
                                "ru": "Мои родители не <w>разрешают</w> мне гулять поздно вечером в учебные дни."
                            }
                        },
                        {
                            "text": "Smoking is not <w>allowed</w> inside the office building.",
                            "target_lang_translations": {
                                "ru": "Курение внутри офисного здания не <w>разрешается</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "permit",
                        "let",
                        "consent",
                        "grant"
                    ],
                    "antonyms": [
                        "forbid",
                        "ban",
                        "prohibit"
                    ],
                    "common_phrases": [
                        "allow me to",
                        "not allowed",
                        "allow access"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать разрешение кому-либо сделать что-либо или не препятствовать чему-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрешать",
                                "target_lang_sense_clarification": "давать согласие"
                            },
                            {
                                "target_lang_word": "позволять",
                                "target_lang_sense_clarification": "не препятствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3724ed22-707d-3b05-a0cb-49cea0713258",
                    "sense_definition": "To make it possible for something to happen or for someone to do something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software <w>allows</w> users to work from home more efficiently.",
                            "target_lang_translations": {
                                "ru": "Новое программное обеспечение <w>позволяет</w> пользователям работать из дома более эффективно."
                            }
                        },
                        {
                            "text": "This wide door <w>allows</w> easy access for people in wheelchairs.",
                            "target_lang_translations": {
                                "ru": "Эта широкая дверь <w>позволяет</w> легко проехать людям в инвалидных колясках."
                            }
                        }
                    ],
                    "synonyms": [
                        "permit",
                        "let"
                    ],
                    "common_phrases": [
                        "allow someone to",
                        "allow for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать возможным что-либо или давать возможность кому-либо сделать что-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позволять",
                                "target_lang_sense_clarification": "давать возможность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68647a38-c83e-3600-bb92-970c987a8ed2",
                    "sense_definition": "To give a specific amount of time, money, or other resources to someone or for a purpose.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>allow</w> at least an hour to get to the airport during rush hour.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>заложить</w> не менее часа, чтобы добраться до аэропорта в час пик."
                            }
                        },
                        {
                            "text": "His parents <w>allow</w> him a small amount of pocket money each week.",
                            "target_lang_translations": {
                                "ru": "Родители <w>выделяют</w> ему небольшую сумму карманных денег каждую неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "allot",
                        "grant",
                        "give",
                        "afford"
                    ],
                    "common_phrases": [
                        "allow time",
                        "allow money"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделять определенное количество времени, денег или других ресурсов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделять",
                                "target_lang_sense_clarification": "ресурсы, деньги"
                            },
                            {
                                "target_lang_word": "отводить",
                                "target_lang_sense_clarification": "время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f16f798d-629f-3a85-a34b-4115fe8ab51b",
                    "sense_definition": "To include something in a plan or calculation; to take into consideration.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When planning the event, we must <w>allow</w> for the possibility of rain.",
                            "target_lang_translations": {
                                "ru": "При планировании мероприятия мы должны <w>учитывать</w> возможность дождя."
                            }
                        },
                        {
                            "text": "The project budget <w>allows</w> for unexpected costs that might arise.",
                            "target_lang_translations": {
                                "ru": "Бюджет проекта <w>предусматривает</w> непредвиденные расходы, которые могут возникнуть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "allow for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Включать что-либо в план или расчет; принимать во внимание"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учитывать",
                                "target_lang_sense_clarification": "принимать в расчет"
                            },
                            {
                                "target_lang_word": "принимать во внимание",
                                "target_lang_sense_clarification": "брать в расчет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f41e83b-0d15-3caf-b2a5-ceed2b518563",
                    "sense_definition": "To admit that something is true or correct, often unwillingly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I must <w>allow</w> that your argument has some merit, even if I disagree.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>признать</w>, что твой аргумент имеет смысл, даже если я не согласен."
                            }
                        },
                        {
                            "text": "She refused to <w>allow</w> that she had made a mistake during the process.",
                            "target_lang_translations": {
                                "ru": "Она отказалась <w>признать</w>, что допустила ошибку в процессе."
                            }
                        }
                    ],
                    "synonyms": [
                        "admit",
                        "concede",
                        "accede",
                        "acquiesce"
                    ],
                    "common_phrases": [
                        "allow that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Признавать, что что-то является правдой или верным, часто неохотно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признавать",
                                "target_lang_sense_clarification": "соглашаться с фактом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d29ed98-faf7-325f-b426-e34f708e0293",
                    "sense_definition": "To officially accept a legal request or appeal.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge decided to <w>allow</w> the appeal after reviewing the new evidence.",
                            "target_lang_translations": {
                                "ru": "Судья решил <w>удовлетворить</w> апелляцию после рассмотрения новых доказательств."
                            }
                        },
                        {
                            "text": "The court <w>allowed</w> the objection raised by the defense attorney.",
                            "target_lang_translations": {
                                "ru": "Суд <w>принял</w> возражение, выдвинутое адвокатом защиты."
                            }
                        }
                    ],
                    "synonyms": [
                        "grant"
                    ],
                    "common_phrases": [
                        "allow an appeal",
                        "allow a claim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально принимать юридический запрос или апелляцию"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удовлетворять",
                                "target_lang_sense_clarification": "апелляцию, иск"
                            },
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "к рассмотрению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72821ab8-2ec0-34b7-9157-172dbc1bbd93",
                    "sense_definition": "To stop doing something or to let a situation pass without reacting.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was annoying me, but I just <w>allowed</w> it to avoid a fight.",
                            "target_lang_translations": {
                                "ru": "Он меня раздражал, но я просто <w>оставил</w> это, чтобы избежать драки."
                            }
                        },
                        {
                            "text": "You should just <w>allow</w> it and move on with your day.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит просто <w>забить</w> на это и жить дальше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "allow it"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used in MLE (Multicultural London English)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставить ситуацию без внимания или перестать реагировать"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "в покое"
                            },
                            {
                                "target_lang_word": "забить",
                                "target_lang_sense_clarification": "сленг: игнорировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
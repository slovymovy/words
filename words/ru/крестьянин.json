    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "29b8703f-9424-365b-a179-5c558dcc4818",
                    "sense_definition": "Сельский житель, основным занятием которого является земледелие и животноводство",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "farmer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый <крестьянин> в этой деревне имеет своё небольшое хозяйство.",
                            "target_lang_translations": {
                                "en": "Every <w>peasant</w> in this village has their own small farm."
                            }
                        },
                        {
                            "text": "Тяжёлый труд <крестьянина> обеспечивает город продуктами питания.",
                            "target_lang_translations": {
                                "en": "The hard labor of a <w>peasant</w> provides the city with food."
                            }
                        }
                    ],
                    "synonyms": [
                        "земледелец",
                        "землепа́шец"
                    ],
                    "common_phrases": [
                        "русский крестьянин",
                        "бедный крестьянин",
                        "зажиточный крестьянин"
                    ],
                    "target_lang_definitions": {
                        "en": "A rural resident whose main occupation is farming and animal husbandry"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "peasant",
                                "target_lang_sense_clarification": "agricultural worker of low social status"
                            },
                            {
                                "target_lang_word": "farmer",
                                "target_lang_sense_clarification": "person who operates a farm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e99be764-2a80-3d02-8b22-86c75cf42c66",
                    "sense_definition": "Человек, находящийся в личной зависимости от помещика и занимающийся сельскохозяйственным трудом (в историческом контексте)",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "serf",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Богатый помещик владел сотнями душ <крестьян>.",
                            "target_lang_translations": {
                                "en": "The rich landlord owned hundreds of <w>peasant</w> souls."
                            }
                        },
                        {
                            "text": "В девятнадцатом веке каждый крепостной <крестьянин> мечтал получить свободу.",
                            "target_lang_translations": {
                                "en": "In the nineteenth century, every bonded <w>peasant</w> dreamed of gaining freedom."
                            }
                        }
                    ],
                    "synonyms": [
                        "крепостной"
                    ],
                    "common_phrases": [
                        "крепостной крестьянин",
                        "освобождение крестьян"
                    ],
                    "target_lang_definitions": {
                        "en": "A person personally dependent on a landlord and engaged in agricultural labor (in a historical context)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serf",
                                "target_lang_sense_clarification": "unfree peasant bound to the land"
                            },
                            {
                                "target_lang_word": "peasant",
                                "target_lang_sense_clarification": "historical agricultural worker"
                            }
                        ]
                    }
                }
            ]
        }
    ]

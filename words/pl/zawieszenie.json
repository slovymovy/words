                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tata poprosił mnie o <w>zawieszenie</w> nowej półki w kuchni.",
                            "target_lang_translations": {

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тестовый матч",
                                "target_lang_sense_clarification": "крикет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1d341033-6b1c-32ec-b5e3-67d843cf1464",
                    "sense_definition": "To check the quality, performance, or reliability of something by using or operating it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "evaluation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists <w>test</w> new medicines on animals before giving them to humans.",
                            "target_lang_translations": {
                                "ru": "Ученые <w>испытывают</w> новые лекарства на животных, прежде чем давать их людям."
                            }
                        },
                        {
                            "text": "You should <w>test</w> the fire alarm regularly to ensure it works properly.",
                            "target_lang_translations": {
                                "ru": "Вам следует регулярно <w>проверять</w> пожарную сигнализацию, чтобы убедиться, что она работает правильно."
                            }
                        }
                    ],
                    "synonyms": [
                        "examination",
                        "trial"
                    ],
                    "common_phrases": [
                        "test a theory",
                        "test the water",
                        "test a product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять качество, эффективность или надежность чего-либо путем использования или эксплуатации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тестировать",
                                "target_lang_sense_clarification": "проверять работу"
                            },
                            {
                                "target_lang_word": "испытывать",
                                "target_lang_sense_clarification": "подвергать испытанию"
                            },
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "удостоверяться в исправности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08a172a3-2c41-3cc0-a9ae-c26e727dfac9",
                    "sense_definition": "To give someone an examination to evaluate their knowledge or skills.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "evaluation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher will <w>test</w> us on irregular verbs next Monday.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>проверит</w> нас по неправильным глаголам в следующий понедельник."
                            }
                        },
                        {
                            "text": "This quiz is designed to <w>test</w> your understanding of the previous chapter.",
                            "target_lang_translations": {
                                "ru": "Эта викторина разработана, чтобы <w>проверить</w> ваше понимание предыдущей главы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "test students",
                        "test one's knowledge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить экзамен или проверку для оценки чьих-либо знаний или навыков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "знания"
                            },
                            {
                                "target_lang_word": "тестировать",
                                "target_lang_sense_clarification": "проводить тест"
                            },
                            {
                                "target_lang_word": "экзаменовать",
                                "target_lang_sense_clarification": "принимать экзамен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8522eb0e-c9c4-33af-acec-461e8935d061",
                    "sense_definition": "To produce a specific result (usually positive or negative) in a medical or chemical examination.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "evaluation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The athlete was banned from the competition after he <w>tested</w> positive for drugs.",
                            "target_lang_translations": {
                                "ru": "Спортсмен был отстранен от соревнований после того, как он <w>сдал положительный тест</w> на наркотики."
                            }
                        },
                        {
                            "text": "She was relieved when she <w>tested</w> negative for the virus.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала облегчение, когда ее <w>тест</w> на вирус <w>оказался</w> отрицательным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "test positive",
                        "test negative"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показать определенный результат (обычно положительный или отрицательный) при медицинском или химическом исследовании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показать результат",
                                "target_lang_sense_clarification": "в анализах"
                            },
                            {
                                "target_lang_word": "сдать тест",
                                "target_lang_sense_clarification": "пройти проверку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c768e81-0d9b-384c-94b0-325b708422a7",
                    "sense_definition": "To put a strain on something; to be a difficult challenge for someone's abilities or patience.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "challenge_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The screaming toddler began to <w>test</w> his mother's patience in the supermarket.",
                            "target_lang_translations": {
                                "ru": "Кричащий малыш начал <w>испытывать</w> терпение своей матери в супермаркете."
                            }
                        },
                        {
                            "text": "The rough terrain will really <w>test</w> the suspension of this vehicle.",
                            "target_lang_translations": {
                                "ru": "Пересеченная местность действительно <w>испытает</w> подвеску этого автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "challenge"
                    ],
                    "common_phrases": [
                        "test one's patience",
                        "test the limits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать нагрузке; быть трудным испытанием для чьих-либо способностей или терпения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испытывать",
                                "target_lang_sense_clarification": "терпение или прочность"
                            }
                        ]
                    }
                }
            ]
        },

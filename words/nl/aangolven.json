                {
                    "sense_id": "f2aebe4f-eb46-3af7-87bf-5ac458051489",
                    "sense_definition": "Naderen in de vorm van golven, meestal gezegd van water of een andere vloeistof.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "naderen_als_golven",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij vloed zagen we het water snel <w>aangolven</w>.",
                            "target_lang_translations": {
                                "en": "At high tide, we saw the water quickly <w>roll in</w>."
                            }
                        },
                        {
                            "text": "De zee kwam van twee kanten tegelijk op de toren <w>aangolven</w>.",
                            "target_lang_translations": {
                                "en": "The sea came <w>surging</w> towards the tower from two sides at once."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanrollen"
                    ],
                    "common_phrases": [
                        "komen aangolven"
                    ],
                    "target_lang_definitions": {
                        "en": "To approach in the form of waves, usually referring to water or liquid."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll in",
                                "target_lang_sense_clarification": "movement of waves towards the shore"
                            },
                            {
                                "target_lang_word": "surge towards",
                                "target_lang_sense_clarification": "forceful movement of liquid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30a9dc5d-788e-3bec-ab69-f660ae80dc7c",
                    "sense_definition": "Naderen in grote, niet te stuiten aantallen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "naderen_als_golven",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De menigte kwam als een onstuitbare vloed op het plein <w>aangolven</w>.",
                            "target_lang_translations": {
                                "en": "The crowd came <w>surging</w> onto the square like an unstoppable flood."
                            }
                        },
                        {
                            "text": "Steeds meer nieuwsgierige mensen kwamen op het geluid <w>aangolven</w>.",
                            "target_lang_translations": {
                                "en": "More and more curious people came <w>streaming</w> towards the sound."
                            }
                        }

            "senses": [
                {
                    "sense_id": "3d8b0eb5-610f-333f-b7d7-41e7e44a77df",
                    "sense_definition": "Used to express thought, reflection, hesitation, or doubt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hm</w>, I need to check my schedule before I can answer.",
                            "target_lang_translations": {
                                "ru": "<w>Хм</w>, мне нужно проверить свое расписание, прежде чем я смогу ответить."
                            }
                        },
                        {
                            "text": "Is this the correct road? <w>Hm</w>, I am not entirely sure.",
                            "target_lang_translations": {
                                "ru": "Это правильная дорога? <w>Хм</w>, я не совсем уверен."
                            }
                        }
                    ],
                    "synonyms": [
                        "hmm",
                        "um",
                        "erm"
                    ],
                    "common_phrases": [
                        "hm, let me see",
                        "hm, interesting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения раздумья, размышления, нерешительности или сомнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хм",
                                "target_lang_sense_clarification": "междометие, выражающее раздумье"
                            },
                            {
                                "target_lang_word": "гм",
                                "target_lang_sense_clarification": "вариант произношения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "597840fd-67b2-34e6-8617-82d11b8925c9",
                    "sense_definition": "Initialism of His Majesty or Her Majesty; a title of respect used for a king or queen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "titles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The official proclamation was signed by <w>HM</w> the King yesterday.",
                            "target_lang_translations": {
                                "ru": "Официальная прокламация была подписана <w>Его Величеством</w> королем вчера."
                            }
                        },
                        {
                            "text": "Guests stood in silence as <w>HM</w> Queen Elizabeth entered the room.",
                            "target_lang_translations": {
                                "ru": "Гости стояли молча, когда <w>Ее Величество</w> королева Елизавета вошла в зал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "HM the King",
                        "HM the Queen",
                        "HM Government"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от His Majesty или Her Majesty (Его/Ее Величество); почтительный титул короля или королевы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Его Величество",
                                "target_lang_sense_clarification": "титул короля"
                            },
                            {
                                "target_lang_word": "Ее Величество",
                                "target_lang_sense_clarification": "титул королевы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb1338f6-e400-3047-83d7-e5d112764327",
                    "sense_definition": "Initialism of heavy metal; a genre of rock music characterized by a thick, massive sound.",

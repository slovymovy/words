                {
                    "sense_id": "876b1dc5-035e-353f-b8e1-bf156e1aee83",
                    "sense_definition": "Skupić myśli na czymś przez chwilę, zastanowić się nad jakimś tematem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "thinking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę chwilę <pomyśleć>, zanim odpowiem na twoje pytanie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немного <подумать>, прежде чем я отвечу на твой вопрос."
                            }
                        },
                        {
                            "text": "Daj mi <pomyśleć>, gdzie położyłem klucze.",
                            "target_lang_translations": {
                                "ru": "Дай мне <подумать>, куда я положил ключи."
                            }
                        }
                    ],
                    "synonyms": [
                        "zastanowić się",
                        "rozważyć"
                    ],
                    "common_phrases": [
                        "daj mi pomyśleć",
                        "pomyśleć nad rozwiązaniem",
                        "pomyśleć o przyszłości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосредоточиться мыслями на чем-то, поразмышлять некоторое время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подумать",
                                "target_lang_sense_clarification": "поразмышлять"
                            },
                            {
                                "target_lang_word": "обдумать",
                                "target_lang_sense_clarification": "рассмотреть вопрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7911de94-005e-3cd9-9921-6c8ef57dcec1",
                    "sense_definition": "Uświadomić sobie coś, dojść do jakiegoś wniosku lub wyobrazić sobie możliwą sytuację.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "thinking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kto by <pomyślał>, że ten film będzie taki ciekawy.",
                            "target_lang_translations": {
                                "ru": "Кто бы <подумал>, что этот фильм будет таким интересным."
                            }
                        },
                        {
                            "text": "Aż strach <pomyśleć>, co mogło się stać na drodze.",
                            "target_lang_translations": {
                                "ru": "Даже страшно <подумать>, что могло случиться на дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyobrazić sobie",
                        "uznać",
                        "stwierdzić"
                    ],
                    "common_phrases": [
                        "kto by pomyślał",
                        "strach pomyśleć",
                        "pomyśl życzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осознать что-то, прийти к выводу или представить себе возможную ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подумать",
                                "target_lang_sense_clarification": "представить себе, прийти к мысли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be4cfc99-6419-37dc-af90-0a1f3e21f679",
                    "sense_definition": "Zadbać o coś, uwzględnić coś w planach lub zająć się organizacją czegoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "care_planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <pomyśleć> o prezencie urodzinowym dla mamy.",
                            "target_lang_translations": {
                                "ru": "Мы должны <подумать> о подарке на день рождения для мамы."
                            }
                        },
                        {
                            "text": "Warto wcześniej <pomyśleć> o rezerwacji hotelu na wakacje.",
                            "target_lang_translations": {
                                "ru": "Стоит заранее <подумать> о бронировании отеля на отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "zadbać",
                        "zająć się",
                        "zaplanować"
                    ],
                    "antonyms": [
                        "zaniedbać",
                        "zlekceważyć"
                    ],
                    "common_phrases": [
                        "pomyśleć o wszystkim",
                        "pomyśleć o sobie",
                        "pomyśleć o obiedzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позаботиться о чем-то, учесть что-то в планах или заняться организацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подумать",
                                "target_lang_sense_clarification": "позаботиться, предусмотреть"
                            },
                            {
                                "target_lang_word": "позаботиться",
                                "target_lang_sense_clarification": "принять меры"

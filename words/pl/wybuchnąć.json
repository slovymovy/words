{
    "word_family": [
        "wybuch",
        "wybuchowy",
        "wybuchowość",
        "niewybuch"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd4eea8f-55fa-385a-9494-cdccf357ee22",
                    "sense_definition": "Rozpocząć się nagle i gwałtownie (dotyczy zjawisk takich jak wojna, pożar, epidemia lub kłótnia).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Druga wojna światowa <wybuchła> pierwszego września tysiąc dziewięćset trzydziestego dziewiątego roku.",
                            "target_lang_translations": {
                                "ru": "Вторая мировая война <вспыхнула> первого сентября тысяча девятьсот тридцать девятого года."
                            }
                        },
                        {
                            "text": "Wczoraj w nocy w starej fabryce <wybuchł> groźny pożar.",
                            "target_lang_translations": {
                                "ru": "Вчера ночью на старой фабрике <вспыхнул> сильный пожар."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpocząć się",
                        "zacząć się"
                    ],
                    "antonyms": [
                        "wygasnąć",
                        "skończyć się",
                        "ustać"
                    ],
                    "common_phrases": [
                        "wybuchła wojna",
                        "wybuchł pożar",
                        "wybuchła epidemia",
                        "wybuchła panika",
                        "wybuchł skandal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начаться внезапно и бурно (о таких явлениях, как война, пожар, эпидемия или ссора)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспыхнуть",
                                "target_lang_sense_clarification": "о войне, пожаре, эпидемии"
                            },
                            {
                                "target_lang_word": "разразиться",
                                "target_lang_sense_clarification": "о войне, скандале, грозе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82fbe5ea-747f-3864-89ec-4f8b6b4fd2ce",
                    "sense_definition": "Ulec gwałtownemu rozerwaniu, często z hukiem, na skutek wysokiego ciśnienia lub reakcji chemicznej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_explosion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary wulkan niespodziewanie <wybuchł> po wielu latach uśpienia.",
                            "target_lang_translations": {
                                "ru": "Старый вулкан неожиданно <извергся> после многих лет спячки."
                            }
                        },
                        {
                            "text": "Jeśli nie będziesz ostrożny z gazem, butla może <wybuchnąć>.",
                            "target_lang_translations": {
                                "ru": "Если ты не будешь осторожен с газом, баллон может <взорваться>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eksplodować",
                        "pęknąć"
                    ],
                    "common_phrases": [
                        "bomba wybuchła",
                        "wulkan wybuchł",
                        "wybuchnąć w powietrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взорваться или разорваться с шумом из-за высокого давления или химической реакции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взорваться",
                                "target_lang_sense_clarification": "о бомбе, газе, устройстве"
                            },
                            {
                                "target_lang_word": "извергнуться",
                                "target_lang_sense_clarification": "о вулкане"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "982998c1-19d5-359d-a010-c1a0c166fcc5",
                    "sense_definition": "Dać nagły upust silnym emocjom lub gwałtownie zacząć wykonywać jakąś czynność, taką jak śmiech lub płacz.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy usłyszała ten zabawny żart, głośno <wybuchnęła> śmiechem.",
                            "target_lang_translations": {
                                "ru": "Когда она услышала эту смешную шутку, она громко <рассмеялась>."
                            }
                        },
                        {
                            "text": "Ojciec <wybuchnął> gniewem, gdy dowiedział się o zachowaniu syna w szkole.",
                            "target_lang_translations": {
                                "ru": "Отец <взорвался> гневом, когда узнал о поведении сына в школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpłakać się",
                        "zaśmiać się",
                        "wściec się",
                        "uniesć się"
                    ],
                    "antonyms": [
                        "uspokoić się",
                        "opanować się"
                    ],
                    "common_phrases": [
                        "wybuchnąć śmiechem",
                        "wybuchnąć płaczem",
                        "wybuchnąć gniewem",
                        "wybuchnąć złością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно дать волю сильным эмоциям или резко начать какое-либо действие, например смех или плач."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взорваться",
                                "target_lang_sense_clarification": "о гневе или эмоциях"
                            },
                            {
                                "target_lang_word": "рассмеяться",
                                "target_lang_sense_clarification": "резко начать смеяться"
                            },
                            {
                                "target_lang_word": "расплакаться",
                                "target_lang_sense_clarification": "резко начать плакать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
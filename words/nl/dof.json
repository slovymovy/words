    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3343f6be-6437-333d-af87-ab57e3c1e254",
                    "sense_definition": "Zonder glans of weerschijn; niet glimmend.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sensory_dullness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lang poetsen kregen de <w>doffe</w> schoenen hun glans terug.",
                            "target_lang_translations": {
                                "en": "After long polishing, the <w>dull</w> shoes regained their shine.",
                                "ru": "После долгой чистки <w>тусклые</w> ботинки снова заблестели."
                            }
                        },
                        {
                            "text": "De auto had een <w>doffe</w> laklaag door de felle zon.",
                            "target_lang_translations": {
                                "en": "The car had a <w>dull</w> coat of paint due to the bright sun.",
                                "ru": "Из-за яркого солнца у машины было <w>матовое</w> лакокрасочное покрытие."
                            }
                        }
                    ],
                    "synonyms": [
                        "mat"
                    ],
                    "antonyms": [
                        "glanzend",
                        "stralend"
                    ],
                    "common_phrases": [
                        "doffe ogen",
                        "een doffe kleur",
                        "dof haar"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking shine or reflection; not glossy.",
                        "ru": "Лишенный блеска или сияния; не блестящий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dull",
                                "target_lang_sense_clarification": "lacking shine or brightness"
                            },
                            {
                                "target_lang_word": "matte",
                                "target_lang_sense_clarification": "surface texture without gloss"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тусклый",
                                "target_lang_sense_clarification": "лишенный яркости или блеска"
                            },
                            {
                                "target_lang_word": "матовый",
                                "target_lang_sense_clarification": "не имеющий глянца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62de92c9-42ac-3f65-ad59-6a58d8f33b52",
                    "sense_definition": "Niet helder klinkend; gedempt en laag van toon.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sensory_dullness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We horen een <w>dof</w> gerommel in de verte.",
                            "target_lang_translations": {
                                "en": "We hear a <w>dull</w> rumbling in the distance.",
                                "ru": "Мы слышим <w>глухой</w> грохот вдалеке."
                            }
                        },
                        {
                            "text": "Met een <w>doffe</w> dreun viel het zware boek op de grond.",
                            "target_lang_translations": {
                                "en": "With a <w>dull</w> thud, the heavy book fell to the ground.",
                                "ru": "С <w>глухим</w> стуком тяжелая книга упала на пол."
                            }
                        }
                    ],
                    "synonyms": [
                        "mat"
                    ],
                    "common_phrases": [
                        "een doffe klap",
                        "een dof geluid",
                        "een doffe dreun"
                    ],
                    "target_lang_definitions": {
                        "en": "Not sounding clear; muffled and low-pitched.",
                        "ru": "Не звонкий; приглушённый и низкий по тону."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dull",
                                "target_lang_sense_clarification": "sound quality"
                            },
                            {
                                "target_lang_word": "muffled",
                                "target_lang_sense_clarification": "sound that is not sharp or clear"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "о звуке: не звонкий, приглушённый"
                            },
                            {
                                "target_lang_word": "приглушённый",
                                "target_lang_sense_clarification": "ослабленный, негромкий звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5186b240-80c2-3848-a1ae-042d3896336c",
                    "sense_definition": "Zonder energie, levendigheid of interesse; lusteloos.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De afhangende schouders en de <w>doffe</w> oogopslag in het afgetobde gezicht waren de opvallendste kenmerken.",
                            "target_lang_translations": {
                                "en": "The drooping shoulders and the <w>dull</w> look in the worn-out face were the most striking features.",
                                "ru": "Опущенные плечи и <w>потухший</w> взгляд на изможденном лице были самыми заметными чертами."
                            }
                        },
                        {
                            "text": "Hij voelde zich <w>dof</w> en had nergens zin in.",
                            "target_lang_translations": {
                                "en": "He felt <w>listless</w> and didn't feel like doing anything.",
                                "ru": "Он чувствовал себя <w>вялым</w>, и ему ничего не хотелось делать."
                            }
                        }
                    ],
                    "synonyms": [
                        "lusteloos",
                        "futloos",
                        "loom",
                        "mat"
                    ],
                    "antonyms": [
                        "levendig",
                        "levenslustig",
                        "geanimeerd"
                    ],
                    "common_phrases": [
                        "zich dof voelen",
                        "een doffe blik"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking energy, liveliness, or interest; listless.",
                        "ru": "Без энергии, живости или интереса; вялый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listless",
                                "target_lang_sense_clarification": "lacking energy or enthusiasm"
                            },
                            {
                                "target_lang_word": "dull",
                                "target_lang_sense_clarification": "lacking expression or vitality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вялый",
                                "target_lang_sense_clarification": "лишенный бодрости, энергии"
                            },
                            {
                                "target_lang_word": "потухший",
                                "target_lang_sense_clarification": "безжизненный (обычно о взгляде или глазах)"
                            },
                            {
                                "target_lang_word": "безжизненный",
                                "target_lang_sense_clarification": "лишенный жизненной силы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f47c979f-1a32-3037-b243-170902bd2c2b",
                    "sense_definition": "Een zwaar, gedempt geluid van een stoot of slag.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "impact_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een zware <w>dof</w> landde de zak aardappelen op de vloer.",
                            "target_lang_translations": {
                                "en": "With a heavy <w>thud</w>, the sack of potatoes landed on the floor.",
                                "ru": "С тяжелым <w>глухим стуком</w> мешок картошки приземлился на пол."
                            }
                        },
                        {
                            "text": "Ik hoorde de <w>dof</w> van zijn voetstappen op het tapijt.",
                            "target_lang_translations": {
                                "en": "I heard the <w>thud</w> of his footsteps on the carpet.",
                                "ru": "Я слышал <w>глухой звук</w> его шагов по ковру."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A heavy, muffled sound of an impact or blow.",
                        "ru": "Тяжелый, приглушённый звук удара или толчка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thud",
                                "target_lang_sense_clarification": "dull, heavy sound of impact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "глухой стук",
                                "target_lang_sense_clarification": "звук падения или удара"
                            },
                            {
                                "target_lang_word": "шлепок",
                                "target_lang_sense_clarification": "звук удара плашмя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c1d736ad-32ac-3c26-8802-7e50b7000dac",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>doffen</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>dof</w> me graag op voor een feestje.",
                            "target_lang_translations": {
                                "en": "I like to <w>dress</w> up for a party.",
                                "ru": "Я люблю <w>наряжаться</w> на вечеринку."
                            }
                        },
                        {
                            "text": "Ik <w>dof</w> de zilveren kandelaars tot ze weer glimmen.",
                            "target_lang_translations": {
                                "en": "I <w>buff</w> the silver candlesticks until they shine again.",
                                "ru": "Я <w>полирую</w> серебряные подсвечники, пока они снова не заблестят."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'doffen' (often used in 'opdoffen').",
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола doffen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dress up",
                                "target_lang_sense_clarification": "context of 'opdoffen'"
                            },
                            {
                                "target_lang_word": "buff",
                                "target_lang_sense_clarification": "to polish or rub"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наряжаюсь",
                                "target_lang_sense_clarification": "в контексте 'opdoffen'"
                            },
                            {
                                "target_lang_word": "полирую",
                                "target_lang_sense_clarification": "в контексте чистки до блеска"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
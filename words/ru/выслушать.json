            "senses": [
                {
                    "sense_id": "ce761da4-304a-3c7b-b187-02f23cfd0170",
                    "sense_definition": "Внимательно, не прерывая и до конца прослушать кого-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прошу тебя успокоиться и <выслушать> меня до конца.",
                            "target_lang_translations": {
                                "en": "I ask you to calm down and <w>hear</w> me <w>out</w>."
                            }
                        },
                        {
                            "text": "Судья должен внимательно <выслушать> обе стороны конфликта перед принятием решения.",
                            "target_lang_translations": {
                                "en": "The judge must carefully <w>hear</w> both sides of the conflict before making a decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "прослушать"
                    ],
                    "antonyms": [
                        "перебить",
                        "проигнорировать"
                    ],
                    "common_phrases": [
                        "выслушать до конца",
                        "выслушать внимательно",
                        "выслушать мнение",
                        "выслушать просьбу"
                    ],
                    "target_lang_definitions": {
                        "en": "To listen to someone attentively until they have finished speaking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hear out",
                                "target_lang_sense_clarification": "to listen to someone until they are finished"
                            },
                            {
                                "target_lang_word": "listen to",
                                "target_lang_sense_clarification": "to pay attention to what someone is saying"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "559c3842-ebda-3f3e-92f9-65f4d08b3cec",
                    "sense_definition": "Прослушать больного с целью определения состояния его внутренних органов на слух.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Доктор достал стетоскоп, чтобы <выслушать> сердцебиение пациента.",
                            "target_lang_translations": {
                                "en": "The doctor took out a stethoscope to <w>listen to</w> the patient's heartbeat."
                            }
                        },
                        {
                            "text": "Терапевт обязан <выслушать> лёгкие при подозрении на бронхит.",
                            "target_lang_translations": {
                                "en": "The general practitioner is obliged to <w>listen to</w> the lungs if bronchitis is suspected."
                            }
                        }
                    ],
                    "synonyms": [
                        "прослушать"
                    ],
                    "common_phrases": [
                        "выслушать сердце",
                        "выслушать лёгкие",
                        "выслушать больного"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "медицина"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To examine a patient by listening to the sounds of internal organs (usually with a stethoscope)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "auscultate",
                                "target_lang_sense_clarification": "medical term for listening to internal sounds"
                            },
                            {
                                "target_lang_word": "listen to",
                                "target_lang_sense_clarification": "to check internal organs like heart or lungs"
                            }
                        ]
                    }
                }
            ]

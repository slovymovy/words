                {
                    "sense_id": "a2d8ab95-f0c2-3cc5-ae14-50cd9e5e8ab4",
                    "sense_definition": "Een mannelijk schaap.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "male_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>ram</w> loopt rustig te grazen in de wei.",
                            "target_lang_translations": {
                                "ru": "<w>Баран</w> спокойно пасется на лугу."
                            }
                        },
                        {
                            "text": "De boer heeft een nieuwe <w>ram</w> gekocht voor zijn kudde.",
                            "target_lang_translations": {
                                "ru": "Фермер купил нового <w>барана</w> для своего стада."
                            }
                        }
                    ],
                    "antonyms": [
                        "ooi"
                    ],
                    "common_phrases": [
                        "een ram en een ooi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самец овцы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баран",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d6aa98e-38f6-39cf-8df0-d914928cf997",
                    "sense_definition": "Iemand die geboren is in de periode van het sterrenbeeld Ram (ca. 21 maart tot 20 april).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "zodiac_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer is een <w>ram</w> en hij is erg energiek.",
                            "target_lang_translations": {
                                "ru": "Мой брат — <w>Овен</w>, и он очень энергичный."
                            }
                        },
                        {
                            "text": "Zij leest elke dag haar horoscoop omdat ze een <w>ram</w is.",
                            "target_lang_translations": {
                                "ru": "Она читает свой гороскоп каждый день, потому что она <w>Овен</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een typische ram"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, родившийся под знаком зодиака Овен (ок. 21 марта - 20 апреля)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Овен",
                                "target_lang_sense_clarification": "знак зодиака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3372eb87-bef9-305a-b385-f12a3ce9be94",
                    "sense_definition": "Een type computergeheugen dat willekeurig toegankelijk is (Random Access Memory).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn laptop heeft 16 gigabyte <w>ram</w>.",
                            "target_lang_translations": {
                                "ru": "В моем ноутбуке 16 гигабайт <w>оперативной памяти</w>."
                            }
                        },
                        {
                            "text": "Je hebt meer <w>ram</w> nodig om dit zware programma te draaien.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно больше <w>оперативной памяти</w>, чтобы запустить эту тяжелую программу."
                            }
                        }
                    ],
                    "synonyms": [
                        "Arie"
                    ],
                    "common_phrases": [
                        "extra ram installeren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип компьютерной памяти с произвольным доступом (Random Access Memory)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оперативная память",
                                "target_lang_sense_clarification": "компьютер"
                            },
                            {
                                "target_lang_word": "ОЗУ",
                                "target_lang_sense_clarification": "аббревиатура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e7b1496-e69a-34bf-aacb-7fd1508600dd",
                    "sense_definition": "Een zware balk of werktuig dat vroeger werd gebruikt om poorten of muren in te beuken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten gebruikten een zware <w>ram</w> om de kasteelpoort te forceren.",
                            "target_lang_translations": {
                                "ru": "Солдаты использовали тяжелый <w>таран</w>, чтобы взломать ворота замка."
                            }
                        },
                        {
                            "text": "In het museum zagen we een middeleeuwse <w>ram</w>.",
                            "target_lang_translations": {
                                "ru": "В музее мы видели средневековый <w>таран</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Arie"
                    ],
                    "common_phrases": [
                        "de ram hanteren"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелое бревно или орудие, которое раньше использовали, чтобы пробивать ворота или стены."

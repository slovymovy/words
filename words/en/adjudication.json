    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a2604ce6-9c55-3b58-b138-916e54ab4a3c",
                    "sense_definition": "The formal process of making an official decision about a dispute or problem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dispute was sent to <w>adjudication</w> to find a fair solution without going to court.",
                            "target_lang_translations": {
                                "ru": "Спор был передан на <w>рассмотрение</w> для поиска справедливого решения без обращения в суд."
                            }
                        },
                        {
                            "text": "The lengthy <w>adjudication</w> process finally ended with a clear decision for the employees.",
                            "target_lang_translations": {
                                "ru": "Длительный процесс <w>вынесения решения</w> наконец завершился четким вердиктом в пользу сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "arbitration",
                        "judgment",
                        "mediation"
                    ],
                    "common_phrases": [
                        "adjudication process",
                        "adjudication panel",
                        "matter for adjudication"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формальный процесс принятия официального решения по спору или проблеме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынесение решения",
                                "target_lang_sense_clarification": "процесс принятия решения"
                            },
                            {
                                "target_lang_word": "судебное разбирательство",
                                "target_lang_sense_clarification": "процесс рассмотрения дела"
                            },
                            {
                                "target_lang_word": "рассмотрение спора",
                                "target_lang_sense_clarification": "разрешение конфликта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de74934b-d4f5-3ec1-9e70-96bf48290bad",
                    "sense_definition": "A formal judgment or decision made by a court or person in authority.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer read the final <w>adjudication</w> carefully to understand the judge's reasoning.",
                            "target_lang_translations": {
                                "ru": "Адвокат внимательно прочитал окончательное <w>судебное решение</w>, чтобы понять аргументацию судьи."
                            }
                        },
                        {
                            "text": "They decided to appeal the <w>adjudication</w> because they believed it was unjust.",
                            "target_lang_translations": {
                                "ru": "Они решили обжаловать <w>постановление</w>, так как считали его несправедливым."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdict",
                        "ruling",
                        "decree",
                        "sentence"
                    ],
                    "common_phrases": [
                        "final adjudication",
                        "court adjudication"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное судебное решение или постановление, вынесенное судом или уполномоченным лицом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судебное решение",
                                "target_lang_sense_clarification": "результат суда"

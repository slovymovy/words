{
    "word_family": [
        "rapport",
        "rapportage",
        "rapporteur"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8623c022-53b3-34cc-9229-4399d5fcb3c2",
                    "sense_definition": "Verslag uitbrengen of officieel melding maken van een gebeurtenis of situatie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reporting_facts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet gevaarlijke situaties direct aan de manager <w>rapporteren</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен немедленно <w>сообщать</w> менеджеру об опасных ситуациях."
                            }
                        },
                        {
                            "text": "De journalist <w>rapporteerde</w> live vanuit het rampgebied over de situatie.",
                            "target_lang_translations": {
                                "ru": "Журналист <w>вел репортаж</w> в прямом эфире из зоны бедствия о ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "melden"
                    ],
                    "antonyms": [
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "een incident rapporteren",
                        "bevindingen rapporteren",
                        "schriftelijk rapporteren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально сообщать или докладывать о каком-либо событии или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "передавать информацию, уведомлять"
                            },
                            {
                                "target_lang_word": "докладывать",
                                "target_lang_sense_clarification": "делать официальное сообщение начальству или должностному лицу"
                            },
                            {
                                "target_lang_word": "рапортовать",
                                "target_lang_sense_clarification": "докладывать о ходе дела или выполнении задания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81551240-ed4a-394f-86d2-f411e66d2d3f",
                    "sense_definition": "Verantwoording afleggen aan een leidinggevende binnen een organisatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reporting_hierarchy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze nieuwe functie <w>rapporteer</w> je rechtstreeks aan de directeur.",
                            "target_lang_translations": {
                                "ru": "В этой новой должности ты <w>отчитываешься</w> напрямую перед директором."
                            }
                        },
                        {
                            "text": "Alle afdelingshoofden <w>rapporteren</w> wekelijks aan het bestuur.",
                            "target_lang_translations": {
                                "ru": "Все начальники отделов еженедельно <w>отчитываются</w> перед правлением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rapporteren aan",
                        "rechtstreeks rapporteren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отчитываться перед руководителем внутри организации о своей работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отчитываться",
                                "target_lang_sense_clarification": "давать отчет о действиях перед кем-либо"
                            },
                            {
                                "target_lang_word": "подчиняться",
                                "target_lang_sense_clarification": "находиться в служебной зависимости (в контексте иерархии)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

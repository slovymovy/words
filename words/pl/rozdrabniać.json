            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd731364-1d9c-336c-8423-cc1f2f362588",
                    "sense_definition": "Dzielić coś na bardzo małe kawałki lub części.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz musiał dokładnie <w>rozdrabniać</w> orzechy do tego ciasta.",
                            "target_lang_translations": {
                                "ru": "Повару нужно было тщательно <w>измельчать</w> орехи для этого пирога."
                            }
                        },
                        {
                            "text": "Ta maszyna służy, aby <w>rozdrabniać</w> gałęzie na drobne wióry.",
                            "target_lang_translations": {
                                "ru": "Эта машина служит для того, чтобы <w>дробить</w> ветки в мелкую щепу."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruszyć",
                        "rozkawałkowywać",
                        "drobić",
                        "rozdzielać"
                    ],
                    "antonyms": [
                        "łączyć",
                        "scalać"
                    ],
                    "common_phrases": [
                        "rozdrabniać pokarm",
                        "rozdrabniać na kawałki",
                        "rozdrabniać gałęzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делить что-то на очень мелкие кусочки или части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измельчать",
                                "target_lang_sense_clarification": "делать мелким"
                            },
                            {
                                "target_lang_word": "дробить",
                                "target_lang_sense_clarification": "разбивать на куски"
                            },
                            {
                                "target_lang_word": "крошить",
                                "target_lang_sense_clarification": "ломать на мелкие части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5cafdbb-a181-3546-a130-6e0717b1845e",
                    "sense_definition": "Poświęcać zbyt wiele uwagi mało istotnym szczegółom, tracąc z oczu główny cel lub całość sprawy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_distraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinniśmy <w>rozdrabniać</w> się na drobne, lecz skupić na strategii.",
                            "target_lang_translations": {
                                "ru": "Мы не должны <w>размениваться</w> по мелочам, а сосредоточиться на стратегии."
                            }
                        },
                        {
                            "text": "Zamiast kończyć projekt, on woli <w>rozdrabniać</w> się nad nieistotnymi detalami.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы заканчивать проект, он предпочитает <w>распыляться</w> на несущественные детали."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpraszać się"
                    ],
                    "antonyms": [
                        "skupiać się",
                        "koncentrować się"
                    ],
                    "common_phrases": [
                        "rozdrabniać się na drobne",
                        "nie warto się rozdrabniać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уделять слишком много внимания незначительным деталям, теряя из виду главную цель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размениваться",
                                "target_lang_sense_clarification": "по мелочам"
                            },
                            {
                                "target_lang_word": "распыляться",
                                "target_lang_sense_clarification": "терять концентрацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "260a9a5d-2fae-3b49-a1ba-2d451d86c6d3",
                    "sense_definition": "Ulegać podziałowi na mniejsze części w sposób samoistny lub pod wpływem czynników zewnętrznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod wpływem silnej erozji skała zaczyna powoli <w>rozdrabniać</w> się na piasek.",
                            "target_lang_translations": {
                                "ru": "Под воздействием сильной эрозии скала начинает медленно <w>крошиться</w> в песок."
                            }
                        },
                        {
                            "text": "Ten stary materiał jest bardzo kruchy i może <w>rozdrabniać</w> się w rękach.",
                            "target_lang_translations": {
                                "ru": "Этот старый материал очень хрупкий и может <w>рассыпаться</w> в руках."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruszyć się",
                        "rozpadać się"
                    ],
                    "antonyms": [
                        "scalać się"
                    ],
                    "common_phrases": [
                        "ziemia się rozdrabnia",
                        "materiał się rozdrabnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распадаться на более мелкие части самопроизвольно или под воздействием внешних факторов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крошиться",
                                "target_lang_sense_clarification": "распадаться на крошки"
                            },
                            {
                                "target_lang_word": "распадаться",
                                "target_lang_sense_clarification": "разрушаться на части"
                            }
                        ]
                    }
                }
            ]
        }
    ]

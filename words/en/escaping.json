            "senses": [
                {
                    "sense_id": "3d716a32-33f3-35ce-aeed-abdd2287e9a5",
                    "sense_definition": "The act of getting away from confinement, danger, or a specific place.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>escaping</w> of the prisoners was planned for months.",
                            "target_lang_translations": {
                                "ru": "<w>Побег</w> заключенных планировался месяцами."
                            }
                        },
                        {
                            "text": "We witnessed the sudden <w>escaping</w> of the birds from the cage.",
                            "target_lang_translations": {
                                "ru": "Мы стали свидетелями внезапного <w>побега</w> птиц из клетки."
                            }
                        }
                    ],
                    "synonyms": [
                        "escape",
                        "flight",
                        "breakout",
                        "departure"
                    ],
                    "antonyms": [
                        "capture",
                        "imprisonment",
                        "confinement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по освобождению из заключения, уходу от опасности или покиданию определенного места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побег",
                                "target_lang_sense_clarification": "из заключения или плена"
                            },
                            {
                                "target_lang_word": "бегство",
                                "target_lang_sense_clarification": "спасение от опасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "066c1608-36cf-3a78-ad93-e45ae21267f7",
                    "sense_definition": "A substance, such as gas or liquid, that leaks out or is emitted.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The faint <w>escapings</w> of gas were barely noticeable in the large room.",
                            "target_lang_translations": {
                                "ru": "Слабые <w>утечки</w> газа были едва заметны в большой комнате."
                            }
                        },
                        {
                            "text": "Engineers tried to stop the dangerous <w>escapings</w> of steam from the pipe.",
                            "target_lang_translations": {
                                "ru": "Инженеры пытались остановить опасные <w>выбросы</w> пара из трубы."
                            }

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низкий",
                                "target_lang_sense_clarification": "о цене, температуре, уровне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e79db8e8-a870-3741-9da6-c5ca6adff9f8",
                    "sense_definition": "O dźwięku lub głosie: brzmiący grubym tonem; mający małą częstotliwość drgań",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aktor miał charakterystyczny, <niski> głos, który wszyscy rozpoznawali.",
                            "target_lang_translations": {
                                "ru": "У актёра был характерный <низкий> голос, который все узнавали."
                            }
                        },
                        {
                            "text": "Z głośników wydobywały się przyjemne, <niskie> tony muzyki jazzowej.",
                            "target_lang_translations": {
                                "ru": "Из динамиков доносились приятные <низкие> тона джазовой музыки."
                            }

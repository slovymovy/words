    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9e80dcb5-ae2c-3853-9c93-69e525ad61b6",
                    "sense_definition": "Держаться на поверхности воды или передвигаться в ней (о людях, животных и предметах).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом мы часто ездим на море и <плаваем> там целый день.",
                            "target_lang_translations": {
                                "en": "In summer we often go to the sea and <w>swim</w> there all day."
                            }
                        },
                        {
                            "text": "В тарелке с супом <плавали> крупные куски картофеля.",
                            "target_lang_translations": {
                                "en": "Large chunks of potato were <w>floating</w> in the bowl of soup."
                            }
                        }
                    ],
                    "synonyms": [
                        "купаться"
                    ],
                    "antonyms": [
                        "тонуть",
                        "идти ко дну"
                    ],
                    "common_phrases": [
                        "уметь плавать",
                        "быстро плавать",
                        "плавать в реке",
                        "плавать на спине"
                    ],
                    "target_lang_definitions": {
                        "en": "To stay on the water surface or move through it (about people, animals, and objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swim",
                                "target_lang_sense_clarification": "active movement in water"
                            },
                            {
                                "target_lang_word": "float",
                                "target_lang_sense_clarification": "stay on the surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f87c0a2f-43a0-3d68-b5ce-a57060fa3081",
                    "sense_definition": "Заниматься плаванием как видом спорта или физическим упражнением.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат профессионально <плавает> и участвует в соревнованиях.",
                            "target_lang_translations": {
                                "en": "My brother <w>swims</w> professionally and participates in competitions."
                            }
                        },
                        {
                            "text": "Врач посоветовал мне <плавать> в бассейне два раза в неделю.",
                            "target_lang_translations": {
                                "en": "The doctor advised me to <w>swim</w> in the pool twice a week."
                            }
                        }
                    ],
                    "common_phrases": [
                        "любить плавать",
                        "ходить плавать",
                        "научиться плавать"
                    ],
                    "target_lang_definitions": {
                        "en": "To practice swimming as a sport or physical exercise."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swim",
                                "target_lang_sense_clarification": "sport or exercise"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0900cf8a-8a22-33e9-81f4-81019e3f2dcd",
                    "sense_definition": "Совершать рейсы, путешествовать или передвигаться по воде (о судах и моряках).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старый капитан <плавал> на торговых судах по всему миру.",
                            "target_lang_translations": {
                                "en": "This old captain <w>sailed</w> on merchant ships all over the world."
                            }
                        },
                        {
                            "text": "Огромный белый паром <плавает> между островами каждый день.",
                            "target_lang_translations": {
                                "en": "A huge white ferry <w>sails</w> between the islands every day."
                            }
                        }
                    ],
                    "synonyms": [
                        "ходить"
                    ],
                    "common_phrases": [
                        "плавать на корабле",
                        "плавать в море",
                        "долго плавать"
                    ],
                    "target_lang_definitions": {
                        "en": "To make voyages, travel, or move across water (about ships and sailors)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sail",
                                "target_lang_sense_clarification": "travel by ship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2bcddd0-d04a-3770-a264-83b7535dfb34",
                    "sense_definition": "Плавно и свободно перемещаться в воздухе.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В голубом небе медленно <плавали> лёгкие белые облака.",
                            "target_lang_translations": {
                                "en": "Light white clouds were slowly <w>floating</w> in the blue sky."
                            }
                        },
                        {
                            "text": "Густой сигаретный дым <плавал> под потолком комнаты.",
                            "target_lang_translations": {
                                "en": "Thick cigarette smoke <w>floated</w> under the ceiling of the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "парить"
                    ],
                    "common_phrases": [
                        "облака плавают",
                        "плавать в воздухе"
                    ],
                    "target_lang_definitions": {
                        "en": "To move smoothly and freely in the air."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "float",
                                "target_lang_sense_clarification": "move gently in air"
                            },
                            {
                                "target_lang_word": "drift",
                                "target_lang_sense_clarification": "move aimlessly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3455d0de-ab5c-311f-a90f-310e5dade1f5",
                    "sense_definition": "Постоянно меняться или колебаться (о ценах, курсе валют, графике).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fluctuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Курс доллара постоянно <плавает>, поэтому сложно планировать бюджет.",
                            "target_lang_translations": {
                                "en": "The dollar exchange rate constantly <w>fluctuates</w>, so it is difficult to plan a budget."
                            }
                        },
                        {
                            "text": "У нас на работе <плавает> график, и я прихожу в разное время.",
                            "target_lang_translations": {
                                "en": "We have a <w>floating</w> schedule at work, and I arrive at different times."
                            }
                        }
                    ],
                    "synonyms": [
                        "колебаться",
                        "меняться",
                        "изменяться"
                    ],
                    "antonyms": [
                        "сохраняться"
                    ],
                    "common_phrases": [
                        "цены плавают",
                        "курс плавает",
                        "график плавает"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To constantly change or fluctuate (about prices, exchange rates, schedules)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fluctuate",
                                "target_lang_sense_clarification": "change continually"
                            },
                            {
                                "target_lang_word": "float",
                                "target_lang_sense_clarification": "currency or schedule"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ec45d0e-b2f3-3327-9c02-ce635069e482",
                    "sense_definition": "Плохо знать материал, отвечать неуверенно, путаться (обычно на экзамене).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "confusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент не подготовился к экзамену и сразу начал <плавать>.",
                            "target_lang_translations": {
                                "en": "The student hadn't prepared for the exam and immediately started to <w>flounder</w>."
                            }
                        },
                        {
                            "text": "Я чувствую, что <плаваю> в этой теме и не могу дать точный ответ.",
                            "target_lang_translations": {
                                "en": "I feel that I <w>am out of my depth</w> in this topic and cannot give an exact answer."
                            }
                        }
                    ],
                    "synonyms": [
                        "путаться"
                    ],
                    "antonyms": [
                        "знать",
                        "разбираться",
                        "понимать",
                        "соображать"
                    ],
                    "common_phrases": [
                        "плавать на экзамене",
                        "плавать в вопросе",
                        "откровенно плавать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To know the material poorly, answer uncertainly, or get confused (usually at an exam)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flounder",
                                "target_lang_sense_clarification": "struggle to answer"
                            },
                            {
                                "target_lang_word": "struggle",
                                "target_lang_sense_clarification": "have difficulty"
                            },
                            {
                                "target_lang_word": "be at sea",
                                "target_lang_sense_clarification": "be confused"
                            }
                        ]
                    }
                }
            ]
        }
    ]

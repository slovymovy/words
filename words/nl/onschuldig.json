{
    "word_family": [
        "onschuld",
        "schuld",
        "schuldig",
        "beschuldigen",
        "verontschuldigen"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "258ef371-9bc2-3c63-a833-79c1cbac1ec8",
                    "sense_definition": "Niet verantwoordelijk voor een fout, misdaad of overtreding; wie niets misdaan heeft.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter verklaarde de verdachte <w>onschuldig</w> wegens gebrek aan bewijs.",
                            "target_lang_translations": {
                                "ru": "Судья признал обвиняемого <w>невиновным</w> из-за недостатка улик."
                            }
                        },
                        {
                            "text": "Zelfs <w>onschuldige</w> kinderen worden in die oorlog niet ontzien.",
                            "target_lang_translations": {
                                "ru": "Даже <w>невинных</w> детей не щадят в этой войне."
                            }
                        }
                    ],
                    "synonyms": [
                        "schuldeloos",
                        "vrijuit"
                    ],
                    "antonyms": [
                        "schuldig"
                    ],
                    "common_phrases": [
                        "onschuldig pleiten",
                        "onschuldig verklaren",
                        "onschuldig zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не несущий ответственности за ошибку, преступление или нарушение; тот, кто не сделал ничего плохого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невиновный",
                                "target_lang_sense_clarification": "в юридическом смысле; не совершивший преступления"
                            },
                            {
                                "target_lang_word": "невинный",
                                "target_lang_sense_clarification": "безгрешный, чистый; часто используется по отношению к жертвам или детям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e23eafa2-e4ef-3bad-afe0-fb7e9bece0bf",
                    "sense_definition": "Zonder kwade bedoelingen, gevaar of schadelijke gevolgen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was maar een <w>onschuldig</w> grapje, ik wilde niemand kwetsen.",
                            "target_lang_translations": {
                                "ru": "Это была всего лишь <w>безобидная</w> шутка, я не хотел никого обидеть."
                            }
                        },
                        {
                            "text": "We spraken over het weer en andere <w>onschuldige</w> onderwerpen.",
                            "target_lang_translations": {
                                "ru": "Мы говорили о погоде и других <w>безобидных</w> темах."
                            }
                        }
                    ],
                    "synonyms": [
                        "ongevaarlijk",
                        "onschadelijk",
                        "argeloos"
                    ],
                    "antonyms": [
                        "schadelijk",
                        "gevaarlijk"
                    ],
                    "common_phrases": [
                        "een onschuldig grapje",
                        "een onschuldige leugen",
                        "een onschuldige vraag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без злых намерений, опасности или вредных последствий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безобидный",
                                "target_lang_sense_clarification": "не причиняющий вреда, безопасный"
                            },
                            {
                                "target_lang_word": "невинный",
                                "target_lang_sense_clarification": "без злого умысла, наивный (например, о шутке или лжи)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
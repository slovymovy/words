    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6afe11e4-bf3d-33a6-a553-774b60fb9930",
                    "sense_definition": "The Earth's sole natural satellite, visible in the night sky.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "astronomy_entity",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "The <w>Moon</w> orbits the Earth once every twenty-seven days.",
                            "target_lang_translations": {
                                "ru": "<w>Луна</w> вращается вокруг Земли один раз каждые двадцать семь дней."
                            }
                        },
                        {
                            "text": "We could see the full <w>Moon</w> shining brightly through the trees.",
                            "target_lang_translations": {
                                "ru": "Мы видели полную <w>луну</w>, ярко сияющую сквозь деревья."
                            }
                        }
                    ],
                    "synonyms": [
                        "Luna"
                    ],
                    "common_phrases": [
                        "full Moon",
                        "new Moon",
                        "dark side of the Moon",
                        "man on the Moon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единственный естественный спутник Земли, видимый в ночном небе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Луна",
                                "target_lang_sense_clarification": "естественный спутник Земли"
                            },
                            {
                                "target_lang_word": "месяц",
                                "target_lang_sense_clarification": "серп луны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "769e87e4-1d86-3200-8cb4-8c22f9cd5683",
                    "sense_definition": "A personification of the moon as a deity or figure in mythology.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mythology",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "In ancient stories, the <w>Moon</w> often guides travelers through the night.",
                            "target_lang_translations": {
                                "ru": "В древних преданиях <w>Луна</w> часто указывает путь путникам в ночи."
                            }
                        },
                        {
                            "text": "Poets have long personified the <w>Moon</w> as a silent observer of human affairs.",
                            "target_lang_translations": {
                                "ru": "Поэты давно олицетворяют <w>Луну</w> как безмолвного наблюдателя за делами людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "Selene",
                        "Cynthia"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "poetic or mythological context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Олицетворение луны как божества или фигуры в мифологии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Луна",
                                "target_lang_sense_clarification": "божество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "436cc447-c219-3e9a-9fa4-bba40c45503d",
                    "sense_definition": "A surname.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Moon</w> will see you in his office now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Мун</w> примет вас в своем кабинете сейчас."
                            }
                        },
                        {
                            "text": "Keith <w>Moon</w> was a famous drummer known for his energetic style.",
                            "target_lang_translations": {
                                "ru": "Кит <w>Мун</w> был известным барабанщиком, прославившимся своим энергичным стилем."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мун",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6538e360-0884-3ffa-b0d9-04706b6ac5d0",
                    "sense_definition": "A shape or object that looks like a crescent or a circle, resembling the moon.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decorated her notebook with stickers of stars and <w>moons</w>.",
                            "target_lang_translations": {
                                "ru": "Она украсила свой блокнот наклейками со звездами и <w>лунами</w>."
                            }
                        },
                        {
                            "text": "The cookie was cut into the shape of a <w>moon</w>.",
                            "target_lang_translations": {
                                "ru": "Печенье было вырезано в форме <w>полумесяца</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "crescent moon",
                        "half moon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма или предмет, напоминающий полумесяц или круг, похожий на луну."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полумесяц",
                                "target_lang_sense_clarification": "форма"
                            },
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4641fbe3-84b4-3f30-a4a8-f7a68d637ed9",
                    "sense_definition": "Any natural satellite that orbits a planet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "astronomy_generic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jupiter has many <w>moons</w>, including Europa and Ganymede.",
                            "target_lang_translations": {
                                "ru": "У Юпитера много <w>лун</w>, включая Европу и Ганимед."
                            }
                        },
                        {
                            "text": "Astronomers are constantly discovering new <w>moons</w> in our solar system.",
                            "target_lang_translations": {
                                "ru": "Астрономы постоянно открывают новые <w>луны</w> в нашей Солнечной системе."
                            }
                        }
                    ],
                    "synonyms": [
                        "satellite"
                    ],
                    "common_phrases": [
                        "moons of Jupiter",
                        "planetary moons"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой естественный спутник, вращающийся вокруг планеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спутник",
                                "target_lang_sense_clarification": "небесное тело"
                            },
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "спутник планеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da4c16a2-ebaa-3c60-86de-241f029f53a0",
                    "sense_definition": "A month, specifically a lunar month; often used in literary or storytelling contexts to measure time.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They had not seen each other for many <w>moons</w>.",
                            "target_lang_translations": {
                                "ru": "Они не видели друг друга много <w>лун</w>."
                            }
                        },
                        {
                            "text": "The tribe celebrated the harvest after three <w>moons</w> had passed.",
                            "target_lang_translations": {
                                "ru": "Племя отпраздновало урожай после того, как прошло три <w>луны</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "month",
                        "lunar month"
                    ],
                    "common_phrases": [
                        "many moons ago"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or poetic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Месяц, особенно лунный; часто используется в литературном контексте для измерения времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "период времени"
                            },
                            {
                                "target_lang_word": "месяц",
                                "target_lang_sense_clarification": "календарный"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "03cfb39b-e083-376c-b7a9-f5045c4561ea",
                    "sense_definition": "Present participle and gerund of <w>order</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>ordering</w> a pizza for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Я <w>заказываю</w> пиццу на ужин сегодня вечером."
                            }
                        },
                        {
                            "text": "The librarian is <w>ordering</w> the books by author.",
                            "target_lang_translations": {
                                "ru": "Библиотекарь <w>упорядочивает</w> книги по авторам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ordering food",
                        "ordering online"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола order."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заказывать",
                                "target_lang_sense_clarification": "товары или услуги"
                            },
                            {
                                "target_lang_word": "упорядочивать",
                                "target_lang_sense_clarification": "приводить в порядок"
                            },
                            {
                                "target_lang_word": "приказывать",
                                "target_lang_sense_clarification": "отдавать распоряжения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e7442da-48d4-30d3-b9b9-66e847c7ed6a",
                    "sense_definition": "The placement of an order for goods or services.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Online <w>ordering</w> has made shopping much faster and easier.",
                            "target_lang_translations": {
                                "ru": "Онлайн-<w>заказ</w> сделал покупки намного быстрее и проще."
                            }
                        },
                        {
                            "text": "The restaurant closed its <w>ordering</w> system at ten o'clock.",
                            "target_lang_translations": {
                                "ru": "Ресторан закрыл свою систему <w>заказов</w> в десять часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "purchasing",
                        "buying"
                    ],
                    "common_phrases": [
                        "online ordering",
                        "mail ordering",
                        "food ordering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размещение заказа на товары или услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заказ",
                                "target_lang_sense_clarification": "действие по покупке"
                            },
                            {
                                "target_lang_word": "оформление заказа",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60b5bcdf-48e1-310f-9f05-296bee85aad8",
                    "sense_definition": "Arrangement in a sequence.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chronological <w>ordering</w> of the historical events helps students understand the timeline.",
                            "target_lang_translations": {
                                "ru": "Хронологическое <w>упорядочение</w> исторических событий помогает студентам понять временную шкалу."
                            }
                        },
                        {
                            "text": "She decided on a new <w>ordering</w> for the files in the cabinet.",
                            "target_lang_translations": {
                                "ru": "Она решила выбрать новую <w>расстановку</w> файлов в шкафу."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrangement",
                        "sequencing",
                        "organization"
                    ],
                    "antonyms": [
                        "disordering"
                    ],
                    "common_phrases": [
                        "rank ordering",
                        "chronological ordering",
                        "alphabetical ordering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположение в определенной последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упорядочение",
                                "target_lang_sense_clarification": "процесс организации"
                            },
                            {
                                "target_lang_word": "расстановка",
                                "target_lang_sense_clarification": "физическое расположение"
                            },
                            {
                                "target_lang_word": "систематизация",
                                "target_lang_sense_clarification": "приведение в систему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0d4b40b-bfe9-357c-b79b-bd49bb02633c",
                    "sense_definition": "The act or ceremony of ordaining, as priests or deacons.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bishop presided over the <w>ordering</w> of the new deacons.",
                            "target_lang_translations": {
                                "ru": "Епископ возглавил <w>рукоположение</w> новых дьяконов."
                            }
                        },
                        {
                            "text": "Many people attended the solemn <w>ordering</w> at the cathedral.",
                            "target_lang_translations": {
                                "ru": "Многие люди посетили торжественное <w>рукоположение</w> в соборе."
                            }
                        }
                    ],
                    "synonyms": [
                        "ordination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обряд посвящения в духовный сан, например, в священники или дьяконы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рукоположение",
                                "target_lang_sense_clarification": "церковный обряд"
                            },
                            {
                                "target_lang_word": "посвящение",
                                "target_lang_sense_clarification": "в духовный сан"
                            }
                        ]
                    }
                }
            ]
        }
    ]

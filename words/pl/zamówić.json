{
    "word_family": [
        "zamówienie",
        "zamawiający"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3876212-893c-3e01-91dd-51c4fb57efbc",
                    "sense_definition": "Zlecić dostarczenie towaru, przygotowanie posiłku lub wykonanie płatnej usługi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ordering_goods_services",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W piątek wieczorem często nie gotujemy, tylko <zamawiamy> jedzenie z ulubionej restauracji.",
                            "target_lang_translations": {
                                "ru": "В пятницу вечером мы часто не готовим, а <заказываем> еду из любимого ресторана."
                            }
                        },
                        {
                            "text": "Muszę <zamówić> taksówkę na rano, żeby zdążyć na pociąg do Warszawy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <заказать> такси на утро, чтобы успеть на поезд в Варшаву."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zamówić pizzę do domu",
                        "zamówić taksówkę na lotnisko",
                        "zamówić kuriera",
                        "zamówić towar w sklepie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поручить доставку товара, приготовление еды или выполнение платной услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заказать",
                                "target_lang_sense_clarification": "товар, услугу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee09f5f9-4c35-319d-9fcd-f16ef3ef7012",
                    "sense_definition": "Dokonać rezerwacji miejsca lub terminu, aby móc z nich skorzystać w przyszłości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "booking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym <zamówić> stolik dla dwóch osób na dzisiejszy wieczór.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <заказать> столик на двоих на сегодняшний вечер."
                            }
                        },
                        {
                            "text": "Przed wyjazdem na wakacje warto wcześniej <zamówić> nocleg w sprawdzonym hotelu.",
                            "target_lang_translations": {
                                "ru": "Перед поездкой в отпуск стоит заранее <забронировать> жилье в проверенном отеле."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarezerwować"
                    ],
                    "common_phrases": [
                        "zamówić stolik w restauracji",
                        "zamówić pokój w hotelu",
                        "zamówić mszę w kościele"
                    ],
                    "target_lang_definitions": {
                        "ru": "Забронировать место или время для использования в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забронировать",
                                "target_lang_sense_clarification": "номер, билет"
                            },
                            {
                                "target_lang_word": "заказать",
                                "target_lang_sense_clarification": "столик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c44e24d-9c0c-39d7-b9b4-59fc2e3eb817",
                    "sense_definition": "Zapowiedzieć swoją wizytę u kogoś lub w jakiejś instytucji (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "announcing_visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <zamówić> się do dentysty, ponieważ od kilku dni boli mnie ząb.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <записаться> к стоматологу, так как у меня уже несколько дней болит зуб."
                            }
                        },
                        {
                            "text": "Klient <zamówił> się na spotkanie z dyrektorem w sprawie podpisania nowej umowy.",
                            "target_lang_translations": {
                                "ru": "Клиент <записался> на встречу с директором по поводу подписания нового договора."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaanonsować się",
                        "zapowiedzieć"
                    ],
                    "common_phrases": [
                        "zamówić się na wizytę",
                        "zamówić się do lekarza",
                        "zamówić się na spotkanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предупредить о своем визите к кому-либо или в учреждение (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записаться",
                                "target_lang_sense_clarification": "на прием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f760e6cb-acff-3bac-abf5-330603e18d5c",
                    "sense_definition": "Wykonać praktyki magiczne lub wypowiedzieć zaklęcia w celu uzdrowienia kogoś lub zdjęcia uroku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "magic_practices",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawniej wierzono, że wiejska szeptucha potrafi <zamówić> każdą chorobę odpowiednimi słowami.",
                            "target_lang_translations": {
                                "ru": "Раньше верили, что деревенская знахарка может <заговорить> любую болезнь особыми словами."
                            }
                        },
                        {
                            "text": "Babcia próbowała <zamówić> urok, który rzekomo rzucono na jej gospodarstwo.",
                            "target_lang_translations": {
                                "ru": "Бабушка пыталась <заговорить> порчу, которую якобы навели на её хозяйство."
                            }
                        }
                    ],
                    "synonyms": [
                        "odczynić"
                    ],
                    "common_phrases": [
                        "zamówić chorobę",
                        "zamówić urok",
                        "zamówić ból"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "dotyczy dawnych wierzeń ludowych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить магические действия или произнести заклинания для исцеления или снятия порчи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заговорить",
                                "target_lang_sense_clarification": "болезнь, боль"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
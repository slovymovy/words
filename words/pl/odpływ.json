{
    "word_family": [
        "odpływać",
        "odpływowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ddbbf2fa-572c-339f-a472-9e987c0c07b1",
                    "sense_definition": "Otwór w dnie umywalki, wanny lub innego zbiornika, przez który wylewa się woda do rur kanalizacyjnych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "plumbing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda powoli schodzi, ponieważ <w>odpływ</w> jest zablokowany włosami.",
                            "target_lang_translations": {
                                "ru": "Вода медленно уходит, так как <w>слив</w> забит волосами."
                            }
                        },
                        {
                            "text": "Hydraulik musiał wymienić stary <w>odpływ</w> pod zlewem w kuchni.",
                            "target_lang_translations": {
                                "ru": "Сантехнику пришлось заменить старый <w>слив</w> под раковиной на кухне."
                            }
                        }
                    ],
                    "antonyms": [
                        "dopływ"
                    ],
                    "common_phrases": [
                        "zatkany odpływ",
                        "wyczyścić odpływ",
                        "odpływ liniowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие в дне раковины, ванны или другого резервуара, через которое вода уходит в канализационные трубы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слив",
                                "target_lang_sense_clarification": "отверстие"
                            },
                            {
                                "target_lang_word": "сток",
                                "target_lang_sense_clarification": "система отвода воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4c88dd4-d576-30d2-b77b-3aa9287cb673",
                    "sense_definition": "Cykliczne opadanie poziomu wody w morzu lub oceanie, spowodowane oddziaływaniem grawitacyjnym Księżyca i Słońca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tides",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas <w>odpływu</w> można spacerować po odsłoniętym dnie morza i zbierać muszle.",
                            "target_lang_translations": {
                                "ru": "Во время <w>отлива</w> можно гулять по обнажившемуся морскому дну и собирать ракушки."
                            }
                        },
                        {
                            "text": "W tym porcie różnica poziomów wody między przypływem a <w>odpływem</w> jest ogromna.",
                            "target_lang_translations": {
                                "ru": "В этом порту разница уровня воды между приливом и <w>отливом</w> огромна."
                            }
                        }
                    ],
                    "antonyms": [
                        "przypływ"
                    ],
                    "common_phrases": [
                        "w czasie odpływu",
                        "fala odpływu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Циклическое понижение уровня воды в море или океане, вызванное гравитационным воздействием Луны и Солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отлив",
                                "target_lang_sense_clarification": "явление природы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82d61bcc-eade-3b7a-9cc5-dc39b7b3d5a5",
                    "sense_definition": "Proces masowego opuszczania jakiegoś miejsca przez ludzi lub wycofywania zasobów, takich jak pieniądze.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kraj boryka się z problemem, jakim jest <w>odpływ</w> wykwalifikowanych pracowników za granicę.",
                            "target_lang_translations": {
                                "ru": "Страна сталкивается с проблемой <w>оттока</w> квалифицированных работников за границу."
                            }
                        },
                        {
                            "text": "Nagły <w>odpływ</w> kapitału zagranicznego spowodował osłabienie waluty.",
                            "target_lang_translations": {
                                "ru": "Внезапный <w>отток</w> иностранного капитала вызвал ослабление валюты."
                            }
                        }
                    ],
                    "antonyms": [
                        "przypływ",
                        "dopływ"
                    ],
                    "common_phrases": [
                        "odpływ kapitału",
                        "odpływ klientów",
                        "odpływ mózgów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс массового ухода людей из какого-либо места или вывода ресурсов, например денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отток",
                                "target_lang_sense_clarification": "капитала, клиентов, населения"
                            },
                            {
                                "target_lang_word": "утечка",
                                "target_lang_sense_clarification": "мозгов, кадров"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
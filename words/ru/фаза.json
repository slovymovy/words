                    "frequency": "Middle",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы изучали <w>фазы</w> Луны на уроке астрономии.",
                            "target_lang_translations": {

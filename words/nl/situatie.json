                    "sense_id": "d702b3c5-350a-3076-9d5b-741e39d41fa6",
                    "sense_definition": "De toestand of het geheel van omstandigheden waarin iemand of iets zich op een bepaald moment bevindt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>situatie</w> in het land is na de verkiezingen weer rustig.",
                            "target_lang_translations": {
                                "en": "The <w>situation</w> in the country is calm again after the elections.",
                                "ru": "<w>Ситуация</w> в стране после выборов снова спокойная."
                            }
                        },
                        {
                            "text": "In deze <w>situatie</w> kunnen we het beste even wachten met onze beslissing.",
                            "target_lang_translations": {
                                "en": "In this <w>situation</w>, it is best to wait a while with our decision.",
                                "ru": "В этой <w>ситуации</w> нам лучше всего немного подождать с решением."
                            }
                        }
                    ],
                    "synonyms": [
                        "toestand",
                        "omstandigheid"
                    ],
                    "common_phrases": [
                        "in deze situatie",
                        "de huidige situatie",
                        "een lastige situatie",
                        "de situatie inschatten"
                    ],
                    "target_lang_definitions": {
                        "en": "The state or set of circumstances in which someone or something finds themselves at a certain moment.",
                        "ru": "Состояние или совокупность обстоятельств, в которых кто-либо или что-либо находится в определенный момент."
                    },

{
    "word_family": [
        "merchant",
        "merchandiser",
        "merchandising"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ae704d8f-15ea-3f8f-941b-033a95dec709",
                    "sense_definition": "Goods which are or were offered or intended for sale.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store has a strict return policy for damaged <w>merchandise</w>.",
                            "target_lang_translations": {
                                "ru": "В магазине действуют строгие правила возврата поврежденного <w>товара</w>."
                            }
                        },
                        {
                            "text": "We need to order more <w>merchandise</w> to stock the shelves for the holiday season.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заказать больше <w>товара</w>, чтобы заполнить полки к праздничному сезону."
                            }
                        }
                    ],
                    "synonyms": [
                        "wares",
                        "product"
                    ],
                    "common_phrases": [
                        "general merchandise",
                        "quality merchandise",
                        "display merchandise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Товары, которые предлагаются или предназначены для продажи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "това́р",
                                "target_lang_sense_clarification": "предмет торговли"
                            },
                            {
                                "target_lang_word": "проду́кция",
                                "target_lang_sense_clarification": "изделия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a774cb50-a259-34b4-9811-7bd1d1eda6b8",
                    "sense_definition": "Commercial goods connected with an entity such as a team, band, company, or work of fiction.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fans lined up for hours to buy official <w>merchandise</w> at the concert.",
                            "target_lang_translations": {
                                "ru": "Фанаты часами стояли в очереди, чтобы купить официальную <w>атрибутику</w> на концерте."
                            }
                        },
                        {
                            "text": "The movie studio made millions from selling toys and other <w>merchandise</w>.",
                            "target_lang_translations": {
                                "ru": "Киностудия заработала миллионы на продаже игрушек и другой <w>сувенирной продукции</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "merch"
                    ],
                    "common_phrases": [
                        "band merchandise",
                        "official merchandise",
                        "team merchandise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Товары с символикой бренда, музыкальной группы, команды или фильма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атрибу́тика",
                                "target_lang_sense_clarification": "с символикой"
                            },
                            {
                                "target_lang_word": "сувени́рная проду́кция",
                                "target_lang_sense_clarification": "брендированные товары"
                            },
                            {
                                "target_lang_word": "мерч",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a6c69b17-6ab6-30e8-93fe-fd48eec35daf",
                    "sense_definition": "To engage in in-store promotion of the sale of goods, as by display and arrangement.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was hired to <w>merchandise</w> the new clothing line in department stores.",
                            "target_lang_translations": {
                                "ru": "Его наняли <w>продвигать</w> новую линию одежды в универмагах."
                            }
                        },
                        {
                            "text": "Retailers must <w>merchandise</w> their products effectively to attract customers.",
                            "target_lang_translations": {
                                "ru": "Розничные торговцы должны эффективно <w>продвигать</w> свои товары, чтобы привлечь покупателей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "merchandise products",
                        "merchandise effectively"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продвигать товары в магазине, например, путем их специальной выкладки и оформления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продвига́ть",
                                "target_lang_sense_clarification": "способствовать продаже"
                            },
                            {
                                "target_lang_word": "реклами́ровать",
                                "target_lang_sense_clarification": "привлекать внимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b8d0906-9d36-3e1f-845e-91248c34380c",
                    "sense_definition": "To engage in trade; to carry on commerce.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company <w>merchandises</w> agricultural goods across the entire region.",
                            "target_lang_translations": {
                                "ru": "Компания <w>торгует</w> сельскохозяйственными товарами по всему региону."
                            }
                        },
                        {
                            "text": "They plan to <w>merchandise</w> these items in international markets next year.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>продавать</w> эти товары на международных рынках в следующем году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "merchandise goods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заниматься торговлей; продавать товары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торгова́ть",
                                "target_lang_sense_clarification": "вести торговлю"
                            },
                            {
                                "target_lang_word": "продава́ть",
                                "target_lang_sense_clarification": "сбывать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
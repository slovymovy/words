            "pos": "noun",
            "senses": [
                {
                    "sense_id": "50953305-fc2e-3284-81cb-9af424fc5cac",
                    "sense_definition": "Een tabel of boekje waarin de dagen, weken en maanden van het jaar zijn aangegeven, vaak met vermelding van feestdagen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_time_tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb jij al een nieuwe <w>kalender</w> in de keuken opgehangen?",
                            "target_lang_translations": {
                                "en": "Have you hung up a new <w>calendar</w> in the kitchen yet?",
                                "ru": "Ты уже повесил новый <w>календарь</w> на кухне?"
                            }
                        },
                        {
                            "text": "Ik heb de verjaardag van oma met rood op de <w>kalender</w> omcirkeld.",
                            "target_lang_translations": {
                                "en": "I circled grandma's birthday in red on the <w>calendar</w>.",
                                "ru": "Я обвел день рождения бабушки красным на <w>календаре</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op de kalender",
                        "nieuwe kalender",
                        "kalender kopen"
                    ],
                    "target_lang_definitions": {
                        "en": "A table or booklet in which the days, weeks, and months of the year are indicated, often mentioning holidays.",
                        "ru": "Таблица или книжка, в которой указаны дни, недели и месяцы года, часто с отмеченными праздниками."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "calendar",
                                "target_lang_sense_clarification": "document or chart"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "календарь",
                                "target_lang_sense_clarification": "таблица или книжка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a64ce5da-ee47-322e-b53d-dbcfbc559be6",
                    "sense_definition": "Een systeem om de tijd in te delen in jaren, maanden, weken en dagen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_time_tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Gregoriaanse <w>kalender</w> wordt tegenwoordig in de meeste landen gebruikt.",
                            "target_lang_translations": {
                                "en": "The Gregorian <w>calendar</w> is used in most countries nowadays.",
                                "ru": "Григорианский <w>календарь</w> сегодня используется в большинстве стран."
                            }
                        },
                        {
                            "text": "In de islamitische <w>kalender</w> begint de dag bij zonsondergang.",
                            "target_lang_translations": {
                                "en": "In the Islamic <w>calendar</w>, the day begins at sunset.",
                                "ru": "В исламском <w>календаре</w> день начинается с заходом солнца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Gregoriaanse kalender",
                        "Juliaanse kalender",
                        "Chinese kalender"
                    ],
                    "target_lang_definitions": {
                        "en": "A system for dividing time into years, months, weeks, and days.",
                        "ru": "Система исчисления времени, делящая его на годы, месяцы, недели и дни."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "calendar",
                                "target_lang_sense_clarification": "system of time division"

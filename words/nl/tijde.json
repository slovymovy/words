                            "target_lang_translations": {
                                "en": "You can call us at any <w>time</w> for more information about the order."
                            }
                        },
                        {
                            "text": "Dit prachtige kasteel werd gebouwd ten <w>tijde</w> van de middeleeuwen.",
                            "target_lang_translations": {

{
    "word_family": [
        "popularity",
        "popularize",
        "popularly",
        "unpopular",
        "populace",
        "population"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "136526c2-ada6-395e-b562-c46a8bb7c516",
                    "sense_definition": "Liked, admired, or enjoyed by many people.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "liked_by_many",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Football is a very <w>popular</w> sport in many countries around the world.",
                            "target_lang_translations": {
                                "ru": "Футбол — очень <w>популярный</w> вид спорта во многих странах мира."
                            }
                        },
                        {
                            "text": "She became <w>popular</w> with her classmates because of her kindness.",
                            "target_lang_translations": {
                                "ru": "Она стала <w>популярной</w> среди одноклассников благодаря своей доброте."
                            }
                        },
                        {
                            "text": "The band played their most <w>popular</w> song at the end of the concert.",
                            "target_lang_translations": {
                                "ru": "Группа сыграла свою самую <w>популярную</w> песню в конце концерта."
                            }
                        }
                    ],
                    "antonyms": [
                        "unpopular"
                    ],
                    "common_phrases": [
                        "popular culture",
                        "popular music",
                        "very popular",
                        "popular with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любимый, уважаемый или нравящийся многим людям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "популярный",
                                "target_lang_sense_clarification": "пользующийся большой любовью или признанием"
                            },
                            {
                                "target_lang_word": "известный",
                                "target_lang_sense_clarification": "знакомый многим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5383488-9b39-3eb9-8d3f-c6b8ee56ac66",
                    "sense_definition": "Existing, accepted, or common among the general public.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "widespread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Contrary to <w>popular</w> belief, camels do not store water in their humps.",
                            "target_lang_translations": {
                                "ru": "Вопреки <w>распространённому</w> мнению, верблюды не запасают воду в горбах."
                            }
                        },
                        {
                            "text": "It is a <w>popular</w> opinion that the city needs more parks.",
                            "target_lang_translations": {
                                "ru": "Существует <w>популярное</w> мнение, что городу нужно больше парков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "popular belief",
                        "popular opinion",
                        "popular misconception"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий, принятый или распространённый среди широкой общественности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространённый",
                                "target_lang_sense_clarification": "часто встречающийся"
                            },
                            {
                                "target_lang_word": "общепринятый",
                                "target_lang_sense_clarification": "признанный большинством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c49cd53-4907-3b66-b6b6-60f0c034bd87",
                    "sense_definition": "Intended for or suited to the understanding of ordinary people rather than specialists.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "non_specialist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He writes <w>popular</w> science books that explain complex theories simply.",
                            "target_lang_translations": {
                                "ru": "Он пишет <w>научно-популярные</w> книги, которые просто объясняют сложные теории."
                            }
                        },
                        {
                            "text": "The museum offers a <w>popular</w> edition of the catalog for visitors.",
                            "target_lang_translations": {
                                "ru": "Музей предлагает посетителям <w>общедоступное</w> издание каталога."
                            }
                        }
                    ],
                    "common_phrases": [
                        "popular science",
                        "popular fiction",
                        "popular press"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для понимания обычными людьми, а не специалистами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общедоступный",
                                "target_lang_sense_clarification": "понятный для всех, несложный"
                            },
                            {
                                "target_lang_word": "популярный",
                                "target_lang_sense_clarification": "изложенный в доступной форме"
                            },
                            {
                                "target_lang_word": "научно-популярный",
                                "target_lang_sense_clarification": "о литературе или передачах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2dbb775-3ccc-3d12-8bc0-9adc2bfcffb5",
                    "sense_definition": "Relating to, representing, or originating from the people or the general public.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "civic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president was elected by a direct <w>popular</w> vote.",
                            "target_lang_translations": {
                                "ru": "Президент был избран прямым <w>всенародным</w> голосованием."
                            }
                        },
                        {
                            "text": "The government finally bowed to <w>popular</w> demand and changed the law.",
                            "target_lang_translations": {
                                "ru": "Правительство наконец уступило <w>народному</w> требованию и изменило закон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "popular vote",
                        "popular sovereignty",
                        "popular demand",
                        "popular support"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к народу, представляющий народ или исходящий от него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "народный",
                                "target_lang_sense_clarification": "принадлежащий народу или исходящий от него"
                            },
                            {
                                "target_lang_word": "всенародный",
                                "target_lang_sense_clarification": "охватывающий весь народ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5eeef321-0235-3b3e-9b18-b9c3f8d5a877",
                    "sense_definition": "A person who is popular and socially dominant, especially in a school environment.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In high school, she always sat at the lunch table with the <w>populars</w>.",
                            "target_lang_translations": {
                                "ru": "В старшей школе она всегда сидела за обеденным столом с <w>популярными</w>."
                            }
                        },
                        {
                            "text": "The movie is about the conflict between the outcasts and the <w>populars</w>.",
                            "target_lang_translations": {
                                "ru": "Фильм рассказывает о конфликте между изгоями и <w>популярными</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in student or teen contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который популярен и социально доминирует, особенно в школьной среде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "популярный",
                                "target_lang_sense_clarification": "как существительное: популярный ученик"
                            },
                            {
                                "target_lang_word": "любимчик",
                                "target_lang_sense_clarification": "тот, кого все любят"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fba03c7-bb20-3b7c-91d6-5587b636031c",
                    "sense_definition": "A newspaper with a large circulation that appeals to the general public; a tabloid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "newspaper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scandal was covered extensively in all the daily <w>populars</w>.",
                            "target_lang_translations": {
                                "ru": "Скандал широко освещался во всех ежедневных <w>массовых газетах</w>."
                            }
                        },
                        {
                            "text": "Serious broadsheets often report news differently than the <w>populars</w> do.",
                            "target_lang_translations": {
                                "ru": "Серьёзные широкоформатные издания часто подают новости иначе, чем <w>таблоиды</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly British English context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Газета с большим тиражом, рассчитанная на широкую публику; таблоид."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массовая газета",
                                "target_lang_sense_clarification": "издание для широкого круга читателей"
                            },
                            {
                                "target_lang_word": "таблоид",
                                "target_lang_sense_clarification": "развлекательная газета"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
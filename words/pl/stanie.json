                {
                    "sense_id": "1d5465ad-5885-39c6-a910-e43c1f5f2d51",
                    "sense_definition": "Rzeczownik odczasownikowy od słowa <w>stać</w>, oznaczający czynność przebywania w pozycji pionowej lub w jednym miejscu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_standing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długie <stanie> w kolejce jest bardzo męczące.",
                            "target_lang_translations": {
                                "ru": "Долгое <стояние> в очереди очень утомительно."
                            }
                        },
                        {
                            "text": "Jego praca wymaga ciągłego <stania> na nogach.",
                            "target_lang_translations": {
                                "ru": "Его работа требует постоянного <стояния> на ногах."
                            }
                        }
                    ],
                    "synonyms": [
                        "przebywanie",
                        "tkwienie"
                    ],
                    "antonyms": [
                        "niestanie"
                    ],
                    "common_phrases": [
                        "stanie w kolejce",
                        "stanie na baczność",
                        "stanie w korku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от слова «стоять», обозначающее действие пребывания в вертикальном положении или на одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стояние",
                                "target_lang_sense_clarification": "процесс нахождения на ногах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "927af2fa-c784-3cd5-bdea-9f5a8bc21401",
                    "sense_definition": "Forma miejscownika i wołacza liczby pojedynczej rzeczownika <w>stan</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie jestem w <stanie> ci teraz pomóc.",
                            "target_lang_translations": {
                                "ru": "Я не в <состоянии> тебе сейчас помочь."
                            }
                        },
                        {
                            "text": "Pacjent znajduje się w ciężkim <stanie>.",
                            "target_lang_translations": {
                                "ru": "Пациент находится в тяжелом <состоянии>."
                            }
                        }
                    ],
                    "common_phrases": [

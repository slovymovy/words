                },
                {
                    "sense_id": "7ec9b313-42c3-35cb-9c22-8b55fdc4ee6f",
                    "sense_definition": "Verkleinwoord van <w>top</w>; het allerhoogste of uiterste puntje van iets.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "diminutive_form",
                    "name_type": "no",
                    "examples": [
                        {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "135f355c-8536-399c-9412-70c00764918b",
                    "sense_definition": "Urządzenie służące do ogrzewania pomieszczeń lub przygotowywania potraw w wysokiej temperaturze.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "device_heating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym domu stał piękny kaflowy <w>piec</w>.",
                            "target_lang_translations": {
                                "ru": "В старом доме стояла красивая изразцовая <w>печь</w>."
                            }
                        },
                        {
                            "text": "Musimy naprawić <w>piec</w>, bo robi się zimno.",
                            "target_lang_translations": {
                                "ru": "Мы должны починить <w>печь</w>, потому что становится холодно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piec kaflowy",
                        "piec węglowy",
                        "dołożyć do pieca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство для отопления помещений или приготовления пищи при высокой температуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печь",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "печка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dca8fbd-86f8-3304-8e7c-a91f940e5e52",
                    "sense_definition": "Przemysłowe urządzenie do obróbki cieplnej materiałów, na przykład wytapiania metali.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "device_heating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownicy obsługują wielki <w>piec</w> w hucie stali.",
                            "target_lang_translations": {
                                "ru": "Рабочие обслуживают доменную <w>печь</w> на сталелитейном заводе."
                            }
                        },
                        {
                            "text": "Temperatura, którą osiąga ten <w>piec</w>, jest niezwykle wysoka.",
                            "target_lang_translations": {
                                "ru": "Температура, которую достигает эта <w>печь</w>, чрезвычайно высока."
                            }

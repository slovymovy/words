    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c74a2b6-dcdf-32a6-a231-6db179fd1621",
                    "sense_definition": "Последовательность чисел или величин, служащая для оценки, измерения или классификации чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "measurement_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач попросил пациента оценить уровень боли по десятибалльной <w>шкале</w>.",
                            "target_lang_translations": {
                                "en": "The doctor asked the patient to rate their pain level on a ten-point <w>scale</w>."
                            }
                        },
                        {
                            "text": "Для измерения силы землетрясений учёные используют <w>шкалу</w> Рихтера.",
                            "target_lang_translations": {
                                "en": "Scientists use the Richter <w>scale</w> to measure the magnitude of earthquakes."
                            }
                        }
                    ],
                    "common_phrases": [
                        "по шкале",
                        "шкала Рихтера",
                        "температурная шкала",
                        "шкала ценностей",
                        "пятибалльная шкала"
                    ],
                    "target_lang_definitions": {
                        "en": "An ordered sequence of numbers or values used for measurement, evaluation, or classification."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scale",
                                "target_lang_sense_clarification": "measurement system"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6293f922-30c8-3d9a-8f67-61c8daf69099",
                    "sense_definition": "Часть измерительного прибора в виде линейки или циферблата с делениями для отсчёта значений.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "instrument_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стрелка манометра медленно ползла вверх по <w>шкале</w>.",
                            "target_lang_translations": {
                                "en": "The pressure gauge needle slowly crept up the <w>scale</w>."
                            }
                        },
                        {
                            "text": "На <w>шкале</w> старого термометра было трудно разобрать цифры.",
                            "target_lang_translations": {
                                "en": "It was hard to make out the numbers on the old thermometer's <w>scale</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "шкала прибора",
                        "деление шкалы",
                        "равномерная шкала"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of a measuring instrument marked with lines or numbers to indicate values."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scale",
                                "target_lang_sense_clarification": "markings on an instrument"
                            },
                            {
                                "target_lang_word": "dial",
                                "target_lang_sense_clarification": "face of a gauge or clock"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "13b84c08-baff-3b89-ab39-641dc405d0d2",
                    "sense_definition": "Taki, który postępuje mądrze i rozważnie, poważnie traktuje swoje obowiązki i można na nim polegać.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cecha_charakteru",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest bardzo <w>odpowiedzialny</w> i zawsze dotrzymuje danego słowa.",
                            "target_lang_translations": {
                                "ru": "Мой брат очень <w>ответственный</w> и всегда держит свое слово."
                            }
                        },
                        {
                            "text": "Szukamy pracownika, który jest uczciwy, punktualny i <w>odpowiedzialny</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем сотрудника, который честен, пунктуален и <w>ответственен</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sumienny",
                        "rzetelny",
                        "solidny",
                        "poważny",
                        "obowiązkowy"
                    ],
                    "antonyms": [
                        "nieodpowiedzialny",
                        "niepoważny",
                        "niesolidny"
                    ],
                    "common_phrases": [
                        "odpowiedzialny człowiek",
                        "odpowiedzialny pracownik",
                        "bardzo odpowiedzialny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто поступает мудро и рассудительно, серьезно относится к своим обязанностям, и на кого можно положиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "надежный, серьезный (о человеке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "354d0af5-1bbf-3f4d-adfd-bed77c239f2f",
                    "sense_definition": "Mający obowiązek dbać o kogoś lub o coś oraz gotowy ponieść konsekwencje swoich działań lub zaniedbań.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obowiazek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy rodzic jest <w>odpowiedzialny</w> za bezpieczeństwo i wychowanie swojego dziecka.",
                            "target_lang_translations": {
                                "ru": "Каждый родитель <w>ответственен</w> за безопасность и воспитание своего ребенка."
                            }
                        },
                        {
                            "text": "Jako kierownik jestem <w>odpowiedzialny</w> za wyniki całego zespołu sprzedażowego.",
                            "target_lang_translations": {
                                "ru": "Как руководитель, я <w>ответственен</w> за результаты всей команды продаж."
                            }
                        }
                    ],
                    "antonyms": [
                        "nieodpowiedzialny"
                    ],
                    "common_phrases": [
                        "odpowiedzialny za",
                        "czuć się odpowiedzialnym",
                        "być odpowiedzialnym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обязанный заботиться о ком-то или о чем-то и готовый понести последствия своих действий или бездействия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "несущий ответственность, обязанный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4162eae3-2be9-3680-bc87-f3ddb3cf25bc",
                    "sense_definition": "Wymagający dużej uwagi, staranności i podejmowania ważnych decyzji (o pracy, zadaniu lub funkcji).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "waga_zadania",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To bardzo <w>odpowiedzialne</w> stanowisko, które wiąże się z dużym stresem.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>ответственная</w> должность, которая связана с большим стрессом."
                            }
                        },
                        {
                            "text": "Powierzono mu trudne i <w>odpowiedzialne</w> zadanie reprezentowania firmy za granicą.",
                            "target_lang_translations": {
                                "ru": "Ему поручили трудное и <w>ответственное</w> задание представлять фирму за границей."
                            }
                        }
                    ],
                    "synonyms": [
                        "poważny"
                    ],
                    "common_phrases": [
                        "odpowiedzialne stanowisko",
                        "odpowiedzialne zadanie",
                        "odpowiedzialna praca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требующий большого внимания, старательности и принятия важных решений (о работе, задании или функции)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "важный, серьезный (о деле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7217d3d-a868-308c-8352-821445bb0002",
                    "sense_definition": "Będący przyczyną jakiegoś zdarzenia, sytuacji lub zjawiska.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "przyczyna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Człowiek jest w dużej mierze <w>odpowiedzialny</w> za postępujące zmiany klimatyczne.",
                            "target_lang_translations": {
                                "ru": "Человек в значительной мере <w>ответственен</w> за прогрессирующие изменения климата."
                            }
                        },
                        {
                            "text": "Ten wirus jest <w>odpowiedzialny</w> za większość zachorowań w okresie jesiennym.",
                            "target_lang_translations": {
                                "ru": "Этот вирус <w>ответственен</w> за большинство заболеваний в осенний период."
                            }
                        }
                    ],
                    "synonyms": [
                        "winny"
                    ],
                    "common_phrases": [
                        "czynnik odpowiedzialny",
                        "odpowiedzialny za zmiany",
                        "głównie odpowiedzialny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся причиной какого-либо события, ситуации или явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "являющийся причиной"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02f233fb-d5e0-31dd-bf04-cbaaf00685db",
                    "sense_definition": "Osoba, która ponosi winę za coś lub ma obowiązek o coś dbać.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "osoba",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja wciąż poszukuje <w>odpowiedzialnego</w> za spowodowanie wczorajszego wypadku.",
                            "target_lang_translations": {
                                "ru": "Полиция все еще ищет <w>ответственного</w> за вчерашнюю аварию."
                            }
                        },
                        {
                            "text": "Trudno wskazać jednego <w>odpowiedzialnego</w>, gdy błąd popełnił cały system.",
                            "target_lang_translations": {
                                "ru": "Трудно указать одного <w>ответственного</w>, когда ошибку совершила вся система."
                            }
                        }
                    ],
                    "synonyms": [
                        "winny"
                    ],
                    "common_phrases": [
                        "szukać odpowiedzialnego",
                        "ukarać odpowiedzialnych",
                        "znaleźć odpowiedzialnego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который несет вину за что-то или обязан о чем-то заботиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "виновник, лицо, несущее ответственность"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd9b5244-7359-39ce-8f67-fa8a80442b7d",
                    "sense_definition": "Процесс возвращения кого-либо или чего-либо в прежнее, исходное или нормальное состояние после разрушения, утраты или болезни.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_restoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач сказал, что полное <w>восстановление</w> здоровья займёт около месяца.",
                            "target_lang_translations": {
                                "en": "The doctor said that a full <w>recovery</w> of health would take about a month."
                            }
                        },
                        {
                            "text": "После войны началось активное <w>восстановление</w> разрушенных городов и дорог.",
                            "target_lang_translations": {
                                "en": "After the war, the active <w>reconstruction</w> of destroyed cities and roads began."
                            }
                        }
                    ],
                    "synonyms": [
                        "возрождение",
                        "реставрация",
                        "реконструкция"
                    ],
                    "antonyms": [
                        "разрушение",
                        "уничтожение",
                        "распад",
                        "порча",
                        "поломка",
                        "гибель"
                    ],
                    "common_phrases": [
                        "восстановление здоровья",
                        "восстановление сил",
                        "восстановление экономики",
                        "восстановление данных",
                        "восстановление системы"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of returning someone or something to a former, original, or normal state after destruction, loss, or illness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recovery",
                                "target_lang_sense_clarification": "health, economy, data"
                            },
                            {
                                "target_lang_word": "restoration",
                                "target_lang_sense_clarification": "buildings, systems, peace"
                            },
                            {
                                "target_lang_word": "reconstruction",
                                "target_lang_sense_clarification": "rebuilding structures"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "217bfa97-3608-3a38-a661-4171f0565739",
                    "sense_definition": "Официальное возвращение утраченного правового статуса, звания или должности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_restoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Суд принял решение о немедленном <w>восстановлении</w> сотрудника в должности.",
                            "target_lang_translations": {
                                "en": "The court decided on the immediate <w>reinstatement</w> of the employee to the position."
                            }
                        },
                        {
                            "text": "Процедура <w>восстановления</w> в родительских правах требует серьёзных доказательств исправления.",
                            "target_lang_translations": {
                                "en": "The procedure for the <w>restoration</w> of parental rights requires serious proof of rehabilitation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "восстановление в правах",
                        "восстановление в должности",
                        "восстановление на работе",
                        "восстановление гражданства"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal or formal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The official return of a lost legal status, rank, or position."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reinstatement",
                                "target_lang_sense_clarification": "job, position"
                            },
                            {
                                "target_lang_word": "restoration",
                                "target_lang_sense_clarification": "rights, citizenship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cc777dc-3742-3dab-9b6d-d0575d187776",
                    "sense_definition": "Научное воссоздание внешнего вида или свойств исчезнувшего объекта на основе исследований.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_restoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Музей заказал <w>восстановление</w> облика древнего воина по найденному черепу.",
                            "target_lang_translations": {
                                "en": "The museum commissioned the <w>reconstruction</w> of the ancient warrior's appearance based on the found skull."
                            }
                        },
                        {
                            "text": "Для историков важно точное <w>восстановление</w> картины событий прошлого.",
                            "target_lang_translations": {
                                "en": "For historians, the accurate <w>reconstruction</w> of the picture of past events is important."
                            }
                        }
                    ],
                    "synonyms": [
                        "реконструкция"
                    ],
                    "common_phrases": [
                        "восстановление облика",
                        "восстановление картины",
                        "восстановление текста"
                    ],
                    "target_lang_definitions": {
                        "en": "The scientific recreation of the appearance or properties of a disappeared object based on research."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reconstruction",
                                "target_lang_sense_clarification": "appearance, events"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f52ee013-44ec-3ec2-a898-ec57a6f0e4d9",
                    "sense_definition": "Химическая реакция, при которой атом или ион присоединяет электроны, понижая свою степень окисления.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В металлургии широко применяется <w>восстановление</w> металлов из их оксидов.",
                            "target_lang_translations": {
                                "en": "In metallurgy, the <w>reduction</w> of metals from their oxides is widely used."
                            }
                        },
                        {
                            "text": "Окисление и <w>восстановление</w> всегда протекают одновременно в одной реакции.",
                            "target_lang_translations": {
                                "en": "Oxidation and <w>reduction</w> always occur simultaneously in the same reaction."
                            }
                        }
                    ],
                    "antonyms": [
                        "окисление"
                    ],
                    "common_phrases": [
                        "реакция восстановления",
                        "процесс восстановления"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "chemistry"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A chemical reaction in which an atom or ion gains electrons, lowering its oxidation state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reduction",
                                "target_lang_sense_clarification": "chemistry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "babdded5-e68c-396a-8583-7ef2e81fb2c1",
                    "sense_definition": "Создание враждебного отношения к кому-либо; настраивание кого-либо против кого-либо.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его интриги были направлены на <w>восстановление</w> коллег друг против друга.",
                            "target_lang_translations": {
                                "en": "His intrigues were aimed at <w>turning</w> colleagues against each other."
                            }
                        },
                        {
                            "text": "Постоянное <w>восстановление</w> общественного мнения против реформ мешало прогрессу.",
                            "target_lang_translations": {
                                "en": "The constant <w>turning</w> of public opinion against the reforms hindered progress."
                            }
                        }
                    ],
                    "common_phrases": [
                        "восстановление против себя"
                    ],
                    "target_lang_definitions": {
                        "en": "Creating a hostile attitude towards someone; setting someone against someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turning against",
                                "target_lang_sense_clarification": "creating hostility"
                            },
                            {
                                "target_lang_word": "setting against",
                                "target_lang_sense_clarification": "inciting conflict"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97a4e543-2ce5-3489-ac16-e6515d768667",
                    "sense_definition": "Построение перпендикуляра к прямой или плоскости из заданной точки.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Задача требовала выполнить <w>восстановление</w> перпендикуляра к середине отрезка.",
                            "target_lang_translations": {
                                "en": "The problem required performing the <w>erection</w> of a perpendicular to the midpoint of the segment."
                            }

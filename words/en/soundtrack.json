                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звуковая дорожка",
                                "target_lang_sense_clarification": "на кинопленке"
                            },
                            {
                                "target_lang_word": "фонограмма",
                                "target_lang_sense_clarification": "запись звука"
                            }
                        ]
                    }
                }
            ]
        },
        {

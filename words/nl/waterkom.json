            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f4a0ceb6-b3d2-33d0-9c83-3a7750b2773a",
                    "sense_definition": "Een kom of bakje dat bedoeld is om water in te doen, vaak gebruikt voor huisdieren om uit te drinken.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond stootte per ongeluk zijn <w>waterkom</w> om in de keuken.",
                            "target_lang_translations": {
                                "ru": "Собака случайно опрокинула свою <w>миску для воды</w> на кухне."
                            }
                        },
                        {
                            "text": "Zorg ervoor dat de <w>waterkom</w> altijd gevuld is met vers en schoon water.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что <w>миска для воды</w> всегда наполнена свежей и чистой водой."
                            }
                        }
                    ],
                    "synonyms": [
                        "drinkbak",
                        "waterbak"
                    ],
                    "common_phrases": [
                        "waterkom voor de hond",
                        "verse waterkom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Миска или емкость, предназначенная для воды, часто используемая для питья домашних животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миска для воды",
                                "target_lang_sense_clarification": "посуда для питья животных"
                            },
                            {
                                "target_lang_word": "поилка",
                                "target_lang_sense_clarification": "устройство или емкость для поения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "054f4f02-bf49-3a8f-a8f5-9db606a17a3c",
                    "sense_definition": "Een lager gelegen of uitgegraven plaats in het landschap waar water wordt opgeslagen, vaak dienend als drinkplaats voor vee.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koeien liepen langzaam naar de <w>waterkom</w> achter in de wei om te drinken.",
                            "target_lang_translations": {
                                "ru": "Коровы медленно шли к <w>водопою</w> в дальнем конце луга, чтобы напиться."
                            }
                        },
                        {
                            "text": "Door de aanhoudende droogte stond de uitgegraven <w>waterkom</w> bijna helemaal leeg.",
                            "target_lang_translations": {
                                "ru": "Из-за продолжительной засухи вырытый <w>водоем</w> был почти полностью пуст."
                            }
                        }
                    ],
                    "synonyms": [
                        "drinkpoel",
                        "waterbekken"
                    ],
                    "common_phrases": [
                        "uitgegraven waterkom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Низина или вырытое углубление в ландшафте, где скапливается вода, часто служащее местом водопоя для скота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водопой",
                                "target_lang_sense_clarification": "место, где пьет скот"
                            },
                            {
                                "target_lang_word": "водоем",
                                "target_lang_sense_clarification": "скопление воды в углублении"
                            },
                            {
                                "target_lang_word": "пруд",
                                "target_lang_sense_clarification": "небольшой водоем"
                            }
                        ]
                    }
                }
            ]
        }

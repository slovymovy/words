        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d8df7929-74f5-38ed-a099-3b3f3ba9873a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>pieren</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>pier</w> met mijn ogen tegen het felle zonlicht.",
                            "target_lang_translations": {
                                "ru": "Я <w>щурюсь</w> от яркого солнечного света.",
                                "en": "I <w>squint</w> my eyes against the bright sunlight."
                            }
                        },
                        {
                            "text": "Als ik mijn bril niet op heb, <w>pier</w> ik om de letters te kunnen lezen.",
                            "target_lang_translations": {
                                "ru": "Когда я без очков, я <w>щурюсь</w>, чтобы разглядеть буквы.",
                                "en": "When I don't have my glasses on, I <w>squint</w> to be able to read the letters."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени глагола 'pieren' (щуриться, всматриваться).",
                        "en": "First person singular present tense of the verb 'pieren' (to squint, to peer)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щурюсь",
                                "target_lang_sense_clarification": "сужать глаза"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "squint",
                                "target_lang_sense_clarification": "narrow eyes"
                            },
                            {
                                "target_lang_word": "peer",
                                "target_lang_sense_clarification": "look closely"
                            }
                        ]
                    }
                },

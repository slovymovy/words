    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "21d63c7f-a60d-35e2-9c68-e5cd574e3c23",
                    "sense_definition": "Een stukje papier dat je krijgt na betaling als bewijs van aankoop.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "receipt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U kunt het artikel ruilen op vertoon van de <w>bon</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете обменять товар при предъявлении <w>чека</w>.",
                                "en": "You can exchange the item upon presentation of the <w>receipt</w>."
                            }
                        },
                        {
                            "text": "Wil je de <w>bon</w> erbij hebben voor de garantie?",
                            "target_lang_translations": {
                                "ru": "Вам нужен <w>чек</w> для гарантии?",
                                "en": "Do you want the <w>receipt</w> with it for the warranty?"
                            }
                        }
                    ],
                    "synonyms": [
                        "bekeuring"
                    ],
                    "common_phrases": [
                        "de bon bewaren",
                        "om de bon vragen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бумажный документ, который вы получаете после оплаты в качестве подтверждения покупки.",
                        "en": "A piece of paper you receive after payment as proof of purchase."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чек",
                                "target_lang_sense_clarification": "кассовый"
                            },
                            {
                                "target_lang_word": "кассовый чек",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "receipt",
                                "target_lang_sense_clarification": "proof of purchase"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05e9b168-f81b-3f76-b26d-b7ae0cfdc149",
                    "sense_definition": "Een officieel papier waarop staat dat je een boete moet betalen voor een overtreding.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kreeg weer een <w>bon</w> voor te hard rijden.",
                            "target_lang_translations": {
                                "ru": "Он снова получил <w>штраф</w> за превышение скорости.",
                                "en": "He got another <w>ticket</w> for speeding."
                            }
                        },
                        {
                            "text": "De politieagent schreef een <w>bon</w> uit omdat de auto fout geparkeerd stond.",
                            "target_lang_translations": {
                                "ru": "Полицейский выписал <w>штраф</w>, так как машина была припаркована неправильно.",
                                "en": "The police officer wrote out a <w>ticket</w> because the car was parked incorrectly."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekeuring"
                    ],
                    "common_phrases": [
                        "op de bon slingeren",
                        "een bon krijgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный документ, предписывающий уплату денежного взыскания за нарушение.",
                        "en": "An official paper stating that you have to pay a fine for a violation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штраф",
                                "target_lang_sense_clarification": "взыскание"
                            },
                            {
                                "target_lang_word": "квитанция о штрафе",
                                "target_lang_sense_clarification": "документ"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "ticket",
                                "target_lang_sense_clarification": "traffic violation"
                            },
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "penalty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "228d3b1b-fb61-3d87-b5c7-7266af3ff740",
                    "sense_definition": "Een waardepapier dat recht geeft op een bepaald bedrag, korting of een product.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "voucher",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zit een <w>bon</w> bij van tien punten.",
                            "target_lang_translations": {
                                "ru": "Прилагается <w>купон</w> на десять баллов.",
                                "en": "A ten-point <w>voucher</w> is included."
                            }
                        },
                        {
                            "text": "Met deze <w>bon</w> krijg je korting op je volgende aankoop.",
                            "target_lang_translations": {
                                "ru": "С этим <w>купоном</w> вы получите скидку на следующую покупку.",
                                "en": "With this <w>coupon</w> you get a discount on your next purchase."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bon inleveren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, дающий право на определенную сумму, скидку или получение товара.",
                        "en": "A document that entitles the holder to a certain amount, discount, or product."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купон",
                                "target_lang_sense_clarification": "на скидку"
                            },
                            {
                                "target_lang_word": "ваучер",
                                "target_lang_sense_clarification": "ценная бумага"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "coupon",
                                "target_lang_sense_clarification": "discount"
                            },
                            {
                                "target_lang_word": "voucher",
                                "target_lang_sense_clarification": "credit or product"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6af4614-02e2-36d7-9840-b6eeda144f8a",
                    "sense_definition": "Een bewijs waarmee men in tijden van schaarste een bepaalde hoeveelheid goederen kan kopen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De suiker is op de <w>bon</w>.",
                            "target_lang_translations": {
                                "ru": "Сахар выдается по <w>талонам</w>.",
                                "en": "Sugar is on the <w>ration</w>."
                            }
                        },
                        {
                            "text": "Tijdens de oorlog was voedsel alleen op de <w>bon</w> verkrijgbaar.",
                            "target_lang_translations": {
                                "ru": "Во время войны продукты можно было получить только по <w>карточкам</w>.",
                                "en": "During the war, food was only available with <w>ration coupons</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op de bon"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to historical rationing systems"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, позволяющий приобрести определенное количество товаров в периоды дефицита.",
                        "en": "A document used to purchase a limited amount of goods during times of scarcity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "талон",
                                "target_lang_sense_clarification": "на продукты"
                            },
                            {
                                "target_lang_word": "карточка",
                                "target_lang_sense_clarification": "система распределения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "ration coupon",
                                "target_lang_sense_clarification": "allowance"
                            },
                            {
                                "target_lang_word": "ration card",
                                "target_lang_sense_clarification": "document"
                            }
                        ]
                    }
                }
            ]
        }
    ]

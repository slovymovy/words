                    "sense_id": "2f6b2aeb-b432-3328-9cbb-e440785f5457",
                    "sense_definition": "Van woonplaats of vestigingsplaats veranderen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>verhuizen</w> morgen naar Rotterdam.",
                            "target_lang_translations": {
                                "en": "We are <w>moving</w> to Rotterdam tomorrow.",
                                "ru": "Мы завтра <w>переезжаем</w> в Роттердам."
                            }
                        },
                        {
                            "text": "Mijn buren zijn vorige maand naar een groter huis <w>verhuisd</w>.",
                            "target_lang_translations": {
                                "en": "My neighbors <w>moved</w> to a bigger house last month.",
                                "ru": "Мои соседи в прошлом месяце <w>переехали</w> в дом побольше."
                            }
                        }
                    ],
                    "antonyms": [

                {
                    "sense_id": "0ce8da90-dc71-3365-b1bb-001ccbc152b7",
                    "sense_definition": "Dawny hełm bojowy o wysokim, spiczastym kształcie, używany głównie w średniowieczu i wczesnej nowożytności.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "armor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz zdjął ciężki <w>szłom</w>, aby otrzeć pot z czoła po walce.",
                            "target_lang_translations": {
                                "ru": "Рыцарь снял тяжелый <w>шелом</w>, чтобы вытереть пот со лба после боя."
                            }
                        },
                        {
                            "text": "W gablocie muzealnej lśnił stalowy <w>szłom</w> z ozdobnym pióropuszem.",
                            "target_lang_translations": {
                                "ru": "В музейной витрине сверкал стальной <w>шелом</w> с декоративным плюмажем."
                            }
                        }
                    ],
                    "synonyms": [
                        "hełm",
                        "szyszak"
                    ],
                    "common_phrases": [
                        "stalowy szłom",
                        "rycerski szłom"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "term referring to historical armor"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинный боевой шлем с высоким, остроконечным верхом, использовавшийся преимущественно в Средневековье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шелом",
                                "target_lang_sense_clarification": "устаревший термин, прямой аналог"
                            },
                            {
                                "target_lang_word": "шлем",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "шишак",
                                "target_lang_sense_clarification": "шлем с острием"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ee148a2-8e94-3cb3-bb74-dff40042f4bb",
                    "sense_definition": "Archaiczna lub gwarowa forma czasownika <w>iść</w> w czasie przeszłym, w pierwszej osobie liczby pojedynczej rodzaju nijakiego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako małe dziecię <w>szłom</w> za matką przez wysokie trawy.",
                            "target_lang_translations": {
                                "ru": "Как малое дитя, <w>шло</w> я за матерью сквозь высокую траву."
                            }
                        },
                        {
                            "text": "Nie pamiętam, dokąd wtedy <w>szłom</w>, ale byłem bardzo zmęczony.",
                            "target_lang_translations": {
                                "ru": "Не помню, куда я тогда <w>шло</w>, но я очень устал."
                            }
                        }
                    ],

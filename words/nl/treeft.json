            "senses": [
                {
                    "sense_id": "3be467d0-df40-35d3-9fd7-4d1fe06f3078",
                    "sense_definition": "Een rooster of onderzetter, meestal van metaal en op drie pootjes, om hete pannen op te zetten zodat de ondergrond niet beschadigt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "trivet_table",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de hete soeppan maar op de <w>treeft</w> om de tafel te beschermen.",
                            "target_lang_translations": {
                                "en": "Just put the hot soup pot on the <w>trivet</w> to protect the table."
                            }
                        },
                        {
                            "text": "Zonder <w>treeft</w> laat de pan lelijke brandplekken achter op het hout.",
                            "target_lang_translations": {
                                "en": "Without a <w>trivet</w>, the pan leaves ugly burn marks on the wood."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderzetter"
                    ],
                    "common_phrases": [
                        "een gietijzeren treeft",
                        "de pan op de treeft"
                    ],
                    "target_lang_definitions": {
                        "en": "A grate or stand, usually made of metal and on three legs, used to place hot pans on to prevent damage to the surface underneath."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trivet",
                                "target_lang_sense_clarification": "stand for hot dishes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd62518f-5069-393e-980b-00a6c0881621",
                    "sense_definition": "Een ijzeren ring op drie poten die in of boven een open vuur wordt gezet om er kookpotten op te plaatsen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "trivet_fire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vroeger kookte men in de open haard door de ketel op een <w>treeft</w> te zetten.",
                            "target_lang_translations": {
                                "en": "In the past, people cooked in the fireplace by placing the kettle on a <w>trivet</w>."
                            }
                        },
                        {
                            "text": "Hij plaatste de zware pot op de <w>treeft</w> midden in het kampvuur.",
                            "target_lang_translations": {
                                "en": "He placed the heavy pot on the <w>trivet</w> in the middle of the campfire."
                            }
                        }
                    ],
                    "synonyms": [
                        "drievoet"
                    ],
                    "common_phrases": [
                        "een treeft in de haard"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to historical cooking methods"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An iron ring on three legs placed in or above an open fire to support cooking pots."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trivet",

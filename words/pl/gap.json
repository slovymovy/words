    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9bd8d6fa-f73a-37b2-becd-2abeeb191d54",
                    "sense_definition": "Forma trybu rozkazującego (druga osoba liczby pojedynczej) czasownika <w>gapić</w> się.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>gap</w> się tak na ludzi, bo to bardzo niegrzeczne.",
                            "target_lang_translations": {
                                "ru": "Не <w>пялься</w> так на людей, это очень невежливо."
                            }
                        },
                        {
                            "text": "<w>Gap</w> się pod nogi, żebyś się nie przewrócił na chodniku.",
                            "target_lang_translations": {
                                "ru": "<w>Смотри</w> под ноги, чтобы не упасть на тротуаре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie gap się"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения глагола \"gapić się\" (пялиться, глазеть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пялиться",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "глазеть",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ecabf7c-8760-3671-83c8-befe4d038aae",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>gapa</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej grupie jest mnóstwo <w>gap</w>, które ciągle o czymś zapominają.",
                            "target_lang_translations": {
                                "ru": "В этой группе полно <w>разинь</w>, которые постоянно о чём-то забывают."
                            }
                        },
                        {
                            "text": "Z drzewa odleciało stado czarnych <w>gap</w>.",
                            "target_lang_translations": {
                                "ru": "С дерева слетела стая чёрных <w>ворон</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного \"gapa\" (разиня, ворона)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разиня",
                                "target_lang_sense_clarification": "о рассеянном человеке"
                            },
                            {
                                "target_lang_word": "ворона",
                                "target_lang_sense_clarification": "птица или рассеянный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3d947d1-0d5b-3f77-80da-8870488ca06a",
                    "sense_definition": "Osoba, która z ciekawością i często bezmyślnie przygląda się jakiemuś wydarzeniu, wypadkowi lub zjawisku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obok rozbitego samochodu stał tłum <w>gapiów</w>, ale nikt nie udzielił rannym pomocy.",
                            "target_lang_translations": {
                                "ru": "Рядом с разбитой машиной стояла толпа <w>зевак</w>, но никто не оказал помощи раненым."
                            }
                        },
                        {
                            "text": "Policja musiała odgrodzić miejsce przestępstwa od wścibskich <w>gapiów</w>.",
                            "target_lang_translations": {
                                "ru": "Полиции пришлось оградить место преступления от любопытных <w>зевак</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tłum gapiów",
                        "ciekawość gapiów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который с любопытством и часто бездумно наблюдает за каким-либо событием; зевака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зевака",
                                "target_lang_sense_clarification": "любопытный наблюдатель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75703377-fd0f-3cec-9877-e7684974cba8",
                    "sense_definition": "Inna nazwa głuptaka niebieskonogiego, dużego ptaka morskiego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Gap</w> niebieskonogi jest znany ze swoich charakterystycznych, jaskrawych stóp.",
                            "target_lang_translations": {
                                "ru": "Голубоногая <w>олуша</w> известна своими характерными яркими лапами."
                            }
                        },
                        {
                            "text": "Podczas wycieczki przyrodniczej udało nam się sfotografować rzadkiego <w>gapia</w>.",
                            "target_lang_translations": {
                                "ru": "Во время экскурсии на природу нам удалось сфотографировать редкую <w>олушу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gap niebieskonogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Другое название голубоногой олуши, крупной морской птицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "олуша",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
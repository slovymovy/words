            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ca554b1a-688c-383e-a004-4bb904c14b31",
                    "sense_definition": "Haaks op de lengterichting of de gebruikelijke richting; in de breedte.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vrachtwagen stond <w>dwars</w> op de weg geparkeerd en blokkeerde het verkeer.",
                            "target_lang_translations": {
                                "ru": "Грузовик был припаркован <w>поперек</w> дороги и блокировал движение."
                            }
                        },
                        {
                            "text": "Schouderbinnenwaarts is dus eigenlijk iets <w>dwarser</w> dan schoudervoor.",
                            "target_lang_translations": {
                                "ru": "\"Плечом внутрь\" — это, по сути, положение чуть более <w>поперек</w>, чем \"плечом вперед\"."
                            }
                        }
                    ],
                    "synonyms": [
                        "haaks"
                    ],
                    "common_phrases": [
                        "dwars op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перпендикулярно длине или обычному направлению; поперек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поперечный",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "поперек",
                                "target_lang_sense_clarification": "наречие места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec55ec93-b20e-381d-851b-0f9ec724dee5",
                    "sense_definition": "Geneigd om tegen te werken, niet mee te werken of niet te gehoorzamen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de maanden die volgden werd het kind steeds <w>dwarser</w> en wilde niet luisteren.",
                            "target_lang_translations": {
                                "ru": "В последующие месяцы ребенок становился все <w>упрямее</w> и не хотел слушаться."
                            }
                        },
                        {
                            "text": "Door zijn <w>dwarse</w> houding ontstonden er veel onnodige conflicten in de groep.",
                            "target_lang_translations": {
                                "ru": "Из-за его <w>строптивого</w> поведения в группе возникало много ненужных конфликтов."
                            }
                        }
                    ],
                    "synonyms": [
                        "weerbarstig"
                    ],
                    "common_phrases": [
                        "een dwarse bui",
                        "dwars gedrag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный к сопротивлению, отказу от сотрудничества или непослушанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упрямый",
                                "target_lang_sense_clarification": "о характере"
                            },
                            {
                                "target_lang_word": "строптивый",
                                "target_lang_sense_clarification": "непокорный"
                            },
                            {
                                "target_lang_word": "своенравный",
                                "target_lang_sense_clarification": "капризный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "792eabdf-4f44-356e-8f2f-f9310282d31b",
                    "sense_definition": "Recht door iets heen of van de ene kant naar de andere kant.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dwars</w> door de sloot lag een omgevallen boomstam waar we overheen klommen.",
                            "target_lang_translations": {
                                "ru": "<w>Поперек</w> канавы лежал упавший ствол дерева, через который мы перелезли."
                            }
                        },
                        {
                            "text": "We besloten niet om te lopen, maar liepen <w>dwars</w> over de akker naar huis.",
                            "target_lang_translations": {
                                "ru": "Мы решили не идти в обход, а пошли <w>напрямик</w> через поле домой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwars door",
                        "dwars over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямо сквозь что-то или с одной стороны на другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напрямик",
                                "target_lang_sense_clarification": "о движении"
                            },
                            {
                                "target_lang_word": "напролом",
                                "target_lang_sense_clarification": "через препятствия"
                            },
                            {
                                "target_lang_word": "поперек",
                                "target_lang_sense_clarification": "пересекая что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6beb5bff-914f-309f-8f17-464a0fa60377",
                    "sense_definition": "Op een manier die tegenwerking biedt of hinder veroorzaakt (vaak in combinatie met 'liggen').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het voorstel kreeg veel bijval, maar er lagen twee landen <w>dwars</w> tijdens de stemming.",
                            "target_lang_translations": {
                                "ru": "Предложение получило большую поддержку, но две страны выступили <w>против</w> во время голосования."
                            }
                        },
                        {
                            "text": "Ik wil niet <w>dwars</w> liggen, maar ik ben het echt niet eens met dit plan.",
                            "target_lang_translations": {
                                "ru": "Я не хочу идти <w>наперекор</w>, но я действительно не согласен с этим планом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwars liggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действуя наперекор, создавая препятствия (часто в сочетании с глаголом 'liggen')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наперекор",
                                "target_lang_sense_clarification": "вопреки"
                            },
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "возражая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edba0d61-6b23-3546-82e8-7be01924bf2b",
                    "sense_definition": "Op een manier die iemand emotioneel hindert, plaagt of bezighoudt (vaak in combinatie met 'zitten').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het zat hem <w>dwars</w> dat hij daarvan valselijk beschuldigd werd door zijn collega.",
                            "target_lang_translations": {
                                "ru": "Его <w>беспокоило</w>, что коллега ложно обвинил его в этом."
                            }
                        },
                        {
                            "text": "Zeg me eerlijk wat je <w>dwars</w> zit, dan kunnen we er samen over praten.",
                            "target_lang_translations": {
                                "ru": "Скажи мне честно, что тебя <w>гложет</w>, и мы сможем это обсудить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwars zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эмоционально задевая, беспокоя или мучая кого-то (часто в сочетании с глаголом 'zitten')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспокоить",
                                "target_lang_sense_clarification": "в составе выражения 'dwars zitten'"
                            },
                            {
                                "target_lang_word": "глодать",
                                "target_lang_sense_clarification": "о мыслях или чувствах"
                            }
                        ]
                    }
                }
            ]
        }

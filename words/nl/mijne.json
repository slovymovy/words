{
    "word_family": [
        "mijn"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "a6f5be06-e25a-3c75-bdd0-1490d7531caa",
                    "sense_definition": "Zelfstandige vorm van het bezittelijk voornaamwoord <w>mijn</w>; dat wat van mij is of aan mij toebehoort.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "possessive_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jij pakt jouw jas en ik pak de <w>mijne</w>.",
                            "target_lang_translations": {
                                "en": "You grab your coat and I grab <w>mine</w>."
                            }
                        },
                        {
                            "text": "Is dit boek het <w>mijne</w> of is het van jou?",
                            "target_lang_translations": {
                                "en": "Is this book <w>mine</w> or is it yours?"
                            }
                        }
                    ],
                    "synonyms": [
                        "van mij"
                    ],
                    "antonyms": [
                        "de jouwe",
                        "het jouwe",
                        "van jou"
                    ],
                    "common_phrases": [
                        "de mijne",
                        "het mijne"
                    ],
                    "target_lang_definitions": {
                        "en": "Independent form of the possessive pronoun 'mijn'; that which is mine or belongs to me."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mine",
                                "target_lang_sense_clarification": "possessive pronoun"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "0f2d2090-5ec2-3273-b0e2-38cbab98021e",
                    "sense_definition": "Verbogen vorm van <w>mijn</w>, voornamelijk gebruikt in formele aanspreekvormen of in Vlaams taalgebruik.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "attributive_determiner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mijne</w> heren, mag ik uw aandacht voor het volgende punt?",
                            "target_lang_translations": {
                                "en": "<w>My</w> lords, may I have your attention for the next point?"
                            }
                        },
                        {
                            "text": "In Vlaanderen zegt men vaak: waar is <w>mijne</w> bril gebleven?",
                            "target_lang_translations": {
                                "en": "In Flanders people often say: where have <w>my</w> glasses gone?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "mijne heren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verbogen"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "Vlaanderen"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "in standaardnederlands"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Inflected form of 'mijn', mainly used in formal forms of address or in Flemish usage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "my",
                                "target_lang_sense_clarification": "formal or regional inflection"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "03defa06-bb15-38a7-a415-b5c1523a1573",
                    "sense_definition": "Aanvoegende wijs van het werkwoord <w>mijnen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Men <w>mijne</w> met grote voorzichtigheid in dit onstabiele gebied.",
                            "target_lang_translations": {
                                "en": "One should <w>mine</w> with great caution in this unstable area."
                            }
                        },
                        {
                            "text": "Wie het object wenst, <w>mijne</w> het nu op de veiling.",
                            "target_lang_translations": {
                                "en": "Whoever desires the object, let him <w>purchase</w> it now at the auction."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Subjunctive mood of the verb 'mijnen'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mine",
                                "target_lang_sense_clarification": "verb - to dig for minerals"
                            },
                            {
                                "target_lang_word": "purchase",
                                "target_lang_sense_clarification": "verb - to buy at an auction"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
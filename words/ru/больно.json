                        "горько",
                        "досадно"
                    ],
                    "antonyms": [
                        "приятно",
                        "легко",
                        "хорошо"
                    ],
                    "common_phrases": [
                        "больно смотреть",
                        "больно слышать",
                        "до слез больно"
                    ],
                    "target_lang_definitions": {

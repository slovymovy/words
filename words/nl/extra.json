                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf gaf het personeel een financieel <w>extraatje</w> aan het einde van het jaar.",
                            "target_lang_translations": {

                },
                {
                    "sense_id": "66fe2696-7cb4-3c81-88e8-1441778a1d6c",
                    "sense_definition": "To extend outward beyond a surface or edge.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The balcony <w>projects</w> over the street below.",
                            "target_lang_translations": {
                                "ru": "Балкон <w>выступает</w> над улицей внизу."
                            }
                        },
                        {
                            "text": "Sharp rocks <w>project</w> from the cliff face.",
                            "target_lang_translations": {
                                "ru": "Острые камни <w>выступают</w> из скалы."
                            }

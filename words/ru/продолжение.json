    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5930eeb-7e7f-3bf4-af8d-cfbf48da3d01",
                    "sense_definition": "Действие, когда что-то не заканчивается, а длится дальше; процесс сохранения или возобновления чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы надеемся на успешное <w>продолжение</w> нашего сотрудничества.",
                            "target_lang_translations": {
                                "en": "We hope for a successful <w>continuation</w> of our cooperation."
                            }
                        },
                        {
                            "text": "Это <w>продолжение</w> старого спора, который начался ещё год назад.",
                            "target_lang_translations": {
                                "en": "This is a <w>continuation</w> of an old dispute that started a year ago."
                            }
                        }
                    ],
                    "synonyms": [

            "senses": [
                {
                    "sense_id": "f86535ec-94ed-3d8a-8ee0-c4ef12a792cb",
                    "sense_definition": "Liczba mnoga od słowa <w>arkan</w>; długa lina z pętlą na końcu, używana dawniej przez ludy stepowe jako broń lub narzędzie do chwytania zwierząt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "lasso_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kozacy mistrzowsko rzucali **arkany** na szyje uciekających wrogów.",
                            "target_lang_translations": {
                                "ru": "Казаки мастерски набрасывали <w>арканы</w> на шеи убегающих врагов."
                            }
                        },
                        {
                            "text": "W muzeum historycznym można zobaczyć tatarskie **arkany** i inną broń z tamtego okresu.",
                            "target_lang_translations": {
                                "ru": "В историческом музее можно увидеть татарские <w>арканы</w> и другое оружие того периода."
                            }
                        }
                    ],
                    "synonyms": [
                        "lassa"
                    ],
                    "common_phrases": [
                        "rzucać arkany",
                        "chwytać na arkany"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "odnosi się do dawnego uzbrojenia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная веревка с затягивающейся петлей на конце, используемая для ловли животных или (в прошлом) врагов; лассо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арканы",
                                "target_lang_sense_clarification": "множественное число от 'аркан', лассо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa1a5daa-389b-3080-8577-9456dcd5328b",
                    "sense_definition": "Rzadziej spotykana forma słowa <w>arkana</w>; wiedza tajemna, skomplikowane szczegóły lub sekrety jakiejś dziedziny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "secrets_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody czeladnik przez lata zgłębiał **arkany** trudnego rzemiosła.",
                            "target_lang_translations": {

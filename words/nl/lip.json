                {
                    "sense_id": "2e4969e2-4e06-3340-ba17-ec7652ba489b",
                    "sense_definition": "De vlezige rand aan de boven- en onderkant van de mondopening.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze stiftte haar <w>lippen</w> felrood voor het feestje.",
                            "target_lang_translations": {
                                "ru": "Она накрасила <w>губы</w> ярко-красной помадой перед вечеринкой.",
                                "en": "She applied bright red lipstick to her <w>lips</w> for the party."
                            }
                        },
                        {
                            "text": "Hij beet op zijn <w>lip</w> om niet hardop te hoeven lachen.",
                            "target_lang_translations": {
                                "ru": "Он прикусил <w>губу</w>, чтобы не рассмеяться вслух.",
                                "en": "He bit his <w>lip</w> to keep from laughing out loud."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan iemands lippen hangen",
                        "op de lippen bijten",
                        "de lippen tuiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясистый край, образующий верхнюю и нижнюю границы рта.",
                        "en": "The fleshy edge forming the upper and lower margins of the mouth."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "губа",
                                "target_lang_sense_clarification": "часть лица"
                            }

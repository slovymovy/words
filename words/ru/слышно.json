    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "af0cacbe-c07c-3823-924c-e85c5f15d66d",
                    "sense_definition": "Оценка ситуации как такой, когда звуки различимы и воспринимаются слухом; можно услышать.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "audibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне отсюда всё очень хорошо <w>слышно</w>.",
                            "target_lang_translations": {
                                "en": "I can <w>hear</w> everything very well from here."
                            }
                        },
                        {
                            "text": "В соседней комнате было <w>слышно</w>, как играет музыка.",
                            "target_lang_translations": {
                                "en": "In the next room, one could <w>hear</w> the music playing."
                            }
                        }
                    ],
                    "synonyms": [
                        "различимо"
                    ],
                    "antonyms": [
                        "неслышно",
                        "тихо"
                    ],
                    "common_phrases": [
                        "хорошо слышно",
                        "плохо слышно",
                        "ничего не слышно",
                        "еле слышно"
                    ],
                    "target_lang_definitions": {
                        "en": "Assessment of a situation where sounds are distinguishable and perceived by ear; one can hear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can be heard",
                                "target_lang_sense_clarification": "ability to perceive sound"
                            },
                            {
                                "target_lang_word": "audible",
                                "target_lang_sense_clarification": "perceptible by ear"
                            },
                            {
                                "target_lang_word": "one can hear",
                                "target_lang_sense_clarification": "impersonal possibility"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3723782-4f0c-367c-bd6a-c876f367f3ec",
                    "sense_definition": "Имеются сведения, новости или информация о ком-либо или чём-либо (обычно в вопросах или отрицаниях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "information",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Что нового <w>слышно</w> о твоей поездке?",
                            "target_lang_translations": {
                                "en": "What new things are <w>heard</w> about your trip?"
                            }
                        },
                        {
                            "text": "Про него уже давно ничего не <w>слышно</w>.",
                            "target_lang_translations": {
                                "en": "Nothing has been <w>heard</w> of him for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "известно"
                    ],
                    "antonyms": [
                        "неизвестно"
                    ],
                    "common_phrases": [
                        "что слышно",
                        "ничего не слышно"
                    ],
                    "target_lang_definitions": {
                        "en": "There is information, news, or knowledge about someone or something (usually in questions or negations)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heard",
                                "target_lang_sense_clarification": "regarding news or information"
                            },
                            {
                                "target_lang_word": "news",
                                "target_lang_sense_clarification": "contextual equivalent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a86fffc-2185-3f13-866f-f804779a7af1",
                    "sense_definition": "Так, что можно услышать; достаточно громко или отчётливо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он говорил еле <w>слышно</w>, боясь кого-то разбудить.",
                            "target_lang_translations": {
                                "en": "He spoke barely <w>audibly</w>, afraid of waking someone up."
                            }
                        },
                        {
                            "text": "Она <w>слышно</w> вздохнула и продолжила читать книгу.",
                            "target_lang_translations": {
                                "en": "She <w>audibly</w> sighed and continued reading the book."
                            }
                        }
                    ],
                    "synonyms": [
                        "громко",
                        "отчётливо",
                        "внятно"
                    ],
                    "antonyms": [
                        "беззвучно",
                        "неслышно"
                    ],
                    "common_phrases": [
                        "еле слышно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that can be heard; sufficiently loud or clearly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "audibly",
                                "target_lang_sense_clarification": "manner of action"
                            },
                            {
                                "target_lang_word": "loudly enough",
                                "target_lang_sense_clarification": "volume"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a093c3e-ef9c-3e3d-b577-2c1177ad9ecb",
                    "sense_definition": "Употребляется как вводное слово, обозначающее «как говорят», «по слухам», «кажется».",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "parenthetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он, <w>слышно</w>, собирается переезжать в другой город.",
                            "target_lang_translations": {
                                "en": "He, <w>they say</w>, is planning to move to another city."
                            }
                        },
                        {
                            "text": "Зима в этом году, <w>слышно</w>, будет очень холодной.",
                            "target_lang_translations": {
                                "en": "Winter this year, <w>rumor has it</w>, will be very cold."
                            }
                        }
                    ],
                    "synonyms": [
                        "говорят",
                        "кажется"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used as a parenthetic word meaning \"they say\", \"rumor has it\", \"it seems\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "they say",
                                "target_lang_sense_clarification": "hearsay"
                            },
                            {
                                "target_lang_word": "rumor has it",
                                "target_lang_sense_clarification": "hearsay"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b083fec6-b5e4-3c5c-aaa9-fde1d1cbd841",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>слышный</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В тишине было <w>слышно</w> каждое слово учителя.",
                            "target_lang_translations": {
                                "en": "In the silence, every word of the teacher was <w>audible</w>."
                            }
                        },
                        {
                            "text": "Эхо в горах было <w>слышно</w> на многие километры.",
                            "target_lang_translations": {
                                "en": "The echo in the mountains was <w>audible</w> for many kilometers."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short neuter singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short neuter singular form of the adjective 'слышный' (audible)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "audible",
                                "target_lang_sense_clarification": "adjective state"
                            },
                            {
                                "target_lang_word": "heard",
                                "target_lang_sense_clarification": "perceived"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b118c8b5-0215-3e58-b897-d88ca55479fc",
                    "sense_definition": "Являющийся передней, верхней или наружной стороной предмета, предназначенной для обозрения (противоположный изнаночному или тыльному).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "side",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При шитье важно не перепутать <w>лицевую</w> и изнаночную стороны ткани.",
                            "target_lang_translations": {
                                "en": "When sewing, it is important not to confuse the <w>right</w> and wrong sides of the fabric."
                            }
                        },
                        {
                            "text": "В этом узоре чередуются <w>лицевые</w> и изнаночные петли.",
                            "target_lang_translations": {
                                "en": "In this pattern, <w>knit</w> and purl stitches alternate."
                            }
                        }
                    ],
                    "synonyms": [
                        "передний",
                        "наружный",
                        "внешний"
                    ],
                    "antonyms": [
                        "тыльный",
                        "задний",
                        "внутренний"
                    ],
                    "common_phrases": [
                        "лицевая сторона",
                        "лицевая петля",
                        "лицевая кладка"
                    ],
                    "target_lang_definitions": {
                        "en": "Being the front, upper, or outer side of an object intended for viewing (opposite to the wrong side or back)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "front",
                                "target_lang_sense_clarification": "general context (e.g., building facade, side of an object)"
                            },
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "context of fabric/sewing (opposite of wrong side)"
                            },
                            {
                                "target_lang_word": "knit",
                                "target_lang_sense_clarification": "context of knitting stitches (opposite of purl)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77ef8c40-f672-3118-b5c8-487346413132",
                    "sense_definition": "Относящийся к передней части головы человека (лицу), расположенный на ней.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач внимательно осмотрел <w>лицевой</w> отдел черепа пациента.",
                            "target_lang_translations": {
                                "en": "The doctor carefully examined the <w>facial</w> section of the patient's skull."
                            }
                        },
                        {
                            "text": "Воспаление <w>лицевого</w> нерва может вызвать сильную боль и нарушение мимики.",
                            "target_lang_translations": {
                                "en": "Inflammation of the <w>facial</w> nerve can cause severe pain and impairment of facial expressions."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лицевой нерв",
                        "лицевая кость",
                        "лицевая мускулатура"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the front part of the human head (face), located on it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "facial",
                                "target_lang_sense_clarification": "anatomy (nerves, bones, muscles)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fe9618e-85a1-390b-91d0-dbad6274b58b",
                    "sense_definition": "Внешний, показной, скрывающий истинную (часто негативную) сущность чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "side",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За этим <w>лицевым</w> благополучием скрывались глубокие внутренние проблемы семьи.",
                            "target_lang_translations": {
                                "en": "Behind this <w>outward</w> well-being lay deep internal family problems."
                            }
                        },
                        {
                            "text": "Это была лишь <w>лицевая</w> сторона медали, о которой все знали.",
                            "target_lang_translations": {
                                "en": "This was only the <w>front</w> side of the coin that everyone knew about."
                            }
                        }
                    ],
                    "synonyms": [
                        "показной",
                        "внешний"
                    ],
                    "antonyms": [
                        "внутренний"
                    ],
                    "target_lang_definitions": {
                        "en": "External, ostensible, concealing the true (often negative) essence of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outward",
                                "target_lang_sense_clarification": "appearance or show"
                            },
                            {
                                "target_lang_word": "surface",
                                "target_lang_sense_clarification": "superficial aspect"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b7f4edc-4348-349b-b453-96ba7b2f5bea",
                    "sense_definition": "О старинных рукописях: содержащий иллюстрации, миниатюры и изображения людей.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В музее представлен уникальный <w>лицевой</w> летописный свод шестнадцатого века.",
                            "target_lang_translations": {
                                "en": "The museum displays a unique <w>illuminated</w> chronicle compilation from the sixteenth century."
                            }
                        },
                        {
                            "text": "Историки изучают древний <w>лицевой</w> манускрипт, богатый красочными миниатюрами.",
                            "target_lang_translations": {
                                "en": "Historians are studying an ancient <w>illuminated</w> manuscript rich in colorful miniatures."
                            }
                        }
                    ],
                    "synonyms": [
                        "иллюстрированный"
                    ],
                    "common_phrases": [
                        "лицевой свод",
                        "лицевая рукопись",
                        "лицевой список"
                    ],
                    "target_lang_definitions": {
                        "en": "Regarding old manuscripts: containing illustrations, miniatures, and depictions of people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "illuminated",
                                "target_lang_sense_clarification": "manuscripts or chronicles with illustrations"
                            }
                        ]
                    }
                }
            ]
        }
    ]

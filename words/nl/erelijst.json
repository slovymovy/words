            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2b209794-af1b-3282-875a-c48502217f6e",
                    "sense_definition": "Een overzicht van behaalde overwinningen, titels en prestaties, voornamelijk gebruikt in de sportwereld.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_achievements",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wielrenner heeft een indrukwekkende <w>erelijst</w> met overwinningen in drie grote rondes.",
                            "target_lang_translations": {
                                "ru": "У велогонщика внушительный <w>послужной список</w> с победами в трех гранд-турах."
                            }
                        },
                        {
                            "text": "Hij had drie etappes in de Tour de France op zijn <w>erelijst</w> staan.",
                            "target_lang_translations": {
                                "ru": "В его <w>послужном списке</w> числились три этапа Тур де Франс."
                            }
                        }
                    ],
                    "synonyms": [
                        "palmares"
                    ],
                    "common_phrases": [
                        "op de erelijst",
                        "indrukwekkende erelijst",
                        "lange erelijst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список завоеванных побед, титулов и достижений; термин используется преимущественно в спорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "послужной список",
                                "target_lang_sense_clarification": "перечень достижений и успехов в карьере"
                            },
                            {
                                "target_lang_word": "список достижений",
                                "target_lang_sense_clarification": "общий перечень успехов"
                            },
                            {
                                "target_lang_word": "список побед",
                                "target_lang_sense_clarification": "перечень выигранных соревнований"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ca66a50-385b-3e35-aac9-b17026e2db80",
                    "sense_definition": "Een lijst met namen van personen die officieel geëerd worden vanwege hun bijzondere verdiensten of offers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "honor_memorial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de militaire begraafplaats was een <w>erelijst</w> te zien met de namen van gevallen soldaten.",
                            "target_lang_translations": {
                                "ru": "На военном кладбище можно было увидеть <w>почетный список</w> с именами павших солдат."
                            }
                        },
                        {
                            "text": "De namen van de verzetshelden zijn opgenomen op de <w>erelijst</w> in het stadhuis.",
                            "target_lang_translations": {
                                "ru": "Имена героев сопротивления занесены в <w>почетный список</w> в ратуше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "erelijst van gevallenen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список имен людей, официально удостоенных почестей за особые заслуги или принесенные жертвы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почетный список",
                                "target_lang_sense_clarification": "список награжденных или отличившихся лиц"
                            },
                            {
                                "target_lang_word": "список памяти",
                                "target_lang_sense_clarification": "в контексте поминовения погибших"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "629b47ed-1025-3247-a3fc-698eabacbec5",
                    "sense_definition": "Een plaats in een winkel waar men zijn aankopen betaalt",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_store",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De klant gaat naar de <w>kassa</w> om zijn boodschappen te betalen.",
                            "target_lang_translations": {
                                "en": "The customer goes to the <w>checkout</w> to pay for his groceries.",
                                "ru": "Клиент идет на <w>кассу</w>, чтобы оплатить свои покупки."
                            }
                        },
                        {
                            "text": "Er stond een lange rij bij de <w>kassa</w> van de supermarkt.",
                            "target_lang_translations": {
                                "en": "There was a long line at the supermarket <w>checkout</w>.",
                                "ru": "У <w>кассы</w> супермаркета стояла длинная очередь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan de kassa",
                        "rij bij de kassa",
                        "kassa erbij"
                    ],
                    "target_lang_definitions": {
                        "en": "A place in a shop where one pays for purchases",
                        "ru": "Место в магазине, где оплачивают покупки"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "checkout",
                                "target_lang_sense_clarification": "place to pay in a supermarket or store"
                            },
                            {
                                "target_lang_word": "cash desk",
                                "target_lang_sense_clarification": "counter where payment is made"
                            },
                            {
                                "target_lang_word": "counter",
                                "target_lang_sense_clarification": "general payment desk"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "касса",
                                "target_lang_sense_clarification": "место оплаты в магазине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e59b92e0-b050-397c-96f2-215529b94d0c",
                    "sense_definition": "Een machine in een winkel om van een klant ontvangen geld te registreren en te bewaren",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De winkelier steekt het ontvangen geld in de <w>kassa</w>.",
                            "target_lang_translations": {
                                "en": "The shopkeeper puts the received money in the <w>cash register</w>.",
                                "ru": "Продавец кладет полученные деньги в <w>кассу</w>."
                            }
                        },
                        {
                            "text": "De oude <w>kassa</w> maakte een rinkelend geluid bij het openen.",
                            "target_lang_translations": {
                                "en": "The old <w>cash register</w> made a ringing sound when opening.",
                                "ru": "Старая <w>касса</w> звенела при открытии."
                            }
                        }
                    ],
                    "synonyms": [
                        "kasregister"
                    ],
                    "common_phrases": [
                        "geld in de kassa",
                        "achter de kassa"
                    ],
                    "target_lang_definitions": {
                        "en": "A machine in a shop used to register and store money received from customers",
                        "ru": "Аппарат в магазине для регистрации и хранения полученных денег"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cash register",
                                "target_lang_sense_clarification": "machine for recording sales"
                            },
                            {
                                "target_lang_word": "till",
                                "target_lang_sense_clarification": "drawer for money or the machine itself"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кассовый аппарат",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "касса",
                                "target_lang_sense_clarification": "ящик для денег"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1a7acd8-2107-3c03-9ef9-b27ac034f43f",
                    "sense_definition": "Een plaats in een theater waar men zijn tickets reserveert of betaalt",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bioscoopgangers moeten lang aanschuiven aan de <w>kassa</w> voor ze naar binnen kunnen gaan.",
                            "target_lang_translations": {
                                "en": "The cinema-goers have to queue for a long time at the <w>box office</w> before they can go inside.",
                                "ru": "Посетителям кинотеатра приходится долго стоять в очереди в <w>кассу</w>, прежде чем они смогут войти."
                            }
                        },
                        {
                            "text": "We hebben onze kaartjes voor het concert direct aan de <w>kassa</w> gekocht.",
                            "target_lang_translations": {
                                "en": "We bought our tickets for the concert directly at the <w>box office</w>.",
                                "ru": "Мы купили билеты на концерт прямо в <w>кассе</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kaartjes aan de kassa"
                    ],
                    "target_lang_definitions": {
                        "en": "A place in a theater where one reserves or pays for tickets",
                        "ru": "Место в театре или кинотеатре, где покупают или бронируют билеты"
                    },
                    "translations": {
                        "en": [
                            {

                        {
                            "text": "Place the cookies on a <w>baking</w> sheet before putting them in the oven.",
                            "target_lang_translations": {
                                "ru": "Положите печенье на противень для <w>выпечки</w>, прежде чем ставить его в духовку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "baking soda",
                        "baking powder",
                        "baking tray",
                        "baking sheet"

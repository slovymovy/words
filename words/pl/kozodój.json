    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c2f274ad-adce-31cc-8538-66f43700466f",
                    "sense_definition": "Nocny ptak owadożerny o szarobrązowym, maskującym upierzeniu, polujący na owady w locie; lelek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ptak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lelek <w>kozodój</w> jest znany ze swojej niezwykłej umiejętności kamuflażu na tle kory drzew.",
                            "target_lang_translations": {
                                "ru": "Обыкновенный <w>козодой</w> известен своей необычайной способностью маскироваться на фоне коры деревьев."
                            }
                        },
                        {
                            "text": "Podczas wieczornego spaceru po lesie udało nam się usłyszeć charakterystyczny głos <w>kozodoja</w>.",
                            "target_lang_translations": {
                                "ru": "Во время вечерней прогулки по лесу нам удалось услышать характерный голос <w>козодоя</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lelek"
                    ],
                    "common_phrases": [
                        "lelek kozodój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ночная насекомоядная птица с серо-коричневым маскирующим оперением, охотящаяся на насекомых в полете; козодой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "козодой",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19e1d16e-ea97-31c6-a55d-02a97ee7671d",
                    "sense_definition": "Osoba zajmująca się dojeniem kóz.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "osoba",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>kozodój</w> każdego ranka przynosił do kuchni wiadro świeżego mleka.",
                            "target_lang_translations": {
                                "ru": "Старый <w>дояр коз</w> каждое утро приносил на кухню ведро свежего молока."
                            }
                        },
                        {
                            "text": "Na pastwisku pracował pasterz oraz jego pomocnik, młody <w>kozodój</w>.",
                            "target_lang_translations": {
                                "ru": "На пастбище работал пастух и его помощник, молодой <w>дояр коз</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, занимающийся доением коз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дояр коз",
                                "target_lang_sense_clarification": "занятие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

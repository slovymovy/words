{
    "word_family": [
        "начало",
        "начинание",
        "начинать",
        "сначала",
        "поначалу"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "13eee8d3-c5fe-3582-9571-2edebad0f5e2",
                    "sense_definition": "Начать совершаться, происходить (о событиях, действиях, процессах).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Урок <начался> ровно в девять часов утра.",
                            "target_lang_translations": {
                                "en": "The lesson <started> at exactly nine o'clock in the morning."
                            }
                        },
                        {
                            "text": "Вчера в городе <начался> музыкальный фестиваль.",
                            "target_lang_translations": {
                                "en": "A music festival <started> in the city yesterday."
                            }
                        }
                    ],
                    "antonyms": [
                        "кончиться",
                        "закончиться",
                        "окончиться",
                        "завершиться"
                    ],
                    "common_phrases": [
                        "урок начался",
                        "фильм начался",
                        "война началась",
                        "начались переговоры"
                    ],
                    "target_lang_definitions": {
                        "en": "To start happening or taking place (about events, actions, processes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "most common, general term"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "slightly more formal than start"
                            },
                            {
                                "target_lang_word": "commence",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b672d04f-79c7-36d1-8c45-3bd2da88d010",
                    "sense_definition": "Настать, наступить (о времени года, периодах времени или погодных явлениях).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На улице неожиданно <начался> сильный дождь.",
                            "target_lang_translations": {
                                "en": "Heavy rain unexpectedly <started> outside."
                            }
                        },
                        {
                            "text": "Скоро <начнётся> долгожданное лето.",
                            "target_lang_translations": {
                                "en": "The long-awaited summer will <begin> soon."
                            }
                        }
                    ],
                    "synonyms": [
                        "наступить",
                        "настать"
                    ],
                    "antonyms": [
                        "кончиться",
                        "закончиться"
                    ],
                    "common_phrases": [
                        "началась зима",
                        "начался дождь",
                        "началось лето",
                        "день начался"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrive, set in, or start (about seasons, time periods, or weather)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "common for weather events like rain"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "common for seasons and periods"
                            },
                            {
                                "target_lang_word": "set in",
                                "target_lang_sense_clarification": "used when a season or weather condition becomes established"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61d0045a-68cb-37f9-aede-3b1b50de9a67",
                    "sense_definition": "Получить начало, возникнуть, иметь исток или причину.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта запутанная история <началась> много лет назад.",
                            "target_lang_translations": {
                                "en": "This complicated story <began> many years ago."
                            }
                        },
                        {
                            "text": "Ссора <началась> из-за глупого недоразумения.",
                            "target_lang_translations": {
                                "en": "The quarrel <started> because of a silly misunderstanding."
                            }
                        }
                    ],
                    "synonyms": [
                        "возникнуть"
                    ],
                    "common_phrases": [
                        "всё началось с",
                        "история началась"
                    ],
                    "target_lang_definitions": {
                        "en": "To originate, arise, or have a source."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "to have a specific origin"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "to come into existence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
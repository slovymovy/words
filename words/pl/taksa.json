    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71065a55-1d57-36d3-9756-68654be76bcc",
                    "sense_definition": "Potoczne określenie samochodu osobowego do wynajęcia wraz z kierowcą, służącego do przewozu pasażerów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy szybko zamówić <w>taksę</w>, żeby zdążyć na ostatni pociąg do domu.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быстро заказать <w>такси</w>, чтобы успеть на последний поезд домой."
                            }
                        },
                        {
                            "text": "Kierowca tej <w>taksy</w> znał miasto jak własną kieszeń i dowiózł nas na miejsce bez korków.",
                            "target_lang_translations": {
                                "ru": "Водитель этого <w>такси</w> знал город как свои пять пальцев и довез нас до места без пробок."
                            }
                        }
                    ],
                    "synonyms": [
                        "taksówka",
                        "taxi",
                        "taryfa"
                    ],
                    "common_phrases": [
                        "zamówić taksę",
                        "złapać taksę",
                        "jechać taksą"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название легкового автомобиля с водителем, предназначенного для перевозки пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "такси",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47467a6d-c724-3410-9c10-5ce5c6f9f0c2",
                    "sense_definition": "Urzędowo ustalona stawka opłaty za określone usługi, czynności prawne lub towary.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Notariusz poinformował nas, ile wyniesie <w>taksa</w> za sporządzenie aktu własności mieszkania.",
                            "target_lang_translations": {
                                "ru": "Нотариус сообщил нам, сколько составит <w>тариф</w> за оформление права собственности на квартиру."
                            }
                        },
                        {
                            "text": "W wielu miejscowościach turystycznych obowiązuje miejscowa <w>taksa</w> klimatyczna, którą muszą uiszczać przyjezdni.",
                            "target_lang_translations": {
                                "ru": "Во многих туристических местах действует местный курортный <w>сбор</w>, который должны оплачивать приезжие."
                            }
                        }
                    ],
                    "synonyms": [
                        "taryfa"
                    ],
                    "common_phrases": [
                        "taksa notarialna",
                        "taksa klimatyczna",
                        "według taksy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально установленная ставка оплаты за определенные услуги, юридические действия или товары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "такса",
                                "target_lang_sense_clarification": "установленная расценка"
                            },
                            {
                                "target_lang_word": "тариф",
                                "target_lang_sense_clarification": "ставка оплаты"
                            },
                            {
                                "target_lang_word": "сбор",
                                "target_lang_sense_clarification": "налог или плата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb14b9cd-6d14-338f-9c8b-10d9cca01d91",
                    "sense_definition": "Forma dopełniacza liczby pojedynczej rzeczownika <w>taks</w> (rzadkie określenie rasy psa – jamnika).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nigdy wcześniej nie widziałem tak głośno szczekającego <w>taksa</w> na spacerze.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не видел так громко лающую <w>таксу</w> на прогулке."
                            }
                        },
                        {
                            "text": "Właściciel tego małego <w>taksa</w> bardzo dba o jego zdrowie i dietę.",
                            "target_lang_translations": {
                                "ru": "Владелец этой маленькой <w>таксы</w> очень заботится о её здоровье и диете."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive singular of 'taks'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа единственного числа существительного «taks» (редкое название породы собак — такса)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "такса",
                                "target_lang_sense_clarification": "порода собаки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

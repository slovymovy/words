    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "24815c03-3c9f-3741-b05b-9b3bd2ba48e3",
                    "sense_definition": "To successfully complete a task or reach a specific goal, usually by working hard.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "success_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>achieve</w> anything if you put your mind to it.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>добиться</w> чего угодно, если действительно захочешь."
                            }
                        },
                        {
                            "text": "Hannah <w>achieved</w> her lifelong dream of winning a medal at the Olympics.",
                            "target_lang_translations": {
                                "ru": "Ханна <w>осуществила</w> свою давнюю мечту — выиграть медаль на Олимпийских играх."
                            }
                        }
                    ],
                    "synonyms": [
                        "accomplish",
                        "fulfill",
                        "realize"
                    ],
                    "antonyms": [
                        "miss"
                    ],
                    "common_phrases": [
                        "achieve a goal",
                        "achieve an objective",
                        "achieve a dream",
                        "achieve a target"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешно выполнить задачу или достичь определенной цели, обычно благодаря упорному труду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "цели, вершины"
                            },
                            {
                                "target_lang_word": "осуществлять",
                                "target_lang_sense_clarification": "мечту, план"
                            },
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "задачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a947f029-3668-307a-a81f-93220f7c8393",
                    "sense_definition": "To gain or obtain a desired result, status, or standard through effort or skill.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "success_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He worked very hard to <w>achieve</w> fame in his profession.",
                            "target_lang_translations": {
                                "ru": "Он очень много работал, чтобы <w>добиться</w> известности в своей профессии."
                            }
                        },
                        {
                            "text": "The team <w>achieved</w> a great victory against their rivals.",
                            "target_lang_translations": {
                                "ru": "Команда <w>добилась</w> великой победы над своими соперниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "attain",
                        "obtain",
                        "gain",
                        "earn",
                        "win",
                        "reach",
                        "acquire"
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "achieve success",
                        "achieve fame",
                        "achieve greatness",
                        "achieve a result"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить желаемый результат, статус или уровень благодаря усилиям или мастерству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добиваться",
                                "target_lang_sense_clarification": "успеха, победы, признания"
                            },
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "уровня, результата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "248d34e6-645a-35cd-a740-4febe9dedff5",
                    "sense_definition": "To perform successfully, especially in an academic or educational setting.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "success_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school encourages every student to <w>achieve</w> at the highest level.",
                            "target_lang_translations": {
                                "ru": "Школа поощряет каждого ученика <w>добиваться</w> самых высоких результатов."
                            }
                        },
                        {
                            "text": "Children often <w>achieve</w> better results when they are supported by their parents.",
                            "target_lang_translations": {
                                "ru": "Дети часто <w>добиваются</w> лучших результатов, когда их поддерживают родители."
                            }
                        }
                    ],
                    "synonyms": [
                        "succeed"
                    ],
                    "common_phrases": [
                        "achieve highly"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "06a9a765-222e-3a78-bd50-7ff811599634",
                    "sense_definition": "Процесс внимательного восприятия, изучения и исследования предметов или явлений.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёные продолжают вести <наблюдение> за поведением редких птиц.",
                            "target_lang_translations": {
                                "en": "Scientists continue to conduct <observation> of the behavior of rare birds."
                            }
                        },
                        {
                            "text": "Для получения точных данных необходимо длительное <наблюдение>.",
                            "target_lang_translations": {
                                "en": "Long-term <observation> is necessary to obtain accurate data."
                            }
                        }
                    ],
                    "synonyms": [
                        "слежение"
                    ],
                    "common_phrases": [
                        "вести наблюдение",
                        "метод наблюдения",
                        "наблюдение за природой"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of carefully watching, studying, or investigating objects or phenomena."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "observation",
                                "target_lang_sense_clarification": "act of watching or studying"
                            },
                            {
                                "target_lang_word": "watching",
                                "target_lang_sense_clarification": "act of looking attentively"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2c1b2d3-d6f7-3282-ba55-619c86c5bf1c",
                    "sense_definition": "Надзор, контроль за чьим-либо состоянием, действиями или поведением.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пациент должен находиться под строгим <наблюдением> врачей.",
                            "target_lang_translations": {
                                "en": "The patient must be under the strict <observation> of doctors."
                            }
                        },
                        {
                            "text": "Полиция установила круглосуточное <наблюдение> за подозрительным домом.",
                            "target_lang_translations": {
                                "en": "The police established round-the-clock <surveillance> of the suspicious house."
                            }
                        }
                    ],
                    "synonyms": [
                        "надзор"
                    ],
                    "common_phrases": [
                        "под наблюдением",
                        "камера наблюдения",
                        "медицинское наблюдение"
                    ],
                    "target_lang_definitions": {
                        "en": "Supervision or monitoring of someone's condition, actions, or behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surveillance",
                                "target_lang_sense_clarification": "monitoring, especially by police or security"
                            },
                            {
                                "target_lang_word": "observation",
                                "target_lang_sense_clarification": "monitoring, especially medical"
                            },
                            {
                                "target_lang_word": "supervision",
                                "target_lang_sense_clarification": "oversight or management"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fa3368c-4204-3a7a-8ec4-11847b80f88c",
                    "sense_definition": "Результат исследования, вывод или замечание, сделанное на основе увиденного.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор записал свои путевые <наблюдения> в отдельный блокнот.",
                            "target_lang_translations": {
                                "en": "The author wrote down his travel <observations> in a separate notebook."
                            }
                        },
                        {
                            "text": "Это очень тонкое <наблюдение>, которое многое объясняет в его характере.",
                            "target_lang_translations": {
                                "en": "This is a very subtle <observation> that explains a lot about his character."
                            }
                        }
                    ],
                    "synonyms": [
                        "заключение",
                        "вывод"
                    ],
                    "common_phrases": [
                        "интересное наблюдение",
                        "поделиться наблюдением",
                        "личные наблюдения"
                    ],
                    "target_lang_definitions": {
                        "en": "A result of research, a conclusion, or a remark based on what has been seen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "observation",
                                "target_lang_sense_clarification": "remark or comment"
                            },
                            {
                                "target_lang_word": "finding",
                                "target_lang_sense_clarification": "result of research or investigation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
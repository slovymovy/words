                {
                    "sense_id": "4bb0bd03-f52b-30db-ac2c-f53166a14201",
                    "sense_definition": "A male given name from the Germanic languages, a Latinized form of <w>Hugh</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My cousin <w>Hugo</w> lives in Berlin with his family.",
                            "target_lang_translations": {
                                "ru": "Мой двоюродный брат <w>Хьюго</w> живет в Берлине со своей семьей."
                            }
                        },
                        {
                            "text": "They decided to name their newborn son <w>Hugo</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного сына <w>Хьюго</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Uncle Hugo",
                        "named Hugo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя германского происхождения, латинизированная форма имени Хью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хьюго",
                                "target_lang_sense_clarification": "мужское имя"
                            }

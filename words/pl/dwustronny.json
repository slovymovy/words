    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c257a096-e0e6-31e4-93fa-17de91586927",
                    "sense_definition": "Taki, który ma dwie strony, jest wykończony tak samo z obu stron lub może być używany na lewą i prawą stronę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama zrobiła mi na szydełku ciepłą <w>dwustronną</w> czapkę na zimę.",
                            "target_lang_translations": {
                                "ru": "Мама связала мне крючком теплую <w>двустороннюю</w> шапку на зиму."
                            }
                        },
                        {
                            "text": "Do przyklejenia tego plakatu na ścianę przyda ci się mocna taśma <w>dwustronna</w>.",
                            "target_lang_translations": {
                                "ru": "Для приклеивания этого плаката на стену тебе пригодится прочный <w>двусторонний</w> скотч."
                            }
                        }
                    ],
                    "antonyms": [
                        "jednostronny"
                    ],
                    "common_phrases": [
                        "taśma dwustronna",
                        "druk dwustronny",
                        "kurtka dwustronna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий две стороны, обработанный одинаково с обеих сторон или пригодный для использования с обеих сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двусторонний",
                                "target_lang_sense_clarification": "имеющий две стороны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbd39fa9-7569-3272-9ce4-3be25aadea81",
                    "sense_definition": "Taki, który dotyczy dwóch stron, angażuje dwa podmioty lub jest wynikiem ich wspólnego działania.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "relations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rozmówcy omówili też stan stosunków <w>dwustronnych</w> i ich perspektywy w najbliższym czasie.",
                            "target_lang_translations": {
                                "ru": "Собеседники также обсудили состояние <w>двусторонних</w> отношений и их перспективы в ближайшее время."
                            }
                        },
                        {
                            "text": "Rządy obu państw podpisały ważną <w>dwustronną</w> umowę o współpracy gospodarczej.",
                            "target_lang_translations": {
                                "ru": "Правительства обеих стран подписали важное <w>двустороннее</w> соглашение об экономическом сотрудничестве."
                            }
                        }
                    ],
                    "synonyms": [
                        "bilateralny",
                        "obopólny",
                        "wzajemny"
                    ],
                    "antonyms": [
                        "jednostronny",
                        "unilateralny"
                    ],
                    "common_phrases": [
                        "umowa dwustronna",
                        "współpraca dwustronna",
                        "porozumienie dwustronne",
                        "stosunki dwustronne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Касающийся двух сторон, вовлекающий два субъекта или являющийся результатом их совместных действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двусторонний",
                                "target_lang_sense_clarification": "с участием двух сторон"
                            }
                        ]
                    }
                }
            ]
        }
    ]

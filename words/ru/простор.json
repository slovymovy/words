    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "179f88de-0ba8-32c4-923c-4def342119da",
                    "sense_definition": "Свободное, обширное пространство, не ограниченное препятствиями, которое можно окинуть взглядом.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы вышли из густого леса на широкий <w>простор</w> полей.",
                            "target_lang_translations": {
                                "en": "We emerged from the dense forest into the wide <w>expanse</w> of the fields."
                            }
                        },
                        {
                            "text": "Морской <w>простор</w> всегда восхищает своей бесконечностью и красотой.",
                            "target_lang_translations": {
                                "en": "The <w>vastness</w> of the sea always delights with its infinity and beauty."
                            }
                        }
                    ],
                    "synonyms": [
                        "даль",
                        "ширь",
                        "приволье"
                    ],
                    "antonyms": [
                        "теснота"
                    ],
                    "common_phrases": [
                        "морской простор",
                        "степные просторы",
                        "небесный простор",
                        "выйти на простор"
                    ],
                    "target_lang_definitions": {
                        "en": "A vast, open area or extent of space, usually of land, sea, or sky, that is free from obstacles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expanse",
                                "target_lang_sense_clarification": "A wide, continuous area of land, water, or sky"
                            },
                            {
                                "target_lang_word": "open space",
                                "target_lang_sense_clarification": "An area free from buildings or obstacles"
                            },
                            {
                                "target_lang_word": "vastness",
                                "target_lang_sense_clarification": "The quality of being very large or extensive"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09031e5e-ee60-3f2b-a60c-9e3e1b788c1e",
                    "sense_definition": "Отсутствие ограничений; полная свобода возможностей для проявления или развития чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта интересная профессия даёт огромный <w>простор</w> для творчества.",
                            "target_lang_translations": {
                                "en": "This interesting profession offers huge <w>scope</w> for creativity."
                            }
                        },
                        {
                            "text": "Здесь есть <w>простор</w> для смелых решений и реализации новых идей.",
                            "target_lang_translations": {
                                "en": "There is <w>room</w> here for bold decisions and the implementation of new ideas."
                            }
                        }
                    ],
                    "synonyms": [
                        "раздолье"
                    ],
                    "antonyms": [
                        "стеснённость"
                    ],
                    "common_phrases": [
                        "простор для творчества",
                        "простор для фантазии",
                        "простор для деятельности",
                        "дать простор"
                    ],
                    "target_lang_definitions": {
                        "en": "Freedom or opportunity to act, think, or develop ideas without restrictions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scope",
                                "target_lang_sense_clarification": "The opportunity or possibility to do or deal with something"
                            },
                            {
                                "target_lang_word": "room",
                                "target_lang_sense_clarification": "Opportunity or space for something to happen"
                            },
                            {
                                "target_lang_word": "space",
                                "target_lang_sense_clarification": "Freedom or opportunity to express oneself"
                            }
                        ]
                    }
                }
            ]
        }
    ]

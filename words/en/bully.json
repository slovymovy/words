                    "target_lang_definitions": {
                        "ru": "Выражение одобрения или поздравления, в современном контексте часто используется саркастически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браво",
                                "target_lang_sense_clarification": "возглас одобрения"
                            },
                            {
                                "target_lang_word": "молодец",
                                "target_lang_sense_clarification": "похвала"
                            }

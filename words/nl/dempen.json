    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "844e3a70-5038-3cc2-8de3-dfe116801688",
                    "sense_definition": "Iets minder hevig, luid of fel maken, zoals geluid, licht of emoties.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "diminishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dikke gordijnen <w>dempen</w> het geluid van het drukke verkeer.",
                            "target_lang_translations": {
                                "en": "The thick curtains <w>muffle</w> the sound of the busy traffic.",
                                "ru": "Толстые шторы <w>приглушают</w> шум оживленного движения."
                            }
                        },
                        {
                            "text": "Je kunt het licht van je telefoon <w>dempen</w> om je ogen te sparen.",
                            "target_lang_translations": {
                                "en": "You can <w>dim</w> the light of your phone to save your eyes.",
                                "ru": "Ты можешь <w>приглушить</w> свет своего телефона, чтобы поберечь глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "temperen",
                        "verzachten",
                        "verzwakken",
                        "matigen"
                    ],
                    "common_phrases": [
                        "het geluid dempen",
                        "het licht dempen",
                        "de verwachtingen dempen",
                        "een schok dempen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something less intense, loud, or bright, such as sound, light, or emotions.",
                        "ru": "Делать что-либо менее сильным, громким или ярким, например звук, свет или эмоции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "muffle",
                                "target_lang_sense_clarification": "to deaden or suppress sound"
                            },
                            {
                                "target_lang_word": "dim",
                                "target_lang_sense_clarification": "to reduce the brightness of light"
                            },
                            {
                                "target_lang_word": "dampen",
                                "target_lang_sense_clarification": "to reduce the intensity of sound, shock, or emotion"
                            },
                            {
                                "target_lang_word": "soften",
                                "target_lang_sense_clarification": "to make less harsh"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приглушать",
                                "target_lang_sense_clarification": "звук, свет"
                            },
                            {
                                "target_lang_word": "смягчать",
                                "target_lang_sense_clarification": "удар, эффект"
                            },
                            {
                                "target_lang_word": "амортизировать",
                                "target_lang_sense_clarification": "толчки, вибрацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f26304d3-eb49-3ce9-9a98-1d9f11c74692",
                    "sense_definition": "Een gat, kuil of watergang dichtmaken door er aarde of ander materiaal in te storten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "filling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente heeft besloten de oude gracht te <w>dempen</w> voor de aanleg van een weg.",
                            "target_lang_translations": {
                                "en": "The municipality has decided to <w>fill in</w> the old canal for the construction of a road.",
                                "ru": "Муниципалитет решил <w>засыпать</w> старый канал для прокладки дороги."
                            }
                        },
                        {
                            "text": "Na het ongeluk wilden de bewoners de diepe put onmiddellijk <w>dempen</w>.",
                            "target_lang_translations": {
                                "en": "After the accident, the residents wanted to <w>fill in</w> the deep well immediately.",
                                "ru": "После несчастного случая жители хотели немедленно <w>засыпать</w> глубокий колодец."
                            }
                        }
                    ],
                    "synonyms": [
                        "volstorten"
                    ],
                    "antonyms": [
                        "uitgraven",
                        "uitbaggeren"
                    ],
                    "common_phrases": [
                        "een gracht dempen",
                        "een put dempen",
                        "een sloot dempen"
                    ],
                    "target_lang_definitions": {
                        "en": "To close a hole, pit, or waterway by filling it with earth or other material.",
                        "ru": "Засыпать яму, углубление или водоем землей или другим материалом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fill in",
                                "target_lang_sense_clarification": "to fill a hole or channel with material"
                            },
                            {
                                "target_lang_word": "fill up",
                                "target_lang_sense_clarification": "to make completely full"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "засыпать",
                                "target_lang_sense_clarification": "яму, канал"
                            }
                        ]
                    }
                }
            ]
        }
    ]

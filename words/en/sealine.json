                            },
                            {
                                "target_lang_word": "лот",
                                "target_lang_sense_clarification": "прибор для измерения глубины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24a04197-4e2b-3f58-9b41-6f8f06990930",

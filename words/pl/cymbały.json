    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "60a6bee7-5286-3f98-8160-5cc51b485553",
                    "sense_definition": "Strunowy instrument muzyczny z grupy chordofonów uderzanych, popularny w muzyce ludowej, na którym gra się uderzając w struny drewnianymi pałeczkami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wirtuoz uderzał w <w>cymbały</w> z niezwykłą precyzją i szybkością.",
                            "target_lang_translations": {
                                "ru": "Виртуоз ударял по <w>цимбалам</w> с необычайной точностью и скоростью."
                            }
                        },
                        {
                            "text": "Podczas koncertu Jankiel dał wspaniały popis gry na <w>cymbałach</w>.",
                            "target_lang_translations": {
                                "ru": "Во время концерта Янкель блестяще сыграл на <w>цимбалах</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać na cymbałach",
                        "koncert na cymbałach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Струнный ударный музыкальный инструмент, на котором играют деревянными палочками; популярен в народной музыке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цимбалы",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "599538fc-5e43-35ef-b8ce-8b4d0d1f68d9",
                    "sense_definition": "Forma liczby mnogiej (mianownik i wołacz) rzeczownika <w>cymbał</w> (określenie osoby nierozgarniętej).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te dwa <w>cymbały</w> znowu zapomniały zamknąć drzwi na klucz.",
                            "target_lang_translations": {
                                "ru": "Эти два <w>болвана</w> снова забыли запереть дверь на ключ."
                            }
                        },
                        {
                            "text": "Nie zachowujcie się jak skończone <w>cymbały</w> i posłuchajcie rady.",
                            "target_lang_translations": {
                                "ru": "Не ведите себя как законченные <w>олухи</w> и послушайте совета."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural of cymbał"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глупые, недалекие люди (множественное число от слова «cymbał»); используется как ругательство."
                    },

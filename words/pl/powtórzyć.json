            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9725cec1-109d-35c3-840e-0f6eba1037f8",
                    "sense_definition": "Powiedzieć lub zrobić coś jeszcze raz, w ten sam sposób.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "repetition_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mógłbyś <w>powtórzyć</w> ostatnie zdanie?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты <w>повторить</w> последнее предложение?"
                            }
                        },
                        {
                            "text": "Aktor musiał <w>powtórzyć</w> tę scenę kilka razy.",
                            "target_lang_translations": {
                                "ru": "Актёру пришлось <w>повторить</w> эту сцену несколько раз."
                            }
                        }
                    ],
                    "synonyms": [
                        "ponowić"
                    ],
                    "common_phrases": [
                        "powtórzyć pytanie",
                        "powtórzyć prośbę",
                        "powtórzyć za mną",
                        "powtórzyć błąd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сказать или сделать что-то еще раз, таким же образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повторить",
                                "target_lang_sense_clarification": "сделать или сказать снова"

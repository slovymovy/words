    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "26ce2e4a-bf94-352e-b67a-b515e8e631a2",
                    "sense_definition": "Een antwoord vinden op een vraagstuk of een einde maken aan een probleem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "problemen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan deze moeilijke som niet <w>oplossen</w>.",
                            "target_lang_translations": {
                                "en": "I cannot <w>solve</w> this difficult sum."
                            }
                        },
                        {
                            "text": "We moeten dit probleem samen <w>oplossen</w>.",
                            "target_lang_translations": {
                                "en": "We must <w>solve</w> this problem together."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitzoeken",
                        "verhelpen",
                        "ophelderen"
                    ],
                    "antonyms": [
                        "neerslaan"
                    ],
                    "common_phrases": [
                        "een probleem oplossen",
                        "een raadsel oplossen",
                        "samen oplossen",
                        "een moord oplossen"
                    ],
                    "target_lang_definitions": {
                        "en": "To find an answer to a question or bring an end to a problem."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "solve",
                                "target_lang_sense_clarification": "to find the answer or explanation for a problem or mystery"
                            },
                            {
                                "target_lang_word": "resolve",
                                "target_lang_sense_clarification": "to settle or find a solution to a conflict or difficulty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf5c139f-fc22-30b7-800b-d3048088b7b6",
                    "sense_definition": "Een vaste stof volledig laten opgaan in een vloeistof.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "scheikunde",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Suiker <w>lost</w> makkelijk <w>op</w> in hete thee.",
                            "target_lang_translations": {
                                "en": "Sugar <w>dissolves</w> easily in hot tea."
                            }
                        },
                        {
                            "text": "Je moet het poeder eerst in water <w>oplossen</w>.",
                            "target_lang_translations": {
                                "en": "You must first <w>dissolve</w> the powder in water."
                            }
                        }
                    ],
                    "antonyms": [
                        "neerslaan"
                    ],
                    "common_phrases": [
                        "in water oplossen",
                        "goed oplossen",
                        "suiker oplossen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a solid substance to become incorporated into a liquid."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dissolve",
                                "target_lang_sense_clarification": "to mix a solid into a liquid until it becomes part of the liquid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f75ded1-707f-3e73-a954-19c5b7ab333b",
                    "sense_definition": "Langzaam verdwijnen of onzichtbaar worden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verdwijning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dichte mist zal snel <w>oplossen</w> als de zon schijnt.",
                            "target_lang_translations": {
                                "en": "The dense fog will quickly <w>dissolve</w> when the sun shines."
                            }
                        },
                        {
                            "text": "De dader leek wel in rook <w>opgelost</w>.",
                            "target_lang_translations": {
                                "en": "The perpetrator seemed to have <w>vanished</w> into thin air."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdwijnen",
                        "vervagen"
                    ],
                    "antonyms": [
                        "neerslaan"
                    ],
                    "common_phrases": [
                        "in rook oplossen",
                        "in het niets oplossen"
                    ],
                    "target_lang_definitions": {
                        "en": "To slowly disappear or become invisible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dissolve",
                                "target_lang_sense_clarification": "to fade away or disappear gradually"
                            },
                            {
                                "target_lang_word": "vanish",
                                "target_lang_sense_clarification": "to disappear suddenly or completely"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
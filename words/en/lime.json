                {
                    "sense_id": "b6e9f25c-3914-3a7c-8215-a08ff2046719",
                    "sense_definition": "A small green citrus fruit with a sour taste, somewhat smaller than a lemon.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "citrus_fruit_and_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She squeezed a fresh <w>lime</w> into her glass of water.",
                            "target_lang_translations": {
                                "ru": "Она выжала свежий <w>лайм</w> в свой стакан с водой."
                            }

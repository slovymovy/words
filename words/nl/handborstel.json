    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9910fbf8-0372-35c5-9ca6-b995380349e1",
                    "sense_definition": "Een kleine borstel die men gemakkelijk in de hand kan houden, meestal gebruikt om schoon te maken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cleaning_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij veegde het vuil van de tafel met een <w>handborstel</w>.",
                            "target_lang_translations": {
                                "ru": "Он смел мусор со стола <w>щеткой-сметкой</w>."
                            }
                        },
                        {
                            "text": "Gebruik een stevige <w>handborstel</w> om de vlek uit het tapijt te schrobben.",
                            "target_lang_translations": {
                                "ru": "Используйте жесткую <w>ручную щетку</w>, чтобы оттереть пятно с ковра."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoffer"
                    ],
                    "common_phrases": [
                        "met een handborstel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая щетка, которую удобно держать в руке, обычно используемая для уборки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щетка",
                                "target_lang_sense_clarification": "общая"
                            },
                            {
                                "target_lang_word": "щетка-сметка",
                                "target_lang_sense_clarification": "для сметания пыли"
                            },
                            {
                                "target_lang_word": "ручная щетка",
                                "target_lang_sense_clarification": "для чистки поверхностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f422c25-6b95-3e16-af68-09cdcc4bba08",
                    "sense_definition": "Een tandenborstel die niet elektrisch wordt aangedreven.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "dental_hygiene",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn tandarts zegt dat je met een <w>handborstel</w> ook goed kunt poetsen.",
                            "target_lang_translations": {
                                "ru": "Мой стоматолог говорит, что <w>обычной зубной щеткой</w> тоже можно хорошо чистить зубы."
                            }
                        },
                        {
                            "text": "Ik neem een gewone <w>handborstel</w> mee op vakantie omdat ik die niet hoef op te laden.",
                            "target_lang_translations": {
                                "ru": "Я беру с собой в отпуск обычную <w>зубную щетку</w>, потому что ее не нужно заряжать."
                            }
                        }
                    ],
                    "synonyms": [
                        "handtandenborstel"
                    ],
                    "antonyms": [
                        "elektrische tandenborstel"
                    ],
                    "common_phrases": [
                        "poetsen met een handborstel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зубная щетка, которая не приводится в действие электричеством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обычная зубная щетка",
                                "target_lang_sense_clarification": "не электрическая"
                            },
                            {
                                "target_lang_word": "мануальная зубная щетка",
                                "target_lang_sense_clarification": "стоматологический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e6ad5ee-1ae6-38af-897c-e63cb7bbdfec",
                    "sense_definition": "Osoba czuwająca nad przestrzeganiem przepisów gry podczas zawodów sportowych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główny <w>sędzia</w> podyktował rzut karny w ostatniej minucie meczu.",
                            "target_lang_translations": {
                                "ru": "Главный <w>судья</w> назначил пенальти на последней минуте матча."
                            }
                        },
                        {
                            "text": "Kibice głośno gwizdali, gdy <w>sędzia</w> pokazał zawodnikowi czerwoną kartkę.",
                            "target_lang_translations": {
                                "ru": "Болельщики громко свистели, когда <w>судья</w> показал игроку красную карточку."
                            }
                        }
                    ],
                    "synonyms": [
                        "arbiter"
                    ],
                    "common_phrases": [
                        "sędzia główny",
                        "sędzia liniowy",
                        "sędzia piłkarski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, контролирующее соблюдение правил игры во время спортивных соревнований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судья",
                                "target_lang_sense_clarification": "в спорте"
                            },
                            {
                                "target_lang_word": "арбитр",
                                "target_lang_sense_clarification": "спортивный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcedea23-2934-30e3-9840-035928417254",
                    "sense_definition": "Urzędnik uprawniony do prowadzenia rozpraw sądowych i wydawania wyroków.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oskarżony wstał, gdy <w>sędzia</w> wszedł na salę rozpraw.",
                            "target_lang_translations": {
                                "ru": "Обвиняемый встал, когда <w>судья</w> вошёл в зал суда."
                            }
                        },
                        {
                            "text": "Wysoki sądzie, proszę o łagodny wymiar kary.",
                            "target_lang_translations": {
                                "ru": "Ваша честь, прошу о мягком наказании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wysoki sądzie",
                        "sędzia śledczy",
                        "niezawisły sędzia",
                        "sędzia pokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должностное лицо, уполномоченное вести судебные дела и выносить приговоры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судья",
                                "target_lang_sense_clarification": "должностное лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7809b3a-c70c-3272-a7bd-da6382f345c7",
                    "sense_definition": "Osoba, która rozstrzyga spory lub ocenia czyjeś postępowanie, niekoniecznie w sądzie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "evaluator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sumienie to najsurowszy <w>sędzia</w> naszych codziennych czynów.",
                            "target_lang_translations": {
                                "ru": "Совесть — самый строгий <w>судья</w> наших повседневных поступков."
                            }
                        },
                        {
                            "text": "Został zaproszony jako <w>sędzia</w> do konkursu talentów w szkole.",
                            "target_lang_translations": {
                                "ru": "Его пригласили в качестве <w>судьи</w> на школьный конкурс талантов."
                            }
                        }
                    ],
                    "synonyms": [
                        "arbiter"
                    ],
                    "common_phrases": [
                        "surowy sędzia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который разрешает споры или оценивает чьи-либо действия, не обязательно в суде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судья",
                                "target_lang_sense_clarification": "тот, кто оценивает"
                            },
                            {
                                "target_lang_word": "ценитель",
                                "target_lang_sense_clarification": "знаток"
                            }
                        ]
                    }
                }
            ]
        }
    ]

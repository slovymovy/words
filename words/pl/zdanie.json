                },
                {
                    "sense_id": "aa462545-4e17-33df-83c2-b08abd8ca1e1",
                    "sense_definition": "Wrodzona zdolność do czegoś; talent.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dialect_talent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On ma do tej roboty <w>zdanie</w>, wszystko mu wychodzi.",
                            "target_lang_translations": {
                                "ru": "У него есть <w>способность</w> к этой работе, у него всё получается."
                            }
                        },
                        {
                            "text": "Dziewczyna ma <w>zdanie</w> do śpiewania.",
                            "target_lang_translations": {
                                "ru": "У девушки есть <w>талант</w> к пению."
                            }
                        }
                    ],
                    "synonyms": [

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "337608ff-67d7-38d9-8044-84831058743c",
                    "sense_definition": "To fit or install a lining into a cylinder.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic <w>linered</w> the engine cylinders to restore compression.",
                            "target_lang_translations": {
                                "ru": "Механик <w>загильзовал</w> цилиндры двигателя, чтобы восстановить компрессию."
                            }
                        },
                        {
                            "text": "These pipes need to be <w>linered</w> with a protective coating before use.",
                            "target_lang_translations": {
                                "ru": "Эти трубы должны быть <w>футерованы</w> защитным покрытием перед использованием."
                            }
                        }
                    ],
                    "synonyms": [

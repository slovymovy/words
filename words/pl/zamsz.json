            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e35ace9-7785-3843-891b-9525dc0f1072",
                    "sense_definition": "Rodzaj miękkiej w dotyku skóry zwierzęcej z delikatnym meszkiem, używanej do produkcji obuwia i odzieży.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłam sobie nowe buty wykonane z brązowego <w>zamszu</w>.",
                            "target_lang_translations": {
                                "ru": "Я купила себе новые ботинки из коричневой <w>замши</w>."
                            }
                        },
                        {
                            "text": "Kurtka z <w>zamszu</w> wymaga specjalnej pielęgnacji i ochrony przed deszczem.",
                            "target_lang_translations": {
                                "ru": "Куртка из <w>замши</w> требует особого ухода и защиты от дождя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "buty z zamszu",
                        "kurtka z zamszu",
                        "szczotka do zamszu",
                        "naturalny zamsz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид мягкой на ощупь кожи с коротким ворсом, используемой для изготовления обуви и одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замша",
                                "target_lang_sense_clarification": "натуральная кожа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b9b922f-b9e8-3b13-a772-f9fc7fc8890a",
                    "sense_definition": "Tkanina o lekko mechatej powierzchni, która imituje skórę zamszową.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fotel w salonie został obity miękkim, syntetycznym <w>zamszem</w>.",
                            "target_lang_translations": {
                                "ru": "Кресло в гостиной было обито мягкой синтетической <w>замшей</w>."
                            }
                        },
                        {
                            "text": "Ten płaszcz uszyto z grubego <w>zamszu</w>, który jest przyjemny w dotyku.",
                            "target_lang_translations": {
                                "ru": "Это пальто сшили из плотной <w>замши</w>, приятной на ощупь."
                            }
                        }
                    ],
                    "synonyms": [

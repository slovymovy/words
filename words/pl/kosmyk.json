    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01257d26-7f6c-3aac-93b2-8f7be3bb9488",
                    "sense_definition": "Niewielkie pasmo lub pukiel włosów, często oddzielające się od reszty fryzury.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hair_strand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na czole zwisał mu niesforny <w>kosmyk</w> włosów, którego nie mógł ułożyć.",
                            "target_lang_translations": {
                                "ru": "На лбу у него свисала непослушная <w>прядь</w> волос, которую он никак не мог уложить."
                            }
                        },
                        {
                            "text": "Dziewczyna w zamyśleniu nawijała sobie <w>kosmyk</w> na palec podczas czytania książki.",
                            "target_lang_translations": {
                                "ru": "Девушка задумчиво наматывала <w>прядь</w> на палец во время чтения книги."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasemko",
                        "lok"
                    ],
                    "common_phrases": [
                        "niesforny kosmyk",
                        "kosmyk włosów",
                        "odgarnąć kosmyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая прядь или пучок волос, часто выбивающийся из прически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прядь",
                                "target_lang_sense_clarification": "волос"
                            },
                            {
                                "target_lang_word": "локон",
                                "target_lang_sense_clarification": "завиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb753a14-d5d2-3b9e-8bdd-3f4096d0e13e",
                    "sense_definition": "Ogon zwierzyny płowej, na przykład sarny lub jelenia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hunting_tail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony myśliwy potrafi rozpoznać zwierzę, widząc jedynie jego <w>kosmyk</w> w zaroślach.",
                            "target_lang_translations": {
                                "ru": "Опытный охотник может распознать зверя, увидев в зарослях лишь его <w>хвост</w>."
                            }
                        },
                        {
                            "text": "W gwarze łowieckiej krótki ogon sarny określa się mianem <w>kosmyka</w>.",
                            "target_lang_translations": {
                                "ru": "На охотничьем жаргоне короткий хвост косули называют <w>хвостом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "omyk",
                        "osmyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хвост оленя или косули (в охотничьей терминологии)."
                    },

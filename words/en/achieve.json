                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добиваться",
                                "target_lang_sense_clarification": "хороших результатов"
                            },
                            {
                                "target_lang_word": "преуспевать",
                                "target_lang_sense_clarification": "быть успешным"

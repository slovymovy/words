            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b65e2eb4-27b5-3b07-9740-4823bb97fb7d",
                    "sense_definition": "Taki, którego życzenia, nadzieje lub oczekiwania nie spełniły się, odczuwający z tego powodu smutek.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat był bardzo <w>zawiedziony</w>, gdy nie dostał wymarzonego prezentu na urodziny.",
                            "target_lang_translations": {
                                "ru": "Мой брат был очень <w>разочарован</w>, когда не получил подарка своей мечты на день рождения."
                            }
                        },
                        {
                            "text": "Jestem głęboko <w>zawiedziony</w> twoją nieodpowiedzialną postawą podczas wczorajszego zebrania.",
                            "target_lang_translations": {
                                "ru": "Я глубоко <w>разочарован</w> твоим безответственным поведением во время вчерашнего собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozczarowany"
                    ],
                    "antonyms": [
                        "zadowolony",
                        "usatysfakcjonowany"
                    ],
                    "common_phrases": [
                        "głęboko zawiedziony",
                        "srogo zawiedziony",
                        "czuć się zawiedziony",
                        "być zawiedzionym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, чьи желания, надежды или ожидания не оправдались, испытывающий из-за этого грусть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разочарованный",
                                "target_lang_sense_clarification": "испытывающий чувство неудовлетворенности"
                            }
                        ]
                    }
                }
            ]
        },
        {

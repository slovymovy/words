{
    "word_family": [
        "verveling",
        "vervelend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "478fc99e-f19d-3b00-93ff-379401940068",
                    "sense_definition": "Zich niet vermaken omdat er niets interessants te doen is of omdat men niet weet wat te doen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "boredom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>verveel</w> me altijd als ik moet wachten op de bus.",
                            "target_lang_translations": {
                                "en": "I always <w>get bored</w> when I have to wait for the bus."
                            }
                        },
                        {
                            "text": "De kinderen <w>verveelden</w> zich dood tijdens de regenachtige vakantie.",
                            "target_lang_translations": {
                                "en": "The children <w>were bored</w> to death during the rainy holiday."
                            }
                        }
                    ],
                    "antonyms": [
                        "zich vermaken",
                        "zich amuseren"
                    ],
                    "common_phrases": [
                        "zich dood vervelen",
                        "zich te pletter vervelen",
                        "zich kapot vervelen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To not enjoy oneself because there is nothing interesting to do or one does not know what to do."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be bored",
                                "target_lang_sense_clarification": "reflexive use"
                            },
                            {
                                "target_lang_word": "get bored",
                                "target_lang_sense_clarification": "reflexive use"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4291eeb-b1f2-3fee-bfb3-e5b147106ce8",
                    "sense_definition": "Saai zijn en daardoor een gevoel van desinteresse of lusteloosheid bij iemand opwekken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "boredom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die lange film begon me na een uur te <w>vervelen</w>.",
                            "target_lang_translations": {
                                "en": "That long movie started to <w>bore</w> me after an hour."
                            }
                        },
                        {
                            "text": "Hij <w>verveelt</w> iedereen met zijn eindeloze verhalen over het weer.",
                            "target_lang_translations": {
                                "en": "He <w>bores</w> everyone with his endless stories about the weather."
                            }
                        }
                    ],
                    "antonyms": [
                        "boeien",
                        "interesseren",
                        "vermaken"
                    ],
                    "target_lang_definitions": {
                        "en": "To be boring and thereby arouse a feeling of disinterest or listlessness in someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bore",
                                "target_lang_sense_clarification": "cause boredom"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c948032c-daf0-31df-a537-23ae1853e29b",
                    "sense_definition": "Iemand irriteren, hinderen of tot last zijn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "annoyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zit je zusje niet zo te <w>vervelen</w> als ze huiswerk maakt!",
                            "target_lang_translations": {
                                "en": "Don't <w>bother</w> your sister like that while she's doing homework!"
                            }
                        },
                        {
                            "text": "De muggen <w>verveelden</w> ons de hele avond tijdens de barbecue.",
                            "target_lang_translations": {
                                "en": "The mosquitoes <w>annoyed</w> us all evening during the barbecue."
                            }
                        }
                    ],
                    "synonyms": [
                        "irriteren",
                        "hinderen",
                        "lastigvallen",
                        "klieren",
                        "ergeren"
                    ],
                    "antonyms": [
                        "met rust laten"
                    ],
                    "target_lang_definitions": {
                        "en": "To irritate, hinder, or be a burden to someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "annoy",
                                "target_lang_sense_clarification": "irritate"
                            },
                            {
                                "target_lang_word": "bother",
                                "target_lang_sense_clarification": "disturb or trouble"
                            },
                            {
                                "target_lang_word": "pester",
                                "target_lang_sense_clarification": "annoy persistently"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
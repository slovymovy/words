    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ac0e33a-4ea6-3a23-9687-8831b4b6647d",
                    "sense_definition": "The act of moving from one place to another to live, often across borders or regions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "living_being_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The war caused a massive <w>migration</w> of refugees to neighboring countries.",
                            "target_lang_translations": {
                                "ru": "Война вызвала массовую <w>миграцию</w> беженцев в соседние страны."
                            }
                        },
                        {
                            "text": "Economic factors often drive the <w>migration</w> of workers to large cities.",
                            "target_lang_translations": {
                                "ru": "Экономические факторы часто стимулируют <w>миграцию</w> рабочих в крупные города."
                            }
                        }
                    ],
                    "synonyms": [
                        "relocation",
                        "resettlement"
                    ],
                    "common_phrases": [
                        "mass migration",
                        "forced migration",
                        "internal migration",
                        "net migration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение людей из одного места в другое для проживания, часто через границы или регионы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миграция",
                                "target_lang_sense_clarification": "перемещение населения"
                            },
                            {
                                "target_lang_word": "переселение",
                                "target_lang_sense_clarification": "смена места жительства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "894f363c-5f79-3f53-87e2-e6094165e60b",
                    "sense_definition": "The seasonal movement of animals from one region to another, usually for breeding or finding food.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "living_being_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists track the annual <w>migration</w> of whales across the ocean.",
                            "target_lang_translations": {
                                "ru": "Учёные отслеживают ежегодную <w>миграцию</w> китов через океан."
                            }
                        },
                        {
                            "text": "The <w>migration</w> of monarch butterflies is a spectacular natural event.",
                            "target_lang_translations": {
                                "ru": "<w>Миграция</w> бабочек-монархов — это захватывающее природное явление."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bird migration",
                        "seasonal migration",
                        "annual migration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сезонное перемещение животных из одного региона в другой, обычно для размножения или поиска пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миграция",
                                "target_lang_sense_clarification": "животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "576d7372-96a2-3abd-89f7-d9368b3e7856",
                    "sense_definition": "The process of moving data, software, or systems from one environment or platform to another.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company completed the <w>migration</w> of its database to the cloud.",
                            "target_lang_translations": {
                                "ru": "Компания завершила <w>миграцию</w> своей базы данных в облако."
                            }
                        },
                        {
                            "text": "We encountered several errors during the server <w>migration</w> process.",
                            "target_lang_translations": {
                                "ru": "Мы столкнулись с несколькими ошибками в процессе <w>миграции</w> сервера."
                            }
                        }
                    ],
                    "synonyms": [
                        "transfer",
                        "transition"
                    ],
                    "common_phrases": [
                        "data migration",
                        "system migration",
                        "cloud migration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс переноса данных, программного обеспечения или систем из одной среды или платформы на другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миграция",
                                "target_lang_sense_clarification": "данных или систем"
                            },
                            {

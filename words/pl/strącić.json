    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6be7f1d7-e848-34c4-9ac3-0145cde388c6",
                    "sense_definition": "Uderzyć lub popchnąć coś znajdującego się wysoko lub na krawędzi, powodując jego upadek na dół.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kot wskoczył na półkę i <strącił> ulubiony wazon babci.",
                            "target_lang_translations": {
                                "ru": "Кот запрыгнул на полку и <сбросил> любимую бабушкину вазу."
                            }
                        },
                        {
                            "text": "Uważaj, żebyś niechcący nie <strącił> tych dokumentów ze stołu.",
                            "target_lang_translations": {
                                "ru": "Смотри, чтобы ты случайно не <сбросил> эти документы со стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrzucić",
                        "zwalić"
                    ],
                    "antonyms": [
                        "podnieść",
                        "ustawić",
                        "postawić"
                    ],
                    "common_phrases": [
                        "strącić ze stołu",
                        "strącić w przepaść",
                        "strącić ze schodów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударить или толкнуть что-то, находящееся высоко или на краю, заставив это упасть вниз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбросить",
                                "target_lang_sense_clarification": "заставить упасть с высоты"
                            },
                            {
                                "target_lang_word": "сбить",
                                "target_lang_sense_clarification": "ударом заставить упасть"
                            },
                            {
                                "target_lang_word": "свалить",
                                "target_lang_sense_clarification": "опрокинуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0a239e3-834e-3989-89d5-8fda673f5598",
                    "sense_definition": "Trafić obiekt latający (np. samolot, ptaka) i spowodować jego upadek na ziemię.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obrona przeciwlotnicza skutecznie <strąciła> wrogi dron nad granicą.",
                            "target_lang_translations": {
                                "ru": "Противовоздушная оборона успешно <сбила> вражеский дрон над границей."
                            }
                        },
                        {
                            "text": "Podczas bitwy powietrznej pilotowi udało się <strącić> dwa myśliwce.",
                            "target_lang_translations": {
                                "ru": "Во время воздушного боя пилоту удалось <сбить> два истребителя."
                            }
                        }
                    ],
                    "synonyms": [
                        "zestrzelić"
                    ],
                    "common_phrases": [
                        "strącić samolot",
                        "strącić rakietę",
                        "strącić bombowiec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попасть в летящий объект (напр. самолет, птицу) и заставить его упасть на землю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбить",
                                "target_lang_sense_clarification": "летающий объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d372ac7e-75e9-3f87-a233-f066885f7387",
                    "sense_definition": "Spowodować wydzielenie się substancji stałej z roztworu w wyniku reakcji chemicznej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dodając odpowiedni odczynnik, chemik <strącił> osad na dnie probówki.",
                            "target_lang_translations": {
                                "ru": "Добавив соответствующий реагент, химик <осадил> осадок на дне пробирки."
                            }
                        },
                        {
                            "text": "W tym doświadczeniu musimy <strącić> chlorek srebra z roztworu.",
                            "target_lang_translations": {
                                "ru": "В этом опыте мы должны <осадить> хлорид серебра из раствора."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytrącić"
                    ],
                    "antonyms": [
                        "rozpuścić"
                    ],
                    "common_phrases": [
                        "strącić osad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать выделение твердого вещества из раствора в результате химической реакции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осадить",
                                "target_lang_sense_clarification": "в химии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff3acd01-59b0-32f2-9d65-3948012a34ac",
                    "sense_definition": "Odjąć część kwoty od sumy pieniędzy należnej komuś; obniżyć cenę lub wynagrodzenie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szef powiedział, że musi mi <strącić> z wypłaty za spóźnienia.",
                            "target_lang_translations": {
                                "ru": "Начальник сказал, что должен <вычесть> у меня из зарплаты за опоздания."
                            }
                        },
                        {
                            "text": "Sprzedawca zgodził się <strącić> kilka złotych z ceny towaru.",
                            "target_lang_translations": {
                                "ru": "Продавец согласился <скинуть> несколько злотых с цены товара."
                            }
                        }
                    ],
                    "synonyms": [
                        "potrącić",
                        "odliczyć",
                        "ująć"
                    ],
                    "antonyms": [
                        "doliczyć",
                        "dodać"
                    ],
                    "common_phrases": [
                        "strącić z ceny",
                        "strącić z pensji"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вычесть часть суммы из причитающихся кому-то денег; снизить цену или вознаграждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вычесть",
                                "target_lang_sense_clarification": "из зарплаты"
                            },
                            {
                                "target_lang_word": "скинуть",
                                "target_lang_sense_clarification": "с цены (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "10a094f8-9ef7-3304-b6e9-05c6d2f1695e",
                    "sense_definition": "An official rule or law that limits what you can do or what can happen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_limitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government decided to lift the travel <w>restrictions</w> after the pandemic ended.",
                            "target_lang_translations": {
                                "ru": "Правительство решило снять <w>ограничения</w> на поездки после окончания пандемии."
                            }
                        },
                        {
                            "text": "There are strict <w>restrictions</w> on how much liquid you can take on a plane.",
                            "target_lang_translations": {
                                "ru": "Существуют строгие <w>ограничения</w> на количество жидкости, которое можно взять в самолет."
                            }
                        }
                    ],
                    "synonyms": [
                        "limitation",
                        "regulation",
                        "constraint",
                        "rule",
                        "control"
                    ],
                    "antonyms": [
                        "freedom",
                        "permission",
                        "allowance"
                    ],
                    "common_phrases": [
                        "impose restrictions",
                        "lift restrictions",
                        "travel restrictions",
                        "dietary restrictions",
                        "severe restrictions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное правило или закон, который ограничивает ваши действия или возможные события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограничение",
                                "target_lang_sense_clarification": "правило, лимит"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d29225b-144b-3681-b9d5-d73c116d0097",
                    "sense_definition": "The act of limiting someone or something, or the condition of being limited.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_limitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>restriction</w> of blood flow to the brain can cause serious damage.",
                            "target_lang_translations": {
                                "ru": "<w>Ограничение</w> притока крови к мозгу может вызвать серьезные повреждения."
                            }
                        },
                        {
                            "text": "He felt a sense of frustration at the <w>restriction</w> of his personal liberty.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал разочарование из-за <w>ограничения</w> его личной свободы."
                            }
                        }
                    ],
                    "synonyms": [
                        "confinement",
                        "limitation",
                        "restraint",
                        "curb"
                    ],
                    "antonyms": [
                        "liberation",
                        "freedom",
                        "expansion"
                    ],
                    "common_phrases": [
                        "without restriction",
                        "restriction of movement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по ограничению кого-либо или чего-либо, или состояние ограниченности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограничение",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26c6d7e5-c523-36c1-9fce-dba82335d392",
                    "sense_definition": "The mechanism by which a cell degrades foreign DNA material.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists use specific enzymes to perform <w>restriction</w> on DNA strands for analysis.",
                            "target_lang_translations": {
                                "ru": "Ученые используют специальные ферменты для выполнения <w>рестрикции</w> цепей ДНК для анализа."
                            }
                        },
                        {
                            "text": "Bacterial cells utilize <w>restriction</w> to defend themselves against viral genetic material.",
                            "target_lang_translations": {
                                "ru": "Бактериальные клетки используют <w>рестрикцию</w> для защиты от вирусного генетического материала."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм, с помощью которого клетка разрушает чужеродный материал ДНК."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рестрикция",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
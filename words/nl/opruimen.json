                    "common_phrases": [
                        "voorraad opruimen",
                        "alles opruimen"
                    ],
                    "target_lang_definitions": {
                        "en": "To sell goods to make room for new stock (clearance).",
                        "ru": "Распродавать товары, чтобы освободить место для новых."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sell off",
                                "target_lang_sense_clarification": "to sell goods, often at a discount, to get rid of them"
                            },
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "to sell stock to empty space (e.g., clear stock)"
                            },
                            {
                                "target_lang_word": "clear out",
                                "target_lang_sense_clarification": "to empty a storage space by selling or removing contents"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "распродавать",
                                "target_lang_sense_clarification": "устраивать распродажу"

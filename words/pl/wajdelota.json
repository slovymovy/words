            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8a55f9c7-6e71-3be0-aa60-292ef72b2a7b",
                    "sense_definition": "Litewski pogański wieszczek, guślarz i pieśniarz, pełniący w kulcie rolę kapłana niższego stopnia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "historical_priest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>wajdelota</w> usiadł przy ognisku i zaczął snuć opowieść o dawnych bohaterach.",
                            "target_lang_translations": {
                                "ru": "Старый <w>вайделот</w> сел у костра и начал повествовать о древних героях."
                            }
                        },
                        {
                            "text": "W dawnych wierzeniach <w>wajdeloci</w> byli pośrednikami między ludźmi a bogami.",
                            "target_lang_translations": {
                                "ru": "В древних верованиях <w>вайделоты</w> были посредниками между людьми и богами."
                            }
                        }
                    ],
                    "synonyms": [
                        "wurszajta",
                        "wejdalota",
                        "wajdeluta"
                    ],
                    "common_phrases": [
                        "pieśń wajdeloty",
                        "stary wajdelota"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dotyczy dawnej Litwy i Prus"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Литовский языческий прорицатель, гусляр и певец, выполнявший роль жреца низшего ранга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вайделот",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1d407a8-53a9-3144-ae23-94e76075c8fa",
                    "sense_definition": "Postać wybitna i przewodnia w jakimś środowisku, zwłaszcza artystycznym lub naukowym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten profesor był prawdziwym <w>wajdelotą</w> dla całego pokolenia młodych badaczy.",
                            "target_lang_translations": {
                                "ru": "Этот профессор был настоящим <w>корифеем</w> для целого поколения молодых исследователей."
                            }
                        },
                        {
                            "text": "Jako duchowy <w>wajdelota</w> narodu, poeta wskazywał rodakom drogę do wolności.",
                            "target_lang_translations": {
                                "ru": "Как духовный <w>наставник</w> нации, поэт указывал соотечественникам путь к свободе."
                            }
                        }
                    ],
                    "synonyms": [
                        "koryfeusz"
                    ],
                    "common_phrases": [
                        "duchowy wajdelota"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдающаяся и ведущая личность в какой-либо среде, особенно в художественной или научной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корифей",
                                "target_lang_sense_clarification": "выдающийся деятель"
                            },
                            {
                                "target_lang_word": "духовный наставник",
                                "target_lang_sense_clarification": "идейный вдохновитель"
                            }
                        ]
                    }
                }
            ]

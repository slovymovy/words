{
    "word_family": [
        "odbiór",
        "odbiorca",
        "odbiornik"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c5930b7e-3b2e-3894-a1ff-5af1fad948bf",
                    "sense_definition": "Podnosić słuchawkę telefonu lub akceptować połączenie przychodzące, aby rozpocząć rozmowę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "telecommunication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię <odbierać> telefonów od nieznanych numerów późnym wieczorem.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <отвечать> на звонки с незнакомых номеров поздно вечером."
                            }
                        },
                        {
                            "text": "Kiedy jestem w pracy, nie mogę <odbierać> prywatnych połączeń.",
                            "target_lang_translations": {
                                "ru": "Когда я на работе, я не могу <отвечать> на личные звонки."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnosić słuchawkę"
                    ],
                    "antonyms": [
                        "rozłączać"
                    ],
                    "common_phrases": [
                        "odbierać telefon",
                        "odbierać połączenie",
                        "odbierać komórkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднимать трубку или отвечать на входящий звонок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "на звонок"
                            },
                            {
                                "target_lang_word": "брать трубку",
                                "target_lang_sense_clarification": "телефона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f4389cd-dad3-35b3-94f9-3acc4e41626a",
                    "sense_definition": "Przychodzić lub przyjeżdżać po kogoś w umówione miejsce, aby go stamtąd zabrać.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mąż codziennie musi <odbierać> nasze dzieci ze szkoły o trzeciej.",
                            "target_lang_translations": {
                                "ru": "Мой муж должен каждый день <забирать> наших детей из школы в три часа."
                            }
                        },
                        {
                            "text": "Czy będziesz <odbierać> gości z lotniska swoim samochodem?",
                            "target_lang_translations": {
                                "ru": "Ты будешь <забирать> гостей из аэропорта на своей машине?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zabierać"
                    ],
                    "antonyms": [
                        "zostawiać",
                        "oddawać"
                    ],
                    "common_phrases": [
                        "odbierać dzieci",
                        "odbierać gości",
                        "odbierać ze szkoły",
                        "odbierać z dworca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приезжать или приходить за кем-то, чтобы забрать (например, детей из школы или гостей с вокзала)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забирать",
                                "target_lang_sense_clarification": "кого-то"
                            },
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "приезжающих"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db62bde4-9bf7-3c90-8c47-f8ce4d8a16c4",
                    "sense_definition": "Iść lub jechać po rzecz, która została gdzieś zostawiona, przysłana lub przygotowana.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę regularnie <odbierać> pocztę, żeby nie przegapić ważnych rachunków.",
                            "target_lang_translations": {
                                "ru": "Мне нужно регулярно <забирать> почту, чтобы не пропустить важные счета."
                            }
                        },
                        {
                            "text": "Klienci mogą <odbierać> zamówione towary bezpośrednio w naszym magazynie.",
                            "target_lang_translations": {
                                "ru": "Клиенты могут <забирать> заказанные товары прямо с нашего склада."
                            }
                        }
                    ],
                    "synonyms": [
                        "podejmować",
                        "brać"
                    ],
                    "antonyms": [
                        "zostawiać",
                        "oddawać",
                        "wręczać"
                    ],
                    "common_phrases": [
                        "odbierać paczkę",
                        "odbierać list",
                        "odbierać wyniki",
                        "odbierać zamówienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти или ехать, чтобы получить вещью, почту или заказ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забирать",
                                "target_lang_sense_clarification": "что-то"
                            },
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "почту, посылку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f7786d9-04c1-3e64-8c8a-26248b1b467b",
                    "sense_definition": "Pozbawiać kogoś czegoś, co posiadał, lub sprawiać, że traci on jakąś cechę, prawo lub możliwość.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "deprivation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciężka choroba powoli <odbierała> mu siły i chęć do życia.",
                            "target_lang_translations": {
                                "ru": "Тяжелая болезнь медленно <отнимала> у него силы и волю к жизни."
                            }
                        },
                        {
                            "text": "Policja ma prawo <odbierać> dokumenty kierowcom łamiącym przepisy.",
                            "target_lang_translations": {
                                "ru": "Полиция имеет право <отбирать> документы у водителей, нарушающих правила."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozbawiać",
                        "zabierać"
                    ],
                    "antonyms": [
                        "obdarzać",
                        "oddawać",
                        "wręczać"
                    ],
                    "common_phrases": [
                        "odbierać nadzieję",
                        "odbierać głos",
                        "odbierać prawo",
                        "odbierać życie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать кого-то чего-то (силы, прав, надежды) или отнимать что-то силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отнимать",
                                "target_lang_sense_clarification": "лишать чего-то"
                            },
                            {
                                "target_lang_word": "отбирать",
                                "target_lang_sense_clarification": "силой или по праву"
                            },
                            {
                                "target_lang_word": "лишать",
                                "target_lang_sense_clarification": "права, возможности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3fee441-cfc9-3a04-94e2-d9355d8a0a69",
                    "sense_definition": "O urządzeniach elektronicznych: wychwytywać i przetwarzać sygnał nadawczy (np. radiowy lub telewizyjny).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W górach nasze stare radio bardzo słabo <odbierało> większość stacji.",
                            "target_lang_translations": {
                                "ru": "В горах наше старое радио очень плохо <ловило> большинство станций."
                            }
                        },
                        {
                            "text": "Ten telewizor nie <odbiera> kanałów cyfrowych bez dodatkowego dekodera.",
                            "target_lang_translations": {
                                "ru": "Этот телевизор не <ловит> цифровые каналы без дополнительного декодера."
                            }
                        }
                    ],
                    "synonyms": [
                        "łapać"
                    ],
                    "common_phrases": [
                        "radio odbiera",
                        "telewizor nie odbiera",
                        "odbierać fale",
                        "odbierać sygnał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Улавливать и воспроизводить сигнал (о радио, телевидении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ловить",
                                "target_lang_sense_clarification": "сигнал, станцию"
                            },
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "сигнал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1193eba5-cae1-3ddb-b353-7bcbb3e9ece3",
                    "sense_definition": "Rozumieć lub interpretować czyjeś słowa, zachowanie lub sytuację w określony sposób.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego milczenie wszyscy <odbierali> jako cichy znak zgody na propozycję.",
                            "target_lang_translations": {
                                "ru": "Его молчание все <воспринимали> как тихий знак согласия на предложение."
                            }
                        },
                        {
                            "text": "Mam nadzieję, że nie będziesz <odbierać> moich uwag jako ataku na siebie.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, ты не будешь <воспринимать> мои замечания как нападки на себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "interpretować",
                        "rozumieć",
                        "traktować"
                    ],
                    "common_phrases": [
                        "odbierać pozytywnie",
                        "odbierać negatywnie",
                        "źle to odbierać",
                        "odbierać jako żart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать или толковать чьи-то слова, поведение или ситуацию определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспринимать",
                                "target_lang_sense_clarification": "понимать, толковать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0055a099-1110-3169-86e7-e7a078d9072e",
                    "sense_definition": "Uroczyste przemówienie lub oficjalne pismo skierowane przez ważną osobistość (np. głowę państwa, dostojnika kościelnego) do ogółu społeczeństwa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydent wygłosił wczoraj wieczorem ważne <w>orędzie</w> do wszystkich obywateli.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером президент выступил с важным <w>обращением</w> ко всем гражданам."
                            }
                        },
                        {
                            "text": "W swoim świątecznym <w>orędziu</w> papież nawoływał do pokoju na świecie.",
                            "target_lang_translations": {
                                "ru": "В своем праздничном <w>послании</w> папа призывал к миру во всем мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "odezwa",
                        "posłanie",
                        "proklamacja",
                        "manifest",
                        "przesłanie"
                    ],
                    "common_phrases": [
                        "orędzie noworoczne",
                        "wygłosić orędzie",
                        "orędzie prezydenta",
                        "orędzie do narodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественная речь или официальное письмо, направленное важным лицом (например, главой государства, церковным иерархом) широкой общественности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "послание",
                                "target_lang_sense_clarification": "официальное письмо или речь, напр. президента или папы римского"
                            },
                            {
                                "target_lang_word": "обращение",
                                "target_lang_sense_clarification": "речь, адресованная народу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f8aa9d4-e031-3de3-ae0a-554d371ca236",
                    "sense_definition": "Przestarzałe określenie oznaczające prośbę lub wstawiennictwo w jakiejś sprawie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachcic wysłał do króla pokorne <w>orędzie</w> z prośbą o ułaskawienie syna.",
                            "target_lang_translations": {
                                "ru": "Шляхтич отправил королю смиренное <w>прошение</w> с просьбой о помиловании сына."
                            }
                        },
                        {
                            "text": "Jego <w>orędzie</w> zostało w końcu wysłuchane przez władcę.",
                            "target_lang_translations": {
                                "ru": "Его <w>ходатайство</w> было наконец услышано правителем."
                            }
                        }
                    ],
                    "synonyms": [
                        "prośba"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie historyczne, obecnie nieużywane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее слово, означающее просьбу или заступничество в каком-либо деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошение",
                                "target_lang_sense_clarification": "письменная просьба, устар."

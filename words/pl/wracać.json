            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e15639bd-86cd-3795-91d8-c3ec822d12ae",
                    "sense_definition": "Przybywać z powrotem do miejsca, w którym się było wcześniej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na święta zawsze <w>wracam</w> do rodzinnego domu.",
                            "target_lang_translations": {
                                "ru": "На праздники я всегда <w>возвращаюсь</w> в родительский дом."
                            }
                        },
                        {
                            "text": "Kiedy <w>wracasz</w> z wakacji nad morzem?",
                            "target_lang_translations": {
                                "ru": "Когда ты <w>возвращаешься</w> из отпуска на море?"
                            }
                        }
                    ],
                    "synonyms": [
                        "powracać"
                    ],
                    "antonyms": [
                        "odchodzić",
                        "wyjeżdżać",
                        "wyruszać"
                    ],
                    "common_phrases": [
                        "wracać do domu",
                        "wracać z pracy",
                        "wracać ze szkoły",
                        "wracać z podróży"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить или приезжать обратно в то место, где был раньше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "домой, на прежнее место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7fb9919-25d7-3566-9609-752cec701d8b",
                    "sense_definition": "Cofać się, iść z powrotem w stronę, z której się przyszło (często po coś zapomnianego).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę się <w>wracać</w>, bo zapomniałem portfela.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>возвращаться</w>, потому что я забыл кошелек."
                            }
                        },
                        {
                            "text": "Nie <w>wracaj</w> się, bo to podobno przynosi pecha.",
                            "target_lang_translations": {
                                "ru": "Не <w>возвращайся</w>, говорят, это к неудаче."
                            }
                        }
                    ],
                    "synonyms": [
                        "cofać się",
                        "zawracać"
                    ],
                    "common_phrases": [
                        "wracać się do domu",
                        "wracać się po klucze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane z zaimkiem 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти назад, в сторону, откуда пришел (часто за чем-то забытым)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "с полпути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dab6d5b3-b180-36af-b8d7-440a79872489",
                    "sense_definition": "Zacząć ponownie mówić na dany temat lub myśleć o czymś z przeszłości.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę już <w>wracać</w> do tej nieprzyjemnej rozmowy.",
                            "target_lang_translations": {
                                "ru": "Я не хочу больше <w>возвращаться</w> к этому неприятному разговору."
                            }
                        },
                        {
                            "text": "Często <w>wracam</w> pamięcią do czasów mojego dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "Я часто <w>возвращаюсь</w> в памяти к временам моего детства."
                            }
                        }
                    ],
                    "synonyms": [
                        "powracać"
                    ],
                    "common_phrases": [
                        "wracać do tematu",
                        "wracać do sprawy",
                        "wracać pamięcią",
                        "wracać myślami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снова начинать говорить на какую-то тему или думать о прошлом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "к теме, к разговору"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfbc68f3-496e-33d9-8267-720b0d118444",
                    "sense_definition": "Zaczynać ponownie zajmować się jakąś czynnością, którą się przerwało.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "resume_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długiej przerwie z radością <w>wracam</w> do treningów.",
                            "target_lang_translations": {
                                "ru": "После долгого перерыва я с радостью <w>возвращаюсь</w> к тренировкам."
                            }
                        },
                        {
                            "text": "Muszę kończyć przerwę i <w>wracać</w> do swoich obowiązków.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заканчивать перерыв и <w>возвращаться</w> к своим обязанностям."
                            }
                        }
                    ],
                    "synonyms": [
                        "wznawiać"
                    ],
                    "antonyms": [
                        "przerywać",
                        "porzucać"
                    ],
                    "common_phrases": [
                        "wracać do pracy",
                        "wracać do gry",
                        "wracać do nałogu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снова приступать к делу или занятию, которое было прервано."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "к работе, к занятиям"
                            },
                            {
                                "target_lang_word": "возобновлять",
                                "target_lang_sense_clarification": "занятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68564bf5-eab1-3cce-876b-76973b48c97b",
                    "sense_definition": "Odzyskiwać równowagę psychiczną, zdrowie lub przytomność.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "regain_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daj mu chwilę spokoju, żeby <w>wrócił</w> do siebie po tym szoku.",
                            "target_lang_translations": {
                                "ru": "Дай ему минуту покоя, чтобы он <w>пришел</w> в себя после этого шока."
                            }
                        },
                        {
                            "text": "Pacjent powoli <w>wraca</w> do siebie po skomplikowanej operacji.",
                            "target_lang_translations": {
                                "ru": "Пациент медленно <w>приходит</w> в себя после сложной операции."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdrowieć"
                    ],
                    "common_phrases": [
                        "wracać do siebie",
                        "wracać do zdrowia",
                        "wracać do formy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстанавливать душевное равновесие, здоровье или сознание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходить в себя",
                                "target_lang_sense_clarification": "после шока или болезни"
                            },
                            {
                                "target_lang_word": "поправляться",
                                "target_lang_sense_clarification": "выздоравливать"
                            },
                            {
                                "target_lang_word": "опоминаться",
                                "target_lang_sense_clarification": "приходить в сознание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebc0f6e3-4da4-35a9-a074-ca60be81170a",
                    "sense_definition": "Pojawiać się ponownie (o zjawiskach, czasie, modzie).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reoccur",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moda na szerokie spodnie co kilka lat <w>wraca</w>.",
                            "target_lang_translations": {
                                "ru": "Мода на широкие брюки <w>возвращается</w> каждые несколько лет."
                            }
                        },
                        {
                            "text": "Zimą w dużych miastach niestety <w>wraca</w> problem smogu.",
                            "target_lang_translations": {
                                "ru": "Зимой в больших городах, к сожалению, <w>возвращается</w> проблема смога."
                            }
                        }
                    ],
                    "synonyms": [

                    "sense_id": "5f7a3302-cf13-3f4c-96c9-530bd7a6a31d",
                    "sense_definition": "Państwo w Europie Południowej, położone na Półwyspie Iberyjskim, ze stolicą w Madrycie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "country_sense",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Stolicą <w>Hiszpanii</w> jest Madryt.",
                            "target_lang_translations": {
                                "ru": "Столица <w>Испании</w> — Мадрид."
                            }
                        },
                        {
                            "text": "W przyszłym roku planujemy pojechać na wakacje do <w>Hiszpanii</w>.",
                            "target_lang_translations": {
                                "ru": "В следующем году мы планируем поехать в отпуск в <w>Испанию</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Królestwo Hiszpanii",
                        "wycieczka do Hiszpanii",
                        "mapa Hiszpanii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство в Южной Европе, расположенное на Пиренейском полуострове, со столицей в Мадриде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Испания",
                                "target_lang_sense_clarification": "государство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08c5f3f2-cac2-39bf-94c4-b84a0d820f54",
                    "sense_definition": "Historyczna kraina obejmująca całość ziem Półwyspu Iberyjskiego, od starożytności po czasy nowożytne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "region_sense",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Starożytna <w>Hiszpania</w> była ważną prowincją Cesarstwa Rzymskiego.",
                            "target_lang_translations": {
                                "ru": "Древняя <w>Испания</w> была важной провинцией Римской империи."
                            }
                        },
                        {
                            "text": "Kupcy feniccy zakładali swoje osady na wybrzeżach dawnej <w>Hiszpanii</w>.",
                            "target_lang_translations": {
                                "ru": "Финикийские купцы основывали свои поселения на побережье древней <w>Испании</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Iberia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческая область, охватывающая всю территорию Пиренейского полуострова, с древности до нового времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Испания",
                                "target_lang_sense_clarification": "историческая область"
                            }
                        ]
                    }
                }
            ]

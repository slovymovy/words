    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51e2ad0b-a567-374e-817a-ccefb3ff08cf",
                    "sense_definition": "A soft, white, fibrous substance that grows around the seeds of the cotton plant, used as a base for making thread and textile fabric.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse used a piece of <w>cotton</w> to clean the small wound.",
                            "target_lang_translations": {
                                "ru": "Медсестра использовала кусочек <w>ваты</w>, чтобы обработать небольшую рану."
                            }
                        },
                        {
                            "text": "This pillow is filled with soft <w>cotton</w>.",
                            "target_lang_translations": {
                                "ru": "Эта подушка набита мягким <w>хлопком</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cotton wool",
                        "cotton ball",
                        "raw cotton",
                        "cotton candy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкое белое волокнистое вещество, растущее вокруг семян хлопчатника, используемое как основа для изготовления нитей и ткани; в медицине и быту часто используется в виде ваты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ва́та",
                                "target_lang_sense_clarification": "processed material for medical/cosmetic use"
                            },
                            {
                                "target_lang_word": "хло́пок",
                                "target_lang_sense_clarification": "raw material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edd70978-38d8-340f-9d0f-7a969162c98e",
                    "sense_definition": "Cloth or fabric made from the soft white fibers of the cotton plant.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She prefers to wear <w>cotton</w> in the summer because it breathes well.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает носить <w>хлопок</w> летом, потому что он хорошо дышит."
                            }

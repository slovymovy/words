{
    "word_family": [
        "aansluiting",
        "aansluitbaar"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "633c1158-70f9-34f8-986c-504c6d9a5d05",
                    "sense_definition": "Verbinden met een apparaat, netwerk, systeem of voorziening (zoals elektriciteit of water).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "connection_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun jij de nieuwe printer even op de computer <w>aansluiten</w>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>подключить</w> новый принтер к компьютеру?"
                            }
                        },
                        {
                            "text": "We moeten wachten tot de monteur het internet komt <w>aansluiten</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно подождать, пока мастер придет <w>подключить</w> интернет."
                            }
                        },
                        {
                            "text": "Hij heeft de televisie gisteren op het netwerk <w>aangesloten</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера он <w>подключил</w> телевизор к сети."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbinden",
                        "koppelen",
                        "installeren"
                    ],
                    "antonyms": [
                        "afsluiten",
                        "loskoppelen",
                        "verwijderen"
                    ],
                    "common_phrases": [
                        "op het netwerk aansluiten",
                        "elektriciteit aansluiten",
                        "een apparaat aansluiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять с устройством, сетью, системой или коммуникациями (например, электричеством или водой)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подключать",
                                "target_lang_sense_clarification": "соединять с источником энергии или сетью"
                            },
                            {
                                "target_lang_word": "подсоединять",
                                "target_lang_sense_clarification": "физически соединять провода или части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1817ec5a-bd48-336b-bc0b-b6dffe91ebf8",
                    "sense_definition": "Lid worden van een groep, vereniging of organisatie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "connection_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij besloot zich bij de lokale voetbalclub <w>aan</w> te <w>sluiten</w>.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>присоединиться</w> к местному футбольному клубу."
                            }
                        },
                        {
                            "text": "Veel nieuwe leden hebben zich afgelopen maand bij ons <w>aangesloten</w>.",
                            "target_lang_translations": {
                                "ru": "В прошлом месяце к нам <w>присоединилось</w> много новых участников."
                            }
                        },
                        {
                            "text": "Als je wilt meedoen, kun je je bij onze groep <w>aansluiten</w>.",
                            "target_lang_translations": {
                                "ru": "Если хочешь участвовать, можешь <w>присоединиться</w> к нашей группе."
                            }
                        }
                    ],
                    "synonyms": [
                        "lid worden",
                        "toetreden",
                        "meedoen"
                    ],
                    "antonyms": [
                        "afhaken",
                        "uitschrijven",
                        "verlaten"
                    ],
                    "common_phrases": [
                        "zich aansluiten bij",
                        "zich bij een groep aansluiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться членом группы, объединения или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присоединяться",
                                "target_lang_sense_clarification": "становиться частью группы"
                            },
                            {
                                "target_lang_word": "вступать",
                                "target_lang_sense_clarification": "становиться официальным участником"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3b6c3c4-fc6d-3e56-b48c-2bd1f958504a",
                    "sense_definition": "Goed passen bij iets anders of er direct op volgen in tijd of ruimte.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "connection_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aankomsttijd van de trein <w>sluit</w> goed <w>aan</w> op de vertrektijd van de bus.",
                            "target_lang_translations": {
                                "ru": "Время прибытия поезда хорошо <w>стыкуется</w> со временем отправления автобуса."
                            }
                        },
                        {
                            "text": "Dit hoofdstuk <w>sluit</w> naadloos <w>aan</w> bij de kennis die je al hebt.",
                            "target_lang_translations": {
                                "ru": "Эта глава идеально <w>стыкуется</w> со знаниями, которые у тебя уже есть."
                            }
                        },
                        {
                            "text": "De kleuren van het tapijt <w>sluiten</w> mooi <w>aan</w> bij de gordijnen.",
                            "target_lang_translations": {
                                "ru": "Цвета ковра красиво <w>сочетаются</w> со шторами."
                            }
                        }
                    ],
                    "synonyms": [
                        "passen",
                        "opvolgen",
                        "corresponderen"
                    ],
                    "antonyms": [
                        "afwijken",
                        "vloeken"
                    ],
                    "common_phrases": [
                        "naadloos aansluiten",
                        "goed op elkaar aansluiten",
                        "aansluiten bij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо сочетаться с чем-то или следовать непосредственно за чем-то во времени или пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стыковаться",
                                "target_lang_sense_clarification": "о расписании или транспорте"
                            },
                            {
                                "target_lang_word": "сочетаться",
                                "target_lang_sense_clarification": "подходить по стилю или смыслу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e89cbb4d-5bbd-37c1-8c1e-a43bab5e92be",
                    "sense_definition": "Zeggen dat je het eens bent met de mening of woorden van iemand anders.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "connection_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>sluit</w> me volledig <w>aan</w> bij de woorden van de vorige spreker.",
                            "target_lang_translations": {
                                "ru": "Я полностью <w>присоединяюсь</w> к словам предыдущего оратора."
                            }
                        },
                        {
                            "text": "Mag ik me bij jouw standpunt <w>aansluiten</w>?",
                            "target_lang_translations": {
                                "ru": "Могу я <w>присоединиться</w> к твоей точке зрения?"
                            }
                        }
                    ],
                    "synonyms": [
                        "instemmen",
                        "bijvallen",
                        "onderschrijven"
                    ],
                    "antonyms": [
                        "tegenspreken",
                        "verwerpen",
                        "bestrijden"
                    ],
                    "common_phrases": [
                        "ik sluit me daarbij aan",
                        "zich volledig aansluiten bij"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in formal discussions or meetings"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать согласие с чьим-либо мнением или словами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присоединяться",
                                "target_lang_sense_clarification": "к мнению или словам"
                            },
                            {
                                "target_lang_word": "соглашаться",
                                "target_lang_sense_clarification": "выражать одобрение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

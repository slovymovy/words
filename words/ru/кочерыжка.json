    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fb7e6aa3-08b9-38cf-82c1-f0d78d10392a",
                    "sense_definition": "Твёрдый, мясистый стебель внутри кочана капусты.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vegetable_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В детстве мы часто грызли сладкую капустную <w>кочерыжку</w>.",
                            "target_lang_translations": {
                                "en": "In childhood, we often gnawed on the sweet cabbage <w>stalk</w>."
                            }
                        },
                        {
                            "text": "Мама вырезала <w>кочерыжку</w> из капусты перед тем, как варить борщ.",
                            "target_lang_translations": {
                                "en": "Mom cut the <w>core</w> out of the cabbage before cooking borscht."
                            }
                        }
                    ],
                    "synonyms": [
                        "кочерыга"
                    ],
                    "common_phrases": [
                        "капустная кочерыжка",
                        "грызть кочерыжку",
                        "сладкая кочерыжка"
                    ],
                    "target_lang_definitions": {
                        "en": "The hard, fleshy stem inside a head of cabbage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cabbage stalk",
                                "target_lang_sense_clarification": "hard stem inside the head"
                            },
                            {
                                "target_lang_word": "cabbage stump",
                                "target_lang_sense_clarification": "hard stem"
                            },
                            {
                                "target_lang_word": "core",
                                "target_lang_sense_clarification": "central part of the vegetable"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e7f80bb-07d8-3c75-b96e-c0b84c90ba12",
                    "sense_definition": "Твёрдая внутренняя часть кукурузного початка, на которой растут зёрна.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vegetable_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На поле после уборки урожая остались лежать сухие кукурузные <w>кочерыжки</w>.",
                            "target_lang_translations": {
                                "en": "After the harvest, dry corn <w>cobs</w> were left lying in the field."
                            }
                        },
                        {
                            "text": "От варёной кукурузы осталась только пустая <w>кочерыжка</w>.",
                            "target_lang_translations": {
                                "en": "Only the empty <w>cob</w> remained from the boiled corn."
                            }
                        }
                    ],
                    "common_phrases": [
                        "кукурузная кочерыжка"
                    ],
                    "target_lang_definitions": {
                        "en": "The hard inner part of a corn cob on which the kernels grow."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cob",
                                "target_lang_sense_clarification": "of corn"
                            },
                            {
                                "target_lang_word": "corncob",
                                "target_lang_sense_clarification": "stripped of kernels"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "971f236a-d4dc-3587-8730-a7290d8aad4d",
                    "sense_definition": "Старый, сгорбленный или дряхлый человек.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сосед в шутку называл себя старой <w>кочерыжкой</w>, хотя был ещё полон сил.",
                            "target_lang_translations": {
                                "en": "The neighbor jokingly called himself an old <w>wreck</w>, although he was still full of strength."
                            }
                        },
                        {
                            "text": "Куда ты собралась в такую погоду, старая <w>кочерыжка</w>?",
                            "target_lang_translations": {
                                "en": "Where are you going in this weather, you <w>old fogey</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "старикашка",
                        "развалина"
                    ],
                    "common_phrases": [
                        "старая кочерыжка"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An old, hunched, or decrepit person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "old wreck",
                                "target_lang_sense_clarification": "colloquial, decrepit person"
                            },
                            {
                                "target_lang_word": "old fogey",
                                "target_lang_sense_clarification": "colloquial"
                            }
                        ]
                    }
                }
            ]
        }
    ]

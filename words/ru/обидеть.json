                    ],
                    "synonyms": [
                        "недодать",
                        "обделить"
                    ],
                    "antonyms": [
                        "переплатить"
                    ],
                    "common_phrases": [
                        "обидеть деньгами",
                        "обидеть зарплатой",
                        "никого не обидеть"

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a53839f9-fcd7-3105-bdbd-9d3a5499d8f5",
                    "sense_definition": "Взять что-либо в руки и унести; переместить предмет или человека с собой из одного места в другое.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_take_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <заберу> эту книгу домой, чтобы почитать на выходных."
                        },
                        {
                            "text": "Не забудь <забрать> свои вещи из раздевалки перед уходом."
                        }
                    ],
                    "synonyms": [
                        "взять"
                    ],
                    "antonyms": [
                        "отдать",
                        "оставить"
                    ],
                    "common_phrases": [
                        "забрать домой",
                        "забрать с собой",
                        "забрать вещи"
                    ]
                },
                {
                    "sense_id": "fd0e1bc9-067f-38e2-a251-bbd57650a9a5",
                    "sense_definition": "Прийти или приехать за кем-либо или чем-либо, чтобы увезти (например, ребенка из школы или заказ из магазина).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_take_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Папа обещал <забрать> меня из школы ровно в три часа."
                        },
                        {
                            "text": "Вы можете <забрать> свой заказ в пункте выдачи завтра утром."
                        }
                    ],
                    "synonyms": [
                        "взять",
                        "получить"
                    ],
                    "antonyms": [
                        "оставить"
                    ],
                    "common_phrases": [
                        "забрать из школы",
                        "забрать заказ",
                        "забрать детей"
                    ]
                },
                {
                    "sense_id": "2ed194f1-6240-3b43-a492-4c939c92c23b",
                    "sense_definition": "Силой или без спроса присвоить чужое; захватить контроль над чем-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_seize_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За долги банк <забрал> у них квартиру и машину."
                        },
                        {
                            "text": "Неприятель быстро <забрал> все пограничные города и двинулся дальше."
                        }
                    ],
                    "synonyms": [
                        "присвоить",
                        "захватить",
                        "овладеть",
                        "завоевать"
                    ],
                    "antonyms": [
                        "вернуть",
                        "отдать"
                    ],
                    "common_phrases": [
                        "забрать власть",
                        "забрать силой",
                        "забрать за долги"
                    ],
                    "traits": [

{
    "word_family": [
        "offering",
        "counteroffer",
        "offerer",
        "reoffer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc99b2a0-d82a-3985-b6fa-e81584d7cc88",
                    "sense_definition": "To give or provide something that someone can accept or refuse.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "give_propose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>offered</w> me a cup of tea when I arrived.",
                            "target_lang_translations": {
                                "ru": "Он <w>предложил</w> мне чашку чая, когда я приехал."
                            }
                        },
                        {
                            "text": "They <w>offered</w> him a job at the new factory.",
                            "target_lang_translations": {
                                "ru": "Ему <w>предложили</w> работу на новом заводе."
                            }
                        }
                    ],
                    "synonyms": [
                        "extend",
                        "offer up"
                    ],
                    "antonyms": [
                        "refuse",
                        "withhold"
                    ],
                    "common_phrases": [
                        "offer a drink",
                        "offer help",
                        "offer advice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать или предоставлять что-то, что кто-то может принять или отвергнуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "давать что-то (несов. вид)"
                            },
                            {
                                "target_lang_word": "предложить",
                                "target_lang_sense_clarification": "дать что-то (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7045863-b32a-3964-9e39-1065143b2588",
                    "sense_definition": "To say that you are willing to do something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "give_propose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>offered</w> to drive us to the train station.",
                            "target_lang_translations": {
                                "ru": "Она <w>предложила</w> подвезти нас до вокзала."
                            }
                        },
                        {
                            "text": "I <w>offered</w> to help with the cleaning, but they said no.",
                            "target_lang_translations": {
                                "ru": "Я <w>предложил</w> помочь с уборкой, но они отказались."
                            }
                        }
                    ],
                    "synonyms": [
                        "extend"
                    ],
                    "antonyms": [
                        "refuse"
                    ],
                    "common_phrases": [
                        "offer to help",
                        "offer to pay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать готовность сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "помощь или услугу"
                            },
                            {
                                "target_lang_word": "вызываться",
                                "target_lang_sense_clarification": "сделать что-то добровольно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16660476-b698-3fa6-acc2-00b81859a350",
                    "sense_definition": "To provide or have something available for use or enjoyment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "provide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hotel <w>offers</w> a swimming pool and a gym for guests.",
                            "target_lang_translations": {
                                "ru": "Отель <w>предлагает</w> гостям бассейн и спортзал."
                            }
                        },
                        {
                            "text": "This course <w>offers</w> a great opportunity to learn new skills.",
                            "target_lang_translations": {
                                "ru": "Этот курс <w>предоставляет</w> отличную возможность освоить новые навыки."
                            }
                        }
                    ],
                    "synonyms": [
                        "showcase",
                        "extend"
                    ],
                    "common_phrases": [
                        "offer an opportunity",
                        "offer facilities",
                        "offer a range"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять или иметь в наличии для использования или удовольствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предоставлять",
                                "target_lang_sense_clarification": "давать возможность пользоваться"
                            },
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "иметь в ассортименте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62280e53-7d50-3bf8-94ad-86f78a1ea92c",
                    "sense_definition": "To say that you are willing to pay a certain amount of money for something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>offered</w> a lower price for the house than they asked.",
                            "target_lang_translations": {
                                "ru": "Мы <w>предложили</w> за дом меньшую цену, чем они просили."
                            }
                        },
                        {
                            "text": "The company <w>offered</w> five thousand dollars to settle the case.",
                            "target_lang_translations": {
                                "ru": "Компания <w>предложила</w> пять тысяч долларов, чтобы уладить дело."
                            }
                        }
                    ],
                    "common_phrases": [
                        "offer a price",
                        "offer money"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать готовность заплатить определенную сумму денег за что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "цену"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f10fd1e-c259-3772-bb9e-b5698aed8c56",
                    "sense_definition": "To show or provide a particular reaction, especially resistance or violence.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thief <w>offered</w> no resistance when the police arrived.",
                            "target_lang_translations": {
                                "ru": "Вор не <w>оказал</w> сопротивления, когда прибыла полиция."
                            }
                        },
                        {
                            "text": "They <w>offered</w> violence to the guards during the escape attempt.",
                            "target_lang_translations": {
                                "ru": "Они <w>применили</w> насилие к охранникам во время попытки побега."
                            }
                        }
                    ],
                    "common_phrases": [
                        "offer resistance",
                        "offer no resistance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять определенную реакцию, особенно сопротивление или насилие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказывать",
                                "target_lang_sense_clarification": "сопротивление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c94ffc8e-fa5e-3291-8856-137976a99cdb",
                    "sense_definition": "To present something to a god as an act of worship.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>offered</w> sacrifices to the gods in the ancient temple.",
                            "target_lang_translations": {
                                "ru": "Они <w>приносили</w> жертвы богам в древнем храме."
                            }
                        },
                        {
                            "text": "The priest <w>offered</w> a prayer for peace during the ceremony.",
                            "target_lang_translations": {
                                "ru": "Священник <w>вознес</w> молитву о мире во время церемонии."
                            }
                        }
                    ],
                    "synonyms": [
                        "offer up"
                    ],
                    "common_phrases": [
                        "offer a sacrifice",
                        "offer prayers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преподносить что-либо божеству в качестве акта поклонения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "в жертву"
                            },
                            {
                                "target_lang_word": "возносить",
                                "target_lang_sense_clarification": "молитву"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a007874-885d-3ebd-8d25-d348e166008c",
                    "sense_definition": "To happen or appear; to present itself.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "happen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "An opportunity <w>offered</w> itself unexpectedly while I was traveling.",
                            "target_lang_translations": {
                                "ru": "Неожиданно <w>представилась</w> возможность, пока я путешествовал."
                            }
                        },
                        {
                            "text": "No better solution <w>offered</w> at the time, so we waited.",
                            "target_lang_translations": {
                                "ru": "Лучшего решения в то время не <w>представилось</w>, поэтому мы ждали."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Случаться или появляться; возникать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представляться",
                                "target_lang_sense_clarification": "о возможности или случае"
                            },
                            {
                                "target_lang_word": "подворачиваться",
                                "target_lang_sense_clarification": "разг. о случае"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b70a6770-9bb7-379d-9c4a-a40878437998",
                    "sense_definition": "A statement saying that one is willing to do something or give something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "proposal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thank you for your kind <w>offer</w> of help.",
                            "target_lang_translations": {
                                "ru": "Спасибо за ваше любезное <w>предложение</w> помощи."
                            }
                        },
                        {
                            "text": "I accepted the job <w>offer</w> immediately after the interview.",
                            "target_lang_translations": {
                                "ru": "Я принял <w>предложение</w> о работе сразу после собеседования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "accept an offer",
                        "decline an offer",
                        "job offer",
                        "kind offer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заявление о готовности сделать что-либо или дать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "готовность помочь или дать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f785390b-067d-3f01-9e01-31a10c84f20d",
                    "sense_definition": "An amount of money that someone says they will pay for something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bid_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They made an <w>offer</w> of two hundred thousand dollars for the apartment.",
                            "target_lang_translations": {
                                "ru": "Они сделали <w>предложение</w> в двести тысяч долларов за квартиру."
                            }
                        },
                        {
                            "text": "The highest <w>offer</w> will secure the purchase of the car.",
                            "target_lang_translations": {
                                "ru": "Самое высокое <w>предложение</w> обеспечит покупку автомобиля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "make an offer",
                        "cash offer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма денег, которую кто-то готов заплатить за что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "цены, финансовое"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "48497d57-dbce-342f-badb-9f9a640f4a6a",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Offer</w> signed the document.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Оффер</w> подписал документ."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Offer</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Оффер</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Оффер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5bdb618-10bf-35fe-a806-fee05509bb61",
                    "sense_definition": "De naam die een persoon bij de geboorte krijgt en die aan de achternaam voorafgaat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>voornaam</w> is Thomas en mijn achternaam is De Vries.",
                            "target_lang_translations": {
                                "en": "My <w>first name</w> is Thomas and my surname is De Vries.",
                                "ru": "Мое <w>имя</w> — Томас, а моя фамилия — Де Врис."
                            }
                        },
                        {
                            "text": "In Nederland spreken collega's elkaar vaak aan met de <w>voornaam</w>.",
                            "target_lang_translations": {
                                "en": "In the Netherlands, colleagues often address each other by <w>first name</w>.",
                                "ru": "В Нидерландах коллеги часто обращаются друг к другу по <w>имени</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "roepnaam"
                    ],
                    "antonyms": [
                        "achternaam"
                    ],
                    "common_phrases": [
                        "bij de voornaam noemen",
                        "iemands voornaam vragen"
                    ],
                    "target_lang_definitions": {
                        "en": "The name a person receives at birth, which precedes the surname.",
                        "ru": "Имя, данное человеку при рождении, которое стоит перед фамилией."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "first name",
                                "target_lang_sense_clarification": "most common term"
                            },
                            {
                                "target_lang_word": "given name",
                                "target_lang_sense_clarification": "formal or legal context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "имя",
                                "target_lang_sense_clarification": "личное имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9f8ccd1f-96be-3d8b-865c-c5b57dc5251f",
                    "sense_definition": "Van groot belang of de hoogste prioriteit hebbend.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>voornaamste</w> reden voor zijn vertrek was het slechte weer.",
                            "target_lang_translations": {
                                "en": "The <w>main</w> reason for his departure was the bad weather.",
                                "ru": "<w>Главной</w> причиной его отъезда была плохая погода."
                            }
                        },
                        {
                            "text": "Gezondheid is voor veel mensen het <w>voornaamste</w> bezit.",
                            "target_lang_translations": {
                                "en": "Health is the <w>most important</w> possession for many people.",
                                "ru": "Для многих людей здоровье — это <w>важнейшее</w> достояние."
                            }
                        }
                    ],
                    "synonyms": [
                        "belangrijk"
                    ],
                    "antonyms": [
                        "onbelangrijk"
                    ],
                    "common_phrases": [
                        "de voornaamste reden",
                        "het voornaamste doel"
                    ],
                    "target_lang_definitions": {
                        "en": "Of great importance or having the highest priority.",
                        "ru": "Имеющий большое значение или высший приоритет."
                    },
                    "translations": {

                },
                {
                    "sense_id": "92cd1a06-5312-3908-8d98-cd19210e2d27",
                    "sense_definition": "Een sterke connectie of gevoelsmatige relatie tussen personen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb geen goede <w>band</w> met mijn buren.",
                            "target_lang_translations": {
                                "ru": "У меня нет хорошей <w>связи</w> с соседями."
                            }
                        },
                        {
                            "text": "De zussen hebben een heel sterke <w>band</w> met elkaar.",
                            "target_lang_translations": {
                                "ru": "У сестер очень сильная <w>связь</w> друг с другом."
                            }

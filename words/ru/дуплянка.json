    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "65ea68fd-3b7b-3942-8e09-5a0c0bcc111b",
                    "sense_definition": "Улей для пчёл, изготовленный из выдолбленного куска ствола дерева.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На старой пасеке ещё сохранилась одна дедовская <дуплянка>.",
                            "target_lang_translations": {
                                "en": "One of grandfather's <log hives> still remained in the old apiary."
                            }
                        },
                        {
                            "text": "Пчёлы в такой <дуплянке> живут в условиях, близких к естественным.",
                            "target_lang_translations": {
                                "en": "Bees in such a <log hive> live in conditions close to natural ones."
                            }
                        }
                    ],
                    "synonyms": [
                        "бездонка"
                    ],
                    "common_phrases": [
                        "старая дуплянка",
                        "пчелиная дуплянка"
                    ],
                    "target_lang_definitions": {
                        "en": "A beehive made from a hollowed-out tree trunk."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "log hive",
                                "target_lang_sense_clarification": "beekeeping"
                            },
                            {
                                "target_lang_word": "gum hive",
                                "target_lang_sense_clarification": "historical/regional"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "341806a6-240a-3f46-a653-5f3e210b7266",
                    "sense_definition": "Искусственное гнездовье для птиц, сделанное из выдолбленного чурбака, похожее на естественное дупло.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лесники развесили на деревьях новые <дуплянки> для привлечения синиц.",
                            "target_lang_translations": {
                                "en": "Foresters hung new <log birdhouses> on the trees to attract titmice."
                            }
                        },
                        {
                            "text": "Скворцы охотно заселились в уютную <дуплянку> в нашем саду.",
                            "target_lang_translations": {
                                "en": "Starlings willingly settled into the cozy <nesting log> in our garden."
                            }
                        }
                    ],
                    "common_phrases": [
                        "развешивать дуплянки",
                        "дуплянка для птиц"
                    ],
                    "target_lang_definitions": {
                        "en": "An artificial nesting box for birds made from a hollowed log."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "log birdhouse",
                                "target_lang_sense_clarification": "ornithology"
                            },
                            {
                                "target_lang_word": "nesting log",
                                "target_lang_sense_clarification": "artificial nest"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67c0c095-072c-3508-b51e-5310ee28f4e8",
                    "sense_definition": "Деревянная кадка или сосуд, выдолбленный из цельного куска дерева.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка хранила мёд в липовой <дуплянке>.",
                            "target_lang_translations": {
                                "en": "Grandmother kept honey in a linden <hollowed-out tub>."
                            }
                        },
                        {
                            "text": "В углу избы стояла тяжёлая <дуплянка> с водой.",
                            "target_lang_translations": {
                                "en": "In the corner of the hut stood a heavy <log tub> with water."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A wooden tub or vessel hollowed out of a single piece of wood."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hollowed-out tub",
                                "target_lang_sense_clarification": "container"
                            },
                            {
                                "target_lang_word": "dugout vessel",
                                "target_lang_sense_clarification": "container"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1be5b775-2c22-35d9-9e46-9847716ae5be",
                    "sense_definition": "Народное название некоторых видов съедобных грибов.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После дождя мы набрали целую корзину <дуплянок>.",
                            "target_lang_translations": {
                                "en": "After the rain, we gathered a whole basket of <hollow-stemmed mushrooms>."
                            }
                        },
                        {
                            "text": "Местные жители называют эти грибы <дуплянками>.",
                            "target_lang_translations": {
                                "en": "Locals call these mushrooms <hollow-stemmed mushrooms>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A folk name for certain species of edible mushrooms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hollow-stemmed mushroom",
                                "target_lang_sense_clarification": "mycology"
                            }
                        ]
                    }
                }
            ]
        }

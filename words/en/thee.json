{
    "word_family": [
        "thou",
        "thy",
        "thine",
        "thyself"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "4956a933-04c2-33b7-bb53-c5140d913db4",
                    "sense_definition": "The archaic or literary word for \"you\" when used as the object of a sentence, referring to one person.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pronoun_obj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With this ring I <w>thee</w> wed.",
                            "target_lang_translations": {
                                "ru": "Этим кольцом я беру <w>тебя</w> в супруги."
                            }
                        },
                        {
                            "text": "I shall give <w>thee</w> everything I have.",
                            "target_lang_translations": {
                                "ru": "Я отдам <w>тебе</w> всё, что у меня есть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "I thee wed",
                        "to thee",
                        "we beseech thee"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "standard in older texts, religious contexts, or poetry"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "still used in some regional UK dialects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшая или поэтическая форма местоимения \"ты\" в косвенных падежах (винительном, дательном, творительном), используемая при обращении к одному лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тебя",
                                "target_lang_sense_clarification": "винительный или родительный падеж"
                            },
                            {
                                "target_lang_word": "тебе",
                                "target_lang_sense_clarification": "дательный падеж"
                            },
                            {
                                "target_lang_word": "тобой",
                                "target_lang_sense_clarification": "творительный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9cbdedc-af4e-38da-a74c-b75ef1b3fdca",
                    "sense_definition": "The word \"you\" used as the subject of a sentence; a dialectal variation of \"thou\".",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pronoun_subj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Does <w>thee</w> want to come with us?",
                            "target_lang_translations": {
                                "ru": "<w>Ты</w> хочешь пойти с нами?"
                            }
                        },
                        {
                            "text": "<w>Thee</w> is a good friend to me.",
                            "target_lang_translations": {
                                "ru": "<w>Ты</w> для меня хороший друг."
                            }
                        }
                    ],
                    "synonyms": [
                        "thou"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "common in Yorkshire or Quaker speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Диалектное употребление формы \"thee\" в качестве подлежащего (вместо нормативного \"thou\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "именительный падеж"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "27c7682e-1b7f-3351-936e-673ed8ce26b3",
                    "sense_definition": "To address a person using the familiar pronoun \"thee\" instead of the formal \"you\".",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two old friends sat by the fire, <w>theeing</w> and thouing each other comfortably.",
                            "target_lang_translations": {
                                "ru": "Два старых друга сидели у огня, уютно <w>тыкая</w> друг другу."
                            }
                        },
                        {
                            "text": "He insulted the magistrate by <w>theeing</w> him in court.",
                            "target_lang_translations": {
                                "ru": "Он оскорбил судью, начав <w>тыкать</w> ему в суде."
                            }
                        }
                    ],
                    "synonyms": [
                        "thou"
                    ],
                    "common_phrases": [
                        "theeing and thouing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к кому-либо на \"ты\", используя фамильярную форму вместо формальной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыкать",
                                "target_lang_sense_clarification": "обращаться на «ты»"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
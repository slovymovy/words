    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bd189fef-c7d1-3260-8c69-78cd3ce40b4f",
                    "sense_definition": "Kobieta lub dziewczyna, która mieszka w pobliżu, w sąsiednim domu lub mieszkaniu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "neighbor_living",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja nowa <w>sąsiadka</w> zaprosiła mnie wczoraj na kawę.",
                            "target_lang_translations": {
                                "ru": "Моя новая <w>соседка</w> пригласила меня вчера на кофе."
                            }
                        },
                        {
                            "text": "Ewa pożyczyła od <w>sąsiadki</w> szklankę mąki.",
                            "target_lang_translations": {
                                "ru": "Ева одолжила у <w>соседки</w> стакан муки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "miła sąsiadka",
                        "sąsiadka z naprzeciwka",
                        "sąsiadka z dołu",
                        "sąsiadka z piętra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина или девушка, живущая поблизости, в соседнем доме или квартире."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соседка",
                                "target_lang_sense_clarification": "живущая рядом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e92ed86c-7f01-32a1-b1d6-b58043007469",
                    "sense_definition": "Kobieta lub dziewczyna zajmująca miejsce obok kogoś, na przykład w ławce szkolnej, przy stole lub w środku transportu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "neighbor_situational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas egzaminu moja <w>sąsiadka</w> z ławki próbowała ściągać.",
                            "target_lang_translations": {
                                "ru": "Во время экзамена моя <w>соседка</w> по парте пыталась списать."
                            }
                        },
                        {
                            "text": "W pociągu trafiła mi się bardzo gadatliwa <w>sąsiadka</w>.",
                            "target_lang_translations": {
                                "ru": "В поезде мне попалась очень болтливая <w>соседка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sąsiadka z ławki",
                        "sąsiadka ze stolika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина или девушка, занимающая место рядом с кем-то, например, за партой, столом или в транспорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соседка",
                                "target_lang_sense_clarification": "сидящая рядом (по парте, столу)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
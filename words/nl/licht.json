    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5210a888-6018-3210-9ea4-8619fc4bef15",
                    "sense_definition": "Met weinig gewicht; niet zwaar.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "gewicht",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze doos is erg <w>licht</w>, dus ik kan hem makkelijk dragen.",
                            "target_lang_translations": {
                                "ru": "Эта коробка очень <w>лёгкая</w>, поэтому я могу легко её нести."
                            }
                        },
                        {
                            "text": "Vogels hebben <w>lichte</w> botten om te kunnen vliegen.",
                            "target_lang_translations": {
                                "ru": "У птиц <w>лёгкие</w> кости, чтобы они могли летать."
                            }
                        }
                    ],
                    "antonyms": [
                        "zwaar"
                    ],
                    "common_phrases": [
                        "zo licht als een veer",
                        "licht gewicht"
                    ],
                    "target_lang_definitions": {
                        "ru": "С малым весом; не тяжелый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "о весе"
                            },
                            {
                                "target_lang_word": "невесомый",
                                "target_lang_sense_clarification": "очень лёгкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72a88b3c-9c95-37fc-b374-93a0ba602382",
                    "sense_definition": "Helder van kleur of met veel licht; niet donker.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "helderheid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer is het tot laat in de avond <w>licht</w>.",
                            "target_lang_translations": {
                                "ru": "Летом <w>светло</w> до позднего вечера."
                            }
                        },
                        {
                            "text": "Zij heeft <w>lichte</w> ogen en blond haar.",
                            "target_lang_translations": {
                                "ru": "У неё <w>светлые</w> глаза и светлые волосы."
                            }
                        }
                    ],
                    "synonyms": [
                        "helder"
                    ],
                    "antonyms": [
                        "donker",
                        "duister"
                    ],
                    "common_phrases": [
                        "lichtblauw",
                        "licht worden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Яркий по цвету или хорошо освещенный; не темный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светлый",
                                "target_lang_sense_clarification": "о цвете или освещении"
                            },
                            {
                                "target_lang_word": "бледный",
                                "target_lang_sense_clarification": "слабо окрашенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1732a7c-e150-3a6e-b205-012486d52ea5",
                    "sense_definition": "Gemakkelijk te doen of te verdragen; weinig inspanning vragend.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "intensiteit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter adviseerde om alleen <w>lichte</w> oefeningen te doen.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал делать только <w>лёгкие</w> упражнения."
                            }
                        },
                        {
                            "text": "Het was een <w>lichte</w> taak die we snel af hadden.",
                            "target_lang_translations": {
                                "ru": "Это была <w>лёгкая</w> задача, с которой мы быстро справились."
                            }
                        }
                    ],
                    "antonyms": [
                        "zwaar",
                        "gewichtig"
                    ],
                    "common_phrases": [
                        "lichte straf",
                        "licht werk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий для выполнения или переносимости; требующий мало усилий."

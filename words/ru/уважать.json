    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2c7d64f2-ef9b-3dd4-93ae-ee39582004d4",
                    "sense_definition": "Испытывать чувство почтения к кому-либо, признавать чьи-либо достоинства, заслуги или высокий статус.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "esteem",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я очень <w>уважаю</w> своего учителя за его терпение и мудрость.",
                            "target_lang_translations": {
                                "en": "I deeply <w>respect</w> my teacher for his patience and wisdom."
                            }
                        },
                        {
                            "text": "Чтобы тебя ценили другие, нужно прежде всего <w>уважать</w> себя.",
                            "target_lang_translations": {
                                "en": "To be valued by others, you must first <w>respect</w> yourself."
                            }
                        }
                    ],
                    "synonyms": [
                        "почитать",
                        "благоговеть",
                        "боготворить"
                    ],
                    "antonyms": [
                        "презирать",
                        "оскорблять",
                        "ненавидеть"
                    ],
                    "common_phrases": [
                        "уважать родителей",
                        "глубоко уважать",
                        "уважать мнение",
                        "уважать за честность"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel a sense of reverence toward someone; to recognize someone's merits, achievements, or high status."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "respect",
                                "target_lang_sense_clarification": "general feeling of deep admiration for someone or something"
                            },
                            {
                                "target_lang_word": "esteem",
                                "target_lang_sense_clarification": "formal; to hold in high regard or value greatly"
                            },
                            {
                                "target_lang_word": "value",
                                "target_lang_sense_clarification": "to consider someone important or beneficial"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa0db5c7-6829-397d-9b48-bc2f47635f7f",
                    "sense_definition": "Соблюдать, не нарушать законы, правила, обычаи или права других людей.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "observe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый гражданин обязан <w>уважать</w> законы своей страны.",
                            "target_lang_translations": {
                                "en": "Every citizen is obliged to <w>respect</w> the laws of their country."
                            }
                        },
                        {
                            "text": "Мы просим гостей <w>уважать</w> традиции нашего народа.",
                            "target_lang_translations": {
                                "en": "We ask guests to <w>respect</w> our people's traditions."
                            }
                        }
                    ],
                    "synonyms": [
                        "соблюдать"
                    ],
                    "antonyms": [
                        "нарушать",
                        "игнорировать"
                    ],
                    "common_phrases": [
                        "уважать закон",
                        "уважать права",
                        "уважать традиции",
                        "уважать выбор"
                    ],
                    "target_lang_definitions": {
                        "en": "To observe and not violate laws, rules, customs, or the rights of others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "respect",
                                "target_lang_sense_clarification": "to show consideration for laws, rights, or customs"
                            },
                            {
                                "target_lang_word": "observe",
                                "target_lang_sense_clarification": "to adhere to or abide by a law or rule"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0b1f514-39d9-3eb4-b4d1-69558dcc914e",
                    "sense_definition": "Любить, испытывать пристрастие к чему-либо (часто к еде или напиткам).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "like_colloquial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка очень <w>уважает</w> крепкий чай с лимоном после обеда.",
                            "target_lang_translations": {
                                "en": "My grandfather is very <w>partial to</w> strong tea with lemon after lunch."
                            }
                        },
                        {
                            "text": "Я не особо <w>уважаю</w> сладкое, мне больше нравится мясо.",
                            "target_lang_translations": {
                                "en": "I don't particularly <w>care for</w> sweets; I prefer meat."
                            }
                        }
                    ],
                    "synonyms": [
                        "почитать",
                        "соблюдать"
                    ],
                    "common_phrases": [
                        "уважать баню",
                        "уважать чай"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "используется в непринужденной речи, часто с оттенком юмора"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To like or have a fondness for something (often used colloquially regarding food or drink)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be partial to",
                                "target_lang_sense_clarification": "to have a particular liking or fondness for something"
                            },
                            {
                                "target_lang_word": "like",
                                "target_lang_sense_clarification": "to find agreeable or enjoyable"
                            },
                            {
                                "target_lang_word": "enjoy",
                                "target_lang_sense_clarification": "to take pleasure in consuming something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
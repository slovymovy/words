                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оби́да",
                                "target_lang_sense_clarification": "чувство огорчения"
                            },
                            {
                                "target_lang_word": "боль",
                                "target_lang_sense_clarification": "душевное страдание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b70cc9ce-a43a-3380-9f0c-2dc581700417",
                    "sense_definition": "Damage, detriment, or harm done to something abstract.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_harm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scandal did little <w>hurt</w> to his reputation in the long run.",

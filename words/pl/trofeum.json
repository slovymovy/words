                    "sense_id": "7edc7ced-4f3f-39b9-9f46-89394b579749",
                    "sense_definition": "Nagroda przyznawana za zwycięstwo w zawodach sportowych, konkursach lub innej rywalizacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zwycięska drużyna z dumą wzniosła zdobyte <w>trofeum</w> przy aplauzie kibiców.",
                            "target_lang_translations": {
                                "ru": "Победившая команда с гордостью подняла завоеванный <w>трофей</w> под аплодисменты болельщиков."
                            }
                        },
                        {
                            "text": "To prestiżowe <w>trofeum</w> jest marzeniem każdego zawodowego tenisisty.",
                            "target_lang_translations": {
                                "ru": "Этот престижный <w>трофей</w> — мечта каждого профессионального теннисиста."
                            }
                        }
                    ],
                    "synonyms": [
                        "nagroda",
                        "puchar",
                        "laur"
                    ],
                    "common_phrases": [

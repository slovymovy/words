                    "sense_id": "f7f053b6-3c36-3890-9a60-925a071e883a",
                    "sense_definition": "Drukt verontwaardiging of boosheid uit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "interjection_anger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Vervloekt</w>, ik ben mijn portemonnee vergeten!",
                            "target_lang_translations": {

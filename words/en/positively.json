{
    "word_family": [
        "positive",
        "positivity",
        "positiveness"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "861bcff0-06f3-3dcf-b5fe-32e0c373b696",
                    "sense_definition": "In a way that shows agreement, optimism, or confidence.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He approached the difficult interview <w>positively</w> and with a smile.",
                            "target_lang_translations": {
                                "ru": "Он подошел к трудному собеседованию <w>позитивно</w> и с улыбкой."
                            }
                        },
                        {
                            "text": "The audience responded <w>positively</w> to the band's new song.",
                            "target_lang_translations": {
                                "ru": "Публика отреагировала на новую песню группы <w>положительно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "optimistically",
                        "favorably",
                        "constructively",
                        "encouragingly"
                    ],
                    "antonyms": [
                        "negatively",
                        "pessimistically"
                    ],
                    "common_phrases": [
                        "think positively",
                        "react positively",
                        "respond positively",
                        "act positively"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, выражающим согласие, оптимизм или уверенность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительно",
                                "target_lang_sense_clarification": "одобрительно, благоприятно"
                            },
                            {
                                "target_lang_word": "позитивно",
                                "target_lang_sense_clarification": "с оптимизмом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d23723db-941f-3b4b-a641-bbd66539435c",
                    "sense_definition": "With complete certainty; without any doubt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I can state <w>positively</w> that he was not at home last night.",
                            "target_lang_translations": {
                                "ru": "Я могу <w>с уверенностью</w> заявить, что прошлой ночью его не было дома."
                            }
                        },
                        {
                            "text": "We need to identify <w>positively</w> the cause of the engine failure.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>точно</w> установить причину отказа двигателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "definitely",
                        "certainly",
                        "unquestionably",
                        "undeniably",
                        "surely"
                    ],
                    "antonyms": [
                        "doubtfully",
                        "uncertainly"
                    ],
                    "common_phrases": [
                        "know positively",
                        "state positively",
                        "identify positively"
                    ],
                    "target_lang_definitions": {
                        "ru": "С полной уверенностью; без всяких сомнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "определенно"
                            },
                            {
                                "target_lang_word": "с уверенностью",
                                "target_lang_sense_clarification": "без сомнений"
                            },
                            {
                                "target_lang_word": "достоверно",
                                "target_lang_sense_clarification": "подлинно, безошибочно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a558e3fe-bc3f-37e5-9d35-e67e7ea09498",
                    "sense_definition": "Completely or absolutely; used to emphasize the truth of a statement.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is <w>positively</w> no reason for you to be afraid.",
                            "target_lang_translations": {
                                "ru": "У тебя нет <w>абсолютно</w> никаких причин бояться."
                            }
                        },
                        {
                            "text": "She <w>positively</w> refused to eat her vegetables at dinner.",
                            "target_lang_translations": {
                                "ru": "Она <w>категорически</w> отказалась есть овощи за ужином."
                            }
                        }
                    ],
                    "synonyms": [
                        "absolutely",
                        "completely",
                        "totally",
                        "utterly",
                        "entirely"
                    ],
                    "common_phrases": [
                        "positively no",
                        "positively refuse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенно или абсолютно; используется для подчеркивания истинности утверждения."
                    },
                    "translations": {
                        "ru": [

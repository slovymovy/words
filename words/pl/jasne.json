                {
                    "sense_id": "9694a190-6644-343a-b258-a7136eca4a3e",
                    "sense_definition": "Wyrażenie potwierdzenia, zgody na coś lub uznania czegoś za oczywiste.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy pójdziesz ze mną do kina? – <Jasne>, z przyjemnością.",
                            "target_lang_translations": {
                                "ru": "Ты пойдешь со мной в кино? – <Конечно>, с удовольствием."
                            }
                        },
                        {
                            "text": "No <jasne>, że ci pomogę w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Ну <конечно>, я помогу тебе в этом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "oczywiście",
                        "naturalnie",
                        "pewnie",
                        "jak najbardziej",
                        "rzecz jasna"
                    ],
                    "common_phrases": [
                        "no jasne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение подтверждения, согласия или признания чего-либо очевидным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечно",
                                "target_lang_sense_clarification": "согласие"
                            },
                            {

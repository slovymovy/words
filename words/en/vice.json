    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d4c6b1b-c0eb-3dd5-841a-176679ddae52",
                    "sense_definition": "A bad habit or immoral behavior.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "moral_fault",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Smoking is a <w>vice</w> that can lead to serious health problems.",
                            "target_lang_translations": {
                                "ru": "Курение — это <w>порок</w>, который может привести к серьезным проблемам со здоровьем."
                            }
                        },
                        {
                            "text": "Greed is often considered a terrible <w>vice</w> in many cultures.",
                            "target_lang_translations": {
                                "ru": "Жадность часто считается ужасным <w>пороком</w> во многих культурах."
                            }
                        },
                        {
                            "text": "He has no major <w>vice</w> other than drinking too much coffee.",
                            "target_lang_translations": {
                                "ru": "У него нет серьезных <w>пороков</w>, кроме того, что он пьет слишком много кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "sin",
                        "wickedness",
                        "bad habit",
                        "weakness"
                    ],
                    "antonyms": [
                        "virtue"
                    ],
                    "common_phrases": [
                        "virtue and vice",
                        "secret vice",
                        "minor vice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дурная привычка или аморальное поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поро́к",
                                "target_lang_sense_clarification": "нравственный недостаток"
                            },
                            {
                                "target_lang_word": "дурная привычка",
                                "target_lang_sense_clarification": "вредная наклонность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1daec891-b428-3cbb-a5f5-3922e7712202",
                    "sense_definition": "A mechanical tool with two jaws used to hold an object firmly while work is done on it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter secured the wooden plank in the <w>vice</w> before sawing it.",
                            "target_lang_translations": {
                                "ru": "Плотник закрепил деревянную доску в <w>тисках</w> перед тем, как пилить её."
                            }
                        },
                        {
                            "text": "He tightened the <w>vice</w> to keep the metal pipe steady.",
                            "target_lang_translations": {
                                "ru": "Он затянул <w>тиски</w>, чтобы удержать металлическую трубу неподвижно."
                            }
                        }
                    ],
                    "synonyms": [
                        "clamp"
                    ],
                    "common_phrases": [
                        "grip like a vice"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British English spelling; US spelling is 'vise'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Механический инструмент с двумя губками для прочной фиксации предмета во время работы."

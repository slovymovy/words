    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "49ca6f81-2299-3551-8044-b0e5a19f2f48",
                    "sense_definition": "Having good luck or being in a favorable situation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "favored",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We were <w>fortunate</w> not to be fined for speeding.",
                            "target_lang_translations": {
                                "ru": "Нам <w>повезло</w>, что нас не оштрафовали за превышение скорости."
                            }
                        },
                        {
                            "text": "This is a time when we think of those less <w>fortunate</w> than ourselves.",
                            "target_lang_translations": {
                                "ru": "Это время, когда мы думаем о тех, кто менее <w>удачлив</w>, чем мы."
                            }
                        }
                    ],
                    "synonyms": [
                        "lucky",
                        "privileged"
                    ],
                    "antonyms": [
                        "unlucky",
                        "unfortunate"
                    ],
                    "common_phrases": [
                        "less fortunate",
                        "consider oneself fortunate",
                        "fortunate enough",
                        "fortunate position"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий удачей или находящийся в выгодном положении"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удачливый",
                                "target_lang_sense_clarification": "о человеке, которому сопутствует удача"
                            },
                            {
                                "target_lang_word": "счастливый",
                                "target_lang_sense_clarification": "обладающий счастьем, удачей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e11d60d-19e4-3b91-8a99-f4614cc0fece",
                    "sense_definition": "Happening because of good luck or a favorable chance.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Patrick earned a <w>fortunate</w> victory over his rival despite playing poorly.",
                            "target_lang_translations": {
                                "ru": "Патрик одержал <w>счастливую</w> победу над соперником, несмотря на плохую игру."
                            }
                        },
                        {
                            "text": "It was a <w>fortunate</w> coincidence that we met at the station.",
                            "target_lang_translations": {
                                "ru": "То, что мы встретились на вокзале, было <w>удачным</w> совпадением."
                            }
                        }
                    ],
                    "synonyms": [
                        "lucky"
                    ],
                    "antonyms": [
                        "unfortunate"
                    ],
                    "common_phrases": [
                        "fortunate outcome",
                        "fortunate coincidence",
                        "fortunate event",
                        "fortunate timing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий благодаря удаче или счастливому случаю"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удачный",
                                "target_lang_sense_clarification": "об исходе дела, событии"
                            },
                            {
                                "target_lang_word": "счастливый",
                                "target_lang_sense_clarification": "о случае, стечении обстоятельств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a1c7102-9bb9-3872-a8e7-fde322aaa684",
                    "sense_definition": "Suggesting that future success is likely; auspicious.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "auspicious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is considered a <w>fortunate</w> sign if the sun shines on a wedding day.",
                            "target_lang_translations": {
                                "ru": "Считается <w>благоприятным</w> знаком, если в день свадьбы светит солнце."
                            }
                        },
                        {
                            "text": "The clear sky was a <w>fortunate</w> omen for the start of their journey.",
                            "target_lang_translations": {
                                "ru": "Ясное небо было <w>счастливым</w> предзнаменованием для начала их путешествия."
                            }
                        }
                    ],
                    "synonyms": [
                        "auspicious",
                        "rosy"
                    ],
                    "common_phrases": [
                        "fortunate sign",
                        "fortunate omen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предвещающий успех в будущем; благоприятный"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благоприятный",
                                "target_lang_sense_clarification": "способствующий успеху, удачный"
                            },
                            {
                                "target_lang_word": "счастливый",
                                "target_lang_sense_clarification": "о примете или знаке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
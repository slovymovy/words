            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f12a02d-d52b-3baa-af89-081887fbd8af",
                    "sense_definition": "Chwilowy stan psychiczny, nastrój, usposobienie w danej chwili (często w kontekście dobrego lub złego samopoczucia).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mood_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wybacz, ale nie mam dzisiaj <w>humoru</w> na żarty.",
                            "target_lang_translations": {
                                "ru": "Извини, но у меня сегодня нет <w>настроения</w> для шуток."
                            }
                        },
                        {
                            "text": "Mimo deszczowej pogody dopisuje mi świetny <w>humor</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на дождливую погоду, у меня отличное <w>настроение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nastrój"
                    ],
                    "common_phrases": [
                        "dobry humor",
                        "zły humor",
                        "mieć humor",
                        "poprawić humor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временное душевное состояние, расположение духа в данный момент (часто в контексте хорошего или плохого самочувствия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настроение",
                                "target_lang_sense_clarification": "душевное состояние"
                            },
                            {
                                "target_lang_word": "расположение духа",
                                "target_lang_sense_clarification": "состояние духа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfe7003e-dd21-3535-91c0-85564b334887",
                    "sense_definition": "Zdolność dostrzegania zabawnych stron życia oraz cecha sytuacji, tekstów lub zachowań, która wywołuje śmiech.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "comedy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego specyficzny <w>humor</w> nie każdego bawi.",
                            "target_lang_translations": {
                                "ru": "Его специфический <w>юмор</w> не всех веселит."
                            }
                        },
                        {
                            "text": "Ten film to doskonały przykład czarnego <w>humoru</w>.",
                            "target_lang_translations": {
                                "ru": "Этот фильм — отличный пример чёрного <w>юмора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "poczucie humoru"
                    ],
                    "common_phrases": [
                        "czarny humor",
                        "angielski humor",
                        "wisielczy humor",
                        "poczucie humoru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность подмечать смешные стороны жизни, а также качество ситуаций, текстов или поведения, вызывающее смех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юмор",
                                "target_lang_sense_clarification": "комичность, смешное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2061bb75-364e-337c-9a65-41d86785b7ff",
                    "sense_definition": "Stan rozdrażnienia, kaprysy i nagłe zmiany nastroju, występujące bez wyraźnego powodu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mood_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań stroić <w>humory</w> i zachowuj się poważnie.",
                            "target_lang_translations": {
                                "ru": "Перестань показывать свои <w>капризы</w> и веди себя серьёзно."
                            }
                        },
                        {
                            "text": "Jej ciągłe <w>humory</w> są męczące dla całego otoczenia.",
                            "target_lang_translations": {
                                "ru": "Её постоянные <w>капризы</w> утомительны для всех окружающих."
                            }
                        }

            "senses": [
                {
                    "sense_id": "1f0f9c41-0aa6-32f9-bfa6-8193a28bdbad",
                    "sense_definition": "Persoon of partij waartegen men strijdt in een wedstrijd, spel of conflict.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de wedstrijd gaf hij zijn <w>tegenstander</w> een hand.",
                            "target_lang_translations": {
                                "en": "After the match, he shook hands with his <w>opponent</w>.",
                                "ru": "После матча он пожал руку своему <w>сопернику</w>."
                            }
                        },
                        {
                            "text": "Onze ploeg speelde gisteren tegen een zeer sterke <w>tegenstander</w>.",
                            "target_lang_translations": {
                                "en": "Our team played against a very strong <w>opponent</w> yesterday.",
                                "ru": "Вчера наша команда играла против очень сильного <w>противника</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rivaal",
                        "opponent",
                        "vijand"
                    ],
                    "common_phrases": [
                        "geduchte tegenstander",
                        "zwakke tegenstander",
                        "verslagen tegenstander"
                    ],
                    "target_lang_definitions": {
                        "en": "A person or team that you compete against in a sport, game, or conflict.",
                        "ru": "Человек или сторона, с которой соревнуются в игре, спорте или конфликте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opponent",
                                "target_lang_sense_clarification": "in a game or competition"
                            },
                            {
                                "target_lang_word": "rival",
                                "target_lang_sense_clarification": "competitor"
                            },
                            {
                                "target_lang_word": "adversary",
                                "target_lang_sense_clarification": "in a conflict"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "соперник",
                                "target_lang_sense_clarification": "в спорте, игре или соревновании"
                            },
                            {
                                "target_lang_word": "противник",
                                "target_lang_sense_clarification": "общий термин для врага или оппонента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "addfa9f8-d30c-3e71-ad9c-78b968a1100b",
                    "sense_definition": "Iemand die het niet eens is met een bepaald idee, plan of beleid en zich daartegen verzet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een fel <w>tegenstander</w> van de nieuwe plannen van de gemeente.",
                            "target_lang_translations": {
                                "en": "He is a fierce <w>opponent</w> of the municipality's new plans.",
                                "ru": "Он ярый <w>противник</w> новых планов муниципалитета."
                            }
                        },
                        {
                            "text": "De politieke <w>tegenstanders</w> gingen met elkaar in debat over de economie.",
                            "target_lang_translations": {
                                "en": "The political <w>opponents</w> debated the economy with each other.",
                                "ru": "Политические <w>противники</w> вступили в дебаты об экономике."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestrijder"
                    ],
                    "antonyms": [
                        "voorstander"
                    ],
                    "common_phrases": [
                        "fel tegenstander",
                        "politieke tegenstander"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who disagrees with a specific idea, plan, or policy and actively opposes it.",
                        "ru": "Человек, который не согласен с какой-либо идеей, планом или политикой и противостоит им."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opponent",
                                "target_lang_sense_clarification": "of an idea or policy"
                            },
                            {
                                "target_lang_word": "adversary",
                                "target_lang_sense_clarification": "opposing party"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "противник",
                                "target_lang_sense_clarification": "тот, кто выступает против чего-либо"
                            },
                            {
                                "target_lang_word": "оппонент",

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мидлендс",
                                "target_lang_sense_clarification": "регион в Англии"
                            },
                            {
                                "target_lang_word": "центральные графства",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]

                    "synonyms": [
                        "full stop",
                        "that's that",
                        "end of discussion"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в конце утверждения, чтобы показать, что решение окончательное и обсуждению не подлежит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и то́чка",
                                "target_lang_sense_clarification": "категоричность"

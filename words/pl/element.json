    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b2912bcc-3c90-3857-a3d8-5a787eb97a95",
                    "sense_definition": "Część składowa tworząca pewną całość, fragment większej struktury, urządzenia lub systemu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mały <w>element</w> jest kluczowy dla poprawnego działania całej maszyny.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая <w>деталь</w> имеет ключевое значение для правильной работы всей машины."
                            }
                        },
                        {
                            "text": "Bezpieczeństwo pracowników jest ważnym <w>elementem</w> strategii naszej firmy.",
                            "target_lang_translations": {
                                "ru": "Безопасность сотрудников является важным <w>элементом</w> стратегии нашей компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "część",
                        "składnik",
                        "fragment"
                    ],
                    "antonyms": [
                        "całość"
                    ],
                    "common_phrases": [
                        "ważny element",
                        "element dekoracyjny",
                        "element konstrukcyjny",
                        "element zaskoczenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составная часть, образующая единое целое; фрагмент большей структуры, устройства или системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "элемент",
                                "target_lang_sense_clarification": "общая составная часть чего-либо"
                            },
                            {
                                "target_lang_word": "деталь",
                                "target_lang_sense_clarification": "часть механизма или машины"
                            },
                            {
                                "target_lang_word": "составная часть",
                                "target_lang_sense_clarification": "компонент целого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dce633f4-495a-38d4-b74b-281213f7c233",
                    "sense_definition": "Pojedynczy obiekt, który należy do określonego zbioru lub klasy w matematyce.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił uczniów o wypisanie wszystkich <w>elementów</w> zbioru A.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников выписать все <w>элементы</w> множества А."
                            }
                        },
                        {
                            "text": "Każdy <w>element</w> tej grupy musi spełniać określone warunki logiczne.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>элемент</w> этой группы должен выполнять определенные логические условия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "element zbioru",
                        "zbiór elementów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный объект, принадлежащий определенному множеству или классу в математике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "элемент",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ce77a26-2ea1-3119-8b40-5ead0cb805cd",
                    "sense_definition": "W dawnej filozofii i alchemii: jedna z podstawowych substancji (woda, ogień, ziemia, powietrze), z których składa się świat.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "nature_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytni filozofowie wierzyli, że świat składa się z czterech podstawowych <w>elementów</w>.",
                            "target_lang_translations": {
                                "ru": "Древние философы верили, что мир состоит из четырех основных <w>элементов</w>."
                            }
                        },
                        {
                            "text": "Woda to <w>element</w>, który w literaturze często symbolizuje życie oraz zmienność.",
                            "target_lang_translations": {
                                "ru": "Вода — это <w>стихия</w>, которая в литературе часто символизирует жизнь и изменчивость."
                            }
                        }
                    ],
                    "synonyms": [
                        "żywioł",
                        "pierwiastek"
                    ],
                    "common_phrases": [
                        "cztery elementy",
                        "piąty element"
                    ],
                    "target_lang_definitions": {
                        "ru": "В античной философии и алхимии: одна из первооснов (вода, огонь, земля, воздух), из которых состоит мир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стихия",
                                "target_lang_sense_clarification": "сила природы (вода, огонь и т.д.)"
                            },
                            {
                                "target_lang_word": "элемент",
                                "target_lang_sense_clarification": "первооснова материи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee2bf706-7c9a-352a-8a2d-47c096fdd8cf",
                    "sense_definition": "Pogardliwe określenie osób lub grup społecznych uważanych za niebezpieczne, niekulturalne lub podejrzane.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej dzielnicy wieczorami pod sklepami zbiera się podejrzany <w>element</w>.",
                            "target_lang_translations": {
                                "ru": "В этом районе по вечерам у магазинов собирается подозрительный <w>элемент</w>."
                            }
                        },
                        {
                            "text": "Policja często patroluje park, aby usunąć stamtąd chuligański <w>element</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция часто патрулирует парк, чтобы удалить оттуда хулиганский <w>элемент</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "element przestępczy",
                        "podejrzany element",
                        "element napływowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное название лиц или социальных групп, считающихся опасными, некультурными или подозрительными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "элемент",
                                "target_lang_sense_clarification": "собирательное, о нежелательных людях"
                            },
                            {
                                "target_lang_word": "субъект",
                                "target_lang_sense_clarification": "подозрительная личность"
                            }
                        ]
                    }
                }
            ]
        }

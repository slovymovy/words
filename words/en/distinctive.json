                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "различи́тельный",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "дифференциа́льный",
                                "target_lang_sense_clarification": "связанный с различиями"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a91b90ce-6e2d-3d48-ad81-46937d7f2c47",
                    "sense_definition": "A distinguishing characteristic, quality, or property.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "One of the <w>distinctives</w> of this organization is its focus on community service.",

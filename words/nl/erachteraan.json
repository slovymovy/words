                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдогонку",
                                "target_lang_sense_clarification": "направление движения вслед за кем-то или чем-то"
                            },
                            {
                                "target_lang_word": "следом",
                                "target_lang_sense_clarification": "непосредственно за кем-то или чем-то"
                            },
                            {
                                "target_lang_word": "за ним",
                                "target_lang_sense_clarification": "в направлении упомянутого объекта или человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ccf4fe8-314f-3be2-b4d8-ceb2009121fe",
                    "sense_definition": "De voornaamwoordelijke vorm van <w>achteraan</w> in combinatie met 'er'.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9fcea4b8-4c99-3e5d-841a-a555445ad608",
                    "sense_definition": "Not expected to happen or to be true; not probable.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is highly <w>unlikely</w> that the team will win the championship this year.",
                            "target_lang_translations": {
                                "ru": "Крайне <w>маловероятно</w>, что команда выиграет чемпионат в этом году."
                            }
                        },
                        {
                            "text": "In the <w>unlikely</w> event of a delay, we will notify you immediately.",
                            "target_lang_translations": {
                                "ru": "В <w>маловероятном</w> случае задержки мы немедленно сообщим вам."
                            }
                        }
                    ],
                    "synonyms": [
                        "improbable",
                        "doubtful",
                        "implausible"
                    ],
                    "antonyms": [
                        "likely",
                        "probable",
                        "certain"
                    ],
                    "common_phrases": [
                        "highly unlikely",
                        "very unlikely",
                        "unlikely to happen",
                        "unlikely event"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не ожидается, что это произойдет или окажется правдой; маловероятный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маловероятный",
                                "target_lang_sense_clarification": "о событии или исходе"
                            },
                            {
                                "target_lang_word": "вряд ли",
                                "target_lang_sense_clarification": "выражение сомнения"
                            },
                            {
                                "target_lang_word": "сомнительный",
                                "target_lang_sense_clarification": "вызывающий сомнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16c9ec40-9f5f-3f90-9f36-5941c1ab7097",
                    "sense_definition": "Surprising because not typical, suitable, or expected to succeed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "suitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two men formed an <w>unlikely</w> friendship despite their different backgrounds.",
                            "target_lang_translations": {
                                "ru": "Эти двое мужчин завязали <w>неожиданную</w> дружбу, несмотря на разное происхождение."
                            }
                        },
                        {
                            "text": "She became an <w>unlikely</w> hero when she saved the dog from the river.",
                            "target_lang_translations": {
                                "ru": "Она стала <w>неожиданным</w> героем, когда спасла собаку из реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "unexpected",
                        "unpromising",
                        "surprising"
                    ],
                    "antonyms": [
                        "expected",
                        "typical",
                        "predictable"
                    ],
                    "common_phrases": [
                        "unlikely hero",
                        "unlikely source",
                        "unlikely candidate",
                        "unlikely friendship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удивительный, так как не является типичным, подходящим или ожидаемым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неожиданный",
                                "target_lang_sense_clarification": "удивительный, внезапный"
                            },
                            {
                                "target_lang_word": "нетипичный",
                                "target_lang_sense_clarification": "несвойственный"
                            },
                            {
                                "target_lang_word": "странный",
                                "target_lang_sense_clarification": "необычный в данном контексте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1e94e57-1bac-30c2-ae8a-683edb062640",
                    "sense_definition": "A person or thing that is not expected to succeed or be chosen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The coach selected a few <w>unlikelies</w> for the squad, hoping to surprise the opponents.",
                            "target_lang_translations": {
                                "ru": "Тренер выбрал в команду нескольких <w>аутсайдеров</w>, надеясь удивить противников."
                            }
                        },
                        {
                            "text": "Among the list of favorites, there were several <w>unlikelies</w> who had little chance of winning.",
                            "target_lang_translations": {
                                "ru": "Среди фаворитов было несколько <w>тёмных лошадок</w>, имевших мало шансов на победу."
                            }
                        }
                    ],
                    "synonyms": [
                        "long shot",
                        "underdog"
                    ],
                    "antonyms": [
                        "favorite"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, от которого не ожидают успеха или победы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аутсайдер",
                                "target_lang_sense_clarification": "участник с малыми шансами на победу"
                            },
                            {
                                "target_lang_word": "тёмная лошадка",
                                "target_lang_sense_clarification": "идиома: неожиданный кандидат"
                            }
                        ]
                    }
                }
            ]
        }

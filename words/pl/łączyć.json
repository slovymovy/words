    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f535bc5b-6bb7-3f90-8465-bdb32d592c56",
                    "sense_definition": "Spajać dwa lub więcej elementów w jedną całość lub stanowić drogę między dwoma punktami",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten most <łączy> dwa brzegi rzeki.",
                            "target_lang_translations": {
                                "ru": "Этот мост <w>соединяет</w> два берега реки."
                            }
                        },
                        {
                            "text": "Dobry klej trwale <łączy> drewno z metalem.",
                            "target_lang_translations": {
                                "ru": "Хороший клей прочно <w>соединяет</w> дерево с металлом."
                            }
                        }
                    ],
                    "synonyms": [
                        "spajać",
                        "scalać"
                    ],
                    "antonyms": [
                        "rozdzielać",
                        "dzielić"
                    ],
                    "common_phrases": [
                        "łączyć siły",
                        "łączyć koniec z końcem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять физические объекты или создавать путь между ними"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединять",
                                "target_lang_sense_clarification": "физически"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "скреплять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42f73807-2b79-3f4d-b64f-216da5075666",
                    "sense_definition": "Umożliwiać rozmowę telefoniczną lub transmisję danych między urządzeniami",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "telecom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sekretarka <łączy> mnie właśnie z dyrektorem.",
                            "target_lang_translations": {
                                "ru": "Секретарь как раз <w>соединяет</w> меня с директором."
                            }
                        },
                        {
                            "text": "Operator nie może <łączyć> rozmów międzynarodowych bez opłaty.",
                            "target_lang_translations": {
                                "ru": "Оператор не может <w>соединять</w> международные звонки бесплатно."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozłączać"
                    ],
                    "common_phrases": [
                        "łączyć z numerem",
                        "łączyć rozmowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать телефонную связь или соединять абонентов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединять",
                                "target_lang_sense_clarification": "по телефону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9bd8633-90ec-3c66-aa40-dc95e111bd9c",
                    "sense_definition": "Dostrzegać związek między różnymi faktami, osobami lub zjawiskami; kojarzyć jedno z drugim",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_association",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja <łączy> te dwa włamania z tą samą grupą przestępczą.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>связывает</w> эти две кражи с одной и той же преступной группировкой."
                            }
                        },
                        {
                            "text": "Nie należy <łączyć> tych dwóch zupełnie różnych spraw.",
                            "target_lang_translations": {
                                "ru": "Не следует <w>связывать</w> эти два совершенно разных дела."
                            }
                        }
                    ],
                    "synonyms": [
                        "kojarzyć (ze sobą)",
                        "zestawiać"
                    ],
                    "common_phrases": [
                        "łączyć fakty",
                        "łączyć nazwisko z"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находить связь между фактами, людьми или явлениями; ассоциировать"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "ассоциировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b9da59d-beac-3eb2-85d8-4852fa015078",
                    "sense_definition": "Sprawiać, że ludzie czują się wspólnotą; zbliżać do siebie emocjonalnie",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_union",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wspólny wróg często <łączy> zwaśnione narody.",
                            "target_lang_translations": {
                                "ru": "Общий враг часто <w>объединяет</w> враждующие народы."
                            }
                        },
                        {
                            "text": "Muzyka potrafi <łączyć> pokolenia mimo różnic wieku.",
                            "target_lang_translations": {
                                "ru": "Музыка способна <w>объединять</w> поколения, несмотря на разницу в возрасте."
                            }
                        }
                    ],
                    "synonyms": [
                        "jednoczyć",
                        "integrować"
                    ],
                    "antonyms": [
                        "dzielić"
                    ],
                    "common_phrases": [
                        "nas łączy",
                        "nic nas nie łączy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединять людей, создавая чувство общности; сближать эмоционально"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "сплачивать"
                            },
                            {
                                "target_lang_word": "сближать",
                                "target_lang_sense_clarification": "эмоционально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "148198b0-ce11-357f-ab25-9f5a5028be0e",
                    "sense_definition": "Posiadać jednocześnie różne cechy lub wykonywać kilka czynności w tym samym czasie",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "combining_traits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta praca idealnie <łączy> w sobie pasję i dobre zarobki.",
                            "target_lang_translations": {
                                "ru": "Эта работа идеально <w>сочетает</w> в себе страсть и хороший заработок."
                            }
                        },
                        {
                            "text": "Trudno jest <łączyć> studia dzienne z pracą na pełen etat.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>совмещать</w> дневное обучение с работой на полную ставку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łączyć przyjemne z pożytecznym",
                        "łączyć cechy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совмещать разные качества или выполнять несколько действий одновременно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочетать",
                                "target_lang_sense_clarification": "качества"
                            },
                            {
                                "target_lang_word": "совмещать",
                                "target_lang_sense_clarification": "действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ba19038-06d6-326a-9b0c-22e11896c9ac",
                    "sense_definition": "Stawać się jednością, tworzyć całość lub grupę z innymi elementami lub osobami (używane z 'się')",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_merge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwie rzeki <łączą się> w jeden duży nurt.",
                            "target_lang_translations": {
                                "ru": "Две реки <w>сливаются</w> в один большой поток."
                            }
                        },
                        {
                            "text": "Robotnicy <łączą się> w walce o swoje prawa pracownicze.",
                            "target_lang_translations": {
                                "ru": "Рабочие <w>объединяются</w> в борьбе за свои трудовые права."
                            }
                        }
                    ],
                    "synonyms": [
                        "złączać się"
                    ],
                    "antonyms": [
                        "rozłączać",
                        "rozłączać"
                    ],
                    "common_phrases": [
                        "łączyć się w bólu",
                        "łączyć się w pary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться единым целым или объединяться в группу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединяться",
                                "target_lang_sense_clarification": "в группу"
                            },
                            {
                                "target_lang_word": "сливаться",
                                "target_lang_sense_clarification": "в одно целое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a86786a-d67d-3132-8b91-aa7adc2ddee2",
                    "sense_definition": "Wynikać z czegoś, towarzyszyć czemuś lub mieć z czymś ścisły związek (używane z 'się')",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_association",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Awans w pracy często <łączy się> z większą odpowiedzialnością.",
                            "target_lang_translations": {
                                "ru": "Повышение на работе часто <w>связано</w> с большей ответственностью."
                            }
                        },
                        {
                            "text": "Każda inwestycja <łączy się> z pewnym ryzykiem utraty pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Любая инвестиция <w>сопряжена</w> с определенным риском потери денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiązać się"
                    ],
                    "common_phrases": [
                        "to się łączy z"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть связанным с чем-то, сопровождаться чем-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть связанным",
                                "target_lang_sense_clarification": "иметь отношение"
                            },
                            {
                                "target_lang_word": "сопрягаться",
                                "target_lang_sense_clarification": "сопровождаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
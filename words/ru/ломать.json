    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2481a5dd-3189-32f5-a655-1acde7ce94f9",
                    "sense_definition": "Разделять предмет на части, нарушая его целостность силой, сгибанием или ударом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный ветер <w>ломает</w> сухие ветки деревьев в саду.",
                            "target_lang_translations": {
                                "en": "A strong wind <w>breaks</w> dry tree branches in the garden."
                            }
                        },
                        {
                            "text": "Не нужно <w>ломать</w> хлеб руками, лучше отрежь ножом.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>break</w> the bread with your hands, better cut it with a knife."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрушать"
                    ],
                    "antonyms": [
                        "чинить"
                    ],
                    "common_phrases": [
                        "ломать дрова",
                        "ломать ветки",
                        "ломать лед"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate an object into parts by force, bending, or striking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "general physical separation"
                            },
                            {
                                "target_lang_word": "fracture",
                                "target_lang_sense_clarification": "bones or hard materials"
                            },
                            {
                                "target_lang_word": "smash",
                                "target_lang_sense_clarification": "violently or into pieces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65088648-f327-3d5c-95e3-41f577d33188",
                    "sense_definition": "Приводить механизм, прибор или устройство в нерабочее состояние; делать негодным к использованию.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький ребенок постоянно <w>ломает</w> свои электронные игрушки.",
                            "target_lang_translations": {
                                "en": "The small child constantly <w>breaks</w> his electronic toys."
                            }
                        },
                        {
                            "text": "Ты <w>ломаешь</w> замок, когда пытаешься открыть его не тем ключом.",
                            "target_lang_translations": {
                                "en": "You are <w>breaking</w> the lock when you try to open it with the wrong key."
                            }
                        }
                    ],
                    "synonyms": [
                        "портить"
                    ],
                    "antonyms": [
                        "чинить"
                    ],
                    "common_phrases": [
                        "ломать технику",
                        "ломать часы",
                        "ломать компьютер"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a mechanism, device, or appliance to stop working; to render unusable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "stop functioning"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "damage irreparably"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cec82c3-1983-3cd4-ad95-4a5101bf9f64",
                    "sense_definition": "Резко менять, преодолевать или уничтожать что-либо (привычки, традиции, сопротивление, характер).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему пришлось <w>ломать</w> себя и свои привычки ради новой работы.",
                            "target_lang_translations": {
                                "en": "He had to <w>break</w> himself and his habits for the sake of the new job."
                            }
                        },
                        {
                            "text": "Армия продолжает <w>ломать</w> сопротивление противника на всех фронтах.",
                            "target_lang_translations": {
                                "en": "The army continues to <w>break</w> the enemy's resistance on all fronts."
                            }
                        }
                    ],
                    "synonyms": [
                        "уничтожать"
                    ],
                    "antonyms": [
                        "восстанавливать"
                    ],
                    "common_phrases": [
                        "ломать стереотипы",
                        "ломать комедию",
                        "ломать голову",
                        "ломать жизнь"
                    ],
                    "target_lang_definitions": {
                        "en": "To abruptly change, overcome, or destroy something (habits, traditions, resistance, character)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "habits, stereotypes, or will"
                            },
                            {
                                "target_lang_word": "crush",
                                "target_lang_sense_clarification": "resistance or opposition"
                            },
                            {
                                "target_lang_word": "overcome",
                                "target_lang_sense_clarification": "obstacles or oneself"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41233271-79b9-3a12-834b-978b8bfb251d",
                    "sense_definition": "Причинять ноющую боль во всем теле, костях или суставах (обычно при болезни или усталости).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У пациента высокая температура, и его всего <w>ломает</w>.",
                            "target_lang_translations": {
                                "en": "The patient has a high fever, and his whole body <w>aches</w>."
                            }
                        },
                        {
                            "text": "К перемене погоды у бабушки часто <w>ломает</w> спину.",
                            "target_lang_translations": {
                                "en": "Before a change in weather, grandma's back often <w>aches</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тело ломает",
                        "кости ломает"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "impersonal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cause aching pain throughout the body, bones, or joints (usually due to illness or fatigue)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ache",
                                "target_lang_sense_clarification": "to suffer dull pain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87881ea2-feb0-3159-bfca-047d2e3e52b6",
                    "sense_definition": "Вызывать сильное нежелание что-либо делать из-за лени или отсутствия настроения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Меня сегодня жутко <w>ломает</w> идти на работу в такой дождь.",
                            "target_lang_translations": {
                                "en": "I <w>can't be bothered</w> at all to go to work in such rain today."
                            }
                        },
                        {
                            "text": "Его <w>ломало</w> вставать с дивана, чтобы открыть дверь.",
                            "target_lang_translations": {
                                "en": "He <w>couldn't be bothered</w> to get off the couch to open the door."
                            }
                        }
                    ],
                    "common_phrases": [
                        "меня ломает"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "impersonal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a strong reluctance to do something due to laziness or lack of mood."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can't be bothered",
                                "target_lang_sense_clarification": "idiomatic phrase for reluctance"
                            },
                            {
                                "target_lang_word": "don't feel like",
                                "target_lang_sense_clarification": "lack of desire"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3da43585-3592-3eac-b9d2-261b62f8cd3d",
                    "sense_definition": "Добывать камень или руду в карьере.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом регионе веками <w>ломали</w> известняк для строительства домов.",
                            "target_lang_translations": {
                                "en": "In this region, they have <w>quarried</w> limestone for building houses for centuries."
                            }
                        },
                        {
                            "text": "Рабочие отправились в горы <w>ломать</w> мрамор.",
                            "target_lang_translations": {
                                "en": "The workers went to the mountains to <w>quarry</w> marble."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ломать камень"
                    ],
                    "target_lang_definitions": {
                        "en": "To extract stone or ore in a quarry."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quarry",
                                "target_lang_sense_clarification": "stone or rock"
                            },
                            {
                                "target_lang_word": "mine",
                                "target_lang_sense_clarification": "ore or minerals"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "childish / parenting context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при обращении к маленькому ребенку с просьбой дать что-то (подразумевает благодарность заранее)."

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fae69467-cce6-3f1c-a194-51c677bacdf3",
                    "sense_definition": "Zichzelf in gedachten een vraag stellen om iets te weten te komen; nieuwsgierig zijn naar iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_questioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb mij vaak <w>afgevraagd</w> wat de echte reden daarvoor is.",
                            "target_lang_translations": {
                                "en": "I have often <w>wondered</w> what the real reason for that is."
                            }
                        },
                        {
                            "text": "Hij <w>vroeg</w> zich hardop <w>af</w> waar hij zijn sleutels had gelaten.",
                            "target_lang_translations": {
                                "en": "He <w>wondered</w> aloud where he had left his keys."
                            }
                        }
                    ],
                    "antonyms": [
                        "weten"
                    ],
                    "common_phrases": [
                        "zich hardop afvragen",
                        "zich ernstig afvragen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wederkerend (reflexive)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To ask oneself a question mentally; to be curious about something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wonder",
                                "target_lang_sense_clarification": "to think about something with curiosity"
                            },
                            {
                                "target_lang_word": "ask oneself",
                                "target_lang_sense_clarification": "to pose a question to oneself"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ac4f90b-9111-3322-8f2f-ebdd86a09c53",
                    "sense_definition": "Twijfelen aan de juistheid, waarheid of wenselijkheid van iets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_questioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vraag</w> me <w>af</w> of uw opvatting over deze zaak wel juist is.",
                            "target_lang_translations": {
                                "en": "I <w>wonder</w> if your view on this matter is actually correct."
                            }
                        },
                        {
                            "text": "Je kunt je ten zeerste <w>afvragen</w> of deze nieuwe regels wel gaan werken.",
                            "target_lang_translations": {
                                "en": "You can strongly <w>question</w> whether these new rules will actually work."
                            }
                        }
                    ],
                    "synonyms": [
                        "betwijfelen"
                    ],
                    "antonyms": [
                        "geloven",
                        "aannemen"
                    ],
                    "common_phrases": [
                        "zich ten zeerste afvragen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wederkerend (reflexive)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To doubt the correctness, truth, or desirability of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "question",
                                "target_lang_sense_clarification": "to have or express doubts about"
                            },
                            {
                                "target_lang_word": "wonder",
                                "target_lang_sense_clarification": "to feel doubt or curiosity about the validity of something"
                            }
                        ]
                    }
                }
            ]
        }
    ]

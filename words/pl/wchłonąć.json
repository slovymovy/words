                {
                    "sense_id": "0b69d244-5c3a-3c14-8943-91825e982591",
                    "sense_definition": "Przyjąć ciecz, gaz, wilgoć lub inną substancję do swojego wnętrza.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_absorption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sucha gąbka błyskawicznie <wchłonęła> rozlaną na stole wodę.",
                            "target_lang_translations": {
                                "ru": "Сухая губка молниеносно <w>впитала</w> разлитую на столе воду."
                            }
                        },
                        {
                            "text": "Rośliny muszą <wchłonąć> składniki odżywcze z gleby, aby rosnąć.",
                            "target_lang_translations": {
                                "ru": "Растения должны <w>впитать</w> питательные вещества из почвы, чтобы расти."
                            }
                        }
                    ],
                    "synonyms": [
                        "wessać",
                        "pochłonąć",
                        "zaabsorbować"
                    ],
                    "antonyms": [
                        "wydalić",
                        "wyemitować"
                    ],
                    "common_phrases": [
                        "wchłonąć wilgoć",
                        "wchłonąć wodę",
                        "wchłonąć zapach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять в себя жидкость, газ или другую субстанцию; также усвоить информацию или включить в состав чего-то большего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "впитать",
                                "target_lang_sense_clarification": "о жидкости, влаге, а также перен. о знаниях"
                            },
                            {
                                "target_lang_word": "поглотить",
                                "target_lang_sense_clarification": "о веществе, энергии или организации"
                            },
                            {
                                "target_lang_word": "усвоить",
                                "target_lang_sense_clarification": "о знаниях, информации"
                            }
                        ]

                {
                    "sense_id": "04d1f1ef-398b-3cc9-966a-3fadef4cb267",
                    "sense_definition": "mający wokół szyi pas piór lub sierści innej barwy, przypominający obrożę (używane głównie w nazwach gatunkowych zwierząt)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "zoological_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bażant <w>obrożny</w> jest jednym z najczęściej spotykanych ptaków łownych w Polsce.",
                            "target_lang_translations": {
                                "ru": "<w>Ошейниковый</w> фазан — одна из наиболее часто встречающихся охотничьих птиц в Польше."
                            }
                        },
                        {
                            "text": "W atlasie ptaków opisano gatunek, którym jest czernica <w>obrożna</w>.",
                            "target_lang_translations": {
                                "ru": "В атласе птиц описан вид — <w>ошейниковая</w> чернеть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bażant obrożny",
                        "krogulec obrożny",
                        "czernica obrożna",
                        "pekariowiec obrożny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий вокруг шеи полосу перьев или шерсти другого цвета, напоминающую ошейник (используется в основном в названиях видов животных)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ошейниковый",
                                "target_lang_sense_clarification": "в названиях животных (напр. ошейниковый пекари)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91b49f7f-1d4e-38f1-b643-ddc0a8d56693",
                    "sense_definition": "mający założoną obrożę",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "wearing_collar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych księgach inwentarzowych wymieniano psy <w>obrożne</w> służące do pilnowania obejścia.",
                            "target_lang_translations": {
                                "ru": "В старинных инвентарных книгах упоминались собаки <w>в ошейниках</w>, служившие для охраны двора."
                            }
                        },
                        {
                            "text": "Na obrazie widniał chart <w>obrożny</w> siedzący u stóp swojego pana.",

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4328d7c4-0476-3b20-b071-5dcac286040a",
                    "sense_definition": "Bijzonder genoeg om opgemerkt te worden; de aandacht trekkend.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>opmerkelijk</w> dat hij nog steeds niet gearriveerd is.",
                            "target_lang_translations": {
                                "ru": "<w>Удивительно</w>, что он всё ещё не приехал.",
                                "en": "It is <w>remarkable</w> that he still hasn't arrived."
                            }
                        },
                        {
                            "text": "Zij heeft een <w>opmerkelijke</w> prestatie geleverd op school.",
                            "target_lang_translations": {
                                "ru": "Она добилась <w>выдающихся</w> успехов в школе.",
                                "en": "She delivered a <w>remarkable</w> performance at school."
                            }
                        },
                        {
                            "text": "Is dat echt zoiets <w>opmerkelijks</w>?",
                            "target_lang_translations": {
                                "ru": "Это действительно что-то настолько <w>примечательное</w>?",
                                "en": "Is that really something so <w>remarkable</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "opmerkenswaardig"
                    ],
                    "antonyms": [
                        "gewoon",
                        "alledaags",
                        "onopvallend"
                    ],
                    "common_phrases": [
                        "opmerkelijk genoeg",
                        "een opmerkelijk feit",
                        "een opmerkelijke prestatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достаточно особенный, чтобы быть замеченным; привлекающий внимание.",
                        "en": "Special enough to be noticed; attracting attention."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примечательный",
                                "target_lang_sense_clarification": "достойный внимания"
                            },
                            {
                                "target_lang_word": "удивительный",
                                "target_lang_sense_clarification": "необычный, вызывающий удивление"
                            },
                            {
                                "target_lang_word": "выдающийся",
                                "target_lang_sense_clarification": "исключительный (о достижениях)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "remarkable",
                                "target_lang_sense_clarification": "worthy of attention; extraordinary"
                            },
                            {
                                "target_lang_word": "notable",
                                "target_lang_sense_clarification": "worthy of note or notice"
                            },
                            {
                                "target_lang_word": "striking",
                                "target_lang_sense_clarification": "attracting attention by being unusual"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "82a19b12-67be-318e-8fa1-1917f212a132",
                    "sense_definition": "In hoge mate of op een manier die de aandacht trekt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was vandaag <w>opmerkelijk</w> stil in de bibliotheek.",
                            "target_lang_translations": {
                                "ru": "Сегодня в библиотеке было <w>удивительно</w> тихо.",
                                "en": "It was <w>remarkably</w> quiet in the library today."
                            }
                        },
                        {
                            "text": "Hij kan <w>opmerkelijk</w> goed piano spelen voor zijn leeftijd.",
                            "target_lang_translations": {
                                "ru": "Он <w>поразительно</w> хорошо играет на пианино для своего возраста.",
                                "en": "He plays the piano <w>remarkably</w> well for his age."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opmerkelijk goed",
                        "opmerkelijk veel",
                        "opmerkelijk snel"
                    ],
                    "target_lang_definitions": {
                        "ru": "В значительной степени или образом, привлекающим внимание.",
                        "en": "To a high degree or in a way that attracts attention."
                    },
                    "translations": {

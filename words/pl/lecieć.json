    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f67c141b-6e78-3d20-a4f7-be44b8a94ccb",
                    "sense_definition": "Poruszać się w powietrzu (o ptakach, owadach, samolotach) lub podróżować drogą lotniczą.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptaki <lecą> na południe, aby spędzić tam zimę.",
                            "target_lang_translations": {
                                "ru": "Птицы <летят> на юг, чтобы провести там зиму."
                            }
                        },
                        {
                            "text": "W przyszłym tygodniu <lecimy> całą rodziną na wakacje do Hiszpanii.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе мы всей семьей <летим> в отпуск в Испанию."
                            }
                        }
                    ],
                    "synonyms": [
                        "frunąć",
                        "szybować"
                    ],
                    "common_phrases": [
                        "lecieć samolotem",
                        "ptaki lecą",
                        "lecieć wysoko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвигаться по воздуху (о птицах, самолетах) или путешествовать самолетом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лететь",
                                "target_lang_sense_clarification": "перемещаться по воздуху"
                            },
                            {
                                "target_lang_word": "летать",
                                "target_lang_sense_clarification": "путешествовать самолетом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1650a040-9c50-37f2-acc8-e60b643c9ac6",
                    "sense_definition": "Biec lub iść gdzieś bardzo szybko, spieszyć się.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę już <lecieć> do domu, bo robi się późno.",
                            "target_lang_translations": {
                                "ru": "Мне уже нужно <бежать> домой, потому что становится поздно."
                            }
                        },
                        {
                            "text": "Marek <leci> do sklepu, żeby kupić świeże bułki na śniadanie.",
                            "target_lang_translations": {
                                "ru": "Марек <мчится> в магазин, чтобы купить свежие булочки на завтрак."
                            }
                        }
                    ],
                    "synonyms": [
                        "gnać",
                        "pędzić"
                    ],
                    "antonyms": [
                        "wlec się"
                    ],
                    "common_phrases": [
                        "muszę lecieć",
                        "lecieć do pracy",
                        "lecieć na spotkanie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бежать или идти куда-то очень быстро, торопиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "быстро двигаться"
                            },
                            {
                                "target_lang_word": "лететь",
                                "target_lang_sense_clarification": "разг. спешить"
                            },
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "торопиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79916b1f-16c9-3eec-ab8a-a8476715a7d5",
                    "sense_definition": "Być wyświetlanym w telewizji, kinie lub odtwarzanym w radiu.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W telewizji właśnie <leci> mój ulubiony serial komediowy.",
                            "target_lang_translations": {
                                "ru": "По телевизору как раз <идет> мой любимый комедийный сериал."
                            }
                        },
                        {
                            "text": "Sprawdź w internecie, co dzisiaj <leci> w kinie.",
                            "target_lang_translations": {
                                "ru": "Проверь в интернете, что сегодня <идет> в кино."
                            }
                        }
                    ],
                    "common_phrases": [
                        "co leci w kinie",
                        "lecieć w radiu",
                        "lecieć w telewizji"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Транслироваться по телевидению, радио или в кино."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о фильме, передаче"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "393c0e57-acb0-342b-956e-a2e41af47ae7",
                    "sense_definition": "Mijać bardzo szybko (o czasie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten czas tak szybko <leci>, że nawet nie zauważyłem, kiedy minął rok.",
                            "target_lang_translations": {
                                "ru": "Время так быстро <летит>, что я даже не заметил, как прошел год."
                            }
                        },
                        {
                            "text": "Lata <lecą>, a my wciąż czujemy się młodo.",
                            "target_lang_translations": {
                                "ru": "Годы <летят>, а мы все еще чувствуем себя молодыми."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czas leci",
                        "lata lecą",
                        "szybko leci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить очень быстро (о времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лететь",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "быстро проходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0aa0969-be00-353e-8b83-dfeb23e00c00",
                    "sense_definition": "Spadać w dół (o przedmiotach, liściach itp.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesienią z drzew <lecą> kolorowe liście.",
                            "target_lang_translations": {
                                "ru": "Осенью с деревьев <летят> разноцветные листья."
                            }
                        },
                        {
                            "text": "Wszystko mi dzisiaj <leci> z rąk, jestem bardzo zmęczona.",
                            "target_lang_translations": {
                                "ru": "У меня сегодня всё <валится> из рук, я очень устала."
                            }
                        }
                    ],
                    "synonyms": [
                        "spadać"
                    ],
                    "common_phrases": [
                        "lecieć z rąk",
                        "lecieć na łeb",
                        "lecieć w dół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Падать вниз (о предметах, листьях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "вниз"
                            },
                            {
                                "target_lang_word": "валиться",
                                "target_lang_sense_clarification": "выпадать (из рук)"
                            },
                            {
                                "target_lang_word": "лететь",
                                "target_lang_sense_clarification": "разг. падать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "836f381a-810f-33cd-a2ff-acf3b44656d9",
                    "sense_definition": "Płynąć lub ciec strumieniem (o cieczach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z kranu w kuchni <leci> tylko zimna woda.",
                            "target_lang_translations": {
                                "ru": "Из крана на кухне <течет> только холодная вода."
                            }
                        },
                        {
                            "text": "Krew <leci> mi z nosa, muszę usiąść i odpocząć.",
                            "target_lang_translations": {
                                "ru": "У меня из носа <идет> кровь, мне нужно сесть и отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "spływać",
                        "kapać"
                    ],
                    "common_phrases": [
                        "krew leci",
                        "woda leci",
                        "lecieć z kranu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Течь или литься струей (о жидкостях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "о воде"
                            },
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о воде, крови"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "831a7c4b-fa21-3fae-b807-11b758fea14d",
                    "sense_definition": "Dziać się, układać się (o życiu, sprawach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cześć, jak ci <leci> w nowej pracy?",
                            "target_lang_translations": {
                                "ru": "Привет, как у тебя <идут дела> на новой работе?"
                            }
                        },
                        {
                            "text": "Jakoś powoli <leci>, nie mogę narzekać na zdrowie.",
                            "target_lang_translations": {
                                "ru": "Как-то потихоньку <идет>, не могу жаловаться на здоровье."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jak leci",
                        "jakoś leci",
                        "dobrze leci"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить, складываться (о жизни, делах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о делах, жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea173668-a79e-3452-b145-cebe2c3bc9e6",
                    "sense_definition": "Bardzo się komuś podobać, czuć do kogoś pociąg.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy w szkole wiedzą, że on na nią <leci>.",
                            "target_lang_translations": {
                                "ru": "Все в школе знают, что он на нее <запал>."
                            }
                        },
                        {
                            "text": "Niektórzy ludzie <lecą> tylko na pieniądze i sławę.",
                            "target_lang_translations": {
                                "ru": "Некоторые люди <ведутся> только на деньги и славу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lecieć na kogoś",
                        "lecieć na kasę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень нравиться кому-то, испытывать влечение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запасть",
                                "target_lang_sense_clarification": "разг. влюбиться"
                            },
                            {
                                "target_lang_word": "вестись",
                                "target_lang_sense_clarification": "разг. на деньги/выгоду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e1dd15c-bbb6-3f65-ad3c-95c48e6f4a02",
                    "sense_definition": "Wydzielać nieprzyjemny zapach, śmierdzieć.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strasznie tu <leci> wilgocią, musimy otworzyć okno.",
                            "target_lang_translations": {
                                "ru": "Здесь ужасно <несет> сыростью, нужно открыть окно."
                            }
                        },
                        {
                            "text": "Od tego bezdomnego psa <leci> bardzo nieprzyjemny zapach.",
                            "target_lang_translations": {
                                "ru": "От этой бездомной собаки <исходит> очень неприятный запах."
                            }
                        }
                    ],
                    "synonyms": [
                        "śmierdzieć",
                        "cuchnąć",
                        "walić",
                        "zalatywać"
                    ],
                    "common_phrases": [
                        "ale leci",
                        "leci od niego"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать неприятный запах, вонять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "неприятным запахом"
                            },
                            {
                                "target_lang_word": "вонять",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
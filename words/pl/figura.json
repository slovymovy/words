                },
                {
                    "sense_id": "6dc6c7aa-e5c9-3d46-8c85-6b1c93a44f39",
                    "sense_definition": "Bierka szachowa ważniejsza od piona (król, hetman, wieża, goniec, skoczek).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "game_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Straciłem ważną <w>figurę</w> na początku partii i trudno było mi wygrać.",
                            "target_lang_translations": {
                                "ru": "Я потерял важную <w>фигуру</w> в начале партии, и мне было трудно выиграть."
                            }
                        },
                        {
                            "text": "Szachista zdecydował się na ryzykowną wymianę <w>figur</w>.",
                            "target_lang_translations": {
                                "ru": "Шахматист решился на рискованный размен <w>фигур</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "pion"
                    ],
                    "common_phrases": [
                        "wymiana figur",
                        "grać figurami",
                        "stracić figurę",
                        "lekkie figury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шахматная фигура, более значимая, чем пешка (король, ферзь, ладья, слон, конь)."

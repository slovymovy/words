    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6a65e100-cb1a-35ed-8919-94e041e43eb5",
                    "sense_definition": "Переместить что-либо внутрь помещения или пространства, обычно волоком или с усилием.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы с трудом <затащили> тяжёлый диван в квартиру.",
                            "target_lang_translations": {
                                "en": "We barely managed to <w>drag</w> the heavy sofa into the apartment."
                            }
                        },
                        {
                            "text": "Помоги мне <затащить> эти коробки в гараж, пока не пошёл дождь.",
                            "target_lang_translations": {
                                "en": "Help me <w>haul</w> these boxes into the garage before it starts raining."
                            }
                        }
                    ],
                    "synonyms": [
                        "смочь",
                        "удасться",
                        "удалить"
                    ],
                    "antonyms": [
                        "вытащить"
                    ],
                    "common_phrases": [
                        "затащить в дом",
                        "затащить наверх",
                        "затащить внутрь"
                    ],
                    "target_lang_definitions": {
                        "en": "To move something inside a room or space, usually by dragging or with effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag in",
                                "target_lang_sense_clarification": "to pull something heavy inside"
                            },
                            {
                                "target_lang_word": "haul in",
                                "target_lang_sense_clarification": "to move something with significant effort"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6e31481-51da-3996-86a4-154dc0c45e6d",
                    "sense_definition": "Уговорами или принуждением заставить кого-либо пойти куда-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Друзья всё-таки <затащили> меня на эту вечеринку.",
                            "target_lang_translations": {
                                "en": "My friends still managed to <w>drag</w> me to this party."
                            }
                        },
                        {
                            "text": "Я пытался <затащить> его к врачу, но он отказался идти.",
                            "target_lang_translations": {
                                "en": "I tried to <w>drag</w> him to the doctor, but he refused to go."
                            }
                        }
                    ],
                    "antonyms": [
                        "отвадить"
                    ],
                    "common_phrases": [
                        "затащить в гости",
                        "затащить в кино",
                        "еле затащил"
                    ],
                    "target_lang_definitions": {
                        "en": "To persuade or force someone to go somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "to force someone to go somewhere against their will"
                            },
                            {
                                "target_lang_word": "rope in",
                                "target_lang_sense_clarification": "to persuade someone to join an activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10190c06-7d40-33f9-9069-3f9a2d035fdc",
                    "sense_definition": "Унести вещь далеко или в такое место, где её трудно найти.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Собака <затащила> мою обувь под кровать.",
                            "target_lang_translations": {
                                "en": "The dog <w>dragged</w> my shoes under the bed."
                            }
                        },
                        {
                            "text": "Дети куда-то <затащили> пульт от телевизора, и мы не можем его найти.",
                            "target_lang_translations": {
                                "en": "The kids <w>dragged</w> the TV remote off somewhere, and we can't find it."
                            }
                        }
                    ],
                    "antonyms": [
                        "вытащить"
                    ],
                    "common_phrases": [
                        "затащить ключи",
                        "куда-то затащить"
                    ],
                    "target_lang_definitions": {
                        "en": "To carry a thing far away or to a place where it is hard to find."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag off",
                                "target_lang_sense_clarification": "to take something away to another place"
                            },
                            {
                                "target_lang_word": "carry off",
                                "target_lang_sense_clarification": "to take something away"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5aa915f0-8ee5-3942-91ec-16598e3317b9",
                    "sense_definition": "Справиться с трудной задачей, выиграть игру или вытянуть сложную ситуацию благодаря личным усилиям.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш вратарь смог <затащить> этот матч практически в одиночку.",
                            "target_lang_translations": {
                                "en": "Our goalkeeper managed to <w>carry</w> this match almost single-handedly."
                            }
                        },
                        {
                            "text": "Я не готовился к экзамену, но каким-то чудом <затащил> его.",
                            "target_lang_translations": {
                                "en": "I didn't prepare for the exam, but somehow miraculously <w>nailed</w> it."
                            }
                        }
                    ],
                    "synonyms": [
                        "смочь",
                        "удасться"
                    ],
                    "common_phrases": [
                        "затащить катку",
                        "затащить игру",
                        "затащить раунд"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in gaming, sports, and youth culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cope with a difficult task, win a game, or pull through a complex situation thanks to personal efforts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "slang: to lead a team to victory"
                            },
                            {
                                "target_lang_word": "pull off",
                                "target_lang_sense_clarification": "to succeed in doing something difficult"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        "days of youth",
                        "misspent youth",
                        "bloom of youth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период жизни между детством и взрослостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молодость",
                                "target_lang_sense_clarification": "период жизни"
                            },
                            {
                                "target_lang_word": "юность",
                                "target_lang_sense_clarification": "ранняя молодость, отрочество"
                            }
                        ]

                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дизайнер посоветовал <размещать> мебель так, чтобы в комнате было больше света.",
                            "target_lang_translations": {
                                "en": "The designer advised <arranging> the furniture so that there would be more light in the room."
                            }
                        },
                        {
                            "text": "Компания начала активно <размещать> рекламу своих товаров в социальных сетях.",
                            "target_lang_translations": {
                                "en": "The company began actively <placing> ads for its products on social networks."
                            }
                        }
                    ],
                    "synonyms": [
                        "располагать",
                        "помещать",
                        "расставлять",
                        "публиковать"
                    ],
                    "antonyms": [
                        "убирать",
                        "удалять",
                        "снимать"
                    ],
                    "common_phrases": [
                        "размещать мебель",
                        "размещать рекламу",
                        "размещать информацию",
                        "размещать объявление",
                        "размещать на сайте"
                    ],
                    "target_lang_definitions": {
                        "en": "To put, lay, or arrange someone or something in a certain place or order; also to publish information in media or the internet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "objects, general"
                            },
                            {
                                "target_lang_word": "arrange",
                                "target_lang_sense_clarification": "furniture, layout"
                            },

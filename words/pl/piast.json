    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7816a23e-e63e-35e7-a530-4489815489e7",
                    "sense_definition": "Imię legendarnego założyciela pierwszej polskiej dynastii królewskiej, od którego wzięła ona swoją nazwę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "historical_figure",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Według legendy <Piast> był skromnym kołodziejem, który ugościł wędrowców.",
                            "target_lang_translations": {
                                "ru": "Согласно легенде, <w>Пяст</w> был скромным колесником, который приютил странников."
                            }
                        },
                        {
                            "text": "Dynastia, którą zapoczątkował <Piast>, rządziła Polską przez kilka stuleci.",
                            "target_lang_translations": {
                                "ru": "Династия, начало которой положил <w>Пяст</w>, правила Польшей на протяжении нескольких столетий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Piast Kołodziej",
                        "dynastia Piastów",
                        "ród Piasta"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имя легендарного основателя первой польской королевской династии, от которого она получила свое название."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пяст",
                                "target_lang_sense_clarification": "легендарный князь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb8e07f0-fd77-3109-b975-da4e4914c20f",
                    "sense_definition": "Przedstawiciel polskiej dynastii panującej, rządzącej Polską od początków państwowości do XIV wieku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dynasty_member",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Kazimierz Wielki był ostatnim <Piastem> na polskim tronie.",
                            "target_lang_translations": {
                                "ru": "Казимир Великий был последним <w>Пястом</w> на польском троне."
                            }
                        },
                        {
                            "text": "Wielu <Piastów> walczyło o władzę w okresie rozbicia dzielnicowego.",
                            "target_lang_translations": {
                                "ru": "Многие <w>Пясты</w> боролись за власть в период феодальной раздробленности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ostatni Piast",
                        "książęta z rodu Piastów"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель польской правящей династии, правившей Польшей с начала государственности до XIV века."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пяст",
                                "target_lang_sense_clarification": "член династии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb078e4b-aacc-34c0-bc46-9e10fd234cf0",
                    "sense_definition": "Polskie ugrupowanie polityczne o charakterze ludowym, aktywne w pierwszej połowie XX wieku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "political_party",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Wincenty Witos był wieloletnim przywódcą partii <Piast>.",
                            "target_lang_translations": {
                                "ru": "Винценты Витос был многолетним лидером партии \"<w>Пяст</w>\"."
                            }
                        },
                        {
                            "text": "Ugrupowanie <Piast> odegrało kluczową rolę w parlamencie II Rzeczypospolitej.",
                            "target_lang_translations": {
                                "ru": "Группировка \"<w>Пяст</w>\" сыграла ключевую роль в парламенте Второй Речи Посполитой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "PSL Piast"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская политическая группировка народного характера, действовавшая в первой половине XX века."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пяст",
                                "target_lang_sense_clarification": "политическая партия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6129c9d7-a8bc-30d7-bf58-0f42cd077027",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>piasta</w> (część koła).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W warsztacie brakowało zapasowych <piast> do rowerów górskich.",
                            "target_lang_translations": {
                                "ru": "В мастерской не хватало запасных <w>ступиц</w> для горных велосипедов."
                            }
                        },
                        {
                            "text": "Średnica tych <piast> musi idealnie pasować do osi pojazdu.",
                            "target_lang_translations": {
                                "ru": "Диаметр этих <w>ступиц</w> должен идеально подходить к оси транспортного средства."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного \"piasta\" (ступица, часть колеса)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ступиц",
                                "target_lang_sense_clarification": "форма р.п. мн.ч. слова ступица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f74c0b59-398e-33fc-acf8-0276908a29e5",
                    "sense_definition": "Monarcha lub książę wywodzący się z dynastii piastowskiej (użycie jako rzeczownik pospolity).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dynasty_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śląskim tronie zasiadł kolejny <Piast>, kontynuując rządy ojca.",
                            "target_lang_translations": {
                                "ru": "На силезский трон взошел очередной <w>Пяст</w>, продолжая правление отца."
                            }
                        },
                        {
                            "text": "Historycy badają losy bocznych linii <Piastów> na Mazowszu.",
                            "target_lang_translations": {
                                "ru": "Историки изучают судьбы боковых линий <w>Пястов</w> в Мазовии."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Монарх или князь, происходящий из династии Пястов (используется как нарицательное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пяст",
                                "target_lang_sense_clarification": "князь из рода Пястов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

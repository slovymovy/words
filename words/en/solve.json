{
    "word_family": [
        "solution",
        "solvable",
        "solver",
        "unsolvable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc6d4f07-7360-39f0-9aa8-0552ef046f21",
                    "sense_definition": "To find an answer or solution to a problem or question; to work out.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_solution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>solve</w> this problem before we can continue with the project.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>решить</w> эту проблему, прежде чем мы сможем продолжить проект."
                            }
                        },
                        {
                            "text": "It took me a long time to <w>solve</w> the puzzle.",
                            "target_lang_translations": {
                                "ru": "У меня ушло много времени, чтобы <w>разгадать</w> эту головоломку."
                            }
                        }
                    ],
                    "synonyms": [
                        "resolve",
                        "work out",
                        "figure out",
                        "answer"
                    ],
                    "common_phrases": [
                        "solve a problem",
                        "solve a puzzle",
                        "solve an issue",
                        "solve a mystery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Найти ответ или решение проблемы или вопроса; разобраться в чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решить",
                                "target_lang_sense_clarification": "найти искомое, получить результат (совершенный вид)"
                            },
                            {
                                "target_lang_word": "решать",
                                "target_lang_sense_clarification": "заниматься поиском ответа (несовершенный вид)"
                            },
                            {
                                "target_lang_word": "разгадать",
                                "target_lang_sense_clarification": "найти ответ на загадку или головоломку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16e82c59-303d-35cd-848e-262e4fd3a2e8",
                    "sense_definition": "To find out the perpetrator, the motive, or the details of a crime.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "crime_solution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police are working hard to <w>solve</w> the crime.",
                            "target_lang_translations": {
                                "ru": "Полиция усердно работает над тем, чтобы <w>раскрыть</w> это преступление."
                            }
                        },
                        {
                            "text": "Sherlock Holmes is famous for his ability to <w>solve</w> difficult cases.",
                            "target_lang_translations": {
                                "ru": "Шерлок Холмс известен своей способностью <w>раскрывать</w> сложные дела."
                            }
                        }
                    ],
                    "synonyms": [
                        "crack",
                        "clear up"
                    ],
                    "common_phrases": [
                        "solve a crime",
                        "solve a murder",
                        "solve a case"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выяснить личность преступника, мотив или детали преступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрыть",
                                "target_lang_sense_clarification": "сделать тайное явным (о преступлении)"
                            },
                            {
                                "target_lang_word": "распутать",
                                "target_lang_sense_clarification": "разобраться в сложном деле (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71be80a0-a97c-3c5b-9d0c-bf2bf670c56e",
                    "sense_definition": "To find the correct value or answer for a mathematical equation or problem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_solution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher asked the students to <w>solve</w> the equation on the board.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников <w>решить</w> уравнение на доске."
                            }
                        },
                        {
                            "text": "You must <w>solve</w> for x to get the correct answer.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>найти</w> x, чтобы получить правильный ответ."
                            }
                        }
                    ],
                    "synonyms": [
                        "calculate",
                        "compute"
                    ],
                    "common_phrases": [
                        "solve an equation",
                        "solve for x",
                        "solve a math problem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Найти верное значение или ответ в математическом уравнении или задаче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решить",
                                "target_lang_sense_clarification": "выполнить математические действия для получения ответа"
                            },
                            {
                                "target_lang_word": "вычислить",
                                "target_lang_sense_clarification": "посчитать значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ccae0b5-2a05-33f0-bea9-1e0a639ced82",
                    "sense_definition": "To loosen, separate, or dissolve something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_dissolve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heat will <w>solve</w> the ice into water.",
                            "target_lang_translations": {
                                "ru": "Тепло <w>растопит</w> лёд, превратив его в воду."
                            }
                        },
                        {
                            "text": "He hoped that time would <w>solve</w> the bonds that held them together.",
                            "target_lang_translations": {
                                "ru": "Он надеялся, что время <w>расторгнет</w> узы, которые удерживали их вместе."
                            }
                        }
                    ],
                    "synonyms": [
                        "dissolve",
                        "disintegrate"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage typically prefers 'dissolve' or 'loosen'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ослабить, разделить или растворить что-либо (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растворить",
                                "target_lang_sense_clarification": "превратить в жидкость или раствор"
                            },
                            {
                                "target_lang_word": "расторгнуть",
                                "target_lang_sense_clarification": "прекратить действие чего-либо (например, уз или союза)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "249f4b22-b65c-3de7-ba1c-ef8b8aee7c55",
                    "sense_definition": "An act of solving a problem or mystery; a solution.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_solution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was a really quick <w>solve</w> for such a complex puzzle.",
                            "target_lang_translations": {
                                "ru": "Это была очень быстрая <w>разгадка</w> для такой сложной головоломки."
                            }
                        },
                        {
                            "text": "The detective was praised for the brilliant <w>solve</w> of the cold case.",
                            "target_lang_translations": {
                                "ru": "Детектива хвалили за блестящее <w>раскрытие</w> «глухаря» (давно нераскрытого дела)."
                            }
                        }
                    ],
                    "synonyms": [
                        "solution",
                        "fix"
                    ],
                    "common_phrases": [
                        "good solve",
                        "quick solve"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Often used in puzzle communities or police procedural contexts."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт решения проблемы или тайны; разгадка (существительное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разгадка",
                                "target_lang_sense_clarification": "ответ на загадку или сложную задачу"
                            },
                            {
                                "target_lang_word": "раскрытие",
                                "target_lang_sense_clarification": "успешное завершение расследования"
                            },
                            {
                                "target_lang_word": "решение",
                                "target_lang_sense_clarification": "найденный ответ"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "174fb272-d05d-3df6-96d2-5a822734d9f7",
                    "sense_definition": "Een trein die op alle tussengelegen stations van een traject stopt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik neem elke ochtend de <w>sprinter</w> naar mijn werk omdat die dichtbij stopt.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я езжу на работу на <w>электричке</w>, потому что она останавливается поблизости."
                            }
                        },
                        {
                            "text": "De <w>sprinter</w> vertrekt over vijf minuten van spoor twee.",
                            "target_lang_translations": {
                                "ru": "<w>Электричка</w> отправляется через пять минут со второго пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoptrein"
                    ],
                    "antonyms": [
                        "intercity",
                        "sneltrein"
                    ],
                    "common_phrases": [
                        "de nieuwe sprinter",
                        "met de sprinter"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "in Nederland"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поезд, который останавливается на всех промежуточных станциях маршрута."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электричка",
                                "target_lang_sense_clarification": "пригородный поезд со всеми остановками"
                            },
                            {
                                "target_lang_word": "пригородный поезд",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "138eac2a-bcbc-3bb4-9dc5-2aa5d492d95c",
                    "sense_definition": "Een sporter, zoals een hardloper of wielrenner, die gespecialiseerd is in het afleggen van korte afstanden op zeer hoge snelheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>sprinter</w> kwam als eerste over de finishlijn en won de gouden medaille.",
                            "target_lang_translations": {
                                "ru": "<w>Спринтер</w> первым пересек финишную черту и выиграл золотую медаль."
                            }
                        },
                        {
                            "text": "Hij is een getalenteerde <w>sprinter</w> die vooral uitblinkt op de honderd meter.",
                            "target_lang_translations": {
                                "ru": "Он талантливый <w>спринтер</w>, который особенно хорош на стометровке."
                            }
                        }
                    ],
                    "antonyms": [
                        "duurloper",
                        "stayer"
                    ],
                    "common_phrases": [
                        "een snelle sprinter",
                        "de winnende sprinter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортсмен (бегун или велогонщик), специализирующийся на преодолении коротких дистанций с очень высокой скоростью."
                    },
                    "translations": {

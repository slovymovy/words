            "pos": "noun",
            "senses": [
                {
                    "sense_id": "14e632e2-108f-38a1-a7d5-792d420d96c9",
                    "sense_definition": "Osoba biorąca udział w zawodach sportowych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport_participant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <zawodnik> jest szybki i dobrze wyszkolony technicznie.",
                            "target_lang_translations": {
                                "ru": "Этот <спортсмен> быстр и технически хорошо подготовлен."
                            }
                        },
                        {
                            "text": "Każdy <zawodnik> musi dokładnie znać regulamin turnieju.",
                            "target_lang_translations": {
                                "ru": "Каждый <участник> должен точно знать правила турнира."
                            }
                        }
                    ],
                    "synonyms": [
                        "współzawodnik"
                    ],
                    "common_phrases": [
                        "doświadczony zawodnik",
                        "zawodnik rezerwowy",
                        "czołowy zawodnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, принимающий участие в спортивных соревнованиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спортсмен",
                                "target_lang_sense_clarification": "человек, занимающийся спортом"
                            },
                            {
                                "target_lang_word": "участник",
                                "target_lang_sense_clarification": "соревнований"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a7ff0f9-4a0d-326a-a5f0-65791c9cb46f",
                    "sense_definition": "Uczestnik gry zespołowej, członek drużyny sportowej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport_participant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trener postanowił zdjąć z boiska zmęczonego <zawodnika>.",
                            "target_lang_translations": {
                                "ru": "Тренер решил убрать с поля уставшего <игрока>."
                            }
                        },
                        {
                            "text": "Najlepszy <zawodnik> meczu otrzymał pamiątkową statuetkę.",
                            "target_lang_translations": {
                                "ru": "Лучший <игрок> матча получил памятную статуэтку."
                            }
                        }
                    ],
                    "synonyms": [
                        "gracz"
                    ],
                    "common_phrases": [
                        "zmiana zawodnika",
                        "zawodnik pola"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участник командной игры, член спортивной команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игрок",
                                "target_lang_sense_clarification": "член спортивной команды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cf8bf30-e439-3a73-bdf8-26512b03ff66",
                    "sense_definition": "Osoba, z którą się rywalizuje o zwycięstwo w jakiejś dziedzinie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rivalry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <zawodnik> okazał się znacznie silniejszy, niż przypuszczałem.",
                            "target_lang_translations": {
                                "ru": "Мой <соперник> оказался значительно сильнее, чем я предполагал."
                            }
                        },
                        {
                            "text": "Musisz szanować swojego <zawodnika>, nawet jeśli z nim przegrasz.",
                            "target_lang_translations": {
                                "ru": "Ты должен уважать своего <противника>, даже если проиграешь ему."
                            }
                        }
                    ],
                    "synonyms": [
                        "rywal",
                        "konkurent"
                    ],
                    "common_phrases": [
                        "pokonać zawodnika",
                        "trudny zawodnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, с которым ведется борьба за победу в какой-либо области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соперник",
                                "target_lang_sense_clarification": "конкурент в борьбе"
                            },
                            {
                                "target_lang_word": "противник",
                                "target_lang_sense_clarification": "оппонент"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
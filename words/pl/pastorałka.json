    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c3260db6-046a-32bd-be71-a75fdd1b58d6",
                    "sense_definition": "Lekki utwór muzyczny lub sceniczny o tematyce związanej z Bożym Narodzeniem, często o charakterze ludowym i zawierający motywy pasterskie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "christmas_songs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W czasie świąt cała rodzina śpiewała głośno każdą znaną <w>pastorałkę</w>.",
                            "target_lang_translations": {
                                "ru": "Во время праздников вся семья громко пела каждую знакомую <w>колядку</w>."
                            }
                        },
                        {
                            "text": "Uczniowie wystawili w szkole zabawną <w>pastorałkę</w> dla rodziców.",
                            "target_lang_translations": {
                                "ru": "Ученики поставили в школе забавную <w>рождественскую пастораль</w> для родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolęda"
                    ],
                    "common_phrases": [
                        "śpiewać pastorałki",
                        "wesoła pastorałka",
                        "polskie pastorałki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкое музыкальное или сценическое произведение на рождественскую тематику, часто с народными и пастушескими мотивами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колядка",
                                "target_lang_sense_clarification": "народная рождественская песня"
                            },
                            {
                                "target_lang_word": "рождественская пастораль",
                                "target_lang_sense_clarification": "музыкальный или сценический жанр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a923cf1-32e9-334f-b7a3-12fe8539a5dc",
                    "sense_definition": "Utwór poetycki lub dramatyczny przedstawiający życie pasterzy i wsi w sposób wyidealizowany i sielankowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "literary_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten renesansowy poeta stworzył piękną <w>pastorałkę</w> o miłości pasterzy.",
                            "target_lang_translations": {
                                "ru": "Этот поэт эпохи Возрождения создал прекрасную <w>пастораль</w> о любви пастухов."
                            }
                        },
                        {
                            "text": "Czytaliśmy na lekcji polskiego staropolską <w>pastorałkę</w>.",
                            "target_lang_translations": {
                                "ru": "На уроке польского языка мы читали старопольскую <w>пастораль</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sielanka",
                        "idylla",
                        "bukolika",
                        "ekloga"
                    ],
                    "common_phrases": [
                        "pastorałka dramatyczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поэтическое или драматическое произведение, идеализирующее жизнь пастухов и сельский быт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пастораль",
                                "target_lang_sense_clarification": "литературный жанр"
                            },
                            {
                                "target_lang_word": "идиллия",
                                "target_lang_sense_clarification": "жанр"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a285a9c9-f5fc-37ea-af04-e167511ea8da",
                    "sense_definition": "Громкий скандал, ругань, шум, крики или беспорядок.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noise_scandal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Соседи снова подняли <w>хай</w> из-за громкой музыки.",
                            "target_lang_translations": {
                                "en": "The neighbors raised a <w>ruckus</w> again because of the loud music."
                            }
                        },
                        {
                            "text": "Зачем устраивать <w>хай</w> на пустом месте?",
                            "target_lang_translations": {
                                "en": "Why make a <w>fuss</w> over nothing?"
                            }
                        }
                    ],
                    "synonyms": [
                        "скандал",
                        "шум",
                        "крик",
                        "буча",
                        "тарарам",
                        "дебош",
                        "драка"
                    ],
                    "antonyms": [
                        "тишина",
                        "спокойствие"
                    ],
                    "common_phrases": [
                        "поднять хай"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A loud scandal, swearing, noise, shouting, or disorder."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ruckus",
                                "target_lang_sense_clarification": "loud noise or disturbance"
                            },
                            {
                                "target_lang_word": "fuss",
                                "target_lang_sense_clarification": "needless excitement or activity"
                            },
                            {
                                "target_lang_word": "row",
                                "target_lang_sense_clarification": "noisy argument"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a0e79ef-01ae-3a41-8507-bcdc2cab1260",
                    "sense_definition": "Состояние опьянения, эйфории или возбуждения, обычно вызванное приёмом наркотических веществ.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "drug_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он пытался поймать <w>хай</w>, но чувствовал только усталость.",
                            "target_lang_translations": {
                                "en": "He tried to catch a <w>high</w>, but felt only fatigue."
                            }
                        },
                        {
                            "text": "В погоне за вечным <w>хаем</w> он потерял всё.",
                            "target_lang_translations": {
                                "en": "In pursuit of the eternal <w>high</w>, he lost everything."
                            }
                        }
                    ],
                    "synonyms": [
                        "крик"
                    ],
                    "common_phrases": [
                        "словить хай"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A state of intoxication, euphoria, or excitement, usually caused by taking narcotic substances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "high",
                                "target_lang_sense_clarification": "drug-induced euphoria"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "0fc19535-7bf2-3d12-b108-f0fb04a917b4",
                    "sense_definition": "Неформальное дружеское приветствие (от английского «hi»).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— <w>Хай</w>! — весело крикнул он приятелю через улицу.",
                            "target_lang_translations": {
                                "en": "— <w>Hi</w>! — he shouted cheerfully to his friend across the street."
                            }
                        },
                        {
                            "text": "Всем <w>хай</w>, я уже пришёл!",
                            "target_lang_translations": {
                                "en": "<w>Hi</w> everyone, I'm already here!"
                            }
                        }
                    ],
                    "synonyms": [
                        "потасовка"
                    ],
                    "common_phrases": [
                        "всем хай"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "youth slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An informal friendly greeting (derived from English \"hi\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hi",
                                "target_lang_sense_clarification": "informal greeting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdbf3c99-78c4-3870-985d-b0c14843a7e9",
                    "sense_definition": "Частица, выражающая побуждение, согласие или допущение; то же, что «пусть» или «пускай».",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "particle_let",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Хай</w> говорят что хотят, мне всё равно.",
                            "target_lang_translations": {
                                "en": "<w>Let</w> them say what they want, I don't care."
                            }
                        },
                        {
                            "text": "Не трогай кота, <w>хай</w> спит.",
                            "target_lang_translations": {
                                "en": "Don't touch the cat, <w>let</w> him sleep."
                            }
                        }
                    ],
                    "common_phrases": [
                        "хай будет",
                        "хай живёт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "common in South Russia"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A particle expressing a command, agreement, or assumption; equivalent to \"let\" or \"let it be\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let",
                                "target_lang_sense_clarification": "particle allowing something to happen"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

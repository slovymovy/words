    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0a5cdab-88f6-329d-a1f3-694046e8a360",
                    "sense_definition": "A long stick with a sharp tip used as a weapon for throwing or thrusting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weapon_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient warrior raised his <w>spear</w> to signal the attack.",
                            "target_lang_translations": {
                                "ru": "Древний воин поднял своё <w>копьё</w>, чтобы дать сигнал к атаке."
                            }
                        },
                        {
                            "text": "They found a rusty <w>spear</w> buried in the muddy riverbank.",
                            "target_lang_translations": {
                                "ru": "Они нашли ржавое <w>копьё</w>, зарытое в илистом берегу реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "blank",
                        "cut"
                    ],
                    "common_phrases": [
                        "throw a spear",
                        "hunting spear",
                        "sharp spear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная палка с острым наконечником, используемая как оружие для метания или колющих ударов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копьё",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "пика",
                                "target_lang_sense_clarification": "длинное колющее оружие"
                            },
                            {
                                "target_lang_word": "дротик",
                                "target_lang_sense_clarification": "метательное копьё"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c96c55fa-3f08-37c9-968e-9cba66bce54b",
                    "sense_definition": "A long, thin stalk or shoot of a vegetable, especially asparagus or broccoli.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vegetable_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please serve the grilled asparagus <w>spears</w> on a separate plate.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подайте жареные <w>побеги</w> спаржи на отдельной тарелке."
                            }
                        },
                        {
                            "text": "She chopped the broccoli <w>spears</w> into small pieces for the salad.",
                            "target_lang_translations": {
                                "ru": "Она нарезала <w>стебли</w> брокколи на мелкие кусочки для салата."
                            }
                        }
                    ],
                    "synonyms": [
                        "blank",
                        "cut"
                    ],
                    "common_phrases": [
                        "asparagus spear",
                        "broccoli spear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный тонкий стебель или побег овоща, особенно спаржи или брокколи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побег",
                                "target_lang_sense_clarification": "росток растения (спаржи)"
                            },
                            {
                                "target_lang_word": "стебель",
                                "target_lang_sense_clarification": "часть растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a59384e8-42b7-3a14-9870-834dd8080bca",
                    "sense_definition": "A soldier armed with a spear.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "weapon_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king sent a thousand <w>spears</w> to defend the northern border.",
                            "target_lang_translations": {
                                "ru": "Король послал тысячу <w>копий</w> защищать северную границу."
                            }
                        },
                        {
                            "text": "Every <w>spear</w> in the army was ready for the coming battle.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>копейщик</w> в армии был готов к предстоящей битве."
                            }
                        }
                    ],
                    "synonyms": [
                        "blank",
                        "cut"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical or literary context"
                        }
                    ],
                    "target_lang_definitions": {

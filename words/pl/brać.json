    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "723e480b-ed7e-345d-91f1-5fad62703a81",
                    "sense_definition": "Chwytać coś dłonią, aby to podnieść, przenieść lub zatrzymać przy sobie",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mogę <w>wziąć</w> ten długopis ze stołu?",
                            "target_lang_translations": {
                                "ru": "Могу я <w>взять</w> эту ручку со стола?"
                            }
                        },
                        {
                            "text": "Zawsze <w>biorę</w> ze sobą parasol, gdy pada deszcz.",
                            "target_lang_translations": {
                                "ru": "Я всегда <w>беру</w> с собой зонт, когда идет дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwytać",
                        "wziąć",
                        "zabrać"
                    ],
                    "antonyms": [
                        "dostać"
                    ],
                    "common_phrases": [
                        "brać do ręki",
                        "brać na ręce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хватать что-то рукой, чтобы поднять, перенести или оставить у себя"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "физическое действие"
                            },
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4fc5e00-6620-3b91-bcec-d030a15c72f4",
                    "sense_definition": "Przyjmować do organizmu lekarstwa lub inne substancje",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ingestion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent musi regularnie <w>brać</w> przepisane leki.",
                            "target_lang_translations": {
                                "ru": "Пациент должен регулярно <w>принимать</w> прописанные лекарства."
                            }
                        },
                        {
                            "text": "Ona nie <w>bierze</w> żadnych witamin ani suplementów.",
                            "target_lang_translations": {
                                "ru": "Она не <w>принимает</w> никаких витаминов и добавок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brać leki",
                        "brać tabletki",
                        "brać antybiotyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать внутрь лекарства или другие вещества"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "лекарства"
                            },
                            {
                                "target_lang_word": "пить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab7ff17c-a53a-3d86-9a73-c7bd81e8f315",
                    "sense_definition": "Wykonywać czynność określoną przez następujący rzeczownik (używane w stałych zwrotach)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "light_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano <w>biorę</w> szybki prysznic przed pracą.",
                            "target_lang_translations": {
                                "ru": "Каждое утро перед работой я быстро <w>принимаю</w> душ."
                            }
                        },
                        {
                            "text": "Moi przyjaciele <w>biorą</w> ślub w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Мои друзья <w>женятся</w> в следующем месяце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brać prysznic",
                        "brać ślub",
                        "brać kąpiel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять действие, обозначенное следующим существительным (используется в устойчивых выражениях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "душ, ванну"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c389b4a-19d6-3d84-a903-7913cfb2942b",
                    "sense_definition": "Uczestniczyć w jakimś wydarzeniu lub działaniu (w zwrocie 'brać udział')",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "participation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy uczniowie chętnie <w>biorą</w> udział w zawodach sportowych.",
                            "target_lang_translations": {
                                "ru": "Все ученики охотно <w>принимают</w> участие в спортивных соревнованиях."
                            }
                        },
                        {
                            "text": "Nie chcę <w>brać</w> udziału w tej kłótni.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>принимать</w> участия в этой ссоре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brać udział"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участвовать в каком-либо событии или действии (в выражении 'brać udział')"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "участие"
                            },
                            {
                                "target_lang_word": "участвовать",
                                "target_lang_sense_clarification": "глагол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffee86e4-dbe6-30f6-89ee-cfbfc13b0300",
                    "sense_definition": "Żądać lub otrzymywać pieniądze w zamian za towary lub usługi",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mechanik dużo <w>bierze</w> za naprawę samochodu.",
                            "target_lang_translations": {
                                "ru": "Этот механик много <w>берет</w> за ремонт машины."
                            }
                        },
                        {
                            "text": "Nie powinniśmy <w>brać</w> pieniędzy od rodziców na drobne wydatki.",
                            "target_lang_translations": {
                                "ru": "Мы не должны <w>брать</w> деньги у родителей на мелкие расходы."
                            }
                        }
                    ],
                    "antonyms": [
                        "dostać"
                    ],
                    "common_phrases": [
                        "brać pieniądze",
                        "ile bierzesz",
                        "brać łapówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать или получать деньги в обмен на товары или услуги"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "деньги, плату"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9e7caec-e4b6-3cce-9d99-653ba932f7d1",
                    "sense_definition": "Rozpoczynać wykonywanie jakiejś pracy lub czynności (jako czasownik zwrotny 'brać się')",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <w>brać</w> się do roboty, bo mamy mało czasu.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>браться</w> за работу, так как у нас мало времени."
                            }
                        },
                        {
                            "text": "Nareszcie <w>biorę</w> się za naukę języka hiszpańskiego.",
                            "target_lang_translations": {
                                "ru": "Наконец-то я <w>берусь</w> за изучение испанского языка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brać się do roboty",
                        "brać się za siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать выполнять какую-либо работу или действие (как возвратный глагол 'brać się')"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браться",
                                "target_lang_sense_clarification": "за дело"
                            },
                            {
                                "target_lang_word": "приниматься",
                                "target_lang_sense_clarification": "за работу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a305741e-4ef9-3d21-a333-374cc539aad4",
                    "sense_definition": "Błędnie sądzić, że ktoś jest kimś innym lub ma inne cechy",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludzie często <w>biorą</w> mnie za moją starszą siostrę.",
                            "target_lang_translations": {
                                "ru": "Люди часто <w>принимают</w> меня за мою старшую сестру."
                            }
                        },
                        {
                            "text": "Nie <w>bierz</w> wszystkiego, co on mówi, za prawdę.",
                            "target_lang_translations": {
                                "ru": "Не <w>принимай</w> всё, что он говорит, за правду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brać za kogoś",
                        "brać na poważnie",
                        "brać do siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ошибочно полагать, что кто-то является кем-то другим или обладает иными качествами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "за кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15fd288c-742e-3e08-bdca-cb2f8ef79203",
                    "sense_definition": "Mieć swoje źródło lub przyczynę w czymś (jako czasownik zwrotny 'brać się')",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, skąd <w>biorą</w> się u niego takie dziwne pomysły.",
                            "target_lang_translations": {
                                "ru": "Не знаю, откуда у него <w>берутся</w> такие странные идеи."
                            }
                        },
                        {
                            "text": "Wiele problemów zdrowotnych <w>bierze</w> się ze stresu.",
                            "target_lang_translations": {
                                "ru": "Многие проблемы со здоровьем <w>берутся</w> из-за стресса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skąd to się bierze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь свой источник или причину в чем-либо (как возвратный глагол 'brać się')"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браться",
                                "target_lang_sense_clarification": "появляться"
                            },
                            {
                                "target_lang_word": "возникать",
                                "target_lang_sense_clarification": "происходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe90c2eb-4549-30ba-a575-c798d16014ed",
                    "sense_definition": "O rybach: atakować i chwytać przynętę na wędce",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzisiaj ryby w ogóle nie <w>biorą</w> w tym jeziorze.",
                            "target_lang_translations": {
                                "ru": "Сегодня рыба в этом озере вообще не <w>клюет</w>."
                            }
                        },
                        {
                            "text": "Kiedy najlepiej <w>biorą</w> szczupaki?",
                            "target_lang_translations": {
                                "ru": "Когда лучше всего <w>клюют</w> щуки?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ryba bierze"
                    ],
                    "target_lang_definitions": {
                        "ru": "О рыбах: хватать наживку на удочке"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клевать",
                                "target_lang_sense_clarification": "о рыбе"
                            },
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "наживку"
                            }
                        ]
                    }
                }
            ]
        },

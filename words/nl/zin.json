                    "synonyms": [
                        "lust",
                        "trek",
                        "goesting",
                        "animo"
                    ],
                    "common_phrases": [
                        "zin hebben in",
                        "zin maken",
                        "zin krijgen"
                    ],
                    "target_lang_definitions": {

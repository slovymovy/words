    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "df157027-22e9-3f56-9137-012bae5ec976",
                    "sense_definition": "Niezgodny ze zdrowym rozsądkiem, pozbawiony sensu lub logiki; taki, który wydaje się głupi lub śmieszny.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "nonsense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest zupełnie <w>absurdalny</w> pomysł, którego nie da się zrealizować.",
                            "target_lang_translations": {
                                "ru": "Это совершенно <w>абсурдная</w> идея, которую невозможно реализовать."
                            }
                        },
                        {
                            "text": "NFZ w <w>absurdalny</w> sposób rozlicza się ze szpitalami.",
                            "target_lang_translations": {
                                "ru": "НФЗ <w>абсурдным</w> образом рассчитывается с больницами."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezsensowny",
                        "niedorzeczny",
                        "nonsensowny",
                        "bzdurny",
                        "bez sensu"
                    ],
                    "antonyms": [
                        "sensowny"
                    ],
                    "common_phrases": [
                        "absurdalny pomysł",
                        "absurdalna sytuacja",
                        "w absurdalny sposób",
                        "absurdalne zarzuty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Противоречащий здравому смыслу, лишенный смысла или логики; кажущийся глупым или смешным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абсурдный",
                                "target_lang_sense_clarification": "лишенный смысла"
                            },
                            {
                                "target_lang_word": "нелепый",
                                "target_lang_sense_clarification": "глупый, смешной"
                            },
                            {
                                "target_lang_word": "бессмысленный",
                                "target_lang_sense_clarification": "не имеющий смысла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21452aa9-3522-3300-b65f-614377cffdd5",
                    "sense_definition": "Bardzo duży, przesadny lub wygórowany (zwykle w odniesieniu do ceny, kosztów lub ilości).",

                            },
                            {
                                "target_lang_word": "gloom",
                                "target_lang_sense_clarification": "state of depression or sadness"
                            },
                            {
                                "target_lang_word": "obscurity",
                                "target_lang_sense_clarification": "lack of knowledge or clarity"
                            }
                        ]
                    }
                }
            ]
        }

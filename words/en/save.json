    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "32f09017-7ec5-3da5-95db-5e80791800be",
                    "sense_definition": "To rescue someone or something from danger, harm, or death.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "rescue_protect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The firefighters managed to <w>save</w> the family from the burning house.",
                            "target_lang_translations": {
                                "ru": "Пожарным удалось <w>спасти</w> семью из горящего дома."
                            }
                        },
                        {
                            "text": "She jumped into the river to <w>save</w> the drowning dog.",
                            "target_lang_translations": {
                                "ru": "Она прыгнула в реку, чтобы <w>спасти</w> тонущую собаку."
                            }
                        }
                    ],
                    "synonyms": [
                        "barring",
                        "except for",
                        "save for",
                        "except"
                    ],
                    "antonyms": [
                        "endanger",
                        "harm"
                    ],
                    "common_phrases": [
                        "save a life",
                        "save the day",
                        "save from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спасать кого-то или что-то от опасности, вреда или смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спасать",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "спасти",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51cb04ea-54d6-3d40-a4e5-6062a124366b",
                    "sense_definition": "To keep something to use it later.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "keep_store",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>save</w> a seat for me next to you.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>займи</w> для меня место рядом с собой."
                            }
                        },
                        {
                            "text": "I will <w>save</w> the rest of the cake for tomorrow.",
                            "target_lang_translations": {
                                "ru": "Я <w>оставлю</w> остаток торта на завтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "Sabi",
                        "saving",
                        "barring",
                        "except for",
                        "save for",
                        "except"
                    ],
                    "antonyms": [
                        "discard",
                        "use"
                    ],
                    "common_phrases": [
                        "save for later",
                        "save space"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять или сохранять что-то для использования в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "приберечь"
                            },
                            {
                                "target_lang_word": "сохранить",
                                "target_lang_sense_clarification": "сберечь"
                            },
                            {
                                "target_lang_word": "приберечь",
                                "target_lang_sense_clarification": "оставить про запас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37da34da-3f2e-33d8-9a26-68fbe7e798fb",
                    "sense_definition": "To accumulate money for future use.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am trying to <w>save</w> money to buy a new car.",
                            "target_lang_translations": {
                                "ru": "Я пытаюсь <w>копить</w> деньги, чтобы купить новую машину."
                            }
                        },
                        {
                            "text": "He <w>saved</w> up for months to afford the trip.",
                            "target_lang_translations": {
                                "ru": "Он <w>копил</w> несколько месяцев, чтобы позволить себе эту поездку."
                            }
                        }
                    ],
                    "antonyms": [
                        "spend",
                        "waste"
                    ],
                    "common_phrases": [
                        "save money",
                        "save up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Накапливать деньги для будущего использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копить",
                                "target_lang_sense_clarification": "собирать деньги"
                            },
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "сберегать часть дохода"
                            },
                            {
                                "target_lang_word": "накопить",
                                "target_lang_sense_clarification": "собрать определенную сумму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f473c7e-14fe-3ab3-b463-87b29a87f256",
                    "sense_definition": "To record data to a storage medium like a disk or cloud.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't forget to <w>save</w> your document before closing the program.",
                            "target_lang_translations": {
                                "ru": "Не забудь <w>сохранить</w> свой документ перед закрытием программы."
                            }
                        },
                        {
                            "text": "I need to <w>save</w> these photos to my hard drive.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>сохранить</w> эти фотографии на жесткий диск."
                            }
                        }
                    ],
                    "common_phrases": [
                        "save a file",
                        "save changes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записывать данные на носитель информации, такой как диск или облако."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохранять",
                                "target_lang_sense_clarification": "записывать файл"
                            },
                            {
                                "target_lang_word": "записать",
                                "target_lang_sense_clarification": "на диск или устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01e92394-bb4f-3c95-b60f-8117de466a8c",
                    "sense_definition": "To use less of something to prevent waste.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conserve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We should turn off the lights to <w>save</w> electricity.",
                            "target_lang_translations": {
                                "ru": "Нам следует выключать свет, чтобы <w>экономить</w> электричество."
                            }
                        },
                        {
                            "text": "Taking the shortcut will <w>save</w> us a lot of time.",
                            "target_lang_translations": {
                                "ru": "Использование короткого пути <w>сэкономит</w> нам кучу времени."
                            }
                        }
                    ],
                    "antonyms": [
                        "waste"
                    ],
                    "common_phrases": [
                        "save time",
                        "save energy",
                        "save water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать меньше чего-либо, чтобы предотвратить растрату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экономить",
                                "target_lang_sense_clarification": "бережно расходовать"
                            },
                            {
                                "target_lang_word": "сберечь",
                                "target_lang_sense_clarification": "сохранить ресурсы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2230dc50-8106-3109-9ab6-adbd2bf12a3a",
                    "sense_definition": "To prevent the need for someone to do something unpleasant or difficult.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will drive you there to <w>save</w> you the long walk.",
                            "target_lang_translations": {
                                "ru": "Я отвезу тебя туда, чтобы <w>избавить</w> тебя от долгой прогулки."
                            }
                        },
                        {
                            "text": "Using this machine <w>saves</w> a lot of hard work.",
                            "target_lang_translations": {
                                "ru": "Использование этой машины <w>избавляет</w> от тяжелой работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "save the trouble",
                        "save the hassle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избавлять кого-то от необходимости делать что-то неприятное или трудное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избавить",
                                "target_lang_sense_clarification": "освободить от хлопот"
                            },
                            {
                                "target_lang_word": "сберечь",
                                "target_lang_sense_clarification": "силы или время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c196b86b-623d-3f84-a797-ef8d9abf5fef",
                    "sense_definition": "To stop a ball or puck from entering the goal in sports.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The goalkeeper <w>saved</w> the penalty kick at the last second.",
                            "target_lang_translations": {
                                "ru": "Вратарь <w>отбил</w> пенальти в последнюю секунду."
                            }
                        },
                        {
                            "text": "He managed to <w>save</w> the shot with his foot.",
                            "target_lang_translations": {
                                "ru": "Ему удалось <w>отбить</w> удар ногой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "save a goal",
                        "save a penalty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановить мяч или шайбу, не дав им попасть в ворота (в спорте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отбить",
                                "target_lang_sense_clarification": "удар по воротам"
                            },
                            {
                                "target_lang_word": "спасти",
                                "target_lang_sense_clarification": "ворота"
                            },
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "мяч"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2686ae0c-0c3a-313d-a8a1-f7e6e1ea3f92",
                    "sense_definition": "To keep something safe; to safeguard.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rescue_protect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The national anthem begins with the words God <w>save</w> the Queen.",
                            "target_lang_translations": {
                                "ru": "Государственный гимн начинается со слов Боже, <w>храни</w> Королеву."
                            }
                        },
                        {
                            "text": "They fought to <w>save</w> their country from invasion.",
                            "target_lang_translations": {
                                "ru": "Они сражались, чтобы <w>спасти</w> свою страну от вторжения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "God save the Queen",
                        "save our souls"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беречь что-то, обеспечивать безопасность; хранить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хранить",
                                "target_lang_sense_clarification": "оберегать"
                            },
                            {
                                "target_lang_word": "спасать",
                                "target_lang_sense_clarification": "защищать страну/людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5255b79-63be-36c7-96c9-5fe9a097f107",
                    "sense_definition": "To stop oneself from saying something; to keep silent about.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "silence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>save</w> your excuses because I do not believe you.",
                            "target_lang_translations": {
                                "ru": "Можешь <w>оставить</w> свои оправдания при себе, потому что я тебе не верю."
                            }
                        },
                        {
                            "text": "<w>Save</w> your breath, he is not going to listen to anyone.",
                            "target_lang_translations": {
                                "ru": "<w>Побереги</w> дыхание, он никого не станет слушать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "save it",
                        "save your breath"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in imperative phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздержаться от высказывания; промолчать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приберечь",
                                "target_lang_sense_clarification": "слова или аргументы"
                            },
                            {
                                "target_lang_word": "оставить при себе",
                                "target_lang_sense_clarification": "мнение или оправдания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b826b8cb-5448-3af2-96ac-159261d48395",
                    "sense_definition": "To redeem someone from sin or eternal damnation.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "theology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The preacher spoke about how faith can <w>save</w> the soul.",
                            "target_lang_translations": {
                                "ru": "Проповедник говорил о том, как вера может <w>спасти</w> душу."
                            }
                        },
                        {
                            "text": "They prayed to be <w>saved</w> from their sins.",
                            "target_lang_translations": {
                                "ru": "Они молились о том, чтобы <w>спастись</w> от своих грехов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Избавлять кого-то от греха или вечного проклятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спасать",
                                "target_lang_sense_clarification": "душу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b1f5b39-ca9e-36c0-92fe-e076a9389953",
                    "sense_definition": "The act or result of saving data to a storage medium.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The game performs an automatic <w>save</w> after each level.",

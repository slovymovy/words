{
    "word_family": [
        "dufheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "93ba08db-139c-391f-94ea-e86d85210d6d",
                    "sense_definition": "Saai, oninteressant en zonder levendigheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "boring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De film was zo <w>duf</w> dat ik halverwege in slaap viel.",
                            "target_lang_translations": {
                                "en": "The movie was so <w>boring</w> that I fell asleep halfway through.",
                                "ru": "Фильм был таким <w>скучным</w>, что я заснул на середине."
                            }
                        },
                        {
                            "text": "We belandden op een <w>duf</w> feestje waar niemand aan het dansen was.",
                            "target_lang_translations": {
                                "en": "We ended up at a <w>dull</w> party where no one was dancing.",
                                "ru": "Мы попали на <w>унылую</w> вечеринку, где никто не танцевал."
                            }
                        }
                    ],
                    "synonyms": [
                        "saai",
                        "oninteressant"
                    ],
                    "antonyms": [
                        "boeiend",
                        "spannend"
                    ],
                    "common_phrases": [
                        "een duffe boel",
                        "een duf verhaal"
                    ],
                    "target_lang_definitions": {
                        "en": "Boring, uninteresting, and lacking liveliness.",
                        "ru": "Скучный, неинтересный и лишенный живости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boring",
                                "target_lang_sense_clarification": "causing boredom; not interesting"
                            },
                            {
                                "target_lang_word": "dull",
                                "target_lang_sense_clarification": "lacking interest, excitement, or brightness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "скучный",
                                "target_lang_sense_clarification": "неинтересный, наводящий тоску"
                            },
                            {
                                "target_lang_word": "унылый",
                                "target_lang_sense_clarification": "безрадостный, вялый (о событии или атмосфере)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3232c3e4-1985-31d8-9370-e98cb794290d",
                    "sense_definition": "Niet helder kunnen denken; slaperig of suf.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "drowsy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik voel me vandaag erg <w>duf</w> omdat ik vannacht slecht heb geslapen.",
                            "target_lang_translations": {
                                "en": "I feel very <w>groggy</w> today because I slept poorly last night.",
                                "ru": "Я чувствую себя сегодня очень <w>вялым</w>, потому что плохо спал ночью."
                            }
                        },
                        {
                            "text": "Door de zware verkoudheid is mijn hoofd de hele dag <w>duf</w>.",
                            "target_lang_translations": {
                                "en": "Because of the bad cold, my head has been <w>muzzy</w> all day.",
                                "ru": "Из-за сильной простуды у меня весь день <w>мутная</w> голова."
                            }
                        }
                    ],
                    "synonyms": [
                        "suf"
                    ],
                    "antonyms": [
                        "helder",
                        "fit"
                    ],
                    "common_phrases": [
                        "zich duf voelen",
                        "duf uit de ogen kijken"
                    ],
                    "target_lang_definitions": {
                        "en": "Unable to think clearly; sleepy or groggy.",
                        "ru": "Неспособность ясно мыслить; сонливость или вялость."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "groggy",
                                "target_lang_sense_clarification": "dazed, weak, or unsteady from illness or lack of sleep"
                            },
                            {
                                "target_lang_word": "drowsy",
                                "target_lang_sense_clarification": "sleepy and lethargic; half-asleep"
                            },
                            {
                                "target_lang_word": "muzzy",
                                "target_lang_sense_clarification": "confused, indistinct, or foggy in the mind"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вялый",
                                "target_lang_sense_clarification": "лишенный энергии, заторможенный"
                            },
                            {
                                "target_lang_word": "мутный",
                                "target_lang_sense_clarification": "о голове: неясно соображающий, тяжелый"
                            },
                            {
                                "target_lang_word": "сонный",
                                "target_lang_sense_clarification": "хотящий спать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0aee40ae-8ed3-3fd8-b606-cbf5a0c7b1b6",
                    "sense_definition": "Niet fris ruikend door gebrek aan ventilatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "smell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het ruikt hier een beetje <w>duf</w>, zullen we een raam openzetten?",
                            "target_lang_translations": {
                                "en": "It smells a bit <w>musty</w> in here, shall we open a window?",
                                "ru": "Здесь немного пахнет <w>затхлостью</w>, может, откроем окно?"
                            }
                        },
                        {
                            "text": "Na jaren op zolder te hebben gelegen, roken de oude kleren erg <w>duf</w>.",
                            "target_lang_translations": {
                                "en": "After lying in the attic for years, the old clothes smelled very <w>musty</w>.",
                                "ru": "Пролежав годы на чердаке, старая одежда пахла очень <w>затхло</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "muf",
                        "bedompt"
                    ],
                    "antonyms": [
                        "fris"
                    ],
                    "common_phrases": [
                        "een duffe geur",
                        "duf ruiken"
                    ],
                    "target_lang_definitions": {
                        "en": "Not smelling fresh due to lack of ventilation.",
                        "ru": "Несвежий запах из-за отсутствия вентиляции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "musty",
                                "target_lang_sense_clarification": "having a stale, damp, or moldy smell"
                            },
                            {
                                "target_lang_word": "stuffy",
                                "target_lang_sense_clarification": "lacking fresh air or ventilation"
                            },
                            {
                                "target_lang_word": "stale",
                                "target_lang_sense_clarification": "no longer fresh; smelling old"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "затхлый",
                                "target_lang_sense_clarification": "о запахе: сырой, залежалый"
                            },
                            {
                                "target_lang_word": "спертый",
                                "target_lang_sense_clarification": "о воздухе: душный, несвежий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
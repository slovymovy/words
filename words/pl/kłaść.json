    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "48ca085e-2ef2-3602-a562-2e3268b1d416",
                    "sense_definition": "Umieszczać przedmiot w pozycji poziomej lub płaskiej na jakiejś powierzchni.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "placement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <kładę> klucze na szafce przy wejściu, żeby ich nie zgubić.",
                            "target_lang_translations": {
                                "ru": "Я всегда <кладу> ключи на шкафчик у входа, чтобы их не потерять."
                            }
                        },
                        {
                            "text": "Gdzie <kładziesz> te ciężkie torby z zakupami?",
                            "target_lang_translations": {
                                "ru": "Куда ты <кладешь> эти тяжелые сумки с покупками?"
                            }
                        }
                    ],
                    "synonyms": [
                        "umieszczać",
                        "stawiać",
                        "wkładać"
                    ],
                    "antonyms": [
                        "podnosić"
                    ],
                    "common_phrases": [
                        "kłaść na stole",
                        "kłaść książkę",
                        "kłaść ręce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать предмет в горизонтальное положение на какую-либо поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "помещать горизонтально"
                            },
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f576f4e-04d3-320a-a307-fc3779a73d75",
                    "sense_definition": "Przyjmować pozycję leżącą; kłaść własne ciało na czymś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem bardzo zmęczony, więc <kładę się> na chwilę na kanapie.",
                            "target_lang_translations": {
                                "ru": "Я очень устал, поэтому <ложусь> на минуту на диван."
                            }
                        },
                        {
                            "text": "Pies <kładł się> pod stołem, czekając na jedzenie.",
                            "target_lang_translations": {
                                "ru": "Собака <ложилась> под стол в ожидании еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokładać się"
                    ],
                    "antonyms": [
                        "wstawać",
                        "podnosić się"
                    ],
                    "common_phrases": [
                        "kłaść się na plecach",
                        "kłaść się na boku",
                        "kłaść się na ziemi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать лежачее положение; ложиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ложиться",
                                "target_lang_sense_clarification": "о человеке или животном"
                            },
                            {
                                "target_lang_word": "укладываться",
                                "target_lang_sense_clarification": "устраиваться лежать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "423caa95-f5db-3e33-b646-647ec1330908",
                    "sense_definition": "Iść do łóżka w celu zaśnięcia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zazwyczaj <kładziemy się> spać przed północą, bo rano wcześnie wstajemy.",
                            "target_lang_translations": {
                                "ru": "Мы обычно <ложимся> спать до полуночи, так как утром рано встаем."
                            }
                        },
                        {
                            "text": "Dzieci muszą <kłaść się> wcześniej, żeby być wypoczęte do szkoły.",
                            "target_lang_translations": {
                                "ru": "Дети должны <ложиться> раньше, чтобы выспаться к школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "iść spać"
                    ],
                    "antonyms": [
                        "wstawać"
                    ],
                    "common_phrases": [
                        "kłaść się spać",
                        "kłaść się wcześnie",
                        "kłaść się późno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти в постель с целью уснуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ложиться спать",
                                "target_lang_sense_clarification": "отходить ко сну"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17a67dc8-f13c-3c7c-aba0-9e522325bd5e",
                    "sense_definition": "Pomagać komuś, zwłaszcza dziecku lub choremu, przyjąć pozycję leżącą, często w celu snu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "placement_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka codziennie <kładzie> dzieci spać o ósmej wieczorem.",
                            "target_lang_translations": {
                                "ru": "Мать каждый день <укладывает> детей спать в восемь вечера."
                            }
                        },
                        {
                            "text": "Pielęgniarka ostrożnie <kładła> pacjenta na łóżku szpitalnym.",
                            "target_lang_translations": {
                                "ru": "Медсестра осторожно <укладывала> пациента на больничную койку."
                            }
                        }
                    ],
                    "synonyms": [
                        "układać"
                    ],
                    "antonyms": [
                        "podnosić"
                    ],
                    "common_phrases": [
                        "kłaść dzieci spać",
                        "kłaść do łóżka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помогать кому-то принять лежачее положение, обычно для сна или отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "помогать лечь, готовить ко сну"
                            },
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "помещать в больницу или на кровать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f26dbe35-34c7-36fb-ab0b-71ae5ca7d80b",
                    "sense_definition": "Wykonywać prace wykończeniowe polegające na montowaniu elementów na powierzchni lub układać coś w określonym porządku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "construction_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad samodzielnie <kładzie> kafelki w swojej nowej łazience.",
                            "target_lang_translations": {
                                "ru": "Мой сосед самостоятельно <кладет> плитку в своей новой ванной."
                            }
                        },
                        {
                            "text": "Lubię wieczorami <kłaść> pasjansa, żeby się zrelaksować po pracy.",
                            "target_lang_translations": {
                                "ru": "Я люблю по вечерам <раскладывать> пасьянс, чтобы расслабиться после работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "układać"
                    ],
                    "common_phrases": [
                        "kłaść kafelki",
                        "kłaść podłogę",
                        "kłaść pasjansa",
                        "kłaść tynk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять отделочные работы (монтаж покрытия) или раскладывать что-то в определенном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "плитку, печь, стены"
                            },
                            {
                                "target_lang_word": "раскладывать",
                                "target_lang_sense_clarification": "пасьянс, карты"
                            },
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "пол, покрытие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a74f4c50-99f9-31a0-8e5f-cad88bc23515",
                    "sense_definition": "Pokrywać powierzchnię warstwą jakiejś substancji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "construction_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malarz <kładzie> drugą warstwę farby na ścianę w salonie.",
                            "target_lang_translations": {
                                "ru": "Маляр <наносит> второй слой краски на стену в гостиной."
                            }
                        },
                        {
                            "text": "Moja siostra <kładzie> makijaż bardzo starannie przed każdym wyjściem.",
                            "target_lang_translations": {
                                "ru": "Моя сестра очень тщательно <наносит> макияж перед каждым выходом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kłaść farbę",
                        "kłaść makijaż",
                        "kłaść gładź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность слоем какого-либо вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наносить",
                                "target_lang_sense_clarification": "макияж, краску"
                            },
                            {
                                "target_lang_word": "накладывать",
                                "target_lang_sense_clarification": "грим, слой вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad2f0077-221a-3bb9-927a-548c035b1529",
                    "sense_definition": "Powodować niepowodzenie jakiegoś przedsięwzięcia lub planu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nieodpowiedzialne zachowanie <kładzie> cały nasz projekt.",
                            "target_lang_translations": {
                                "ru": "Его безответственное поведение <губит> весь наш проект."
                            }
                        },
                        {
                            "text": "Brak funduszy <kładł> każdą nową inicjatywę dyrektora.",
                            "target_lang_translations": {
                                "ru": "Отсутствие средств <хоронило> каждую новую инициативу директора."
                            }
                        }
                    ],
                    "synonyms": [
                        "doprowadzać do upadku",
                        "utrącać"
                    ],
                    "antonyms": [
                        "wspierać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить к провалу какого-либо начинания или плана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "губить",
                                "target_lang_sense_clarification": "разрушать планы"
                            },
                            {
                                "target_lang_word": "проваливать",
                                "target_lang_sense_clarification": "дело, проект"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "reasonable",
        "unreasonable",
        "reasoning",
        "reasonably",
        "reasonableness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8e579ad-ede9-3abe-8d72-0059da8a21a2",
                    "sense_definition": "A cause or explanation that explains why something happens or why someone acts a certain way.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "causality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She explained the <w>reason</w> for her decision to leave the company.",
                            "target_lang_translations": {
                                "ru": "Она объяснила <w>причину</w> своего решения уйти из компании."
                            }
                        },
                        {
                            "text": "For this <w>reason</w>, we have decided to cancel the outdoor event.",
                            "target_lang_translations": {
                                "ru": "По этой <w>причине</w> мы решили отменить мероприятие на открытом воздухе."
                            }
                        },
                        {
                            "text": "There is no <w>reason</w> why we cannot finish the project on time.",
                            "target_lang_translations": {
                                "ru": "Нет никакой <w>причины</w>, по которой мы не можем закончить проект вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "motive",
                        "rationale",
                        "excuse",
                        "that which causes"
                    ],
                    "common_phrases": [
                        "the reason for",
                        "for this reason",
                        "the reason why",
                        "main reason",
                        "for some reason"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причина или объяснение того, почему что-то происходит или почему кто-то поступает определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причи́на",
                                "target_lang_sense_clarification": "явление, обстоятельство, вызывающее другое явление"
                            },
                            {
                                "target_lang_word": "основа́ние",
                                "target_lang_sense_clarification": "обоснование, оправдывающее действие или вывод"
                            },
                            {
                                "target_lang_word": "по́вод",
                                "target_lang_sense_clarification": "обстоятельство, способное стать предлогом для действия"
                            },
                            {
                                "target_lang_word": "резо́н",
                                "target_lang_sense_clarification": "разг. разумное основание, смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3907d936-6ea8-34d8-898f-7245571d4d45",
                    "sense_definition": "The mental ability to think, understand, and form judgments logically.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intellect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must use your <w>reason</w> to solve this difficult problem.",
                            "target_lang_translations": {
                                "ru": "Вы должны использовать свой <w>рассудок</w>, чтобы решить эту трудную задачу."
                            }
                        },
                        {
                            "text": "He refused to listen to <w>reason</w> and continued shouting at the manager.",
                            "target_lang_translations": {
                                "ru": "Он отказался прислушаться к <w>голосу разума</w> и продолжал кричать на менеджера."
                            }
                        }
                    ],
                    "synonyms": [
                        "rationale",
                        "rationale"
                    ],
                    "common_phrases": [
                        "lose one's reason",
                        "listen to reason",
                        "within reason",
                        "voice of reason",
                        "stand to reason"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умственная способность логически мыслить, понимать и формировать суждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ра́зум",
                                "target_lang_sense_clarification": "высшая ступень познавательной деятельности, способность мыслить"
                            },
                            {
                                "target_lang_word": "рассу́док",
                                "target_lang_sense_clarification": "способность здраво, логично размышлять; здравый смысл"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "def08054-6301-37c2-8578-b01ed1ca24e5",
                    "sense_definition": "To form a conclusion or judgment by thinking logically.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jury <w>reasoned</w> that the evidence was insufficient for a conviction.",
                            "target_lang_translations": {
                                "ru": "Присяжные <w>рассудили</w>, что улик недостаточно для вынесения обвинительного приговора."
                            }
                        },
                        {
                            "text": "We need to sit down and <w>reason</w> out a solution together.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сесть и <w>продумать</w> решение вместе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reason that",
                        "reason out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить к выводу или суждению путем логического мышления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассужда́ть",
                                "target_lang_sense_clarification": "мыслить, строить умозаключения"
                            },
                            {
                                "target_lang_word": "делать выводы",
                                "target_lang_sense_clarification": "приходить к заключению на основе фактов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdbe8d83-49f5-309b-bea3-0c12b418f8f9",
                    "sense_definition": "To talk to someone and use arguments to persuade them to be sensible.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police tried to <w>reason</w> with the angry crowd before taking action.",
                            "target_lang_translations": {
                                "ru": "Полиция пыталась <w>вразумить</w> разгневанную толпу, прежде чем предпринимать действия."
                            }
                        },
                        {
                            "text": "It is difficult to <w>reason</w> with someone who is so emotional.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>вразумить</w> человека, который настолько эмоционален."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reason with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговаривать с кем-то, используя аргументы, чтобы убедить его действовать разумно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вразумлять",
                                "target_lang_sense_clarification": "наставлять, убеждать действовать разумно"
                            },
                            {
                                "target_lang_word": "убеждать",
                                "target_lang_sense_clarification": "склонять к какому-либо мнению или поступку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "24ed156b-b468-341f-b27f-5503e91ba91b",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We met Mr. <w>Reason</w> at the conference last week.",
                            "target_lang_translations": {
                                "ru": "Мы встретили мистера <w>Ризона</w> на конференции на прошлой неделе."
                            }
                        },
                        {
                            "text": "The <w>Reason</w> family has lived in this town for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Ризон</w> живет в этом городе уже несколько поколений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ри́зон",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
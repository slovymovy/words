{
    "word_family": [
        "distract",
        "distraction",
        "distractedly",
        "distractive",
        "distractor"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7fa4e1b4-afc5-3b3b-85b9-753d48dae06c",
                    "sense_definition": "Unable to concentrate or focus because one's attention is drawn elsewhere.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attention_diverted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was too <w>distracted</w> by the noise outside to finish her homework on time.",
                            "target_lang_translations": {
                                "ru": "Она была слишком <w>отвлечена</w> шумом на улице, чтобы закончить домашнюю работу вовремя."
                            }
                        },
                        {
                            "text": "If you get <w>distracted</w> while driving, you might cause a serious accident.",
                            "target_lang_translations": {
                                "ru": "Если вы <w>отвлечётесь</w> за рулём, вы можете стать причиной серьёзной аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "preoccupied",
                        "inattentive",
                        "absent-minded",
                        "unfocused"
                    ],
                    "antonyms": [
                        "focused",
                        "attentive",
                        "alert",
                        "concentrated"
                    ],
                    "common_phrases": [
                        "easily distracted",
                        "get distracted",
                        "seem distracted",
                        "look distracted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неспособный сосредоточиться или сфокусироваться, так как внимание отвлечено на что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвлечённый",
                                "target_lang_sense_clarification": "состояние, когда внимание переключено на посторонний объект"
                            },
                            {
                                "target_lang_word": "рассеянный",
                                "target_lang_sense_clarification": "невнимательный, несобранный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "576f88d8-da05-376a-acc2-88cace44c490",
                    "sense_definition": "Extremely agitated, confused, or upset due to worry or grief.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>distracted</w> mother searched the entire park frantically for her missing child.",
                            "target_lang_translations": {
                                "ru": "<w>Обезумевшая</w> мать лихорадочно обыскивала весь парк в поисках пропавшего ребёнка."
                            }
                        },
                        {
                            "text": "He paced around the room, looking <w>distracted</w> and unable to settle down.",
                            "target_lang_translations": {
                                "ru": "Он ходил по комнате, выглядя <w>встревоженным</w> и неспособным успокоиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "distraught",
                        "frantic",
                        "agitated",
                        "frenzied"
                    ],
                    "antonyms": [
                        "calm",
                        "composed",
                        "serene"
                    ],
                    "common_phrases": [
                        "distracted with grief",
                        "distracted by worry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайне взволнованный, растерянный или расстроенный из-за беспокойства или горя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обезумевший",
                                "target_lang_sense_clarification": "потерявший рассудок от сильных эмоций (горя, страха)"
                            },
                            {
                                "target_lang_word": "встревоженный",
                                "target_lang_sense_clarification": "сильно обеспокоенный, не находящий себе места"
                            },
                            {
                                "target_lang_word": "растерянный",
                                "target_lang_sense_clarification": "сбитый с толку, не знающий, что делать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "53aa2f3c-2103-3db4-a3f9-a87d4ec532fd",

                    "synonyms": [
                        "schaapje"
                    ],
                    "common_phrases": [
                        "als een lam"
                    ],
                    "target_lang_definitions": {
                        "en": "The young of a sheep.",
                        "ru": "Детёныш овцы."
                    },
                    "translations": {

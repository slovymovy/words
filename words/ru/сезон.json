{
    "word_family": [
        "сезонный",
        "межсезонье"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "083f1c9d-771d-383a-8458-a225c935e900",
                    "sense_definition": "Одно из четырёх времён года (весна, лето, осень, зима).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этот <сезон> часто идут холодные дожди.",
                            "target_lang_translations": {
                                "en": "Cold rains often fall during this <season>."
                            }
                        },
                        {
                            "text": "Каждый <сезон> природа меняет свои краски и настроение.",
                            "target_lang_translations": {
                                "en": "Every <season> nature changes its colors and mood."
                            }
                        }
                    ],
                    "synonyms": [
                        "время года"
                    ],
                    "common_phrases": [
                        "тёплый сезон",
                        "холодный сезон"
                    ],
                    "target_lang_definitions": {
                        "en": "One of the four main periods of the year (spring, summer, autumn, winter)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "season",
                                "target_lang_sense_clarification": "time of year"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fe942a3-efc8-3894-b761-8fc21152c7df",
                    "sense_definition": "Период времени, наиболее подходящий для определённой деятельности, отдыха или работы.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы поедем на море, когда начнётся бархатный <сезон>.",
                            "target_lang_translations": {
                                "en": "We will go to the sea when the velvet <season> begins."
                            }
                        },
                        {
                            "text": "Грибной <сезон> в этом году начался довольно рано.",
                            "target_lang_translations": {
                                "en": "The mushroom <season> started quite early this year."
                            }
                        }
                    ],
                    "synonyms": [
                        "пора"
                    ],
                    "common_phrases": [
                        "бархатный сезон",
                        "курортный сезон",
                        "мёртвый сезон",
                        "открыть сезон",
                        "высокий сезон"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific period of time suitable for a particular activity, vacation, or work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "season",
                                "target_lang_sense_clarification": "period for activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7606c29-43ef-37ec-90e5-92da778cab8c",
                    "sense_definition": "Группа эпизодов телесериала или передачи, выходящая в эфир с определённой периодичностью.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сценаристы уже пишут интересный сюжет на следующий <сезон>.",
                            "target_lang_translations": {
                                "en": "The screenwriters are already writing an interesting plot for the next <season>."
                            }
                        },
                        {
                            "text": "Первый <сезон> этого сериала стал невероятно популярным.",
                            "target_lang_translations": {
                                "en": "The first <season> of this series became incredibly popular."
                            }
                        }
                    ],
                    "common_phrases": [
                        "новый сезон",
                        "финальный сезон"
                    ],
                    "target_lang_definitions": {
                        "en": "A set of episodes of a TV series broadcast within a specific period."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "season",
                                "target_lang_sense_clarification": "TV series"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "292f09b0-14ed-3966-8ffe-e1d15489374c",
                    "sense_definition": "Период года, в течение которого работают театры и концертные залы.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Большой театр торжественно открыл свой юбилейный <сезон>.",
                            "target_lang_translations": {
                                "en": "The Bolshoi Theatre ceremoniously opened its jubilee <season>."
                            }
                        },
                        {
                            "text": "В конце весны театральный <сезон> обычно заканчивается.",
                            "target_lang_translations": {
                                "en": "The theatrical <season> usually ends in late spring."
                            }
                        }
                    ],
                    "common_phrases": [
                        "театральный сезон",
                        "концертный сезон"
                    ],
                    "target_lang_definitions": {
                        "en": "The period of the year during which theaters and concert halls hold performances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "season",
                                "target_lang_sense_clarification": "theatrical/arts"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
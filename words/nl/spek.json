            "senses": [
                {
                    "sense_id": "38736459-38ff-32e5-86db-6af893c29851",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>spekken</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>spek</w> de kas van de vereniging met een gulle donatie.",
                            "target_lang_translations": {
                                "ru": "Я <w>пополняю</w> кассу ассоциации щедрым пожертвованием."
                            }
                        },
                        {
                            "text": "<w>Spek</w> het magere vlees met stukjes vet voor meer smaak.",
                            "target_lang_translations": {
                                "ru": "<w>Нашпигуй</w> постное мясо кусочками жира для вкуса."
                            }
                        }
                    ],
                    "common_phrases": [

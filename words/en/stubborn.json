    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ab4e1583-fe46-3086-a967-352071769ff4",
                    "sense_definition": "Refusing to change one's opinion or chosen course of action; difficult to move, remove, or cure.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother is so <w>stubborn</w> that he never listens to anyone's advice.",
                            "target_lang_translations": {
                                "ru": "Мой брат такой <w>упрямый</w>, что никогда не слушает ничьих советов."
                            }
                        },
                        {
                            "text": "She tried everything to remove the <w>stubborn</w> stain from the white shirt.",
                            "target_lang_translations": {
                                "ru": "Она перепробовала всё, чтобы вывести это <w>упорное</w> пятно с белой рубашки."
                            }
                        }
                    ],
                    "synonyms": [
                        "obstinate",
                        "headstrong",
                        "willful",
                        "unyielding",
                        "persistent",
                        "adamant"
                    ],
                    "antonyms": [
                        "flexible",
                        "compliant",
                        "yielding"
                    ],
                    "common_phrases": [
                        "stubborn as a mule",
                        "stubborn stain",
                        "stubborn resistance",
                        "stubborn pride"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказывающийся менять свое мнение или выбранный курс действий; трудно сдвинуть с места, удалить или исправить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упрямый",
                                "target_lang_sense_clarification": "о человеке или характере"
                            },
                            {
                                "target_lang_word": "упёртый",
                                "target_lang_sense_clarification": "разговорное; крайне неуступчивый"
                            },
                            {
                                "target_lang_word": "упорный",
                                "target_lang_sense_clarification": "о сопротивлении, фактах или пятнах"
                            },
                            {
                                "target_lang_word": "неусту́пчивый",
                                "target_lang_sense_clarification": "не желающий уступать"
                            }
                        ]

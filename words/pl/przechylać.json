{
    "word_family": [
        "przechylenie",
        "przechył",
        "chylić",
        "odchylać",
        "wychylać",
        "schylać",
        "nachylać",
        "uchylać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e3b9fa92-4dc5-3327-866f-a2f3a88b6c5c",
                    "sense_definition": "Zmieniać położenie przedmiotu lub części ciała z pionowego na ukośne; powodować, że coś się pochyla.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tilt_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner zaczął ostrożnie <przechylać> butelkę, aby nalać wina do kieliszka.",
                            "target_lang_translations": {
                                "ru": "Официант начал осторожно <наклонять> бутылку, чтобы налить вина в бокал."
                            }
                        },
                        {
                            "text": "Pies zaczął zabawnie <przechylać> głowę, słysząc nieznany dźwięk.",
                            "target_lang_translations": {
                                "ru": "Собака начала забавно <наклонять> голову, услышав незнакомый звук."
                            }
                        }
                    ],
                    "synonyms": [
                        "pochylać",
                        "nachylać"
                    ],
                    "antonyms": [
                        "prostować",
                        "pionizować"
                    ],
                    "common_phrases": [
                        "przechylać głowę",
                        "przechylać butelkę",
                        "przechylać szalę zwycięstwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять положение предмета или части тела с вертикального на наклонное; заставлять что-то наклониться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наклонять",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "склонять",
                                "target_lang_sense_clarification": "обычно о голове"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f82838e4-c342-366e-96bc-5b413847af3e",
                    "sense_definition": "Tracić pionową pozycję, chylić się lub zginać w jedną stronę (zwykle używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tilt_intransitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare drzewo zaczęło niebezpiecznie <przechylać> się pod naporem silnego wiatru.",
                            "target_lang_translations": {
                                "ru": "Старое дерево начало опасно <наклоняться> под напором сильного ветра."
                            }
                        },
                        {
                            "text": "Nie wolno <przechylać> się przez barierkę na balkonie, bo można wypaść.",
                            "target_lang_translations": {
                                "ru": "Нельзя <перегибаться> через перила на балконе, так как можно выпасть."
                            }
                        }
                    ],
                    "synonyms": [
                        "pochylać się",
                        "chylić się"
                    ],
                    "antonyms": [
                        "prostować się"
                    ],
                    "common_phrases": [
                        "przechylać się na bok",
                        "przechylać się do tyłu",
                        "przechylać się przez okno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять вертикальное положение, сгибаться в одну сторону (обычно возвратный глагол)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наклоняться",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "перегибаться",
                                "target_lang_sense_clarification": "через что-либо, например, через перила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9e00e61-01da-3a51-a822-c5b76c4fa937",
                    "sense_definition": "Kłaść się na bok pod wpływem wiatru lub fali (dotyczy statków i łodzi).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tilt_nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas sztormu statek zaczął gwałtownie <przechylać> się na lewą burtę.",
                            "target_lang_translations": {
                                "ru": "Во время шторма судно начало резко <крениться> на левый борт."
                            }
                        },
                        {
                            "text": "Silny podmuch wiatru sprawił, że jacht zaczął się mocno <przechylać>.",
                            "target_lang_translations": {
                                "ru": "Сильный порыв ветра заставил яхту сильно <крениться>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ложиться на бок под воздействием ветра или волн (о судах и лодках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крениться",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
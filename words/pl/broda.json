    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca5d101c-d3e5-323f-b9bc-2a0952b98584",
                    "sense_definition": "Zarost rosnący na dolnej części twarzy, występujący głównie u dorosłych mężczyzn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek ma długą i siwą <w>brodę</w>.",
                            "target_lang_translations": {
                                "ru": "У моего дедушки длинная и седая <w>борода</w>."
                            }
                        },
                        {
                            "text": "Mężczyzna postanowił zgolić <w>brodę</w> na lato.",
                            "target_lang_translations": {
                                "ru": "Мужчина решил сбрить <w>бороду</w> на лето."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gęsta broda",
                        "zapuszczać brodę",
                        "golić brodę",
                        "kozia broda",
                        "siwa broda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Волосяной покров на нижней части лица, растущий в основном у взрослых мужчин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борода",
                                "target_lang_sense_clarification": "волосы на лице"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c356d9c-688a-3080-9971-20fe2031db52",
                    "sense_definition": "Dolna, przednia część twarzy człowieka znajdująca się poniżej ust.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko oparło <w>brodę</w> na stole i patrzyło na nas.",
                            "target_lang_translations": {
                                "ru": "Ребёнок положил <w>подбородок</w> на стол и смотрел на нас."
                            }
                        },
                        {
                            "text": "Podczas upadku chłopiec uderzył się mocno w <w>brodę</w>.",
                            "target_lang_translations": {
                                "ru": "При падении мальчик сильно ударился <w>подбородком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "podbródek"
                    ],
                    "common_phrases": [
                        "zadzierać brodę",
                        "oprzeć brodę",
                        "dołek w brodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя передняя часть лица человека, находящаяся ниже рта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подбородок",
                                "target_lang_sense_clarification": "часть лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa200ba9-8210-313f-8064-5bf81eb40145",
                    "sense_definition": "Duża grupa pszczół gromadząca się na zewnątrz ula, zazwyczaj z powodu gorąca.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalne popołudnie pszczoły utworzyły wielką <w>brodę</w> przed wylotkiem.",
                            "target_lang_translations": {
                                "ru": "В жаркий полдень пчелы образовали большую <w>бороду</w> перед летком."
                            }
                        },
                        {
                            "text": "Pszczelarz zaniepokoił się, widząc <w>brodę</w> wiszącą pod ulem.",
                            "target_lang_translations": {
                                "ru": "Пчеловод забеспокоился, увидев <w>бороду</w>, висящую под ульем."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая группа пчел, собирающаяся снаружи улья, обычно из-за жары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "борода",
                                "target_lang_sense_clarification": "скопление пчел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5531430a-9820-3995-b0a7-7a31a0050083",
                    "sense_definition": "Dolna część ostrza siekiery lub topora.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kowal naprawił wyszczerbioną <w>brodę</w> starego topora.",
                            "target_lang_translations": {
                                "ru": "Кузнец починил выщербленную <w>бородку</w> старого топора."
                            }
                        },
                        {
                            "text": "Ostrze tej siekiery ma charakterystycznie wydłużoną <w>brodę</w>.",
                            "target_lang_translations": {
                                "ru": "Лезвие этого топора имеет характерно удлиненную <w>бородку</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя часть лезвия топора или секиры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бородка",
                                "target_lang_sense_clarification": "часть топора"
                            }
                        ]
                    }
                }
            ]
        },

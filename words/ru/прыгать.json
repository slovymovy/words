    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a9b18e80-dc9c-3229-ae4d-afe17741ebb1",
                    "sense_definition": "Отрываться от земли или другой поверхности резким толчком ног; совершать прыжок.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_jump",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кошка ловко <прыгает> со стула на стол.",
                            "target_lang_translations": {
                                "en": "The cat deftly <jumps> from the chair to the table."
                            }
                        },
                        {
                            "text": "Я могу <прыгать> очень высоко и далеко.",
                            "target_lang_translations": {
                                "en": "I can <jump> very high and far."
                            }
                        }
                    ],
                    "synonyms": [
                        "скакать"
                    ],
                    "antonyms": [
                        "стоять"
                    ],
                    "common_phrases": [
                        "высоко прыгать",
                        "прыгать от радости",
                        "прыгать на месте"
                    ],
                    "target_lang_definitions": {
                        "en": "To detach from the ground or another surface with a sharp push of the legs; to perform a jump."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jump",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "leap",
                                "target_lang_sense_clarification": "over a distance or high obstacle"
                            },
                            {
                                "target_lang_word": "spring",
                                "target_lang_sense_clarification": "sudden, energetic movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b043d323-76eb-3ec8-8d72-7ffa07699a49",
                    "sense_definition": "Делать небольшие подскоки много раз подряд; передвигаться прыжками.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_jump",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девочка весело <прыгает> через скакалку во дворе.",
                            "target_lang_translations": {
                                "en": "The girl is happily <skipping> rope in the yard."
                            }
                        },
                        {
                            "text": "Маленькие воробьи <прыгают> по дорожке в поисках еды.",
                            "target_lang_translations": {
                                "en": "Small sparrows <hop> along the path in search of food."
                            }
                        }
                    ],
                    "synonyms": [
                        "скакать",
                        "подпрыгивать"
                    ],
                    "common_phrases": [
                        "прыгать через скакалку",
                        "прыгать на одной ноге",
                        "прыгать по лужам"
                    ],
                    "target_lang_definitions": {
                        "en": "To make small hops many times in a row; to move by jumping."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "skip",
                                "target_lang_sense_clarification": "move lightly or use a rope"
                            },
                            {
                                "target_lang_word": "hop",
                                "target_lang_sense_clarification": "short jumps, typical of birds or on one leg"
                            },
                            {
                                "target_lang_word": "jump",
                                "target_lang_sense_clarification": "general action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ea293c4-7683-349e-8180-2b9358239712",
                    "sense_definition": "Бросаться вниз с высоты (обычно в воду или с парашютом).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_jump",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом дети любят <прыгать> в реку с моста.",
                            "target_lang_translations": {
                                "en": "In summer, children love to <jump> into the river from the bridge."
                            }
                        },
                        {
                            "text": "Он боится <прыгать> с парашютом в первый раз.",
                            "target_lang_translations": {
                                "en": "He is afraid to <jump> with a parachute for the first time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "прыгать с парашютом",
                        "прыгать в воду",
                        "прыгать с вышки"
                    ],
                    "target_lang_definitions": {
                        "en": "To throw oneself down from a height (usually into water or with a parachute)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jump",
                                "target_lang_sense_clarification": "general action, including parachuting"
                            },
                            {
                                "target_lang_word": "dive",
                                "target_lang_sense_clarification": "specifically into water"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2073b597-a4a0-341d-8db4-dfc9d666d727",
                    "sense_definition": "Быстро и неожиданно меняться (о давлении, температуре, ценах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_fluctuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У бабушки к вечеру часто <прыгает> давление.",
                            "target_lang_translations": {
                                "en": "Grandma's blood pressure often <fluctuates> towards evening."
                            }
                        },
                        {
                            "text": "Курс валют постоянно <прыгает> из-за кризиса.",
                            "target_lang_translations": {
                                "en": "The exchange rate constantly <fluctuates> due to the crisis."
                            }
                        }
                    ],
                    "synonyms": [
                        "скакать"
                    ],
                    "common_phrases": [
                        "давление прыгает",
                        "температура прыгает",
                        "цены прыгают"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech regarding metrics"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To change quickly and unexpectedly (regarding pressure, temperature, prices)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fluctuate",
                                "target_lang_sense_clarification": "move up and down irregularly"
                            },
                            {
                                "target_lang_word": "jump",
                                "target_lang_sense_clarification": "rise sharply"
                            }

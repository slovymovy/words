            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3cd51dd6-292f-3ea1-b349-b6b71bc3160e",
                    "sense_definition": "Osoba zajmująca się nielegalną sprzedażą środków odurzających.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zatrzymała <w>dilera</w>, który rozprowadzał narkotyki w klubach.",
                            "target_lang_translations": {
                                "ru": "Полиция задержала <w>дилера</w>, который распространял наркотики в клубах."
                            }
                        },
                        {
                            "text": "Mieszkańcy zgłosili, że na rogu ulicy stoi podejrzany <w>diler</w>.",
                            "target_lang_translations": {
                                "ru": "Жители сообщили, что на углу улицы стоит подозрительный <w>дилер</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "handlarz narkotykami"
                    ],
                    "common_phrases": [
                        "diler narkotykowy",
                        "osiedlowy diler"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, занимающийся незаконной продажей наркотических средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наркоторговец",
                                "target_lang_sense_clarification": "продавец наркотиков"
                            },
                            {
                                "target_lang_word": "дилер",
                                "target_lang_sense_clarification": "разговорное, о наркотиках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebc7e430-6dd8-3180-9e01-00f8e988c57d",
                    "sense_definition": "Autoryzowany przedstawiciel handlowy zajmujący się sprzedażą produktów konkretnej firmy, zwłaszcza samochodów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę pojechać do <w>dilera</w>, aby odebrać nowy samochód.",
                            "target_lang_translations": {
                                "ru": "Мне нужно поехать к <w>дилеру</w>, чтобы забрать новую машину."
                            }
                        },
                        {
                            "text": "Jako autoryzowany <w>diler</w>, firma zapewnia pełny serwis gwarancyjny.",
                            "target_lang_translations": {
                                "ru": "Как авторизованный <w>дилер</w>, компания обеспечивает полное гарантийное обслуживание."
                            }
                        }
                    ],
                    "synonyms": [
                        "dystrybutor",
                        "przedstawiciel handlowy",
                        "pośrednik"
                    ],
                    "antonyms": [
                        "klient",
                        "nabywca"
                    ],
                    "common_phrases": [
                        "autoryzowany diler",
                        "diler samochodowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Авторизованный торговый представитель, занимающийся продажей продукции конкретной фирмы, особенно автомобилей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дилер",
                                "target_lang_sense_clarification": "торговый представитель"
                            },
                            {
                                "target_lang_word": "торговый представитель",
                                "target_lang_sense_clarification": "официальный посредник"
                            }
                        ]
                    }
                }
            ]
        }

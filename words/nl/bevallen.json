    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "48ac06a1-43f7-32b0-971f-565f3d53897a",
                    "sense_definition": "Iets als aangenaam of prettig ervaren; in de smaak vallen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pleasing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuwe werk <w>bevalt</w> hem erg goed.",
                            "target_lang_translations": {
                                "en": "The new job <w>suits</w> him very well.",
                                "ru": "Новая работа ему очень <w>нравится</w>."
                            }
                        },
                        {
                            "text": "Die arrogante houding <w>beviel</w> mij helemaal niet.",
                            "target_lang_translations": {
                                "en": "That arrogant attitude did not <w>please</w> me at all.",
                                "ru": "Это высокомерное отношение мне совсем не <w>понравилось</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanstaan",
                        "behagen"
                    ],
                    "antonyms": [
                        "tegenvallen"
                    ],
                    "common_phrases": [
                        "goed bevallen",
                        "slecht bevallen",
                        "hoe bevalt het"
                    ],
                    "target_lang_definitions": {
                        "en": "To be experienced as pleasant or agreeable; to be to one's liking.",
                        "ru": "Нравиться, быть по душе, вызывать удовлетворение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "please",
                                "target_lang_sense_clarification": "to give satisfaction or pleasure"
                            },
                            {
                                "target_lang_word": "suit",
                                "target_lang_sense_clarification": "to be convenient or acceptable"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нравиться",
                                "target_lang_sense_clarification": "вызывать положительные чувства"
                            },
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "подходить, удовлетворять требованиям"
                            },
                            {
                                "target_lang_word": "приходиться по вкусу",
                                "target_lang_sense_clarification": "быть приятным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bfc0029-1a5a-33d8-a8f3-0e4c46ec6193",
                    "sense_definition": "Het leven schenken aan een kind; een kind ter wereld brengen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is thuis van een gezonde zoon <w>bevallen</w>.",
                            "target_lang_translations": {
                                "en": "She <w>gave birth</w> to a healthy son at home.",
                                "ru": "Она <w>родила</w> здорового сына дома."
                            }
                        },
                        {
                            "text": "Mijn zus moet volgende week in het ziekenhuis <w>bevallen</w>.",
                            "target_lang_translations": {
                                "en": "My sister has to <w>give birth</w> in the hospital next week.",
                                "ru": "Моя сестра должна <w>рожать</w> в больнице на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekoren"
                    ],
                    "common_phrases": [
                        "bevallen van",
                        "thuis bevallen",
                        "in het ziekenhuis bevallen"
                    ],
                    "target_lang_definitions": {
                        "en": "To give life to a child; to bring a child into the world.",
                        "ru": "Рожать, производить на свет ребенка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give birth",
                                "target_lang_sense_clarification": "general term for the mother's action"
                            },
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "formal or medical context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рожать",
                                "target_lang_sense_clarification": "процесс рождения ребенка"
                            },
                            {
                                "target_lang_word": "родить",
                                "target_lang_sense_clarification": "завершённое действие рождения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "zeur",
        "gezeur",
        "zeurderig",
        "zeurkous",
        "zeurpiet"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6df12c62-8b8d-3e4a-91b6-46f8d321b648",
                    "sense_definition": "Op een vervelende toon klagen over onbelangrijke zaken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "complaining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn collega loopt de hele dag te <w>zeuren</w> over de temperatuur op kantoor.",
                            "target_lang_translations": {
                                "ru": "Мой коллега весь день ходит и <w>ноет</w> по поводу температуры в офисе.",
                                "en": "My colleague walks around <w>whining</w> all day about the temperature in the office."
                            }
                        },
                        {
                            "text": "Je moet niet zo <w>zeuren</w> over dingen die je toch niet kunt veranderen.",
                            "target_lang_translations": {
                                "ru": "Не стоит так <w>ныть</w> из-за вещей, которые ты всё равно не можешь изменить.",
                                "en": "You shouldn't <w>whine</w> so much about things you can't change anyway."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaniken",
                        "zeiken"
                    ],
                    "common_phrases": [
                        "niet zo zeuren",
                        "zeuren over kleinigheden",
                        "lopen te zeuren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жаловаться или ныть по пустякам раздражающим тоном.",
                        "en": "To complain in an annoying tone about unimportant matters."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ныть",
                                "target_lang_sense_clarification": "издавать жалобные звуки или жаловаться на что-то"
                            },
                            {
                                "target_lang_word": "ворчать",
                                "target_lang_sense_clarification": "выражать недовольство негромким голосом"
                            },
                            {
                                "target_lang_word": "жаловаться",
                                "target_lang_sense_clarification": "высказывать недовольство"
                            },
                            {
                                "target_lang_word": "нудить",
                                "target_lang_sense_clarification": "надоедливо жаловаться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "whine",
                                "target_lang_sense_clarification": "to complain in a childish or annoying way"
                            },
                            {
                                "target_lang_word": "moan",
                                "target_lang_sense_clarification": "to complain or grumble about something"
                            },
                            {
                                "target_lang_word": "gripe",
                                "target_lang_sense_clarification": "to complain persistently"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cccf1eb2-099c-3aea-8fc9-eae0a3315907",
                    "sense_definition": "Steeds opnieuw om iets vragen op een vervelende manier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "complaining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kleine kind bleef maar <w>zeuren</w> om een ijsje bij de kraam.",
                            "target_lang_translations": {
                                "ru": "Маленький ребёнок продолжал <w>канючить</w> мороженое у киоска.",
                                "en": "The small child kept <w>whining</w> for an ice cream at the stall."
                            }
                        },
                        {
                            "text": "Stop met <w>zeuren</w> aan mijn hoofd, want ik heb nu geen tijd.",
                            "target_lang_translations": {
                                "ru": "Хватит <w>действовать мне на нервы</w>, потому что у меня сейчас нет времени.",
                                "en": "Stop <w>nagging</w> me, because I don't have time right now."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaniken"
                    ],
                    "common_phrases": [
                        "zeuren om aandacht",
                        "zeuren aan het hoofd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назойливо выпрашивать что-то, постоянно повторяя просьбу.",
                        "en": "To ask for something repeatedly in an annoying way."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канючить",
                                "target_lang_sense_clarification": "надоедливо просить, обычно о детях"
                            },
                            {
                                "target_lang_word": "клянчить",
                                "target_lang_sense_clarification": "настойчиво выпрашивать"
                            },
                            {
                                "target_lang_word": "приставать",
                                "target_lang_sense_clarification": "надоедать просьбами"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "nag",
                                "target_lang_sense_clarification": "to annoy someone with constant requests"
                            },
                            {
                                "target_lang_word": "pester",
                                "target_lang_sense_clarification": "to trouble or annoy with frequent requests"
                            },
                            {
                                "target_lang_word": "whine",
                                "target_lang_sense_clarification": "to make complaining demands"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b26c104b-872a-329c-96b8-c13dab809c3e",
                    "sense_definition": "Op een oneerlijke manier proberen te winnen bij een spel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cheating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je gaat <w>zeuren</w> met de regels, wil ik niet meer met je spelen.",
                            "target_lang_translations": {
                                "ru": "Если ты будешь <w>мухлевать</w> с правилами, я не хочу больше с тобой играть.",
                                "en": "If you are going to <w>cheat</w> with the rules, I don't want to play with you anymore."
                            }
                        },
                        {
                            "text": "Hij werd uit het spel gezet omdat hij probeerde te <w>zeuren</w>.",
                            "target_lang_translations": {
                                "ru": "Его выгнали из игры, потому что он пытался <w>жульничать</w>.",
                                "en": "He was kicked out of the game because he tried to <w>cheat</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "valsspelen"
                    ],
                    "common_phrases": [
                        "niet zeuren"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "vooral gebruikt door kinderen"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться нечестно выиграть или спорить о правилах в игре.",
                        "en": "To try to win a game in an unfair way or argue about rules."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мухлевать",
                                "target_lang_sense_clarification": "хитрить в игре, нарушать правила"
                            },
                            {
                                "target_lang_word": "жульничать",
                                "target_lang_sense_clarification": "обманывать в игре"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cheat",
                                "target_lang_sense_clarification": "to act dishonestly to gain an advantage"
                            },
                            {
                                "target_lang_word": "quibble",
                                "target_lang_sense_clarification": "to argue over trivial matters or rules"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6149266f-e7c0-3b1f-ab12-073343c2571c",
                    "sense_definition": "Meervoud van <w>zeur</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die klanten zijn enorme <w>zeuren</w> die over elke cent moeilijk doen.",
                            "target_lang_translations": {
                                "ru": "Эти клиенты — ужасные <w>зануды</w>, которые создают проблемы из-за каждого цента.",
                                "en": "Those customers are huge <w>nags</w> who make a fuss about every cent."
                            }
                        },
                        {
                            "text": "Ik ga niet met die <w>zeuren</w> mee, want ze klagen de hele tijd.",
                            "target_lang_translations": {
                                "ru": "Я не пойду с этими <w>нытиками</w>, потому что они всё время жалуются.",
                                "en": "I'm not going with those <w>whiners</w>, because they complain all the time."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, которые постоянно жалуются или надоедают (множественное число).",
                        "en": "People who complain constantly or are annoying (plural)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зануды",
                                "target_lang_sense_clarification": "скучные, надоедливые люди"
                            },
                            {
                                "target_lang_word": "нытики",
                                "target_lang_sense_clarification": "люди, которые постоянно ноют"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "whiners",
                                "target_lang_sense_clarification": "people who complain habitually"
                            },
                            {
                                "target_lang_word": "nags",
                                "target_lang_sense_clarification": "people who constantly scold or complain"
                            },
                            {
                                "target_lang_word": "bores",
                                "target_lang_sense_clarification": "tedious, annoying people"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
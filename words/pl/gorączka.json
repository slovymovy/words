    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa3b1402-5142-38e5-ae4e-c489ceea4fe1",
                    "sense_definition": "Stan chorobowy, w którym temperatura ciała jest wyższa niż normalna (zwykle powyżej 38°C).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz zostać w łóżku przez kilka dni, ponieważ masz wysoką <w>gorączkę</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен остаться в постели на несколько дней, так как у тебя высокая <w>температура</w>."
                            }
                        },
                        {
                            "text": "Lekarz przepisał mi skuteczne leki, aby szybko zbić <w>gorączkę</w>.",
                            "target_lang_translations": {
                                "ru": "Врач прописал мне эффективные лекарства, чтобы быстро сбить <w>температуру</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "temperatura"
                    ],
                    "common_phrases": [
                        "wysoka gorączka",
                        "mieć gorączkę",
                        "zbić gorączkę",
                        "stan podgorączkowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезненное состояние, при котором температура тела выше нормальной (обычно выше 38°C)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "температура",
                                "target_lang_sense_clarification": "разговорное, наиболее частое обозначение жара"
                            },
                            {
                                "target_lang_word": "жар",
                                "target_lang_sense_clarification": "высокая температура тела"
                            },
                            {
                                "target_lang_word": "лихорадка",
                                "target_lang_sense_clarification": "медицинский термин; состояние с ознобом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49fff94e-18cc-311e-aafe-7931b89c74e4",
                    "sense_definition": "Stan silnego pobudzenia, pośpiechu, niepokoju lub wielkich emocji, często udzielający się grupie ludzi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepach panowała przedświąteczna <w>gorączka</w> i wszędzie były tłumy ludzi.",
                            "target_lang_translations": {
                                "ru": "В магазинах царила предпраздничная <w>лихорадка</w>, и везде были толпы людей."
                            }
                        },
                        {
                            "text": "Kibiców ogarnęła prawdziwa <w>gorączka</w>, gdy ich drużyna strzeliła decydującego gola.",
                            "target_lang_translations": {
                                "ru": "Болельщиков охватила настоящая <w>лихорадка</w>, когда их команда забила решающий гол."
                            }
                        }
                    ],
                    "synonyms": [
                        "podniecenie",
                        "ekscytacja",
                        "szał",
                        "ferwor",
                        "zapał"
                    ],
                    "common_phrases": [
                        "gorączka złota",
                        "przedświąteczna gorączka",
                        "gorączka sobotniej nocy",
                        "wypieki na twarzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние сильного возбуждения, спешки, беспокойства или сильных эмоций, часто охватывающее группу людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лихорадка",
                                "target_lang_sense_clarification": "переносное значение; сильное возбуждение"
                            },
                            {
                                "target_lang_word": "ажиотаж",
                                "target_lang_sense_clarification": "сильное волнение, борьба интересов"
                            },
                            {
                                "target_lang_word": "суматоха",
                                "target_lang_sense_clarification": "беспорядочная спешка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc20223f-ca08-3f74-b99c-95f1d5914947",
                    "sense_definition": "Bardzo wysoka temperatura powietrza; upalna pogoda.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zewnątrz panuje taka <w>gorączka</w>, że nie da się wytrzymać bez klimatyzacji.",
                            "target_lang_translations": {
                                "ru": "На улице стоит такая <w>жара</w>, что невозможно выдержать без кондиционера."
                            }
                        },
                        {
                            "text": "Lipcowa <w>gorączka</w> sprawiła, że ulice miasta opustoszały w godzinach południowych.",
                            "target_lang_translations": {
                                "ru": "Июльская <w>жара</w> привела к тому, что улицы города опустели в полуденные часы."
                            }
                        }
                    ],
                    "synonyms": [
                        "upał"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень высокая температура воздуха; знойная погода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жара",
                                "target_lang_sense_clarification": "высокая температура воздуха"
                            },
                            {
                                "target_lang_word": "зной",
                                "target_lang_sense_clarification": "сильная, палящая жара"
                            },
                            {
                                "target_lang_word": "пекло",
                                "target_lang_sense_clarification": "разговорное; невыносимая жара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91832426-8b44-36b7-bc22-e6d88d7e7aba",
                    "sense_definition": "Osoba bardzo porywcza, gwałtowna i łatwo wpadająca w gniew.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój szef to straszny <w>gorączka</w>, krzyczy z byle powodu, a potem przeprasza.",
                            "target_lang_translations": {
                                "ru": "Мой начальник — ужасная <w>горячая голова</w>, кричит по любому поводу, а потом извиняется."
                            }
                        },
                        {
                            "text": "Nie bądź taki <w>gorączka</w>, uspokój się i przemyśl to jeszcze raz na chłodno.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <w>горячей головой</w>, успокойся и обдумай это ещё раз на холодную голову."
                            }
                        }
                    ],
                    "synonyms": [
                        "gorąca głowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень вспыльчивый, резкий человек, который легко впадает в гнев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горячая голова",
                                "target_lang_sense_clarification": "фразеологизм; о вспыльчивом человеке"
                            },
                            {
                                "target_lang_word": "вспыльчивый человек",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        }
    ]

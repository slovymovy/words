                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Burns</w> is the owner of the local power plant.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Бёрнс</w> — владелец местной электростанции."
                            }

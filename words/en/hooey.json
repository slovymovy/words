            "senses": [
                {
                    "sense_id": "f71756e4-bad5-3f34-a1a8-c9b20d6be6ce",
                    "sense_definition": "Silly talk, writing, or nonsense; false assertions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I heard his speech, and it sounded like a whole lot of <w>hooey</w> to me.",
                            "target_lang_translations": {
                                "ru": "Я слышал его речь, и она показалась мне полной <w>чушью</w>."
                            }
                        },
                        {
                            "text": "Don't believe a word of that story; it is just a bunch of <w>hooey</w>.",
                            "target_lang_translations": {
                                "ru": "Не верь ни единому слову из этой истории; это просто <w>ерунда</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nonsense",
                        "rubbish",
                        "baloney",
                        "hogwash",
                        "bunk"
                    ],
                    "antonyms": [
                        "truth",
                        "fact",
                        "sense"
                    ],
                    "common_phrases": [
                        "load of hooey",
                        "bunch of hooey",
                        "lot of hooey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глупые разговоры, вздор или бессмыслица; ложные утверждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ерунда",
                                "target_lang_sense_clarification": "вздор, чепуха (разговорное)"
                            },
                            {
                                "target_lang_word": "чушь",
                                "target_lang_sense_clarification": "нелепость, бессмыслица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbf7cf7b-a54e-356b-bed8-286480fc4195",
                    "sense_definition": "A specific type of knot or hog-tie used in rodeo events.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cowboy secured the calf with a fast <w>hooey</w> before raising his hands.",
                            "target_lang_translations": {
                                "ru": "Ковбой закрепил телёнка быстрым <w>узлом</w>, прежде чем поднять руки."
                            }
                        },
                        {
                            "text": "Judges watch closely to ensure the <w>hooey</w> holds tight during the competition.",
                            "target_lang_translations": {
                                "ru": "Судьи внимательно следят за тем, чтобы <w>узел</w> крепко держался во время соревнования."
                            }
                        }
                    ],
                    "target_lang_definitions": {

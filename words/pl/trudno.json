                    "sense_id": "6e511eea-a1f2-3616-a336-df41916649ce",
                    "sense_definition": "W sposób wymagający dużego wysiłku, pracy lub pokonania przeszkód; niełatwo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "difficulty_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po poważnym wypadku ten starszy mężczyzna porusza się bardzo <w>trudno</w>.",
                            "target_lang_translations": {
                                "ru": "После серьезной аварии этот пожилой мужчина передвигается с большим <w>трудом</w>."
                            }
                        },
                        {
                            "text": "Zadanie domowe z matematyki było skomplikowane i rozwiązywało się je <w>trudno</w>.",
                            "target_lang_translations": {
                                "ru": "Домашнее задание по математике было сложным, и решалось <w>трудно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciężko",
                        "niełatwo",
                        "z trudem",
                        "z trudnością",
                        "opornie"
                    ],
                    "antonyms": [
                        "łatwo",
                        "nietrudno",
                        "bez problemu",
                        "z górki"
                    ],
                    "common_phrases": [
                        "trudno dostępny",
                        "trudno gojący",
                        "trudno uwierzyć",
                        "trudno zrozumieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "С усилием, требуя большого напряжения или преодоления препятствий; нелегко."

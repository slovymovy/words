    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "229e3f73-31fc-3e0c-920d-a1228cc4dc2b",
                    "sense_definition": "Хищная птица семейства соколиных с сильным клювом и длинными острыми крыльями, отличающаяся быстрым полётом.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bird_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В небе над степью парил гордый <сокол>, высматривая добычу.",
                            "target_lang_translations": {
                                "en": "A proud <falcon> soared in the sky over the steppe, looking out for prey."
                            }
                        },
                        {
                            "text": "Для королевской охоты специально обучали молодых <соколов>.",
                            "target_lang_translations": {
                                "en": "Young <falcons> were specially trained for the royal hunt."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ясный сокол",
                        "охота с соколом",
                        "зрение как у сокола"
                    ],
                    "target_lang_definitions": {
                        "en": "A bird of prey of the falcon family with a strong beak and long pointed wings, known for its fast flight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "falcon",
                                "target_lang_sense_clarification": "bird of prey"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6e036f3-0f40-3b74-8f30-9b165f486353",
                    "sense_definition": "Ласковое или поэтическое название молодого, смелого и красивого мужчины.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bird_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вернулся домой наш ясный <сокол>, живой и невредимый после долгого похода.",
                            "target_lang_translations": {
                                "en": "Our clear <falcon> returned home, safe and sound after the long campaign."
                            }
                        },
                        {
                            "text": "Эх, какой <сокол> пропадает в этой глуши без настоящего дела!",
                            "target_lang_translations": {
                                "en": "Oh, what a <falcon> is being wasted in this backwater without real work!"
                            }
                        }
                    ],
                    "synonyms": [
                        "орёл",
                        "молодец",
                        "удалец"
                    ],
                    "common_phrases": [
                        "сокол ты мой",
                        "финист ясный сокол"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "народно-поэтическое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An affectionate or poetic term for a young, brave, and handsome man."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "falcon",
                                "target_lang_sense_clarification": "poetic metaphor for a brave man"
                            },
                            {
                                "target_lang_word": "hero",
                                "target_lang_sense_clarification": "figurative meaning"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d38c70d3-c68c-30aa-b4a2-300f6a57c867",
                    "sense_definition": "Старинное стенобитное орудие в виде большого гладкого бревна, окованного железом (отсюда происходит известная поговорка).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tool_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого бедняка нет ни гроша за душой, он гол как <сокол>.",
                            "target_lang_translations": {
                                "en": "This poor man has not a penny to his name, he is as naked as a <falcon>."
                            }
                        },
                        {
                            "text": "Воины подкатили тяжёлый <сокол> к воротам осаждённой крепости, чтобы выбить их.",
                            "target_lang_translations": {
                                "en": "The warriors rolled a heavy <battering ram> to the gates of the besieged fortress to break them down."
                            }
                        }
                    ],
                    "synonyms": [
                        "таран"
                    ],
                    "common_phrases": [
                        "гол как сокол"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "историческое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An ancient battering ram in the form of a large smooth log bound with iron (the origin of a well-known saying)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "battering ram",
                                "target_lang_sense_clarification": "ancient siege weapon"
                            },
                            {
                                "target_lang_word": "ram",
                                "target_lang_sense_clarification": "siege tool"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1caba72-e43f-3a4c-b194-160a9a4b0d60",
                    "sense_definition": "Строительный инструмент штукатура в виде квадратного щитка с ручкой для нанесения и разглаживания раствора.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tool_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мастер наложил цементный раствор на <сокол> и приступил к выравниванию стены.",
                            "target_lang_translations": {
                                "en": "The master put cement mortar on the <hawk> and started leveling the wall."
                            }
                        },
                        {
                            "text": "Деревянный <сокол> удобен для удерживания небольшого количества штукатурки во время работы.",
                            "target_lang_translations": {
                                "en": "A wooden <hawk> is convenient for holding a small amount of plaster during work."
                            }
                        }
                    ],
                    "synonyms": [
                        "соколо́к"
                    ],
                    "common_phrases": [
                        "штукатурный сокол"
                    ],
                    "target_lang_definitions": {
                        "en": "A plasterer's construction tool in the form of a square shield with a handle for applying and smoothing mortar."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hawk",
                                "target_lang_sense_clarification": "plastering tool"
                            },
                            {
                                "target_lang_word": "mortarboard",
                                "target_lang_sense_clarification": "construction tool"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "56ddba62-f852-39aa-9d11-9daa576202bf",
                    "sense_definition": "Название города в Вологодской области, а также ряда других населённых пунктов и районов в России.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_concept",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Поезд прибывает на станцию <Сокол> рано утром строго по расписанию.",
                            "target_lang_translations": {
                                "en": "The train arrives at <Sokol> station early in the morning strictly on schedule."
                            }
                        },
                        {
                            "text": "Мой друг живёт в Москве, недалеко от станции метро <Сокол>.",
                            "target_lang_translations": {
                                "en": "My friend lives in Moscow, not far from the <Sokol> metro station."
                            }
                        }
                    ],
                    "common_phrases": [
                        "город Сокол",
                        "станция Сокол"
                    ],
                    "target_lang_definitions": {
                        "en": "The name of a city in the Vologda region, as well as a number of other settlements and districts in Russia."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Sokol",
                                "target_lang_sense_clarification": "place name"
                            }
                        ]
                    }
                }
            ]
        }
    ]

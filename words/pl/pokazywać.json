{
    "word_family": [
        "pokaz",
        "pokazowy",
        "pokazać",
        "wskazówka",
        "okazać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d6499c4-a74a-31c3-a213-5ed261c91529",
                    "sense_definition": "Dawać możliwość zobaczenia czegoś, czynić coś widocznym dla innych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_presentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lubię <pokazywać> gościom zdjęcia z moich dalekich podróży.",
                            "target_lang_translations": {
                                "ru": "Я люблю <w>показывать</w> гостям фотографии из моих дальних путешествий."
                            }
                        },
                        {
                            "text": "Ten film <pokazuje> życie zwierząt w dzikiej dżungli.",
                            "target_lang_translations": {
                                "ru": "Этот фильм <w>показывает</w> жизнь животных в диких джунглях."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawiać",
                        "ukazywać",
                        "prezentować"
                    ],
                    "antonyms": [
                        "ukrywać (się)",
                        "chować (się)"
                    ],
                    "common_phrases": [
                        "pokazywać zdjęcia",
                        "pokazywać język",
                        "pokazywać swoje umiejętności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать возможность увидеть что-то, делать что-то видимым для других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "делать видимым"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "представлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ffc4e46-802f-31a3-ab12-46f332e6c60a",
                    "sense_definition": "Wykonywać gest ręką lub przedmiotem, aby zwrócić na coś uwagę lub wskazać kierunek.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "gestures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niegrzecznie jest <pokazywać> palcem na innych ludzi w miejscu publicznym.",
                            "target_lang_translations": {
                                "ru": "Невежливо <w>показывать</w> пальцем на других людей в общественном месте."
                            }
                        },
                        {
                            "text": "Przechodzień <pokazuje> turyście drogę do najbliższego dworca.",
                            "target_lang_translations": {
                                "ru": "Прохожий <w>показывает</w> туристу дорогу к ближайшему вокзалу."
                            }
                        }
                    ],
                    "synonyms": [
                        "wskazywać"
                    ],
                    "common_phrases": [
                        "pokazywać palcem",
                        "pokazywać drogę",
                        "pokazywać kierunek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать жест рукой или предметом, чтобы обратить внимание или указать направление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "указывать жестом"
                            },
                            {
                                "target_lang_word": "указывать",
                                "target_lang_sense_clarification": "направление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b19898a3-b562-323a-9d17-756622f85cbb",
                    "sense_definition": "Stawać się widocznym, ujawniać swoją obecność (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po gwałtownej burzy na niebie często <pokazuje> się piękna tęcza.",
                            "target_lang_translations": {
                                "ru": "После сильной грозы на небе часто <w>появляется</w> красивая радуга."
                            }
                        },
                        {
                            "text": "Aktor rzadko <pokazuje> się publicznie od czasu zakończenia kariery.",
                            "target_lang_translations": {
                                "ru": "Актер редко <w>появляется</w> на публике с момента завершения карьеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojawić się",
                        "ukazać się"
                    ],
                    "antonyms": [
                        "chować (się)",
                        "ukrywać (się)"
                    ],
                    "common_phrases": [
                        "pokazywać się publicznie",
                        "słońce się pokazuje",
                        "pokazywać się na ekranie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться видимым, обнаруживать своё присутствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показываться",
                                "target_lang_sense_clarification": "становиться видимым"
                            },
                            {
                                "target_lang_word": "появляться",
                                "target_lang_sense_clarification": "возникать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b63f7a1d-66c8-35e2-88ee-66474103bf15",
                    "sense_definition": "Objaśniać sposób działania czegoś lub uczyć, wykonując daną czynność na oczach innej osoby.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "demonstration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Instruktor dokładnie <pokazuje> kursantom, jak bezpiecznie prowadzić samochód.",
                            "target_lang_translations": {
                                "ru": "Инструктор подробно <w>показывает</w> курсантам, как безопасно водить машину."
                            }
                        },
                        {
                            "text": "Mama cierpliwie <pokazywała> córce, jak należy wiązać sznurowadła.",
                            "target_lang_translations": {
                                "ru": "Мама терпеливо <w>показывала</w> дочери, как нужно завязывать шнурки."
                            }
                        }
                    ],
                    "synonyms": [
                        "demonstrować",
                        "prezentować",
                        "zademonstrować"
                    ],
                    "common_phrases": [
                        "pokazywać na przykładzie",
                        "pokazywać jak to zrobić",
                        "pokazywać ćwiczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объяснять, как что-то работает, или учить, показывая действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "объяснять на примере"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "наглядно показывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a32cd556-f08b-3c1e-85c6-e181bcf0a88c",
                    "sense_definition": "Informować o wartości, stanie lub czasie (o urządzeniach pomiarowych).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary zegarek ścienny nie <pokazuje> już dokładnej godziny.",
                            "target_lang_translations": {
                                "ru": "Мои старые настенные часы больше не <w>показывают</w> точное время."
                            }
                        },
                        {
                            "text": "Termometr za oknem <pokazuje> dzisiaj minus pięć stopni Celsjusza.",
                            "target_lang_translations": {
                                "ru": "Термометр за окном <w>показывает</w> сегодня минус пять градусов Цельсия."
                            }
                        }
                    ],
                    "synonyms": [
                        "wskazywać"
                    ],
                    "common_phrases": [
                        "zegar pokazuje godzinę",
                        "termometr pokazuje temperaturę",
                        "licznik pokazuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать значение, состояние или время (о приборах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "о приборах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
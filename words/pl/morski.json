    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c1bc35ab-4911-3b61-9cd9-6d4b14db09d3",
                    "sense_definition": "Związany z morzem, pochodzący z morza, żyjący w morzu lub odbywający się na morzu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relation_to_sea",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latarnia <morska> ostrzega statki przed niebezpiecznymi skałami przy brzegu.",
                            "target_lang_translations": {
                                "ru": "<Морской> маяк предупреждает корабли об опасных скалах у берега."
                            }
                        },
                        {
                            "text": "Podczas wakacji uwielbiam wdychać świeże <morskie> powietrze spacerując po plaży.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска я люблю вдыхать свежий <морской> воздух, гуляя по пляжу."
                            }
                        }
                    ],
                    "antonyms": [
                        "lądowy"
                    ],
                    "common_phrases": [
                        "latarnia morska",
                        "choroba morska",
                        "wilk morski",
                        "sól morska",
                        "okoń morski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с морем, происходящий из моря, обитающий в море или происходящий на море."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морской",
                                "target_lang_sense_clarification": "относящийся к морю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ab74146-ca2f-355a-9d29-edfc8a8fdc34",
                    "sense_definition": "Mający kolor łączący niebieski z zielonym, przypominający barwę wody w morzu.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej oczy mają niezwykły, <morski> kolor, który zmienia się w zależności od światła.",
                            "target_lang_translations": {
                                "ru": "У её глаз необычный цвет <морской волны>, который меняется в зависимости от освещения."
                            }
                        },
                        {
                            "text": "Postanowiliśmy pomalować ściany w łazience na jasny <morski> odcień.",
                            "target_lang_translations": {
                                "ru": "Мы решили покрасить стены в ванной в светлый оттенок <морской волны>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niebieskozielony"
                    ],
                    "common_phrases": [
                        "morski kolor",
                        "morska zieleń",
                        "morski błękit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий цвет, сочетающий синий с зеленым, напоминающий цвет морской воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет морской волны",
                                "target_lang_sense_clarification": "сине-зелёный цвет"
                            }
                        ]
                    }
                }
            ]
        }
    ]

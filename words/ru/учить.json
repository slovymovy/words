    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b957b992-58f9-3139-aa80-21aba8d5095d",
                    "sense_definition": "Передавать знания, умения или навыки кому-либо; обучать.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "teaching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <учит> меня английскому языку уже год.",
                            "target_lang_translations": {
                                "en": "She has been <w>teaching</w> me English for a year."
                            }
                        },
                        {
                            "text": "Отец <учит> сына кататься на велосипеде в парке.",
                            "target_lang_translations": {
                                "en": "The father is <w>teaching</w> his son to ride a bike in the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "обучать",
                        "преподавать"
                    ],
                    "common_phrases": [
                        "учить детей",
                        "учить плавать",
                        "учить читать",
                        "учить музыке"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass knowledge, skills, or abilities to someone; to educate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "teach",
                                "target_lang_sense_clarification": "to give lessons or knowledge to someone"
                            },
                            {
                                "target_lang_word": "instruct",
                                "target_lang_sense_clarification": "to give formal training or instructions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "166d841a-425d-3d8d-a404-3379735f4dff",
                    "sense_definition": "Усваивать знания, приобретать навыки; изучать какой-либо предмет.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "learning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент <учит> физику, чтобы сдать экзамен.",
                            "target_lang_translations": {
                                "en": "The student is <w>studying</w> physics to pass the exam."
                            }
                        },
                        {
                            "text": "Мы <учим> новые слова на каждом уроке.",
                            "target_lang_translations": {
                                "en": "We <w>learn</w> new words in every lesson."
                            }
                        }
                    ],
                    "synonyms": [
                        "изучать",
                        "штудировать"
                    ],
                    "common_phrases": [
                        "учить уроки",
                        "учить язык",
                        "учить правила"
                    ],
                    "target_lang_definitions": {
                        "en": "To assimilate knowledge, acquire skills; to study a subject."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "learn",
                                "target_lang_sense_clarification": "to gain knowledge or skill"
                            },
                            {
                                "target_lang_word": "study",
                                "target_lang_sense_clarification": "to engage in the process of learning a subject"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "621fe406-d0b5-336f-8d9d-7e24d2b2772f",
                    "sense_definition": "Запоминать информацию наизусть, заучивать.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "learning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Школьник должен <учить> стихотворение наизусть к завтрашнему дню.",
                            "target_lang_translations": {
                                "en": "The schoolboy must <w>learn</w> the poem by heart by tomorrow."
                            }
                        },
                        {
                            "text": "Актёр <учит> свою роль для нового спектакля.",
                            "target_lang_translations": {
                                "en": "The actor is <w>learning</w> his role for the new play."
                            }
                        }
                    ],
                    "synonyms": [
                        "зубрить"
                    ],
                    "antonyms": [
                        "забывать"
                    ],
                    "common_phrases": [
                        "учить наизусть",
                        "учить стихи",
                        "учить роль",
                        "учить слова"
                    ],
                    "target_lang_definitions": {
                        "en": "To commit information to memory; to learn by heart."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "memorize",
                                "target_lang_sense_clarification": "to commit to memory"
                            },
                            {
                                "target_lang_word": "learn",
                                "target_lang_sense_clarification": "to commit to memory (e.g., lines, a poem)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f490b2d0-4626-30ee-baef-f7ed150b83c0",
                    "sense_definition": "Физически наказывать, бить (обычно с целью воспитания).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "punishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старину детей часто <учили> розгами за непослушание.",
                            "target_lang_translations": {
                                "en": "In the old days, children were often <w>punished</w> with birch rods for disobedience."
                            }
                        },
                        {
                            "text": "Нельзя <учить> ребёнка ремнём, нужно объяснять словами.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>punish</w> a child with a belt; you need to explain with words."
                            }
                        }
                    ],
                    "synonyms": [
                        "наказывать"
                    ],
                    "common_phrases": [
                        "учить ремнём",
                        "учить кулаками"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in context of physical punishment"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To physically punish or beat (usually for disciplinary purposes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "punish",
                                "target_lang_sense_clarification": "to impose a penalty for a fault"
                            },
                            {
                                "target_lang_word": "discipline",
                                "target_lang_sense_clarification": "to train or punish to ensure obedience"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "04375308-f63d-3072-8dc5-c0806c82a202",
                    "sense_definition": "Having a shape like a circle or a ball; curved.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "shape_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought a large <w>round</w> table for the dining room.",
                            "target_lang_translations": {
                                "ru": "Мы купили большой <w>круглый</w> стол для столовой."
                            }
                        },
                        {
                            "text": "The moon looks very <w>round</w> and bright tonight.",
                            "target_lang_translations": {
                                "ru": "Луна выглядит очень <w>круглой</w> и яркой сегодня вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "circular",
                        "spherical",
                        "rounded"
                    ],
                    "antonyms": [
                        "flat"
                    ],
                    "common_phrases": [
                        "round table",
                        "round face",
                        "round window"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму круга или шара; изогнутый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кру́глый",
                                "target_lang_sense_clarification": "имеющий форму круга или шара"
                            },
                            {
                                "target_lang_word": "окру́глый",
                                "target_lang_sense_clarification": "напоминающий круг, с плавными очертаниями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ef96492-1fba-3ac1-8a2b-a660490b5dcb",
                    "sense_definition": "Describing a number that ends in zero or is easy to calculate with.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "math_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please give me a <w>round</w> number instead of the exact decimals.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, назовите мне <w>круглое</w> число, а не точные десятичные дроби."
                            }
                        },
                        {
                            "text": "One hundred dollars is a nice <w>round</w> sum to work with.",
                            "target_lang_translations": {
                                "ru": "Сто долларов — это хорошая <w>круглая</w> сумма для работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "round number",
                        "round figure",
                        "round sum"
                    ],
                    "target_lang_definitions": {
                        "ru": "О числе, которое заканчивается на ноль или с которым легко производить вычисления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кру́глый",
                                "target_lang_sense_clarification": "о числе: оканчивающееся нулем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78c6962e-0728-3854-9449-ccb23b772ede",
                    "sense_definition": "Having a curved body shape; slightly fat or plump.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby has a <w>round</w> belly and chubby cheeks.",
                            "target_lang_translations": {
                                "ru": "У ребенка <w>круглый</w> животик и пухлые щечки."
                            }
                        },
                        {
                            "text": "He was a short and <w>round</w> man with a happy smile.",
                            "target_lang_translations": {
                                "ru": "Он был невысоким и <w>полным</w> мужчиной со счастливой улыбкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "plump",
                        "rotund"
                    ],
                    "common_phrases": [
                        "round belly",
                        "short and round"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий округлые формы тела; слегка толстый или пухлый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́лный",
                                "target_lang_sense_clarification": "о человеке: тучный, упитанный"
                            },
                            {
                                "target_lang_word": "окру́глый",
                                "target_lang_sense_clarification": "о формах тела: плавный, пухлый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60074d70-7988-39b5-84e8-0e83c4ada87f",
                    "sense_definition": "Complete or whole; not lacking anything.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "completeness_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baker gave us a <w>round</w> dozen of fresh rolls.",
                            "target_lang_translations": {
                                "ru": "Пекарь дал нам <w>полную</w> дюжину свежих булочек."
                            }
                        },
                        {
                            "text": "It took a <w>round</w> year to finish the construction project.",
                            "target_lang_translations": {
                                "ru": "На завершение строительного проекта ушел <w>целый</w> год."
                            }
                        }
                    ],
                    "synonyms": [
                        "complete",
                        "entire",
                        "whole"
                    ],
                    "common_phrases": [
                        "round dozen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный или целый; без недостатка чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́лный",
                                "target_lang_sense_clarification": "целый, завершенный (о мере или времени)"
                            },
                            {
                                "target_lang_word": "це́лый",
                                "target_lang_sense_clarification": "весь, без изъятия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "2c48b792-35a8-3ed8-aa7b-1872ce21524a",
                    "sense_definition": "On all sides of something; surrounding.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family sat <w>round</w> the table for dinner.",
                            "target_lang_translations": {
                                "ru": "Семья сидела <w>вокруг</w> стола за ужином."
                            }
                        },
                        {
                            "text": "There is a high fence <w>round</w> the garden.",
                            "target_lang_translations": {
                                "ru": "<w>Вокруг</w> сада стоит высокий забор."
                            }
                        }
                    ],
                    "common_phrases": [
                        "round the world",
                        "round the corner",
                        "round the clock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Со всех сторон чего-либо; окружая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вокру́г",
                                "target_lang_sense_clarification": "предлог: кругом, около"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3ad80157-7412-3283-b9a7-857a575a623d",
                    "sense_definition": "In a circle or curve; changing direction to the opposite way.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please turn <w>round</w> so I can see your back.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, повернись <w>кругом</w>, чтобы я мог видеть твою спину."
                            }
                        },
                        {
                            "text": "The wheels of the bus go <w>round</w> and round.",
                            "target_lang_translations": {
                                "ru": "Колеса автобуса крутятся <w>по кругу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "turn round",
                        "go round",
                        "look round"
                    ],
                    "target_lang_definitions": {
                        "ru": "По кругу или дуге; меняя направление на противоположное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круго́м",
                                "target_lang_sense_clarification": "наречие: вокруг, по кругу"
                            },
                            {
                                "target_lang_word": "вокру́г",
                                "target_lang_sense_clarification": "наречие: в окружности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c2d79a38-22cc-366d-8e52-1462de28429d",
                    "sense_definition": "A specific stage, level, or period of play in a game or sport.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sports_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer was very tired after the final <w>round</w>.",
                            "target_lang_translations": {
                                "ru": "Боксер очень устал после последнего <w>раунда</w>."
                            }
                        },
                        {
                            "text": "We qualified for the next <w>round</w> of the competition.",
                            "target_lang_translations": {
                                "ru": "Мы прошли в следующий <w>раунд</w> соревнований."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first round",
                        "final round",
                        "qualifying round"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный этап, уровень или период игры в спорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ра́унд",
                                "target_lang_sense_clarification": "этап спортивного состязания (бокс, игры)"
                            },
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "этап соревнования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02da3f49-bb5a-3ab3-8351-c4483caafab0",
                    "sense_definition": "A set of drinks purchased for a group of people at the same time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He bought a <w>round</w> of drinks for all his friends.",
                            "target_lang_translations": {
                                "ru": "Он купил <w>круг</w> напитков для всех своих друзей."
                            }
                        },
                        {
                            "text": "It is my turn to buy the next <w>round</w> at the bar.",
                            "target_lang_translations": {
                                "ru": "Моя очередь покупать следующий <w>круг</w> в баре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "round of drinks",
                        "buy a round"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор напитков, купленных для группы людей одновременно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "угощение напитками всей компании по очереди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5be7177a-54c9-327c-89c3-586a115054d2",
                    "sense_definition": "A short period of applause or cheering from an audience.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The speaker received a loud <w>round</w> of applause.",
                            "target_lang_translations": {
                                "ru": "Оратор получил громкие <w>аплодисменты</w>."
                            }
                        },
                        {
                            "text": "Let's give the performers a big <w>round</w> of applause.",
                            "target_lang_translations": {
                                "ru": "Давайте наградим исполнителей бурными <w>аплодисментами</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "round of applause"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий период аплодисментов или приветственных возгласов аудитории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "резкое проявление чувств (аплодисментов)"
                            },
                            {
                                "target_lang_word": "швал",
                                "target_lang_sense_clarification": "бурное проявление (аплодисментов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f28ae752-c5ae-305e-82f8-916fb3a8dd3d",
                    "sense_definition": "A regular route taken to deliver something or inspect an area.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "route_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor is doing her morning <w>rounds</w> at the hospital.",
                            "target_lang_translations": {
                                "ru": "Врач делает свой утренний <w>обход</w> в больнице."
                            }
                        },
                        {
                            "text": "The security guard makes a <w>round</w> of the building every hour.",
                            "target_lang_translations": {
                                "ru": "Охранник делает <w>обход</w> здания каждый час."
                            }
                        }
                    ],
                    "synonyms": [
                        "routine"
                    ],
                    "common_phrases": [
                        "paper round",
                        "make the rounds",
                        "ward rounds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярный маршрут для доставки чего-либо или осмотра территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обхо́д",
                                "target_lang_sense_clarification": "посещение ряда мест по очереди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91362e85-36b0-3f10-bb8d-67576cdd401f",
                    "sense_definition": "A circular object or a slice of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She cut a <w>round</w> of cheese for the guests.",
                            "target_lang_translations": {
                                "ru": "Она отрезала <w>круг</w> сыра для гостей."
                            }
                        },
                        {
                            "text": "I ate a <w>round</w> of toast with my breakfast.",
                            "target_lang_translations": {
                                "ru": "Я съел <w>кружочек</w> тоста на завтрак."
                            }
                        }
                    ],
                    "common_phrases": [
                        "round of toast",
                        "round of cheese"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглый предмет или ломтик чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "предмет в форме круга (сыра)"
                            },
                            {
                                "target_lang_word": "кружо́к",
                                "target_lang_sense_clarification": "маленький круглый ломтик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3afe253-3fb0-3233-aaf6-38dd30e7875f",
                    "sense_definition": "A single bullet or shot fired from a gun.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weapon_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldier had only one <w>round</w> left in his rifle.",
                            "target_lang_translations": {
                                "ru": "У солдата остался только один <w>патрон</w> в винтовке."
                            }
                        },
                        {
                            "text": "They fired a <w>round</w> into the air to warn the crowd.",
                            "target_lang_translations": {
                                "ru": "Они сделали <w>выстрел</w> в воздух, чтобы предупредить толпу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "live round",
                        "fire a round"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один патрон или выстрел из оружия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патро́н",
                                "target_lang_sense_clarification": "боеприпас"
                            },
                            {
                                "target_lang_word": "вы́стрел",
                                "target_lang_sense_clarification": "единичный залп"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1fe5743-c185-3357-ad91-692e5ec9c03d",
                    "sense_definition": "A cut of beef from the rear part of the animal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We roasted a <w>round</w> of beef for Sunday dinner.",
                            "target_lang_translations": {
                                "ru": "Мы запекли говяжий <w>огузок</w> на воскресный ужин."
                            }
                        },
                        {
                            "text": "The butcher suggested a top <w>round</w> steak for the recipe.",
                            "target_lang_translations": {
                                "ru": "Мясник посоветовал стейк из верхней части <w>бедра</w> для этого рецепта."
                            }
                        }
                    ],
                    "synonyms": [
                        "rump"
                    ],
                    "common_phrases": [
                        "round of beef",
                        "top round"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок говядины из задней части животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огу́зок",
                                "target_lang_sense_clarification": "часть туши"
                            },
                            {
                                "target_lang_word": "бедро́",
                                "target_lang_sense_clarification": "часть туши"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab73f423-321d-3a6e-b1a8-686fa3142e04",
                    "sense_definition": "To adjust a number to the nearest whole number or convenient unit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "math_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>round</w> the total to the nearest dollar.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>округлить</w> сумму до ближайшего доллара."
                            }
                        },
                        {
                            "text": "The teacher asked the students to <w>round</w> up the figures.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил студентов <w>округлить</w> цифры в большую сторону."
                            }
                        }
                    ],
                    "common_phrases": [
                        "round up",
                        "round down",
                        "round off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять число до ближайшего целого или удобной единицы."
                    },
                    "translations": {
                        "ru": [

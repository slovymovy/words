    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9d956cf-cbe7-3944-a5e6-52e3d1d86d13",
                    "sense_definition": "Деталь машины или механизма в виде стержня, которая вращается на опорах и передаёт движение другим частям.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mechanics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Механик сказал, что нужно срочно заменить карданный <w>вал</w>.",
                            "target_lang_translations": {
                                "en": "The mechanic said that the drive <w>shaft</w> needed to be replaced urgently."
                            }
                        },
                        {
                            "text": "Двигатель вращает <w>вал</w>, который приводит в движение колёса автомобиля.",
                            "target_lang_translations": {
                                "en": "The engine rotates the <w>shaft</w> which drives the car's wheels."
                            }
                        }
                    ],
                    "synonyms": [
                        "шпиндель"
                    ],
                    "common_phrases": [
                        "карданный вал",
                        "коленчатый вал",
                        "вал двигателя",
                        "распределительный вал"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A rod-like machine part that rotates on supports and transmits motion to other parts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shaft",
                                "target_lang_sense_clarification": "mechanical component"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75794d72-3985-373f-a0c6-641d96c04afb",
                    "sense_definition": "Очень высокая, большая волна (обычно на море).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wave_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный пенистый <w>вал</w> с шумом обрушился на берег.",
                            "target_lang_translations": {
                                "en": "A huge foamy <w>roller</w> crashed noisily onto the shore."
                            }
                        },
                        {
                            "text": "Во время шторма каждый новый <w>вал</w> казался выше предыдущего.",
                            "target_lang_translations": {
                                "en": "During the storm, every new <w>wave</w> seemed higher than the previous one."
                            }
                        }
                    ],
                    "synonyms": [
                        "волна"
                    ],
                    "common_phrases": [
                        "девятый вал",
                        "морской вал",
                        "пенный вал"
                    ],
                    "target_lang_definitions": {
                        "en": "A very high, large wave, usually at sea."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "billow",
                                "target_lang_sense_clarification": "large surging wave"
                            },
                            {
                                "target_lang_word": "roller",
                                "target_lang_sense_clarification": "long heavy wave"
                            },
                            {
                                "target_lang_word": "swell",
                                "target_lang_sense_clarification": "unbroken wave"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7981dd8b-c425-3e7c-954d-07ed1d5a4991",
                    "sense_definition": "Высокая земляная насыпь, служащая для защиты местности, укрепления или ограждения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fortification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вокруг древней крепости сохранился высокий земляной <w>вал</w>.",
                            "target_lang_translations": {
                                "en": "A high earthen <w>rampart</w> has been preserved around the ancient fortress."
                            }
                        },
                        {
                            "text": "Туристы поднялись на <w>вал</w>, чтобы осмотреть окрестности города.",
                            "target_lang_translations": {
                                "en": "The tourists climbed onto the <w>embankment</w> to view the city's surroundings."
                            }
                        }
                    ],
                    "synonyms": [
                        "насыпь"
                    ],
                    "antonyms": [
                        "ров",
                        "канава"
                    ],
                    "common_phrases": [
                        "крепостной вал",
                        "земляной вал",
                        "городской вал"
                    ],
                    "target_lang_definitions": {
                        "en": "A high earthen mound used for protection, fortification, or enclosure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rampart",
                                "target_lang_sense_clarification": "fortification"
                            },
                            {
                                "target_lang_word": "bank",
                                "target_lang_sense_clarification": "earthen mound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87851841-c0e9-3f08-bc23-857e2afdc1f6",
                    "sense_definition": "Массовый наплыв, огромное количество чего-либо, появляющееся одновременно (переносное значение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "wave_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На сотрудников службы поддержки обрушился <w>вал</w> звонков от клиентов.",
                            "target_lang_translations": {
                                "en": "A <w>wave</w> of calls from clients hit the support staff."
                            }
                        },
                        {
                            "text": "Информационный <w>вал</w> в интернете мешает найти действительно важные новости.",
                            "target_lang_translations": {
                                "en": "The information <w>flood</w> on the Internet makes it difficult to find truly important news."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вал проблем",
                        "информационный вал",
                        "вал звонков"
                    ],
                    "target_lang_definitions": {
                        "en": "A massive influx or huge amount of something appearing simultaneously."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wave",
                                "target_lang_sense_clarification": "sudden increase"
                            },
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "overwhelming amount"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7699708c-1fdf-319e-b6ce-94932af9d65f",
                    "sense_definition": "Общий объём продукции, произведённой за определённый период (в экономике).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "economics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завод выполнил план по <w>валу</w>, но качество изделий снизилось.",
                            "target_lang_translations": {
                                "en": "The factory met the <w>gross output</w> plan, but the quality of the products decreased."
                            }
                        },
                        {
                            "text": "Экономисты подсчитали общий <w>вал</w> производства за прошлый год.",
                            "target_lang_translations": {
                                "en": "Economists calculated the total <w>gross output</w> of production for the last year."
                            }
                        }
                    ],
                    "common_phrases": [
                        "план по валу",
                        "гнать вал"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "professional"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The total volume of products produced over a certain period."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gross output",
                                "target_lang_sense_clarification": "economics"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c56ab7e3-4da4-3be1-a8ef-9ca02ea4671d",
                    "sense_definition": "Сплошной заградительный артиллерийский огонь.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Артиллерия создала огневой <w>вал</w>, чтобы остановить наступление противника.",
                            "target_lang_translations": {
                                "en": "The artillery created a <w>barrage</w> of fire to stop the enemy's offensive."
                            }
                        },
                        {
                            "text": "Солдаты ждали в укрытии, пока не прекратится смертельный <w>вал</w>.",
                            "target_lang_translations": {
                                "en": "The soldiers waited in the shelter until the deadly <w>barrage</w> ceased."
                            }
                        }
                    ],
                    "common_phrases": [
                        "огневой вал"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Continuous defensive or offensive artillery fire."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "barrage",
                                "target_lang_sense_clarification": "military fire"
                            }
                        ]
                    }
                }
            ]

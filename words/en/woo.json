                    ],
                    "common_phrases": [
                        "woo hoo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения радости, восторга или энтузиазма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ух ты",
                                "target_lang_sense_clarification": "возглас удивления и радости"
                            },
                            {
                                "target_lang_word": "ура",
                                "target_lang_sense_clarification": "возглас ликования"
                            },
                            {
                                "target_lang_word": "ого",
                                "target_lang_sense_clarification": "возглас удивления"
                            }
                        ]
                    }
                }
            ]
        },
        {

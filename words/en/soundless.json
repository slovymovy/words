    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "85f3345f-1528-3e14-8348-965e42381f5d",
                    "sense_definition": "Making no noise or sound; completely silent.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quiet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cat moved across the room with <w>soundless</w> steps.",
                            "target_lang_translations": {
                                "ru": "Кошка пересекла комнату <w>бесшумными</w> шагами."
                            }
                        },
                        {
                            "text": "She cried <w>soundless</w> tears as she watched the sad movie.",
                            "target_lang_translations": {
                                "ru": "Она лила <w>беззвучные</w> слезы, пока смотрела грустный фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "noiseless",
                        "silent"
                    ],
                    "antonyms": [
                        "noisy",
                        "loud"
                    ],
                    "common_phrases": [
                        "soundless steps",
                        "soundless night",
                        "soundless tears"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не издающий ни звука; совершенно тихий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беззву́чный",
                                "target_lang_sense_clarification": "не слышный"
                            },
                            {
                                "target_lang_word": "бесшу́мный",
                                "target_lang_sense_clarification": "не производящий шума"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "408c94d2-bdcd-3993-b3e4-1d72c6b38676",
                    "sense_definition": "Impossible to measure the depth of; extremely deep or unfathomable.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "deep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ship sailed over the <w>soundless</w> depths of the ocean.",
                            "target_lang_translations": {
                                "ru": "Корабль плыл над <w>бездонными</w> глубинами океана."
                            }
                        },
                        {
                            "text": "Ancient sailors feared the <w>soundless</w> abyss of the open sea.",
                            "target_lang_translations": {
                                "ru": "Древние моряки боялись <w>бездонной</w> пучины открытого моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "bottomless",
                        "fathomless",
                        "unfathomable"
                    ],
                    "antonyms": [
                        "shallow"
                    ],
                    "common_phrases": [
                        "soundless deep",
                        "soundless sea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неизмеримой глубины; бездонный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бездо́нный",
                                "target_lang_sense_clarification": "очень глубокий"
                            },
                            {
                                "target_lang_word": "неизмери́мый",
                                "target_lang_sense_clarification": "невозможно измерить"
                            }
                        ]
                    }
                }
            ]
        }
    ]

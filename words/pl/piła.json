                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пила",
                                "target_lang_sense_clarification": "глагол"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c21590e2-595a-317c-a025-e96a45175969",
                    "sense_definition": "narzędzie ręczne lub mechaniczne z zębatym ostrzem, służące do cięcia twardych materiałów, głównie drewna lub metalu",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "tool_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ojciec wziął z warsztatu <piłę>, aby pociąć drewno do kominka.",
                            "target_lang_translations": {
                                "ru": "Отец взял из мастерской <пилу>, чтобы напилить дров для камина."
                            }
                        },
                        {
                            "text": "Ta <piła> mechaniczna jest bardzo głośna, więc załóż słuchawki ochronne.",
                            "target_lang_translations": {
                                "ru": "Эта механическая <пила> очень шумная, поэтому надень защитные наушники."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piła mechaniczna",
                        "piła łańcuchowa",
                        "piła tarczowa",
                        "ostra piła"
                    ],
                    "target_lang_definitions": {
                        "ru": "ручной или механический инструмент с зубчатым лезвием для резки твердых материалов, в основном дерева или металла"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пила",
                                "target_lang_sense_clarification": "инструмент"
                            },
                            {
                                "target_lang_word": "ножовка",
                                "target_lang_sense_clarification": "ручная пила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b19988cc-d33f-33b7-97cb-8a1e10eb3e83",
                    "sense_definition": "potoczne określenie osoby bardzo wymagającej, surowej i nieustępliwej, najczęściej nauczyciela",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza nowa matematyczka to straszna <piła>, u której trudno dostać dobrą ocenę.",
                            "target_lang_translations": {
                                "ru": "Наша новая математичка — ужасная <мегера>, у которой трудно получить хорошую оценку."
                            }
                        },
                        {
                            "text": "Nie bądź taka <piła> i pozwól nam wyjść dzisiaj wcześniej.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <занудой> и позволь нам уйти сегодня пораньше."
                            }
                        }
                    ],
                    "synonyms": [
                        "ryba piła",
                        "gw. (Górny Śląsk) piyła, zega, zyjga",
                        "wykres piłokształtny"

                {
                    "sense_id": "06fe7090-14fd-310a-9efd-b64b869a8698",
                    "sense_definition": "Spullen sorteren of ordenen om overzicht te creëren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_sort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het weekend ga ik mijn kledingkast eens flink <w>uitzoeken</w>.",
                            "target_lang_translations": {
                                "en": "This weekend I am going to really <w>sort out</w> my wardrobe.",
                                "ru": "В выходные я собираюсь хорошенько <w>разобрать</w> свой платяной шкаф."
                            }
                        },
                        {
                            "text": "Zij heeft alle oude foto's <w>uitgezocht</w> en in albums geplakt.",
                            "target_lang_translations": {
                                "en": "She <w>sorted</w> all the old photos and stuck them in albums.",
                                "ru": "Она <w>разобрала</w> все старые фотографии и вклеила их в альбомы."
                            }
                        }
                    ],
                    "synonyms": [
                        "sorteren",
                        "ordenen",
                        "schiften"
                    ],
                    "common_phrases": [
                        "de was uitzoeken",
                        "oude spullen uitzoeken"
                    ],
                    "target_lang_definitions": {
                        "en": "To sort or organize items to create order.",
                        "ru": "Сортировать или упорядочивать вещи для создания порядка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sort out",
                                "target_lang_sense_clarification": "to arrange things systematically or separate them"
                            },
                            {
                                "target_lang_word": "go through",
                                "target_lang_sense_clarification": "to look at items one by one, often to sort them"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "разбирать",
                                "target_lang_sense_clarification": "наводить порядок, сортировать вещи"
                            },
                            {
                                "target_lang_word": "сортировать",
                                "target_lang_sense_clarification": "классифицировать"
                            }
                        ]
                    }
                }

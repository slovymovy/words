                },
                {
                    "sense_id": "b569d33c-5182-3ea1-87d2-c0ca729163e6",
                    "sense_definition": "Broń biała służąca do kłucia lub cięcia, podobna do narzędzia kuchennego, ale przeznaczona do walki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tool_weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz nosił długi <nóż> przy pasie podczas misji.",
                            "target_lang_translations": {
                                "ru": "Солдат носил длинный <нож> на поясе во время задания."
                            }
                        },
                        {
                            "text": "Napastnik wyciągnął <nóż> i zażądał od przechodnia pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Нападавший вытащил <нож> и потребовал у прохожего деньги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nóż bojowy",
                        "nóż myśliwski",
                        "atak nożem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Холодное оружие для колющих или режущих ударов, похожее на кухонный инструмент, но предназначенное для боя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нож",
                                "target_lang_sense_clarification": "оружие"

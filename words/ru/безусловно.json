    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b5c98033-0aa1-3a0f-8d4c-91630388005a",
                    "sense_definition": "Вводное слово, выражающее полную уверенность говорящего в истинности высказывания; конечно, разумеется.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "confidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вы, <w>безусловно</w>, правы в этом непростом вопросе.",
                            "target_lang_translations": {
                                "en": "You are <w>certainly</w> right in this difficult matter."
                            }
                        },
                        {
                            "text": "Мы, <w>безусловно</w>, поможем вам закончить этот проект вовремя.",
                            "target_lang_translations": {
                                "en": "We will <w>undoubtedly</w> help you finish this project on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "конечно",
                        "разумеется",
                        "несомненно",
                        "естественно",
                        "конечно же",
                        "само собой разумеется",
                        "очевидно"
                    ],
                    "antonyms": [
                        "вряд ли"
                    ],
                    "common_phrases": [
                        "ты безусловно прав",
                        "безусловно да"
                    ],
                    "target_lang_definitions": {
                        "en": "An introductory word expressing the speaker's complete confidence in the truth of the statement; certainly, of course."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "certainly",
                                "target_lang_sense_clarification": "expressing certainty"
                            },
                            {
                                "target_lang_word": "undoubtedly",
                                "target_lang_sense_clarification": "without doubt"
                            },
                            {
                                "target_lang_word": "of course",
                                "target_lang_sense_clarification": "naturally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b3a5dc5-c723-337b-82e1-1efcfab2ac0f",
                    "sense_definition": "Совершенно, полностью, без каких-либо условий и оговорок.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты должны <w>безусловно</w> подчиняться приказам своего командира.",
                            "target_lang_translations": {
                                "en": "Soldiers must <w>unconditionally</w> obey their commander's orders."
                            }
                        },
                        {
                            "text": "Я <w>безусловно</w> верю каждому твоему слову и обещанию.",
                            "target_lang_translations": {
                                "en": "I <w>unconditionally</w> believe every word and promise of yours."
                            }
                        }
                    ],
                    "synonyms": [
                        "категорически",
                        "однозначно"
                    ],
                    "common_phrases": [
                        "подчиняться безусловно",
                        "безусловно доверять"
                    ],
                    "target_lang_definitions": {
                        "en": "Completely, fully, without any conditions or reservations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unconditionally",
                                "target_lang_sense_clarification": "without conditions"
                            },
                            {
                                "target_lang_word": "absolutely",
                                "target_lang_sense_clarification": "completely"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "35ad980f-0b9f-3ec8-8828-b23407967d69",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>безусловный</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ваше право на защиту в суде <w>безусловно</w> и гарантировано законом.",
                            "target_lang_translations": {
                                "en": "Your right to defense in court is <w>unconditional</w> and guaranteed by law."
                            }
                        },
                        {
                            "text": "Влияние климата на здоровье человека <w>безусловно</w>.",
                            "target_lang_translations": {
                                "en": "The influence of climate on human health is <w>indisputable</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short form"
                        }
                    ],
                    "target_lang_definitions": {

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загадка",
                                "target_lang_sense_clarification": "вопрос или задание"
                            },
                            {
                                "target_lang_word": "головоломка",
                                "target_lang_sense_clarification": "сложная задача"

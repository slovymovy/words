                    "target_lang_definitions": {
                        "ru": "Говорить или петь громко и четко, чтобы голос был слышен на расстоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направлять голос",
                                "target_lang_sense_clarification": "говорить так, чтобы было слышно далеко"
                            },
                            {
                                "target_lang_word": "звучать",
                                "target_lang_sense_clarification": "быть слышным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cae712b2-807f-30dc-a031-692f50530da5",
                    "sense_definition": "To assume qualities or mindsets in others based on one's own personality.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tends to <w>project</w> his own insecurities onto his colleagues.",
                            "target_lang_translations": {
                                "ru": "Он склонен <w>проецировать</w> свою неуверенность на коллег."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2a9ec623-cfbe-3dff-b258-1f8f47f5ec4c",
                    "sense_definition": "Официальный правовой акт или распоряжение, издаваемое государственным органом или должностным лицом.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "official_decision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство приняло новое <w>постановление</w> о повышении пенсий.",
                            "target_lang_translations": {
                                "en": "The government adopted a new <w>decree</w> on increasing pensions."
                            }
                        },
                        {
                            "text": "Мы должны строго следовать этому правительственному <w>постановлению</w>.",
                            "target_lang_translations": {
                                "en": "We must strictly follow this government <w>decree</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "указ",
                        "декрет"
                    ],
                    "common_phrases": [
                        "правительственное постановление",
                        "проект постановления",
                        "согласно постановлению",
                        "принять постановление"
                    ],
                    "target_lang_definitions": {
                        "en": "An official legal act or order issued by a government body or official."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decree",
                                "target_lang_sense_clarification": "government or executive order"
                            },
                            {
                                "target_lang_word": "resolution",
                                "target_lang_sense_clarification": "formal decision by a body"
                            },
                            {
                                "target_lang_word": "order",
                                "target_lang_sense_clarification": "administrative instruction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "045304cb-74d5-3ea5-a37e-9ea4042a2dac",
                    "sense_definition": "Решение, принятое коллективным органом, собранием или судом по результатам обсуждения какого-либо вопроса.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official_decision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Суд вынес <w>постановление</w> об аресте подозреваемого.",
                            "target_lang_translations": {

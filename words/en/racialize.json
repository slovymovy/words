    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8231fabf-bf4b-3b57-bb16-7d8c60495685",
                    "sense_definition": "To categorize or perceive someone or something according to race.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sociologists often study how institutions <w>racialize</w> specific groups of people.",
                            "target_lang_translations": {
                                "ru": "Социологи часто изучают, как институты <w>расиализируют</w> определенные группы людей."
                            }
                        },
                        {
                            "text": "The media should not <w>racialize</w> crime by focusing only on the background of the suspects.",
                            "target_lang_translations": {
                                "ru": "СМИ не должны <w>придавать расовый характер</w> преступности, фокусируясь только на происхождении подозреваемых."
                            }
                        }
                    ],
                    "synonyms": [
                        "ethnicize"
                    ],
                    "antonyms": [
                        "deracialize"
                    ],
                    "common_phrases": [
                        "racialize a group",
                        "racialize poverty",
                        "racialize the discourse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Классифицировать или воспринимать кого-либо или что-либо исходя из расовой принадлежности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расиализировать",
                                "target_lang_sense_clarification": "социологический термин"
                            },
                            {
                                "target_lang_word": "придавать расовый характер",
                                "target_lang_sense_clarification": "в социальном или политическом контексте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a68ce2c9-97f1-3bab-ba2c-e3036fc96d03",
                    "sense_definition": "To adapt or alter something to fit the ethnic qualities or characteristics of a particular race.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The director tried to <w>racialize</w> the performance to make it feel more authentic to the local culture.",
                            "target_lang_translations": {
                                "ru": "Режиссер пытался <w>придать расовую специфику</w> постановке, чтобы она казалась более аутентичной для местной культуры."
                            }
                        },
                        {
                            "text": "Critics argued that the author sought to <w>racialize</w> the narrative unnecessarily.",
                            "target_lang_translations": {
                                "ru": "Критики утверждали, что автор стремился излишне <w>акцентировать расовый аспект</w> в повествовании."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Адаптировать или изменять что-либо в соответствии с этническими качествами или характеристиками определенной расы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придавать расовую специфику",
                                "target_lang_sense_clarification": "подчеркивать особенности"
                            },
                            {
                                "target_lang_word": "акцентировать расовый аспект",
                                "target_lang_sense_clarification": "выделять расовую составляющую"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "a1b301f4-ece7-3954-a424-2951d996a693",
                    "sense_definition": "Een specerij met een frisse, peperachtige smaak, gemaakt van de wortelstok van de galangaplant.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spice_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt verse <w>laos</w> kopen bij de Aziatische supermarkt.",
                            "target_lang_translations": {
                                "ru": "Свежий <w>галангал</w> можно купить в азиатском супермаркете."
                            }
                        },
                        {
                            "text": "Voeg een theelepel gemalen <w>laos</w> toe aan de soep voor extra smaak.",
                            "target_lang_translations": {
                                "ru": "Добавьте чайную ложку молотого <w>галангала</w> в суп для дополнительного вкуса."
                            }
                        }
                    ],
                    "synonyms": [
                        "laoswortel",
                        "laospoeder"
                    ],
                    "common_phrases": [
                        "verse laos",
                        "gemalen laos",
                        "laos en gember"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пряность со свежим перечным вкусом, приготовленная из корневища растения галангал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галангал",
                                "target_lang_sense_clarification": "пряность"
                            },
                            {
                                "target_lang_word": "калган",
                                "target_lang_sense_clarification": "разговорное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6fdfa16-7374-31f2-a32b-68aeb39a761a",
                    "sense_definition": "De plant Alpinia galanga, familie van de gember, waarvan de wortel als specerij wordt gebruikt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spice_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de botanische tuin zagen we een bloeiende <w>laos</w>.",
                            "target_lang_translations": {
                                "ru": "В ботаническом саду мы видели цветущий <w>галангал</w>."
                            }
                        },
                        {
                            "text": "De <w>laos</w> groeit het beste in een warm en vochtig klimaat.",
                            "target_lang_translations": {
                                "ru": "<w>Галангал</w> лучше всего растет в теплом и влажном климате."
                            }
                        }
                    ],
                    "synonyms": [
                        "laoswortel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение Альпиния галанга семейства Имбирные, корень которого используется в качестве пряности."

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "504b749f-d591-329a-b8b1-3492056c366a",
                    "sense_definition": "Per dag; wat elke dag plaatsvindt of gedaan wordt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts heeft voorgeschreven dat u deze pillen tweemaal <w>daags</w> moet innemen.",
                            "target_lang_translations": {
                                "en": "The doctor prescribed that you must take these pills twice <w>daily</w>.",
                                "ru": "Врач прописал вам принимать эти таблетки два раза <w>в день</w>."
                            }
                        },
                        {
                            "text": "Het nieuws op de website wordt <w>daags</w> bijgewerkt met de laatste berichten.",
                            "target_lang_translations": {
                                "en": "The news on the website is updated <w>daily</w> with the latest reports.",
                                "ru": "Новости на сайте <w>ежедневно</w> обновляются последними сообщениями."
                            }
                        }
                    ],
                    "synonyms": [
                        "dagelijks"
                    ],
                    "common_phrases": [
                        "tweemaal daags",
                        "driemaal daags"
                    ],
                    "target_lang_definitions": {
                        "en": "Per day; occurring or done every day.",
                        "ru": "В день; происходящее или делаемое каждый день."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "daily",
                                "target_lang_sense_clarification": "happening every day"
                            },
                            {
                                "target_lang_word": "a day",
                                "target_lang_sense_clarification": "per day, used in frequencies"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в день",
                                "target_lang_sense_clarification": "используется для указания частоты"
                            },
                            {
                                "target_lang_word": "ежедневно",
                                "target_lang_sense_clarification": "каждый день"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78b5f0f2-b3b9-317b-8a03-6114de40ef02",
                    "sense_definition": "Op de dag; aanduiding van een dag in relatie tot een andere gebeurtenis, meestal gebruikt met 'voor' of 'na'.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relative_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Daags</w> voor de verkiezingen werd de partijleider zenuwachtig voor de uitslag.",
                            "target_lang_translations": {
                                "en": "<w>The day</w> before the elections, the party leader became nervous about the result.",
                                "ru": "<w>За день</w> до выборов лидер партии начал нервничать из-за результатов."
                            }
                        },
                        {
                            "text": "Hij vertrok <w>daags</w> na het grote feest naar het buitenland voor een lange reis.",
                            "target_lang_translations": {
                                "en": "He left for a long trip abroad <w>the day</w> after the big party.",
                                "ru": "Он уехал за границу в долгое путешествие <w>на следующий день</w> после большой вечеринки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "daags voor",
                        "daags na",
                        "daags tevoren"
                    ],
                    "target_lang_definitions": {
                        "en": "On the day; indicating a day in relation to another event, usually used with 'before' or 'after'.",
                        "ru": "В тот же день; указание на день относительно другого события (обычно используется с 'voor' или 'na')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the day",
                                "target_lang_sense_clarification": "used in expressions indicating time relative to an event"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "за день",
                                "target_lang_sense_clarification": "в выражении 'daags voor' (за день до)"
                            },
                            {
                                "target_lang_word": "накануне",
                                "target_lang_sense_clarification": "в выражении 'daags voor' (накануне)"
                            },
                            {
                                "target_lang_word": "на следующий день",
                                "target_lang_sense_clarification": "в выражении 'daags na' (на следующий день после)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e3b95e66-69af-35a8-af2e-dedb46902eda",
                    "sense_definition": "Wat betrekking heeft op het gewone, dagelijkse leven; alledaags.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "everyday",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de vakantie was hij even verlost van zijn <w>daagse</w> beslommeringen.",
                            "target_lang_translations": {
                                "en": "During the holiday he was briefly relieved of his <w>daily</w> worries.",
                                "ru": "Во время отпуска он ненадолго освободился от своих <w>повседневных</w> забот."
                            }
                        },
                        {
                            "text": "In zijn <w>daagse</w> kloffie zag hij er heel anders uit dan in zijn nette pak.",
                            "target_lang_translations": {
                                "en": "In his <w>everyday</w> clothes he looked very different than in his smart suit.",
                                "ru": "В своей <w>будничной</w> одежде он выглядел совсем иначе, чем в строгом костюме."
                            }
                        }
                    ],
                    "synonyms": [
                        "dagelijks"
                    ],
                    "common_phrases": [
                        "daagse kleding",
                        "daagse leven"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to ordinary, daily life; everyday.",
                        "ru": "Относящийся к обычной, повседневной жизни."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "everyday",
                                "target_lang_sense_clarification": "ordinary, commonplace"
                            },
                            {
                                "target_lang_word": "daily",
                                "target_lang_sense_clarification": "relating to ordinary life"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "повседневный",
                                "target_lang_sense_clarification": "обыденный, ежедневный"
                            },
                            {
                                "target_lang_word": "будничный",
                                "target_lang_sense_clarification": "простой, не праздничный"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "wake",
        "awaken",
        "waken",
        "wakeful",
        "awakening"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ea3a4500-86a3-3648-9ace-2cbd5903666d",
                    "sense_definition": "Not asleep; conscious.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_consciousness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was <w>awake</w> all night worrying about the exam.",
                            "target_lang_translations": {
                                "ru": "Я <w>не спал</w> всю ночь, волнуясь об экзамене."
                            }
                        },
                        {
                            "text": "Please stay <w>awake</w> while I tell you the rest of the story.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>не спи</w>, пока я рассказываю остальную часть истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "conscious",
                        "wide awake"
                    ],
                    "antonyms": [
                        "asleep",
                        "unconscious"
                    ],
                    "common_phrases": [
                        "wide awake",
                        "stay awake",
                        "keep awake",
                        "half awake"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не спящий; находящийся в сознании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бодрствующий",
                                "target_lang_sense_clarification": "находящийся в состоянии бодрствования"
                            },
                            {
                                "target_lang_word": "не спящий",
                                "target_lang_sense_clarification": "буквальный перевод"
                            },
                            {
                                "target_lang_word": "проснувшийся",
                                "target_lang_sense_clarification": "тот, кто уже проснулся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "237852ae-4fef-3c5a-9b09-8027392bb84a",
                    "sense_definition": "Alert, aware, or knowing about something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must be <w>awake</w> to the risks involved in this project.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>осознавать</w> риски, связанные с этим проектом."
                            }
                        },
                        {
                            "text": "She is fully <w>awake</w> to the opportunities this job offers.",
                            "target_lang_translations": {
                                "ru": "Она полностью <w>осознает</w> возможности, которые предлагает эта работа."
                            }
                        }
                    ],
                    "synonyms": [
                        "alert",
                        "aware",
                        "mindful",
                        "vigilant",
                        "observant"
                    ],
                    "antonyms": [
                        "oblivious",
                        "heedless",
                        "unconscious"
                    ],
                    "common_phrases": [
                        "awake to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бдительный, осведомленный или знающий о чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осознающий",
                                "target_lang_sense_clarification": "понимающий суть ситуации"
                            },
                            {
                                "target_lang_word": "осведомленный",
                                "target_lang_sense_clarification": "обладающий информацией"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8cc5eae3-67c5-3ff8-989e-241c57e6fd27",
                    "sense_definition": "To become conscious after having slept.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_waking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>awoke</w> to find the sun shining brightly through the window.",
                            "target_lang_translations": {
                                "ru": "Он <w>проснулся</w> и обнаружил, что солнце ярко светит в окно."
                            }
                        },
                        {
                            "text": "I usually <w>awake</w> at six o'clock in the morning without an alarm.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>просыпаюсь</w> в шесть часов утра без будильника."
                            }
                        }
                    ],
                    "synonyms": [
                        "wake up",
                        "awaken",
                        "wake",
                        "arise",
                        "stir"
                    ],
                    "antonyms": [
                        "fall asleep"
                    ],
                    "common_phrases": [
                        "awake from sleep",
                        "awake early"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прийти в сознание после сна; проснуться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просыпаться",
                                "target_lang_sense_clarification": "процесс выхода из сна (несов. вид)"
                            },
                            {
                                "target_lang_word": "проснуться",
                                "target_lang_sense_clarification": "результат выхода из сна (сов. вид)"
                            },
                            {
                                "target_lang_word": "пробуждаться",
                                "target_lang_sense_clarification": "книжный или поэтический вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b615b46-8365-3324-837c-d357a5920cf9",
                    "sense_definition": "To cause somebody to stop sleeping.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action_waking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud noise <w>awoke</w> the baby in the next room.",
                            "target_lang_translations": {
                                "ru": "Громкий шум <w>разбудил</w> ребенка в соседней комнате."
                            }
                        },
                        {
                            "text": "Please do not <w>awake</w> him until noon.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>буди</w> его до полудня."
                            }
                        }
                    ],
                    "synonyms": [
                        "wake",
                        "wake up",
                        "rouse",
                        "awaken"
                    ],
                    "antonyms": [
                        "put to sleep"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то перестать спать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будить",
                                "target_lang_sense_clarification": "прерывать чей-то сон (несов. вид)"
                            },
                            {
                                "target_lang_word": "разбудить",
                                "target_lang_sense_clarification": "заставить проснуться (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3eaf8ab-e701-36bc-94cc-9e0b2753767d",
                    "sense_definition": "To come out of a state of inaction or dormancy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_waking_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Old memories <w>awoke</w> in his mind as he walked through the town.",
                            "target_lang_translations": {
                                "ru": "Старые воспоминания <w>пробудились</w> в его памяти, когда он шел по городу."
                            }
                        },
                        {
                            "text": "The national spirit <w>awoke</w> after years of oppression.",
                            "target_lang_translations": {
                                "ru": "Национальный дух <w>проснулся</w> после долгих лет угнетения."
                            }
                        }
                    ],
                    "synonyms": [
                        "arise",
                        "emerge",
                        "revive"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выйти из состояния бездействия или покоя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробуждаться",
                                "target_lang_sense_clarification": "о чувствах, воспоминаниях или природе"
                            },
                            {
                                "target_lang_word": "оживать",
                                "target_lang_sense_clarification": "становиться активным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "target_lang_definitions": {
                        "ru": "Фамилия шотландского и североанглийского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Белл",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea1ef3e9-c1f4-3c4b-804b-a6cda691586c",
                    "sense_definition": "The Bell telephone company, named after Alexander Graham Bell.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_org",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "He worked for <w>Bell</w> for over thirty years before retiring.",
                            "target_lang_translations": {
                                "ru": "Он проработал в компании <w>Белл</w> более тридцати лет перед выходом на пенсию."
                            }
                        },
                        {
                            "text": "The <w>Bell</w> system was once the primary telephone provider in the country.",
                            "target_lang_translations": {
                                "ru": "Система <w>Белл</w> когда-то была основным поставщиком телефонной связи в стране."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Телефонная компания Bell, названная в честь Александра Грэма Белла."
                    },
                    "translations": {

                {
                    "sense_id": "0ba56828-45c0-3819-a8eb-88e01cacbcde",
                    "sense_definition": "A large farm or estate, especially in a tropical or semitropical country, where crops like cotton, sugar, or coffee are cultivated by resident workers.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "farm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy family owned a vast sugar <w>plantation</w> in the Caribbean.",
                            "target_lang_translations": {
                                "ru": "Богатая семья владела огромной сахарной <w>плантацией</w> на Карибах."
                            }
                        },
                        {
                            "text": "Tourists can visit the historic <w>plantation</w> house to learn about the region's past.",
                            "target_lang_translations": {
                                "ru": "Туристы могут посетить исторический дом на <w>плантации</w>, чтобы узнать о прошлом региона."
                            }
                        }
                    ],
                    "synonyms": [
                        "estate",
                        "farm",
                        "ranch"
                    ],
                    "common_phrases": [
                        "cotton plantation",
                        "sugar plantation",
                        "coffee plantation",
                        "plantation owner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное фермерское хозяйство или поместье, особенно в тропических странах, где выращиваются такие культуры, как хлопок, сахар или кофе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плантация",
                                "target_lang_sense_clarification": "крупное сельскохозяйственное угодье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98dc4892-112a-3103-aa11-dffed2d85c53",
                    "sense_definition": "An area of land planted with trees for commercial production or environmental purposes.",

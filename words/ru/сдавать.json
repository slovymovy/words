                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом раунде твоя очередь <сдавать> карты.",
                            "target_lang_translations": {

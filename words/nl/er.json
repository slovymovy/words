            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5ab558af-c272-3fb0-9713-1500b2fb1a0d",
                    "sense_definition": "Gebruikt als voorlopig onderwerp om aan te geven dat iets of iemand bestaat of aanwezig is (vaak in combinatie met 'zijn').",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Er</w> is geen melk meer in de koelkast."
                        },
                        {
                            "text": "Is <w>er</w> nog iemand in de vergaderzaal?"
                        }
                    ],
                    "common_phrases": [
                        "er is",
                        "er zijn"
                    ]
                },
                {
                    "sense_id": "588f6b09-ec10-39bd-a295-acce39d7c11d",
                    "sense_definition": "Verwijst naar een hoeveelheid of aantal van iets dat eerder genoemd is; betekent 'van die' of 'van dat'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer heeft drie kinderen en ik heb <w>er</w> twee."
                        },
                        {
                            "text": "Hoeveel appels wil je hebben? Ik wil <w>er</w> graag vijf."
                        }
                    ],
                    "synonyms": [
                        "ervan"
                    ]
                },
                {
                    "sense_id": "71499819-4dfb-322b-91a1-7d809ba0d853",
                    "sense_definition": "Verwijst naar dingen, dieren of zaken in combinatie met een voorzetsel; vervangt 'het' of 'ze'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>er</w> gisteren lang met hem over gepraat."
                        },
                        {
                            "text": "Je kunt <w>er</w> de hoge bergen boven zien uitsteken."
                        }
                    ]
                },
                {
                    "sense_id": "6891e5d4-286a-34e1-91a7-e16e752d3784",
                    "sense_definition": "Gebruikt als onderwerp in passieve zinnen om een handeling te beschrijven zonder specifieke dader.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Er</w> wordt hard gewerkt aan de nieuwe weg."
                        },
                        {
                            "text": "Tijdens het feest werd <w>er</w> veel gelachen en gedanst."
                        }
                    ],
                    "common_phrases": [

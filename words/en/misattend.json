    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "96cef61b-7d2d-3086-947c-7a54186817bc",
                    "sense_definition": "To fail to pay attention to something, often resulting in misunderstanding or disregard.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "lack_of_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The researcher noted that tired subjects tend to <w>misattend</w> to subtle visual cues during the experiment.",
                            "target_lang_translations": {
                                "ru": "Исследователь отметил, что уставшие испытуемые склонны <w>упускать из виду</w> тонкие визуальные сигналы во время эксперимента."
                            }
                        },
                        {
                            "text": "If you <w>misattend</w> to the specific details of the contract, you might agree to unfavorable terms.",
                            "target_lang_translations": {
                                "ru": "Если вы <w>невнимательно отнесетесь</w> к конкретным деталям контракта, вы можете согласиться на невыгодные условия."
                            }
                        }
                    ],
                    "synonyms": [
                        "disregard",
                        "ignore",
                        "overlook",
                        "neglect"
                    ],
                    "antonyms": [
                        "heed",
                        "notice",
                        "attend"
                    ],
                    "common_phrases": [
                        "misattend to facts",
                        "misattend to duties"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не уделить должного внимания чему-либо, что часто приводит к недопониманию или игнорированию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не уделить внимания",
                                "target_lang_sense_clarification": "отсутствие концентрации на объекте"
                            },
                            {
                                "target_lang_word": "упустить из виду",
                                "target_lang_sense_clarification": "не заметить что-то важное"
                            },
                            {
                                "target_lang_word": "проигнорировать",
                                "target_lang_sense_clarification": "намеренно или ненамеренно не обратить внимания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e3a7c7c-d150-3a66-8898-1fd1dbedc480",
                    "sense_definition": "To fail to look after or care for someone or something properly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "lack_of_care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The board investigated the physician who was alleged to <w>misattend</w> his patients in the rural district.",
                            "target_lang_translations": {
                                "ru": "Комиссия провела расследование в отношении врача, который, как утверждалось, <w>не уделял должного внимания</w> своим пациентам в сельском районе."
                            }
                        },
                        {
                            "text": "It is a serious professional error to <w>misattend</w> the execution of important legal documents.",
                            "target_lang_translations": {
                                "ru": "Серьезная профессиональная ошибка — <w>халатно отнестись</w> к оформлению важных юридических документов."
                            }
                        }
                    ],
                    "synonyms": [
                        "neglect",
                        "mistreat"
                    ],
                    "antonyms": [
                        "tend",
                        "nurture",
                        "care for"
                    ],
                    "common_phrases": [
                        "misattend the poor",
                        "misattend patients"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не обеспечить надлежащего ухода или заботы о ком-либо или чем-либо; проявить халатность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "халатно относиться",
                                "target_lang_sense_clarification": "проявлять небрежность в обязанностях"
                            },
                            {
                                "target_lang_word": "пренебрегать",
                                "target_lang_sense_clarification": "не заботиться должным образом"
                            },
                            {
                                "target_lang_word": "плохо ухаживать",
                                "target_lang_sense_clarification": "не оказывать должной помощи или сервиса"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "75e72fd1-66b2-30f8-8564-68bf04cdf082",
                    "sense_definition": "Rodzaj łyżwy przykręcanej do buta.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znaleźliśmy na strychu zardzewiałe <w>kanadyjki</w>, które pamiętają czasy przedwojenne.",
                            "target_lang_translations": {
                                "ru": "Мы нашли на чердаке ржавые <w>канадки</w>, которые помнят довоенные времена."
                            }
                        },
                        {
                            "text": "Kiedyś dzieci jeździły na <w>kanadyjkach</w> przykręcanych kluczykiem do zimowych butów.",
                            "target_lang_translations": {
                                "ru": "Когда-то дети катались на <w>канадках</w>, прикручиваемых ключиком к зимним ботинкам."
                            }
                        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02218bc4-26c8-3c8c-98b3-4493bb5fc5b5",
                    "sense_definition": "A person who acts on behalf of another person or organization, especially in sales or customer service; a short form of <w>representative</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_representative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our sales <w>rep</w> will visit your office tomorrow to discuss the contract.",
                            "target_lang_translations": {
                                "ru": "Наш торговый <w>представитель</w> посетит ваш офис завтра, чтобы обсудить контракт."
                            }
                        },
                        {
                            "text": "I spoke to a customer service <w>rep</w> about the billing error.",
                            "target_lang_translations": {
                                "ru": "Я говорил с <w>представителем</w> службы поддержки клиентов об ошибке в счете."
                            }
                        }
                    ],
                    "synonyms": [
                        "agent",
                        "delegate"
                    ],
                    "common_phrases": [
                        "sales rep",
                        "customer service rep",
                        "union rep"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping of representative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, действующий от имени другого лица или организации, особенно в сфере продаж или обслуживания клиентов; сокращение от <w>representative</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представитель",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "торговый представитель",
                                "target_lang_sense_clarification": "в контексте продаж"
                            },
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "посредник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ead1cdcf-c075-378e-802c-2b05557e1b48",
                    "sense_definition": "A single complete performance of an exercise movement; a short form of <w>repetition</w>.",

                    "sense_id": "bcbe72af-b76c-375a-8e16-29b5c8d4eb6f",
                    "sense_definition": "De actie van het vertrekken of weggaan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "departure_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>vertrek</w> kwam nogal onaangekondigd en verraste iedereen.",
                            "target_lang_translations": {
                                "en": "His <w>departure</w> came rather unannounced and surprised everyone.",
                                "ru": "Его <w>отъезд</w> был довольно неожиданным и удивил всех."
                            }
                        },
                        {
                            "text": "Het vliegtuig is helemaal klaar voor <w>vertrek</w> naar het zuiden.",
                            "target_lang_translations": {
                                "en": "The plane is completely ready for <w>departure</w> to the south.",
                                "ru": "Самолет полностью готов к <w>отправлению</w> на юг."
                            }
                        }
                    ],
                    "synonyms": [
                        "afreis",
                        "weggaan"
                    ],
                    "antonyms": [
                        "aankomst"
                    ],
                    "common_phrases": [
                        "klaar voor vertrek",
                        "bij vertrek",
                        "tijd van vertrek"
                    ],
                    "target_lang_definitions": {
                        "en": "The action of leaving or going away.",
                        "ru": "Действие, связанное с уходом или отъездом откуда-либо; начало пути."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "departure",
                                "target_lang_sense_clarification": "act of leaving"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отъезд",
                                "target_lang_sense_clarification": "отбытие человека куда-либо"
                            },
                            {
                                "target_lang_word": "отправление",
                                "target_lang_sense_clarification": "начало движения транспорта"
                            },
                            {
                                "target_lang_word": "уход",
                                "target_lang_sense_clarification": "действие по значению уходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13cfb68d-66f2-37a0-94fc-d50b2027e93d",
                    "sense_definition": "Een afgesloten deel van een woning of gebouw.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "room_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij verliet het <w>vertrek</w> en begaf zich direct naar het balkon.",
                            "target_lang_translations": {
                                "en": "He left the <w>room</w> and went straight to the balcony.",
                                "ru": "Он покинул <w>помещение</w> и сразу направился на балкон."
                            }
                        },
                        {
                            "text": "In het grote <w>vertrek</w> stonden alleen een tafel en twee stoelen.",
                            "target_lang_translations": {
                                "en": "In the large <w>room</w> stood only a table and two chairs.",
                                "ru": "В большой <w>комнате</w> стояли только стол и два стула."
                            }
                        }
                    ],
                    "synonyms": [
                        "kamer",
                        "ruimte"
                    ],
                    "common_phrases": [
                        "in het vertrek",
                        "het aangrenzende vertrek"

{
    "word_family": [
        "ubrać",
        "ubranie",
        "ubiór",
        "rozbierać",
        "przebierać",
        "dobierać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7da089a4-0d76-3c81-acb6-dd9b79335c8a",
                    "sense_definition": "Zakładać na siebie ubranie, nosić odzież (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra zawsze <ubiera> się bardzo modnie i elegancko.",
                            "target_lang_translations": {
                                "ru": "Моя сестра всегда <одевается> очень модно и элегантно."
                            }
                        },
                        {
                            "text": "Musisz <ubierać> się ciepło, ponieważ na zewnątrz jest mróz.",
                            "target_lang_translations": {
                                "ru": "Ты должен <одеваться> тепло, так как на улице мороз."
                            }
                        }
                    ],
                    "synonyms": [
                        "odziewać się",
                        "przyodziewać się"
                    ],
                    "antonyms": [
                        "rozbierać się"
                    ],
                    "common_phrases": [
                        "ubierać się modnie",
                        "ubierać się ciepło",
                        "ubierać się na czarno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать на себя одежду, носить одежду (используется с возвратным местоимением \"się\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеваться",
                                "target_lang_sense_clarification": "надевать одежду на себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fde9c01d-4db3-32a3-96e1-644f5a156ab4",
                    "sense_definition": "Zakładać na kogoś ubranie; pomagać komuś w założeniu odzieży.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama codziennie rano <ubiera> swojego małego synka do przedszkola.",
                            "target_lang_translations": {
                                "ru": "Мама каждое утро <одевает> своего маленького сына в детский сад."
                            }
                        },
                        {
                            "text": "Pielęgniarka pomaga <ubierać> pacjenta po badaniu lekarskim.",
                            "target_lang_translations": {
                                "ru": "Медсестра помогает <одевать> пациента после врачебного осмотра."
                            }
                        }
                    ],
                    "synonyms": [
                        "odziewać",
                        "przyodziewać"
                    ],
                    "antonyms": [
                        "rozbierać"
                    ],
                    "common_phrases": [
                        "ubierać dziecko",
                        "ubierać lalkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать на кого-то одежду; помогать кому-то одеться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одевать",
                                "target_lang_sense_clarification": "облачать кого-либо в одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78beb971-8c1d-35ab-8ef2-f5a9e8eaad90",
                    "sense_definition": "Ozdabiać coś, dekorować elementami ozdobnymi (najczęściej używane w kontekście choinki).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "decorating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Wigilię cała nasza rodzina wspólnie <ubiera> choinkę w salonie.",
                            "target_lang_translations": {
                                "ru": "В сочельник вся наша семья вместе <наряжает> ёлку в гостиной."
                            }
                        },
                        {
                            "text": "Dzieci uwielbiają <ubierać> świąteczne drzewko kolorowymi bombkami.",
                            "target_lang_translations": {
                                "ru": "Дети обожают <наряжать> праздничное дерево разноцветными шарами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ubierać choinkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать что-либо, декорировать (чаще всего используется в контексте новогодней ёлки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наряжать",
                                "target_lang_sense_clarification": "украшать (обычно ёлку)"
                            },
                            {
                                "target_lang_word": "украшать",
                                "target_lang_sense_clarification": "декорировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21f0f311-e237-3f6c-8c1a-20ca10036e6e",
                    "sense_definition": "Zakładać konkretną część garderoby (użycie potoczne, często uznawane za niepoprawne w języku starannym, gdzie preferowane jest słowo \"zakładać\").",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing_item",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szybko <ubieraj> buty, bo zaraz ucieknie nam autobus.",
                            "target_lang_translations": {
                                "ru": "Быстро <надевай> ботинки, а то сейчас уйдёт наш автобус."
                            }
                        },
                        {
                            "text": "Nie <ubieraj> tej kurtki, bo jest na ciebie za mała.",
                            "target_lang_translations": {
                                "ru": "Не <надевай> эту куртку, так как она тебе мала."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakładać",
                        "nakładać"
                    ],
                    "common_phrases": [
                        "ubierać buty",
                        "ubierać kurtkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often criticized by prescriptivists who prefer 'zakładać'"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "Małopolska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать конкретный предмет одежды (разговорное, часто считается неправильным, где предпочтительнее \"zakładać\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надевать",
                                "target_lang_sense_clarification": "надевать что-то на себя (литературная норма)"
                            },
                            {
                                "target_lang_word": "одевать",
                                "target_lang_sense_clarification": "надевать что-то на себя (разговорное, часто считается ошибкой)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "415d21b2-364a-3c48-aef9-293ded12b42d",
                    "sense_definition": "Een korte periode van rust of onderbreking tijdens werk of een andere activiteit.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we even een <w>break</w> nemen en koffie gaan drinken?",
                            "target_lang_translations": {
                                "ru": "Сделаем небольшой <w>перерыв</w> и выпьем кофе?"
                            }
                        },
                        {
                            "text": "Na drie uur studeren had de student echt behoefte aan een <w>break</w>.",
                            "target_lang_translations": {
                                "ru": "После трех часов занятий студенту был просто необходим <w>перерыв</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pauze",
                        "onderbreking"
                    ],
                    "common_phrases": [
                        "een korte break",
                        "even een break",
                        "een break nemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий период отдыха или перерыв в работе или другой деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перерыв",
                                "target_lang_sense_clarification": "отдых"
                            },
                            {
                                "target_lang_word": "пауза",
                                "target_lang_sense_clarification": "временная остановка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d532347e-822e-3c66-837f-f4eb5f8fc054",
                    "sense_definition": "Een type auto waarbij het dak naar achteren doorloopt, waardoor er meer bagageruimte is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat ze drie kinderen hebben, rijden ze in een ruime <w>break</w>.",
                            "target_lang_translations": {
                                "ru": "Так как у них трое детей, они ездят на просторном <w>универсале</w>."
                            }
                        },
                        {
                            "text": "Alle koffers voor de vakantie pasten gemakkelijk achterin de <w>break</w>.",
                            "target_lang_translations": {
                                "ru": "Все чемоданы для отпуска легко поместились в багажник <w>универсала</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stationwagen",
                        "stationcar",
                        "stationwagon",
                        "combi"
                    ],
                    "common_phrases": [
                        "een ruime break",
                        "rijden in een break"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип автомобиля, у которого крыша продлена назад, что обеспечивает больше места для багажа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "универсал",
                                "target_lang_sense_clarification": "тип кузова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26435b65-bc17-3e92-9996-b4b8f2fcfcab",
                    "sense_definition": "Het winnen van een spel bij tennis terwijl de tegenstander aan de beurt is om te serveren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een beslissende <w>break</w> wist hij de eerste set te winnen.",
                            "target_lang_translations": {
                                "ru": "Сделав решающий <w>брейк</w>, он сумел выиграть первый сет."
                            }
                        },
                        {
                            "text": "Het lukte de tennisser niet om de <w>break</w> van zijn tegenstander te herstellen.",
                            "target_lang_translations": {
                                "ru": "Теннисисту не удалось отыграть <w>брейк</w> соперника."
                            }
                        }
                    ],
                    "synonyms": [
                        "servicebreak"
                    ],
                    "common_phrases": [
                        "een break forceren",
                        "de beslissende break"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выигрыш гейма в теннисе во время подачи соперника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брейк",
                                "target_lang_sense_clarification": "теннис"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a23777b6-5544-3332-bf32-fd478d920daa",
                    "sense_definition": "Een instrumentaal tussenstuk in een muzieknummer waarin het ritme of de percussie centraal staat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De drummer speelde een indrukwekkende <w>break</w> midden in het lied.",
                            "target_lang_translations": {
                                "ru": "Барабанщик сыграл впечатляющий <w>брейк</w> в середине песни."
                            }
                        },
                        {
                            "text": "Tijdens de <w>break</w> stopte de zang en begon iedereen te dansen op het ritme.",
                            "target_lang_translations": {
                                "ru": "Во время <w>брейка</w> вокал прекратился, и все начали танцевать в ритм."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een funky break",
                        "de break in het nummer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструментальная вставка в музыкальной композиции, где акцент делается на ритм или перкуссию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брейк",
                                "target_lang_sense_clarification": "музыка"
                            },
                            {
                                "target_lang_word": "сбивка",
                                "target_lang_sense_clarification": "барабанная партия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69ce3d28-9b39-3d03-a469-283ee757e5d3",
                    "sense_definition": "Een groot, open rijtuig met vier wielen en banken aan de zijkanten.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op zondag maakte de familie een tochtje met de <w>break</w> door het park.",
                            "target_lang_translations": {
                                "ru": "В воскресенье семья совершила прогулку по парку на <w>брейке</w>."
                            }
                        },
                        {
                            "text": "Het paard trok de zware <w>break</w> over de onverharde weg.",
                            "target_lang_translations": {
                                "ru": "Лошадь тянула тяжелый <w>брейк</w> по грунтовой дороге."
                            }
                        }

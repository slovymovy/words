{
    "word_family": [
        "magisch",
        "magiër"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fbddd994-a3fd-3a01-9422-ed5c7c622cd0",
                    "sense_definition": "Het gebruik van rituelen, spreuken of symbolen om bovennatuurlijke krachten aan te roepen of te beïnvloeden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het sprookje gebruikte de oude heks duistere <w>magie</w> om de prins te vervloeken.",
                            "target_lang_translations": {
                                "ru": "В сказке старая ведьма использовала темную <w>магию</w>, чтобы проклясть принца."
                            }
                        },
                        {
                            "text": "Veel mensen geloven niet in <w>magie</w>, maar vinden de verhalen erover wel spannend.",
                            "target_lang_translations": {
                                "ru": "Многие люди не верят в <w>магию</w>, но находят истории о ней захватывающими."
                            }
                        }
                    ],
                    "synonyms": [
                        "toverij",
                        "toverkracht",
                        "toverkunst",
                        "tovenarij"
                    ],
                    "common_phrases": [
                        "zwarte magie",
                        "witte magie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использование ритуалов, заклинаний или символов для обращения к сверхъестественным силам или влияния на них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магия",
                                "target_lang_sense_clarification": "сверхъестественные силы"
                            },
                            {
                                "target_lang_word": "колдовство",
                                "target_lang_sense_clarification": "чародейство"
                            },
                            {
                                "target_lang_word": "волшебство",
                                "target_lang_sense_clarification": "чудодейственная сила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bec59da-549f-33c6-a88a-193c7ed3ad8b",
                    "sense_definition": "Een bijzondere, betoverende sfeer of aantrekkingskracht die iets heel speciaal maakt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen de zon onderging, voelde iedereen de <w>magie</w> van het prachtige landschap.",
                            "target_lang_translations": {
                                "ru": "Когда солнце садилось, все ощутили <w>магию</w> прекрасного пейзажа."
                            }
                        },
                        {
                            "text": "De <w>magie</w> van hun eerste ontmoeting is iets wat ze nooit zullen vergeten.",
                            "target_lang_translations": {
                                "ru": "<w>Магия</w> их первой встречи — это то, что они никогда не забудут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de magie van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особая, чарующая атмосфера или притягательность, делающая что-либо особенным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магия",
                                "target_lang_sense_clarification": "особая атмосфера"
                            },
                            {
                                "target_lang_word": "очарование",
                                "target_lang_sense_clarification": "притягательность"
                            },
                            {
                                "target_lang_word": "волшебство",
                                "target_lang_sense_clarification": "метафорическое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08cd1d88-b914-3dae-99c0-6f6eff4872c1",
                    "sense_definition": "De kunst van het vermaken van publiek door middel van trucs en illusies; goochelkunst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen keken met open mond naar de <w>magie</w> van de goochelaar op het podium.",
                            "target_lang_translations": {
                                "ru": "Дети с открытыми ртами смотрели на <w>магию</w> фокусника на сцене."
                            }
                        },
                        {
                            "text": "Hij oefent elke dag op zijn kaarttrucs omdat hij dol is op <w>magie</w> en illusionisme.",
                            "target_lang_translations": {
                                "ru": "Он каждый день тренирует свои карточные трюки, так как обожает <w>магию</w> и иллюзионизм."
                            }
                        }
                    ],
                    "synonyms": [
                        "goochelkunst",
                        "illusionisme"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусство развлечения публики с помощью трюков и иллюзий; сценическая магия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фокусы",
                                "target_lang_sense_clarification": "трюки"
                            },
                            {
                                "target_lang_word": "иллюзионизм",
                                "target_lang_sense_clarification": "сценическое искусство"
                            },
                            {
                                "target_lang_word": "магия",
                                "target_lang_sense_clarification": "сценические чудеса"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
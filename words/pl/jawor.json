                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Явор",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3721ae0b-0235-3066-8183-332ab34e1451",
                    "sense_definition": "Rzadkie imię męskie pochodzenia słowiańskiego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "given_names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Jawor</w> to bardzo oryginalne imię, które rzadko spotyka się w Polsce.",
                            "target_lang_translations": {
                                "ru": "<w>Явор</w> — это очень оригинальное имя, которое редко встречается в Польше."
                            }
                        },
                        {
                            "text": "Rodzice postanowili nazwać syna <w>Jawor</w>, nawiązując do siły i piękna natury.",
                            "target_lang_translations": {
                                "ru": "Родители решили назвать сына <w>Явор</w>, ссылаясь на силу и красоту природы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Редкое мужское имя славянского происхождения."
                    },
                    "translations": {

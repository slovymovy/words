{
    "word_family": [
        "pleasure",
        "pleasant",
        "displeasure",
        "unpleasant",
        "pleasurable"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "07fa2b8e-19df-3b56-af90-cef78ed65633",
                    "sense_definition": "Used to make a polite request.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could you tell me the time, <w>please</w>?",
                            "target_lang_translations": {
                                "ru": "Скажите, <w>пожалуйста</w>, который час?"
                            }
                        },
                        {
                            "text": "Would you <w>please</w> sign this form for me?",
                            "target_lang_translations": {
                                "ru": "Подпишите, <w>пожалуйста</w>, этот бланк для меня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "yes please",
                        "please do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения вежливой просьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "стандартная вежливая форма"
                            },
                            {
                                "target_lang_word": "будьте добры",
                                "target_lang_sense_clarification": "вежливое обращение"
                            },
                            {
                                "target_lang_word": "прошу",
                                "target_lang_sense_clarification": "более формальная просьба"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e79ab007-722b-3ca0-bbb7-fb363b6179ce",
                    "sense_definition": "To make happy or satisfy; to give pleasure to.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her excellent presentation <w>pleased</w> the executives.",
                            "target_lang_translations": {
                                "ru": "Её отличная презентация <w>порадовала</w> руководство."
                            }
                        },
                        {
                            "text": "It is very difficult to <w>please</w> everyone all the time.",
                            "target_lang_translations": {
                                "ru": "Очень трудно всё время <w>угождать</w> всем."
                            }
                        }
                    ],
                    "synonyms": [
                        "satisfy",
                        "delight",
                        "gratify",
                        "content",
                        "gladden"
                    ],
                    "antonyms": [
                        "displease",
                        "annoy",
                        "irritate",
                        "disgust"
                    ],
                    "common_phrases": [
                        "hard to please",
                        "aim to please",
                        "pleased to meet you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать счастливым или довольным; доставлять удовольствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радовать",
                                "target_lang_sense_clarification": "доставлять радость"
                            },
                            {
                                "target_lang_word": "угождать",
                                "target_lang_sense_clarification": "удовлетворять чьи-либо желания или требования"
                            },
                            {
                                "target_lang_word": "нравиться",
                                "target_lang_sense_clarification": "быть приятным кому-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "241ae251-bc60-303c-9abf-e2e28acd0dcf",
                    "sense_definition": "To desire, wish, or will to do something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are free to do exactly as you <w>please</w>.",
                            "target_lang_translations": {
                                "ru": "Вы вольны поступать именно так, как <w>пожелаете</w>."
                            }
                        },
                        {
                            "text": "He does not listen to advice and just says whatever he <w>pleases</w>.",
                            "target_lang_translations": {
                                "ru": "Он не слушает советов и просто говорит всё, что <w>хочет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "desire"
                    ],
                    "common_phrases": [
                        "as you please",
                        "do as you please"
                    ],
                    "target_lang_definitions": {
                        "ru": "Желать, хотеть или иметь намерение сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "иметь желание"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "стремиться к чему-либо"
                            },
                            {
                                "target_lang_word": "угодно",
                                "target_lang_sense_clarification": "в конструкциях типа \"как вам угодно\""
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1650867c-be75-354c-baa7-5f404d9e720e",
                    "sense_definition": "Used as a polite affirmative response to an offer.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Would you like some more tea? — Yes, <w>please</w>.",
                            "target_lang_translations": {
                                "ru": "Хотите ещё чаю? — Да, <w>пожалуйста</w>."
                            }
                        },
                        {
                            "text": "May I help you with your bags? — <w>Please</w>.",
                            "target_lang_translations": {
                                "ru": "Могу я помочь вам с сумками? — <w>Пожалуйста</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "yes please"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как вежливый утвердительный ответ на предложение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "вежливое согласие"
                            },
                            {
                                "target_lang_word": "с удовольствием",
                                "target_lang_sense_clarification": "выражение радости при согласии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7a9f1b7-afa4-31ce-98ef-f50a57266e51",
                    "sense_definition": "An expression of annoyance, impatience, or exasperation.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oh, <w>please</w>, do we really have to hear that story again?",
                            "target_lang_translations": {
                                "ru": "Ой, <w>я тебя умоляю</w>, неужели нам обязательно слушать эту историю снова?"
                            }
                        },
                        {
                            "text": "You think you can fix it in five minutes? <w>Please</w>!",
                            "target_lang_translations": {
                                "ru": "Ты думаешь, что сможешь починить это за пять минут? <w>Не смеши</w>!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение раздражения, нетерпения или негодования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "я тебя умоляю",
                                "target_lang_sense_clarification": "разговорное выражение недоверия или раздражения"
                            },
                            {
                                "target_lang_word": "да ладно",
                                "target_lang_sense_clarification": "скептическое восклицание"
                            },
                            {
                                "target_lang_word": "ой, всё",
                                "target_lang_sense_clarification": "выражение нежелания продолжать разговор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24fbf9d9-b83a-3041-9064-27058d4e2afc",
                    "sense_definition": "Said as a request to repeat information that was not heard or understood.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I didn't quite catch what you said. <w>Please</w>?",
                            "target_lang_translations": {
                                "ru": "Я не совсем расслышал, что вы сказали. <w>Простите</w>?"
                            }
                        },
                        {
                            "text": "When the customer mumbled his order, the clerk simply asked, \"<w>Please</w>?\"",
                            "target_lang_translations": {
                                "ru": "Когда клиент пробормотал свой заказ, клерк просто переспросил: «<w>Простите</w>?»"
                            }
                        }
                    ],
                    "synonyms": [
                        "pardon",
                        "excuse me"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Cincinnati, Ohio"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как просьба повторить информацию, которая не была услышана или понята."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простите?",
                                "target_lang_sense_clarification": "вежливый переспрос"
                            },
                            {
                                "target_lang_word": "что?",
                                "target_lang_sense_clarification": "простой вопрос при непонимании"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fastener_lock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>bolt</w> the heavy shelves to the wall for safety.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>прикрутите</w> тяжёлые полки к стене для безопасности."
                            }
                        },
                        {
                            "text": "They <w>bolted</w> the metal plates together to form the frame.",
                            "target_lang_translations": {
                                "ru": "Они <w>скрепили</w> металлические пластины болтами, чтобы собрать раму."
                            }
                        }
                    ],
                    "synonyms": [
                        "fasten",
                        "secure",
                        "screw"
                    ],
                    "antonyms": [
                        "unbolt"
                    ],
                    "common_phrases": [
                        "bolt down",
                        "bolt together"

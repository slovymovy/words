{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "7a6dc836-7a00-391d-a9f9-c74434374bc5",
                    "sense_definition": "Lower in spatial position than.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_pos",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The treasure is buried two meters <w>below</w> the surface.",
                            "target_lang_translations": {
                                "ru": "Сокровище зарыто на два метра <w>ниже</w> поверхности."
                            }
                        },
                        {
                            "text": "The marmalade is on the shelf <w>below</w> the bread.",
                            "target_lang_translations": {
                                "ru": "Мармелад стоит на полке <w>под</w> хлебом."
                            }
                        }
                    ],
                    "synonyms": [
                        "under",
                        "beneath",
                        "underneath"
                    ],
                    "antonyms": [
                        "above",
                        "over"
                    ],
                    "common_phrases": [
                        "below the surface",
                        "below the knee",
                        "below the horizon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в более низком пространственном положении, чем что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "под",
                                "target_lang_sense_clarification": "указывает на нахождение непосредственно снизу от чего-либо"
                            },
                            {
                                "target_lang_word": "ниже",
                                "target_lang_sense_clarification": "указывает на более низкий уровень в пространстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bf8f39e-1971-34b0-b8f6-288efe3e8e23",
                    "sense_definition": "Lower than in value, price, rank, or amount.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "value_pos",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature is <w>below</w> zero.",
                            "target_lang_translations": {
                                "ru": "Температура <w>ниже</w> нуля."
                            }
                        },
                        {
                            "text": "Liverpool are <w>below</w> Manchester City in the Premier League.",
                            "target_lang_translations": {
                                "ru": "«Ливерпуль» находится <w>ниже</w> «Манчестер Сити» в Премьер-лиге."
                            }
                        }
                    ],
                    "synonyms": [
                        "under"
                    ],
                    "antonyms": [
                        "above",
                        "over"
                    ],
                    "common_phrases": [
                        "below average",
                        "below zero",
                        "below expectations"
                    ],
                    "target_lang_definitions": {
                        "ru": "Меньше по величине, цене, рангу или количеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниже",
                                "target_lang_sense_clarification": "указывает на меньшую степень, ранг или количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cb33c14-16d4-3ef9-8202-ab99fd404353",
                    "sense_definition": "Unsuitable to the rank or dignity of.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dignity_pos",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Such petty behavior is <w>below</w> me.",
                            "target_lang_translations": {
                                "ru": "Такое мелочное поведение <w>ниже</w> моего достоинства."
                            }
                        },
                        {
                            "text": "He thinks no task is <w>below</w> his regard.",
                            "target_lang_translations": {
                                "ru": "Он считает, что ни одна задача не <w>ниже</w> его внимания."
                            }
                        }
                    ],
                    "synonyms": [
                        "beneath"
                    ],
                    "common_phrases": [
                        "below one's dignity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Недостойный чьего-либо положения или статуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниже",
                                "target_lang_sense_clarification": "недостойный, унизительный для кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d6a3d73-d9b9-3d58-8595-4548d6147c05",
                    "sense_definition": "Further down a river or stream than.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spatial_pos",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The River Thames is tidal <w>below</w> Teddington Lock.",
                            "target_lang_translations": {
                                "ru": "Темза подвержена приливам <w>ниже</w> шлюза Теддингтон."
                            }
                        },
                        {
                            "text": "Fishing is prohibited in the area <w>below</w> the dam.",
                            "target_lang_translations": {
                                "ru": "Рыбалка запрещена в зоне <w>ниже</w> плотины."
                            }
                        }
                    ],
                    "synonyms": [
                        "downstream"
                    ],
                    "antonyms": [
                        "upstream"
                    ],
                    "common_phrases": [
                        "below the bridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дальше вниз по течению реки или ручья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниже",
                                "target_lang_sense_clarification": "далее по течению"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [

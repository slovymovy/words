                            }
                        },
                        {
                            "text": "Every ticket <w>holder</w> must present identification at the gate.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>обладатель</w> билета должен предъявить удостоверение личности у входа."
                            }
                        }
                    ],
                    "synonyms": [
                        "owner",
                        "possessor",
                        "bearer",
                        "keeper"
                    ],
                    "common_phrases": [
                        "account holder",
                        "ticket holder",
                        "passport holder",
                        "policy holder",
                        "office holder"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4ec339e7-97b5-3b07-bafc-754beab1f381",
                    "sense_definition": "Рассматривать несколько вариантов и определять, какой из них нужен, подходит или нравится больше всего.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго ходили по магазинам и <выбирали> подарок для мамы.",
                            "target_lang_translations": {
                                "en": "We spent a long time shopping and <picked> a gift for mom."
                            }
                        },
                        {
                            "text": "В ресторане ты всегда долго <выбираешь>, что заказать на ужин.",
                            "target_lang_translations": {
                                "en": "At a restaurant, you always take a long time to <choose> what to order for dinner."
                            }
                        },
                        {
                            "text": "В жизни часто приходится <выбирать> между карьерой и семьёй.",
                            "target_lang_translations": {
                                "en": "In life, one often has to <choose> between career and family."
                            }
                        }
                    ],
                    "synonyms": [
                        "отбирать",
                        "брать",
                        "находить"
                    ],
                    "common_phrases": [
                        "выбирать подарок",
                        "выбирать одежду",
                        "выбирать путь",
                        "выбирать из двух",
                        "выбирать профессию"
                    ],
                    "target_lang_definitions": {
                        "en": "To decide which option or person you want from a group of possibilities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "general term for making a decision"
                            },
                            {
                                "target_lang_word": "pick",
                                "target_lang_sense_clarification": "informal, often for everyday choices"
                            },
                            {
                                "target_lang_word": "select",
                                "target_lang_sense_clarification": "implies a careful or formal choice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "734a7240-9237-3ef5-9bfe-0c8a685f258d",
                    "sense_definition": "Назначать кого-либо на должность или поручать выполнение обязанностей путём голосования.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "election",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждые несколько лет граждане страны идут на участки, чтобы <выбирать> президента.",
                            "target_lang_translations": {
                                "en": "Every few years, the country's citizens go to the polls to <elect> a president."
                            }
                        },
                        {
                            "text": "На общем собрании жильцы решили <выбирать> нового председателя.",
                            "target_lang_translations": {
                                "en": "At the general meeting, the tenants decided to <elect> a new chairman."
                            }
                        }
                    ],
                    "synonyms": [
                        "избирать",
                        "голосовать"
                    ],
                    "common_phrases": [
                        "выбирать президента",
                        "выбирать депутата",
                        "выбирать председателя",
                        "выбирать мэра"
                    ],
                    "target_lang_definitions": {
                        "en": "To appoint someone to an official position through a voting process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "elect",
                                "target_lang_sense_clarification": "to choose by voting"
                            },
                            {
                                "target_lang_word": "vote for",
                                "target_lang_sense_clarification": "the action of casting a ballot"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "986e21bb-56f5-32c4-b6bd-75fb53b667bb",
                    "sense_definition": "Находить свободное время или удобный момент для совершения какого-либо действия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Несмотря на занятость, он старается <выбирать> время для спорта.",
                            "target_lang_translations": {
                                "en": "Despite his busyness, he tries to <find> time for sports."
                            }
                        },
                        {
                            "text": "Нужно уметь <выбирать> подходящий момент для серьёзного разговора.",
                            "target_lang_translations": {
                                "en": "You need to know how to <pick> the right moment for a serious conversation."
                            }
                        }
                    ],
                    "synonyms": [
                        "находить",
                        "выделять"
                    ],
                    "common_phrases": [
                        "выбирать время",
                        "выбирать момент",
                        "выбирать минутку",
                        "выбирать час"
                    ],
                    "target_lang_definitions": {
                        "en": "To find or set aside a specific time or opportunity to do something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "used with 'time' (find time)"
                            },
                            {
                                "target_lang_word": "pick",
                                "target_lang_sense_clarification": "used with 'moment' or specific dates"
                            },
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "to select a specific time slot"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "685bf378-8c06-300b-9645-f609b443026b",
                    "sense_definition": "Извлекать, вынимать или доставать что-либо откуда-либо полностью; подтягивать к себе (о снастях, сетях, верёвках).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбаки начали медленно <выбирать> тяжёлую сеть из холодной воды.",
                            "target_lang_translations": {
                                "en": "The fishermen began to slowly <haul in> the heavy net from the cold water."
                            }
                        },
                        {
                            "text": "Повар аккуратно <выбирал> все кости из рыбы перед приготовлением блюда.",
                            "target_lang_translations": {
                                "en": "The cook carefully <picked out> all the bones from the fish before preparing the dish."
                            }
                        },
                        {
                            "text": "Матросы получили команду <выбирать> якорь и готовиться к отплытию.",
                            "target_lang_translations": {
                                "en": "The sailors received the command to <weigh> anchor and prepare for departure."
                            }
                        }
                    ],
                    "synonyms": [
                        "извлекать",
                        "выделять",
                        "изымать",
                        "извлекать"
                    ],
                    "common_phrases": [
                        "выбирать сеть",
                        "выбирать якорь",
                        "выбирать слабину"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull something in (like a net or rope) or to remove specific items from a mass."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "haul in",
                                "target_lang_sense_clarification": "for nets, ropes, or heavy lines"
                            },
                            {
                                "target_lang_word": "pick out",
                                "target_lang_sense_clarification": "to remove small objects (like bones)"
                            },
                            {
                                "target_lang_word": "weigh",
                                "target_lang_sense_clarification": "specifically for anchors (weigh anchor)"
                            }
                        ]
                    }
                }
            ]
        }

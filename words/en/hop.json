                            {
                                "target_lang_word": "хоп",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "между узлами сети"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af68574e-7991-32fb-a419-134c2650f5bc",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "19261658-46ac-3982-ba98-d6f5ec974c3c",
                    "sense_definition": "Mieć coś w dłoniach, ramionach lub zębach i nie wypuszczać tego; chwycić coś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_hold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka mocno <trzyma> swoje dziecko za rękę podczas spaceru.",
                            "target_lang_translations": {
                                "ru": "Мать крепко <держит> своего ребенка за руку во время прогулки."
                            }
                        },
                        {
                            "text": "Czy możesz chwilę <potrzymać> moją torbę, kiedy będę szukać kluczy?",
                            "target_lang_translations": {
                                "ru": "Ты можешь немного <подержать> мою сумку, пока я буду искать ключи?"
                            }
                        }
                    ],
                    "synonyms": [
                        "dzierżyć"
                    ],
                    "antonyms": [
                        "puszczać",
                        "wypuszczać"
                    ],
                    "common_phrases": [
                        "trzymać za rękę",
                        "trzymać w ręku",
                        "trzymać mocno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держать что-то в руках, объятиях или зубах и не отпускать; схватить что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держать",
                                "target_lang_sense_clarification": "в руках"

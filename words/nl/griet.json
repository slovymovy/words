            "pos": "noun",
            "senses": [
                {
                    "sense_id": "994e1ff6-4de3-3043-9042-8cb73ead235d",
                    "sense_definition": "Een jonge vrouw of meisje, vaak informeel gebruikt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die nieuwe <griet> uit onze klas is erg spontaan.",
                            "target_lang_translations": {
                                "ru": "Эта новая <девчонка> из нашего класса очень непосредственная."
                            }
                        },
                        {
                            "text": "Hij stond bij de bar te praten met een leuke <griet>.",
                            "target_lang_translations": {
                                "ru": "Он стоял у бара и разговаривал с симпатичной <девицей>."
                            }
                        }
                    ],
                    "synonyms": [
                        "meid",
                        "meisje"
                    ],
                    "antonyms": [
                        "jongen",
                        "knul"
                    ],
                    "common_phrases": [
                        "leuke griet",
                        "stoere griet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informeel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодая женщина или девушка; слово часто используется в неформальном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девчонка",
                                "target_lang_sense_clarification": "разговорное, общее значение"
                            },
                            {
                                "target_lang_word": "девица",
                                "target_lang_sense_clarification": "разговорное, иногда ироничное"
                            },
                            {
                                "target_lang_word": "бабёнка",
                                "target_lang_sense_clarification": "фамильярное, может быть грубым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5abd416f-4ba1-3f4c-bf1e-d0428ad83bb5",
                    "sense_definition": "Een platvis die voorkomt in gematigde wateren en verwant is aan de tarbot.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De visser haalde een grote <griet> uit zijn netten.",
                            "target_lang_translations": {
                                "ru": "Рыбак достал из сетей большого <бриля>."
                            }
                        },
                        {
                            "text": "In het visrestaurant bestelden we gebakken <griet> met groenten.",
                            "target_lang_translations": {
                                "ru": "В рыбном ресторане мы заказали жареного <бриля> с овощами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gebakken griet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Камбалообразная рыба, обитающая в умеренных водах, родственная тюрбо (камбале-ромб)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бриль",
                                "target_lang_sense_clarification": "вид рыбы"
                            },
                            {
                                "target_lang_word": "гладкий ромб",
                                "target_lang_sense_clarification": "научное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ce3e333-7a43-3808-9599-9a6d8865bbeb",
                    "sense_definition": "Een eufemisme voor God, gebruikt in uitroepen om vloeken te vermijden.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Och <griet>, wat ben ik geschrokken van dat lawaai!",
                            "target_lang_translations": {
                                "ru": "О <Боже>, как я испугался этого шума!"
                            }
                        },
                        {
                            "text": "<Griet> nog aan toe, wat een rommel is het hier in huis.",
                            "target_lang_translations": {
                                "ru": "<Боже ж ты мой>, какой здесь в доме бардак."
                            }
                        }
                    ],
                    "common_phrases": [
                        "griet nog aan toe"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "verouderd"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Эвфемизм слова \"Бог\", используемый в восклицаниях, чтобы избежать богохульства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Боже",
                                "target_lang_sense_clarification": "восклицание"
                            },
                            {
                                "target_lang_word": "батюшки",
                                "target_lang_sense_clarification": "выражение удивления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c83ddd50-9358-3c0c-a4ff-505c1fbfeb9b",
                    "sense_definition": "Een alternatieve benaming voor de <w>grutto</w> (een weidevogel).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer wees naar de <griet> die in het weiland liep.",
                            "target_lang_translations": {
                                "ru": "Фермер указал на <веретенника>, который ходил по лугу."
                            }
                        },
                        {
                            "text": "In sommige dialecten wordt deze vogel nog steeds een <griet> genoemd.",
                            "target_lang_translations": {
                                "ru": "В некоторых диалектах эту птицу до сих пор называют <веретенником>."
                            }
                        }
                    ],
                    "synonyms": [
                        "grutto"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dialect"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Альтернативное название веретенника (луговой птицы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "веретенник",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                }
            ]
        }
    ]

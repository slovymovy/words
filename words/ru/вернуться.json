    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e07f0824-8e63-3cdb-8ead-014626800850",
                    "sense_definition": "Прийти или приехать обратно на то место, где находился раньше.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обещаю <вернуться> домой до ужина.",
                            "target_lang_translations": {
                                "en": "I promise to <return> home before dinner."
                            }
                        },
                        {
                            "text": "Мы <вернулись> из отпуска только вчера вечером.",
                            "target_lang_translations": {
                                "en": "We <returned> from vacation only yesterday evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "возвратиться"
                    ],
                    "antonyms": [
                        "уйти",
                        "уехать"
                    ],
                    "common_phrases": [
                        "вернуться домой",
                        "вернуться назад",
                        "вернуться поздно"
                    ],
                    "target_lang_definitions": {
                        "en": "To come or go back to a place where one was before."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "to a place"
                            },
                            {
                                "target_lang_word": "come back",
                                "target_lang_sense_clarification": "to return to a place"
                            },
                            {
                                "target_lang_word": "get back",
                                "target_lang_sense_clarification": "to arrive back"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f4b2a7b-5faa-3a7b-9d60-b1aff623d3ca",
                    "sense_definition": "Возобновить прерванное занятие, разговор или снова начать думать о чём-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давайте <вернёмся> к обсуждению этого вопроса завтра.",
                            "target_lang_translations": {
                                "en": "Let's <return> to the discussion of this issue tomorrow."
                            }
                        },
                        {
                            "text": "Писатель решил <вернуться> к работе над своим романом.",
                            "target_lang_translations": {
                                "en": "The writer decided to <return> to work on his novel."
                            }
                        }
                    ],
                    "synonyms": [
                        "возвратиться"
                    ],
                    "common_phrases": [
                        "вернуться к теме",
                        "вернуться к работе",
                        "вернуться к разговору"
                    ],
                    "target_lang_definitions": {
                        "en": "To resume an interrupted activity, conversation, or start thinking about something again."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "to a topic or activity"
                            },
                            {
                                "target_lang_word": "get back",
                                "target_lang_sense_clarification": "to resume an activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee45fe52-f178-315d-82ec-180bc2a6818e",
                    "sense_definition": "Снова появиться, возникнуть или восстановиться (о состоянии, чувстве или качестве).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После отдыха к нему <вернулись> силы и бодрость.",
                            "target_lang_translations": {
                                "en": "After rest, strength and vigor <returned> to him."
                            }
                        },
                        {
                            "text": "Улыбка <вернулась> на её лицо, когда она увидела подарок.",
                            "target_lang_translations": {
                                "en": "A smile <returned> to her face when she saw the gift."
                            }
                        }
                    ],
                    "synonyms": [
                        "возвратиться"
                    ],
                    "antonyms": [
                        "исчезнуть",
                        "пропасть"
                    ],
                    "common_phrases": [
                        "вернуться в норму",
                        "вернуться к жизни"
                    ],
                    "target_lang_definitions": {
                        "en": "To appear again, arise, or be restored (regarding a state, feeling, or quality)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "to recur or come again"
                            },
                            {
                                "target_lang_word": "come back",
                                "target_lang_sense_clarification": "to reappear"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
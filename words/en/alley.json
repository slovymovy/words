                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Элли",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "502c0754-6a3e-3ac9-ab9f-73400517c1e5",
                    "sense_definition": "A female given name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My cousin <w>Alley</w> is coming to visit us this summer.",
                            "target_lang_translations": {
                                "ru": "Моя кузина <w>Элли</w> приедет навестить нас этим летом."
                            }
                        },
                        {
                            "text": "<w>Alley</w> wrote her name clearly at the top of the paper.",
                            "target_lang_translations": {
                                "ru": "<w>Элли</w> четко написала свое имя в верхней части листа."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Элли",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

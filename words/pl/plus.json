    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1976b015-fde7-3690-8253-7b2735cbed86",
                    "sense_definition": "Znak graficzny w kształcie krzyżyka (+), oznaczający dodawanie lub liczbę większą od zera.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sign_math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia, aby napisał na tablicy duży <w>plus</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика написать на доске большой <w>плюс</w>."
                            }
                        },
                        {
                            "text": "W tym równaniu matematycznym brakuje jednego znaku, którym jest <w>plus</w>.",
                            "target_lang_translations": {
                                "ru": "В этом математическом уравнении не хватает одного знака — <w>плюса</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "minus"
                    ],
                    "common_phrases": [
                        "znak plusa",
                        "postawić plus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графический знак в форме крестика (+), обозначающий сложение или число больше нуля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плюс",
                                "target_lang_sense_clarification": "знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90118713-3dc3-3af3-9153-8e212fe77548",
                    "sense_definition": "Dodatnia, korzystna cecha kogoś lub czegoś; zaleta.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "advantage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Największym <w>plusem</w> tego mieszkania jest jego świetna lokalizacja.",
                            "target_lang_translations": {
                                "ru": "Самым большим <w>плюсом</w> этой квартиры является её отличное расположение."
                            }
                        },
                        {
                            "text": "Przed podjęciem decyzji musimy rozważyć wszystkie <w>plusy</w> i minusy.",
                            "target_lang_translations": {
                                "ru": "Перед принятием решения мы должны взвесить все <w>плюсы</w> и минусы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaleta"
                    ],
                    "antonyms": [
                        "minus",
                        "minus"
                    ],
                    "common_phrases": [
                        "wielki plus",
                        "plusy i minusy",
                        "mieć plusy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положительная, выгодная черта кого-либо или чего-либо; преимущество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плюс",
                                "target_lang_sense_clarification": "преимущество"
                            },
                            {
                                "target_lang_word": "преимущество",
                                "target_lang_sense_clarification": "достоинство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e799a2f1-700a-3487-bcdd-1114a3f19731",
                    "sense_definition": "Znak dodawany do oceny szkolnej, oznaczający, że jest ona nieco wyższa niż standardowa.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "grade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kasia bardzo się cieszyła, ponieważ dostała z matematyki piątkę z <w>plusem</w>.",
                            "target_lang_translations": {
                                "ru": "Кася очень радовалась, так как получила по математике пятёрку с <w>плюсом</w>."
                            }
                        },
                        {
                            "text": "Nauczyciel postawił mi czwórkę z <w>plusem</w> za aktywność na lekcji.",
                            "target_lang_translations": {
                                "ru": "Учитель поставил мне четвёрку с <w>плюсом</w> за активность на уроке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piątka z plusem",
                        "ocena z plusem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак, добавляемый к школьной оценке, означающий, что она немного выше стандартной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плюс",
                                "target_lang_sense_clarification": "к оценке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "693ecb47-454c-302c-bb30-53f11d2f7241",
                    "sense_definition": "Słowo używane przy dodawaniu liczb, oznaczające sumowanie składników.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sign_math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każde dziecko wie, że dwa <w>plus</w> dwa równa się cztery.",
                            "target_lang_translations": {
                                "ru": "Каждый ребёнок знает, что два <w>плюс</w> два равно четыре."
                            }
                        },
                        {
                            "text": "Jeśli policzymy koszt biletu <w>plus</w> dodatkowe opłaty, wyjdzie spora suma.",
                            "target_lang_translations": {
                                "ru": "Если посчитать стоимость билета <w>плюс</w> дополнительные сборы, выйдет значительная сумма."
                            }
                        }
                    ],
                    "synonyms": [
                        "dodać"
                    ],
                    "antonyms": [
                        "minus"
                    ],
                    "common_phrases": [
                        "dwa plus dwa",
                        "jeden plus jeden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое при сложении чисел, означающее суммирование слагаемых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плюс",
                                "target_lang_sense_clarification": "математическое действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3aea1af9-90a8-39a3-98a5-d1323f56a78a",
                    "sense_definition": "Słowo łączące elementy, oznaczające, że coś jest dołączone do czegoś innego; oraz.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conjunction_add",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W cenę wycieczki wliczony jest nocleg <w>plus</w> pełne wyżywienie.",
                            "target_lang_translations": {
                                "ru": "В стоимость экскурсии включён ночлег <w>плюс</w> полное питание."
                            }
                        },
                        {
                            "text": "Dostałam od niego piękny bukiet kwiatów <w>plus</w> pudełko czekoladek.",
                            "target_lang_translations": {
                                "ru": "Я получила от него красивый букет цветов <w>плюс</w> коробку шоколадных конфет."
                            }
                        }
                    ],
                    "synonyms": [
                        "dodać",
                        "dodać"
                    ],
                    "common_phrases": [
                        "nocleg plus śniadanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, связывающее элементы, означающее, что что-то присоединено к чему-то другому; а также."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плюс",
                                "target_lang_sense_clarification": "а также"
                            }
                        ]
                    }
                }
            ]
        },
        {

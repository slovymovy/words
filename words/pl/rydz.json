            "senses": [
                {
                    "sense_id": "7eb3d491-b02a-3a22-9530-3cc02120b603",
                    "sense_definition": "Popularny grzyb jadalny o pomarańczowym kapeluszu i miąższu, rosnący głównie w lasach iglastych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mushroom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w lesie znaleźliśmy piękny <w>rydz</w> rosnący pod sosną.",
                            "target_lang_translations": {
                                "ru": "Вчера в лесу мы нашли красивый <w>рыжик</w>, растущий под сосной."
                            }
                        },
                        {
                            "text": "Mimo podeszłego wieku, mój dziadek wciąż jest zdrowy jak <w>rydz</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на преклонный возраст, мой дедушка всё ещё здоров как <w>бык</w>."
                            }
                        },
                        {
                            "text": "Na kolację podano smażone na maśle <w>rydze</w> z dodatkiem pietruszki.",
                            "target_lang_translations": {
                                "ru": "На ужин подали жареные на сливочном масле <w>рыжики</w> с добавлением петрушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "mleczaj rydz",
                        "rydzyk",
                        "rydz pański"
                    ],
                    "common_phrases": [
                        "zdrowy jak rydz",
                        "zbierać rydze",
                        "marynowane rydze",
                        "smażone rydze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярный съедобный гриб с оранжевой шляпкой и мякотью, растущий преимущественно в хвойных лесах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыжик",
                                "target_lang_sense_clarification": "гриб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc14d9ca-3bb6-3e06-9c9a-6ea5bcc23cbd",
                    "sense_definition": "Roślina oleista o drobnych, żółtych kwiatach, uprawiana dla nasion, z których tłoczy się olej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik postanowił posiać <w>rydz</w> na polu, aby wyprodukować własny olej.",
                            "target_lang_translations": {
                                "ru": "Фермер решил посеять <w>рыжик</w> на поле, чтобы произвести собственное масло."
                            }
                        },
                        {
                            "text": "Dawniej na wsiach <w>rydz</w> był powszechnie wykorzystywany w kuchni zamiast rzepaku.",
                            "target_lang_translations": {
                                "ru": "Раньше в деревнях <w>рыжик</w> повсеместно использовали на кухне вместо рапса."
                            }
                        }
                    ],
                    "synonyms": [
                        "lnicznik siewny",
                        "lnianka",
                        "lennica",
                        "judra"
                    ],
                    "common_phrases": [
                        "olej z rydza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Масличное растение с мелкими желтыми цветками, выращиваемое ради семян, из которых отжимают масло."
                    },
                    "translations": {

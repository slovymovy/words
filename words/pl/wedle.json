{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "29c62542-b78f-351f-996e-011a87609a71",
                    "sense_definition": "Przyimek wskazujący na zgodność z czymś, stosowanie się do jakiejś reguły, woli lub powoływanie się na czyjąś opinię.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "zgodnosc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystko zostało wykonane dokładnie <w>wedle</w> twojego życzenia.",
                            "target_lang_translations": {
                                "ru": "Всё было выполнено в точности <w>согласно</w> твоему желанию."
                            }
                        },
                        {
                            "text": "<w>Wedle</w> najnowszych badań, dieta ta jest bardzo skuteczna.",
                            "target_lang_translations": {
                                "ru": "<w>Согласно</w> новейшим исследованиям, эта диета очень эффективна."
                            }
                        },
                        {
                            "text": "Każdy pracownik postępował <w>wedle</w> ustalonych wcześniej zasad bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Каждый сотрудник действовал <w>в соответствии с</w> заранее установленными правилами безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "według",
                        "zgodnie z",
                        "podług"
                    ],
                    "common_phrases": [
                        "wedle uznania",
                        "wedle życzenia",
                        "wedle prawa",
                        "wedle rozkazu",
                        "wedle zasług"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, указывающий на соответствие чему-либо, следование правилу, воле или ссылку на чье-то мнение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласно",
                                "target_lang_sense_clarification": "указывает на соответствие источнику, правилу или желанию"
                            },
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "указывает на основание для действия или мнение"
                            },
                            {
                                "target_lang_word": "в соответствии с",
                                "target_lang_sense_clarification": "официальный или формальный контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be54915a-7b64-3eb4-9e35-2de7b5f17528",
                    "sense_definition": "Przyimek określający miejsce znajdujące się blisko czegoś lub obok kogoś; używany głównie w języku dawnym lub gwarowym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "miejsce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony wędrowiec usiadł <w>wedle</w> drogi, aby odpocząć.",
                            "target_lang_translations": {
                                "ru": "Усталый путник сел <w>у</w> дороги, чтобы отдохнуть."
                            }
                        },
                        {
                            "text": "W starym domu ławka stała zawsze <w>wedle</w> pieca.",
                            "target_lang_translations": {
                                "ru": "В старом доме лавка всегда стояла <w>возле</w> печи."
                            }
                        }
                    ],
                    "synonyms": [
                        "obok",
                        "koło",
                        "blisko",
                        "podle"
                    ],
                    "common_phrases": [
                        "siedzieć wedle siebie",
                        "przechodzić wedle domu"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie rzadziej używane w tym znaczeniu"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "spotykane w gwarach, np. na Śląsku"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, обозначающий нахождение рядом с чем-то или кем-то (устар. или диал.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возле",
                                "target_lang_sense_clarification": "рядом, вблизи"
                            },
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "в непосредственной близости"
                            },
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "рядом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86b30f67-0f9a-301d-88df-5b972d1077e0",
                    "sense_definition": "Przyimek wskazujący na przemieszczanie się wzdłuż jakiegoś obiektu, na przykład drogi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "miejsce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare wierzby rosły gęsto <w>wedle</w> piaszczystej drogi.",
                            "target_lang_translations": {
                                "ru": "Старые ивы густо росли <w>вдоль</w> песчаной дороги."
                            }
                        },
                        {
                            "text": "Szliśmy <w>wedle</w> rzeki, szukając dogodnego miejsca do przeprawy.",
                            "target_lang_translations": {
                                "ru": "Мы шли <w>вдоль</w> реки в поисках удобного места для переправы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzdłuż"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "użycie przestarzałe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, указывающий на расположение или движение вдоль объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдоль",
                                "target_lang_sense_clarification": "по направлению длины чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
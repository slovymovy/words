                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облачать",
                                "target_lang_sense_clarification": "торжественно или устаревшее"
                            },
                            {
                                "target_lang_word": "одевать",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

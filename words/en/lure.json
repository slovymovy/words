{
    "word_family": [
        "allure",
        "alluring"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "45c15d89-56f2-3230-abe2-ec9b1ed543bc",
                    "sense_definition": "To attract by temptation, appeal, or guile.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attract_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The smell of fresh coffee <w>lured</w> him into the kitchen.",
                            "target_lang_translations": {
                                "ru": "Запах свежего кофе <w>заманил</w> его на кухню."
                            }
                        },
                        {
                            "text": "They tried to <w>lure</w> the animal out of its hiding place with food.",
                            "target_lang_translations": {
                                "ru": "Они пытались <w>выманить</w> животное из укрытия едой."
                            }
                        }
                    ],
                    "synonyms": [
                        "entice"
                    ],
                    "antonyms": [
                        "repel",
                        "deter"
                    ],
                    "common_phrases": [
                        "lure into",
                        "lure away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекать искушением, призывом или хитростью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завлекать",
                                "target_lang_sense_clarification": "хитростью или обещаниями"
                            },
                            {
                                "target_lang_word": "приманивать",
                                "target_lang_sense_clarification": "привлекать приманкой"
                            },
                            {
                                "target_lang_word": "манить",
                                "target_lang_sense_clarification": "звать, привлекать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "030f42eb-a859-3c3a-88b9-8c8e9370f4fc",
                    "sense_definition": "Something that tempts or attracts, especially one with a promise of reward or pleasure.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "attract_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He could not resist the <w>lure</w> of easy money.",
                            "target_lang_translations": {
                                "ru": "Он не мог устоять перед <w>соблазном</w> легких денег."
                            }
                        },
                        {
                            "text": "The <w>lure</w> of the big city attracts many young people.",
                            "target_lang_translations": {
                                "ru": "<w>Соблазн</w> большого города привлекает многих молодых людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the lure of",
                        "resist the lure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что искушает или привлекает, особенно обещанием награды или удовольствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соблазн",
                                "target_lang_sense_clarification": "искушение"
                            },
                            {
                                "target_lang_word": "приманка",
                                "target_lang_sense_clarification": "перен. средство привлечения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66bd009f-f8fe-31f7-81d7-f4d61fb5a65b",
                    "sense_definition": "An artificial bait attached to a fishing line to attract fish.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fishing_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He attached a bright green <w>lure</w> to his fishing line.",
                            "target_lang_translations": {
                                "ru": "Он прикрепил ярко-зеленую <w>блесну</w> к своей леске."
                            }
                        },
                        {
                            "text": "The fish ignored the worm but went for the shiny <w>lure</w>.",
                            "target_lang_translations": {
                                "ru": "Рыба проигнорировала червяка, но клюнула на блестящую <w>блесну</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fishing lure",
                        "cast a lure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственная наживка, прикрепленная к леске для привлечения рыбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блесна",
                                "target_lang_sense_clarification": "искусственная наживка"
                            },
                            {
                                "target_lang_word": "приманка",
                                "target_lang_sense_clarification": "рыболовная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a97b51a0-6582-36c8-a918-fa43b593306a",
                    "sense_definition": "A bunch of feathers attached to a line, used in falconry to recall the hawk.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "falconry_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The falconer swung the <w>lure</w> to call the bird back.",
                            "target_lang_translations": {
                                "ru": "Сокольник размахивал <w>вабилом</w>, чтобы подозвать птицу обратно."
                            }
                        },
                        {
                            "text": "Training a hawk requires patience and the proper use of a <w>lure</w>.",
                            "target_lang_translations": {
                                "ru": "Обучение ястреба требует терпения и правильного использования <w>вабила</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Связка перьев на веревке, используемая в соколиной охоте для возвращения птицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вабило",
                                "target_lang_sense_clarification": "для соколиной охоты"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
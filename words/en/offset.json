    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ba0d8955-9dba-3e1e-939f-00364de55aff",
                    "sense_definition": "To counteract or compensate for something by applying a change or force in the opposite direction.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "counterbalance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company's rising revenue helped to <w>offset</w> the increased production costs.",
                            "target_lang_translations": {
                                "ru": "Рост доходов компании помог <w>компенсировать</w> возросшие производственные затраты."
                            }
                        },
                        {
                            "text": "We can <w>offset</w> the environmental impact of our travel by planting trees.",
                            "target_lang_translations": {
                                "ru": "Мы можем <w>компенсировать</w> воздействие наших путешествий на окружающую среду, сажая деревья."
                            }
                        }
                    ],
                    "synonyms": [
                        "compensate for",
                        "counterbalance",
                        "countervail",
                        "set off"
                    ],
                    "common_phrases": [
                        "offset the cost",
                        "offset the risk",
                        "offset losses",
                        "offset the impact"
                    ],
                    "target_lang_definitions": {
                        "ru": "Противодействовать чему-либо или компенсировать что-либо, применяя силу или изменения в противоположном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компенсировать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "возмещать",
                                "target_lang_sense_clarification": "убытки, расходы"
                            },
                            {
                                "target_lang_word": "уравновешивать",
                                "target_lang_sense_clarification": "силу, влияние"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78b1e0b3-0d2c-3e51-a806-1920ecf66897",

{
    "word_family": [
        "gdzie",
        "gdzieś",
        "nigdzie",
        "gdzieniegdzie"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "90abb235-3322-3a63-9b50-dc73f97ffb03",
                    "sense_definition": "W jakimkolwiek miejscu lub do jakiegokolwiek miejsca; w miejscu bliżej nieokreślonym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "indefinite_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Możesz usiąść <w>gdziekolwiek</w>, wszystkie krzesła są wolne.",
                            "target_lang_translations": {
                                "ru": "Ты можешь сесть <w>где угодно</w>, все стулья свободны."
                            }
                        },
                        {
                            "text": "Nie widziałem twojego telefonu <w>gdziekolwiek</w> w tym pokoju.",
                            "target_lang_translations": {
                                "ru": "Я не видел твоего телефона <w>где-либо</w> в этой комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "byle gdzie",
                        "gdzie bądź",
                        "obojętnie gdzie"
                    ],
                    "antonyms": [
                        "tam",
                        "dokładnie tam"
                    ],
                    "common_phrases": [
                        "usiąść gdziekolwiek",
                        "położyć gdziekolwiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "В каком-либо месте или в какое-либо место; в неопределённом месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "где-нибудь",
                                "target_lang_sense_clarification": "в неопределённом месте"
                            },
                            {
                                "target_lang_word": "где-либо",
                                "target_lang_sense_clarification": "в каком бы то ни было месте (часто в вопросах или отрицаниях)"
                            },
                            {
                                "target_lang_word": "куда-нибудь",
                                "target_lang_sense_clarification": "в какое-либо место (направление)"
                            },
                            {
                                "target_lang_word": "где угодно",
                                "target_lang_sense_clarification": "в любом месте (на выбор)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6be89c5d-f9fb-3d53-9c43-fbf6b56e41dc",
                    "sense_definition": "Słowo łączące zdania, oznaczające „w każdym miejscu, w którym” lub „bez względu na to, gdzie”.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relative_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Gdziekolwiek</w> spojrzysz, widać piękne góry i lasy.",
                            "target_lang_translations": {
                                "ru": "<w>Куда бы ни</w> посмотрел, видны красивые горы и леса."
                            }
                        },
                        {
                            "text": "Będę o tobie pamiętać, <w>gdziekolwiek</w> teraz jesteś.",
                            "target_lang_translations": {
                                "ru": "Я буду помнить о тебе, <w>где бы</w> ты сейчас <w>ни</w> был."
                            }
                        }
                    ],
                    "synonyms": [
                        "wszędzie",
                        "dokądkolwiek"
                    ],
                    "common_phrases": [
                        "gdziekolwiek jesteś",
                        "gdziekolwiek pójdziesz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союзное слово, означающее «в любом месте, где» или «независимо от того, где»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "где бы ни",
                                "target_lang_sense_clarification": "независимо от местонахождения"
                            },
                            {
                                "target_lang_word": "куда бы ни",
                                "target_lang_sense_clarification": "независимо от направления движения или взгляда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6c5a7201-1d71-3a7b-ba5b-8e6d3840f3f5",
                    "sense_definition": "Выполнять работу по удовлетворению потребностей клиентов, посетителей или гостей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "service_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом ресторане официанты всегда вежливо <w>обслуживают</w> посетителей.",
                            "target_lang_translations": {
                                "en": "In this restaurant, waiters always politely <w>serve</w> the visitors."
                            }
                        },
                        {
                            "text": "Наш банк <w>обслуживает</w> клиентов круглосуточно через мобильное приложение.",
                            "target_lang_translations": {
                                "en": "Our bank <w>serves</w> clients around the clock via the mobile app."
                            }
                        }
                    ],
                    "common_phrases": [
                        "обслуживать клиентов",
                        "обслуживать посетителей",
                        "обслуживать столик",
                        "быстро обслуживать"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform work to satisfy the needs of clients, visitors, or guests."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "customers, clients"
                            },
                            {
                                "target_lang_word": "attend to",
                                "target_lang_sense_clarification": "needs, customers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b5aa64c-7f83-3766-a2d4-9955717e47b8",
                    "sense_definition": "Следить за работой техники, машин или оборудования, поддерживать их в исправном состоянии.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "service_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Квалифицированный механик должен регулярно <w>обслуживать</w> этот сложный станок.",
                            "target_lang_translations": {
                                "en": "A qualified mechanic must regularly <w>service</w> this complex machine."
                            }
                        },
                        {
                            "text": "Наша компания <w>обслуживает</w> компьютерные сети в крупных офисах города.",
                            "target_lang_translations": {
                                "en": "Our company <w>maintains</w> computer networks in large city offices."
                            }
                        }
                    ],
                    "common_phrases": [
                        "обслуживать технику",
                        "обслуживать оборудование",
                        "обслуживать автомобиль",
                        "обслуживать сеть"
                    ],
                    "target_lang_definitions": {
                        "en": "To monitor the operation of machinery or equipment and keep it in good working order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maintain",
                                "target_lang_sense_clarification": "equipment, systems"
                            },
                            {
                                "target_lang_word": "service",
                                "target_lang_sense_clarification": "cars, machinery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bfaa6cc-6b37-3060-b48e-9a06f46812d5",
                    "sense_definition": "Выплачивать проценты и погашать часть основного долга по финансовым обязательствам.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "service_finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Предприятию становится всё труднее <w>обслуживать</w> свой огромный долг.",
                            "target_lang_translations": {
                                "en": "It is becoming increasingly difficult for the enterprise to <w>service</w> its huge debt."
                            }
                        },
                        {
                            "text": "Государство продолжает своевременно <w>обслуживать</w> внешние кредиты.",
                            "target_lang_translations": {
                                "en": "The state continues to timely <w>service</w> external loans."
                            }
                        }
                    ],
                    "common_phrases": [
                        "обслуживать долг",
                        "обслуживать кредит"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pay interest and repay part of the principal debt on financial obligations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "service",
                                "target_lang_sense_clarification": "debt, loans"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6029582-87cd-345f-bf3d-de4084287d65",
                    "sense_definition": "Выполнять функции судьи на спортивном соревновании, следить за соблюдением правил.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "service_sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный арбитр будет <w>обслуживать</w> финальный матч чемпионата.",
                            "target_lang_translations": {
                                "en": "An experienced referee will <w>officiate</w> the final match of the championship."
                            }
                        },
                        {
                            "text": "Эту важную встречу <w>обслуживает</w> бригада судей из Москвы.",
                            "target_lang_translations": {
                                "en": "A team of referees from Moscow is <w>officiating</w> this important meeting."
                            }
                        }
                    ],
                    "synonyms": [
                        "судить"
                    ],
                    "common_phrases": [
                        "обслуживать матч",
                        "обслуживать игру",
                        "обслуживать встречу"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To perform the functions of a referee at a sports competition, ensuring rules are followed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "officiate",
                                "target_lang_sense_clarification": "sports match"
                            },
                            {
                                "target_lang_word": "referee",
                                "target_lang_sense_clarification": "game"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01d00c1a-fc3b-30fe-a878-90c76f58d7f1",
                    "sense_definition": "Прикосновение губами к кому-либо или чему-либо в знак любви, дружбы, приветствия или почтения",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kiss_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама подарила сыну нежный <w>поцелуй</w> перед сном.",
                            "target_lang_translations": {
                                "en": "Mom gave her son a gentle <w>kiss</w> before bed."
                            }
                        },
                        {
                            "text": "Их первый <w>поцелуй</w> произошёл под проливным дождём.",
                            "target_lang_translations": {
                                "en": "Their first <w>kiss</w> happened in the pouring rain."
                            }
                        }
                    ],
                    "synonyms": [
                        "лобзание",
                        "чмок"
                    ],
                    "common_phrases": [
                        "воздушный поцелуй",
                        "первый поцелуй",
                        "французский поцелуй",
                        "страстный поцелуй"
                    ],
                    "target_lang_definitions": {
                        "en": "A touch with the lips to someone or something as a sign of love, friendship, greeting, or respect"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kiss",
                                "target_lang_sense_clarification": "expression of affection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97b37d5b-d095-3256-945f-6594b2310250",
                    "sense_definition": "Звук, издаваемый при прикосновении губами",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "kiss_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В полной тишине раздался громкий <w>поцелуй</w>.",
                            "target_lang_translations": {
                                "en": "A loud <w>kiss</w> rang out in the complete silence."
                            }
                        },
                        {
                            "text": "Звонкий <w>поцелуй</w> заставил всех гостей обернуться.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b32c0b37-29aa-31fd-8dc5-adf3c5828b82",
                    "sense_definition": "Poruszać się powoli, ciężko i niezgrabnie, często z widocznym wysiłkiem lub zmęczeniem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_heavy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony turysta powoli <w>człapał</w> pod górę z ciężkim plecakiem.",
                            "target_lang_translations": {
                                "ru": "Уставший турист медленно <w>плёлся</w> в гору с тяжелым рюкзаком."
                            }
                        },
                        {
                            "text": "Stary niedźwiedź ociężale <w>człapał</w> przez las w poszukiwaniu jedzenia.",
                            "target_lang_translations": {
                                "ru": "Старый медведь тяжело <w>брёл</w> по лесу в поисках еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "wlec się",
                        "powłóczyć nogami"
                    ],
                    "antonyms": [
                        "biec",
                        "pędzić"
                    ],
                    "common_phrases": [
                        "człapać do domu",
                        "człapać po schodach"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo nacechowane ekspresywnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться медленно, тяжело и неуклюже, часто с видимым усилием или усталостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плестись",
                                "target_lang_sense_clarification": "идти медленно, с трудом, волоча ноги"
                            },
                            {
                                "target_lang_word": "брести",
                                "target_lang_sense_clarification": "идти медленно или с трудом"
                            },
                            {
                                "target_lang_word": "тащиться",
                                "target_lang_sense_clarification": "разг. идти очень медленно, с трудом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c35a4fda-6739-35ed-ae31-e471fff21f0f",
                    "sense_definition": "Iść, wydając charakterystyczny, głośny odgłos uderzania stóp lub luźnego obuwia o podłoże.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słyszałem, jak brat w nocy <w>człapie</w> w luźnych kapciach po korytarzu.",
                            "target_lang_translations": {
                                "ru": "Я слышал, как брат ночью <w>шлёпает</w> в свободных тапочках по коридору."
                            }
                        },
                        {
                            "text": "Małe dziecko zabawnie <w>człapało</w> po pokoju w za dużych butach taty.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок забавно <w>шлёпал</w> по комнате в слишком больших папиных ботинках."
                            }
                        }
                    ],
                    "synonyms": [
                        "szurać"
                    ],
                    "antonyms": [
                        "skradać się",
                        "stąpać bezszelestnie"
                    ],
                    "common_phrases": [
                        "człapać w kapciach",
                        "głośno człapać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти, издавая характерный громкий звук ударов ног или свободной обуви о поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлёпать",
                                "target_lang_sense_clarification": "идти, издавая шлепающие звуки (обычно обувью)"
                            },
                            {
                                "target_lang_word": "шаркать",
                                "target_lang_sense_clarification": "идти, не отрывая ног от земли, производя шум"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
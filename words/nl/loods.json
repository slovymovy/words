            "pos": "noun",
            "senses": [
                {
                    "sense_id": "789707a2-cdd1-38f7-8bec-8a7dac686d50",
                    "sense_definition": "Een groot, eenvoudig gebouw dat dient als opslagplaats voor goederen of voor het stallen van voertuigen en vliegtuigen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer bewaart al zijn machines in de grote <w>loods</w> achter het huis.",
                            "target_lang_translations": {
                                "en": "The farmer keeps all his machines in the large <w>shed</w> behind the house."
                            }
                        },
                        {
                            "text": "In de haven staat een oude <w>loods</w> die nu wordt gebruikt voor festivals.",
                            "target_lang_translations": {
                                "en": "In the harbor stands an old <w>warehouse</w> that is now used for festivals."
                            }
                        }
                    ],
                    "synonyms": [
                        "hangar"
                    ],
                    "common_phrases": [
                        "in de loods",
                        "een leegstaande loods",
                        "houten loods"
                    ],
                    "target_lang_definitions": {
                        "en": "A large, simple building used for storing goods or housing vehicles and aircraft."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shed",
                                "target_lang_sense_clarification": "general storage building"
                            },
                            {
                                "target_lang_word": "warehouse",
                                "target_lang_sense_clarification": "large storage for goods"
                            },
                            {
                                "target_lang_word": "hangar",
                                "target_lang_sense_clarification": "for aircraft"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f18aeafa-9b22-316b-8b6d-df4399588d87",
                    "sense_definition": "Een specialist die kapiteins van schepen adviseert en helpt bij het veilig navigeren door moeilijke wateren, zoals havens of riviermondingen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein wachtte tot de <w>loods</w> aan boord was geklommen.",
                            "target_lang_translations": {
                                "en": "The captain waited until the <w>pilot</w> had climbed aboard."
                            }
                        },
                        {
                            "text": "Zonder de hulp van een ervaren <w>loods</w> is deze rivier gevaarlijk om te bevaren.",
                            "target_lang_translations": {
                                "en": "Without the help of an experienced <w>pilot</w>, this river is dangerous to navigate."
                            }
                        }
                    ],
                    "synonyms": [
                        "loodsman"
                    ],
                    "common_phrases": [
                        "de loods aan boord",
                        "varen met loods"
                    ],
                    "target_lang_definitions": {
                        "en": "A specialist who advises ship captains and helps navigate safely through difficult waters, such as harbors or river mouths."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pilot",
                                "target_lang_sense_clarification": "maritime specialist"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9166978b-3a95-36cb-a381-9f8540946d1c",
                    "sense_definition": "Een vissoort die vaak in gezelschap van haaien of andere grote zeedieren zwemt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kleine <w>loods</w> zwom vlak naast de enorme haai.",
                            "target_lang_translations": {
                                "en": "The small <w>pilot fish</w> swam right next to the huge shark."
                            }
                        },
                        {
                            "text": "Op de foto zie je duidelijk een <w>loods</w> die meelift op de stroming van het grotere dier.",
                            "target_lang_translations": {
                                "en": "In the photo, you can clearly see a <w>pilot fish</w> hitching a ride on the current of the larger animal."
                            }
                        }
                    ],
                    "synonyms": [
                        "loodsmannetje"
                    ],
                    "target_lang_definitions": {
                        "en": "A species of fish that often swims in the company of sharks or other large sea creatures."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pilot fish",
                                "target_lang_sense_clarification": "fish species"
                            }
                        ]
                    }
                }
            ]
        },

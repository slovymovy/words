{
    "word_family": [
        "wellness"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c6eb6df4-f2eb-3f7f-a983-8291c3ef70a9",
                    "sense_definition": "In a good, accurate, competent, or satisfactory way.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adv_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She plays the piano very <w>well</w> for her age.",
                            "target_lang_translations": {
                                "ru": "Она играет на пианино очень <w>хорошо</w> для своего возраста."
                            }
                        },
                        {
                            "text": "Everything is going <w>well</w> with the new project.",
                            "target_lang_translations": {
                                "ru": "С новым проектом всё идёт <w>хорошо</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "badly",
                        "poorly"
                    ],
                    "common_phrases": [
                        "do well",
                        "go well",
                        "as well"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошим, точным, компетентным или удовлетворительным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "качественно, удовлетворительно"
                            },
                            {
                                "target_lang_word": "как следует",
                                "target_lang_sense_clarification": "должным образом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fca4c5d-fc6c-32d9-8f31-ec940736c45b",
                    "sense_definition": "Completely, fully, or thoroughly.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adv_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>well</w> aware of the difficulties we face.",
                            "target_lang_translations": {
                                "ru": "Я <w>хорошо</w> осведомлён о трудностях, с которыми мы сталкиваемся."
                            }
                        },
                        {
                            "text": "Mix the ingredients until they are <w>well</w> combined.",
                            "target_lang_translations": {
                                "ru": "Перемешивайте ингредиенты, пока они <w>хорошо</w> не соединятся."
                            }
                        }
                    ],
                    "synonyms": [
                        "so",
                        "look"
                    ],
                    "common_phrases": [
                        "well done",
                        "well aware",
                        "well known"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью, целиком или тщательно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "в полной мере (напр. осведомлен)"
                            },
                            {
                                "target_lang_word": "тщательно",
                                "target_lang_sense_clarification": "старательно, детально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "006a69c0-a853-3ca3-95a8-bbbc08938ada",
                    "sense_definition": "To a significant degree; considerably.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adv_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The castle is <w>well</w> worth a visit if you are in the area.",
                            "target_lang_translations": {
                                "ru": "Замок <w>определённо</w> стоит посетить, если вы будете в этом районе."
                            }
                        },
                        {
                            "text": "The temperature was <w>well</w> above average for this time of year.",
                            "target_lang_translations": {
                                "ru": "Температура была <w>намного</w> выше средней для этого времени года."
                            }
                        }
                    ],
                    "common_phrases": [
                        "well worth",
                        "well over",
                        "well before"
                    ],
                    "target_lang_definitions": {
                        "ru": "В значительной степени; существенно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "намного",
                                "target_lang_sense_clarification": "значительно (напр. выше, больше)"
                            },
                            {
                                "target_lang_word": "вполне",
                                "target_lang_sense_clarification": "совершенно, полностью (напр. заслуживает)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28c8744e-8179-330f-8475-f33544dbf3c8",
                    "sense_definition": "Very; used as a general intensifier for adjectives.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adv_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That movie we saw last night was <w>well</w> good.",
                            "target_lang_translations": {
                                "ru": "Тот фильм, что мы смотрели вчера, был <w>реально</w> хорошим."
                            }
                        },
                        {
                            "text": "I was <w>well</w> tired after running that marathon.",
                            "target_lang_translations": {
                                "ru": "Я <w>очень</w> устал после того марафона."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень; используется как общий усилитель прилагательных (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очень",
                                "target_lang_sense_clarification": "сильно"
                            },
                            {
                                "target_lang_word": "реально",
                                "target_lang_sense_clarification": "сленг: действительно, очень"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a7808b7a-b4f5-319e-bb07-9dc37f302951",
                    "sense_definition": "Used to introduce a remark, express hesitation, resume a conversation, or fill a pause.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intj_discourse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Well</w>, I think we should probably go home now.",
                            "target_lang_translations": {
                                "ru": "<w>Ну</w>, я думаю, нам, наверное, пора домой."
                            }
                        },
                        {
                            "text": "He said he would help, but, <w>well</w>, he never showed up.",
                            "target_lang_translations": {
                                "ru": "Он сказал, что поможет, но, <w>знаете</w>, он так и не появился."
                            }
                        }
                    ],
                    "synonyms": [
                        "you know",
                        "I mean",
                        "so"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения замечания, выражения нерешительности, возобновления разговора или заполнения паузы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "итак",
                                "target_lang_sense_clarification": "подведение итога или начало речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b811a60-40c6-3ce6-b8fa-c9742caaa362",
                    "sense_definition": "Used to express surprise, skepticism, resignation, or indignation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intj_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Well</w>! I have never been so insulted in my life.",
                            "target_lang_translations": {
                                "ru": "<w>Ну и ну</w>! Меня ещё никогда так не оскорбляли в жизни."
                            }
                        },
                        {
                            "text": "<w>Well</w>, <w>well</w>, look who finally decided to join us.",
                            "target_lang_translations": {
                                "ru": "<w>Ну</w>, <w>ну</w>, посмотрите, кто наконец решил к нам присоединиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "look",
                        "see"
                    ],
                    "common_phrases": [
                        "well well",
                        "very well"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения удивления, скептицизма, смирения или возмущения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну и ну",
                                "target_lang_sense_clarification": "выражение удивления"
                            },
                            {
                                "target_lang_word": "надо же",
                                "target_lang_sense_clarification": "выражение удивления или иронии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5bc62c53-874a-340f-9b8b-ff13c7e7577f",
                    "sense_definition": "In good health; free from illness.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adj_health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I hope you get <w>well</w> soon after your surgery.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, ты скоро будешь <w>здоров</w> после операции."
                            }
                        },
                        {
                            "text": "She did not feel <w>well</w> so she stayed home from school.",
                            "target_lang_translations": {
                                "ru": "Она неважно себя <w>чувствовала</w>, поэтому осталась дома и не пошла в школу."
                            }
                        }
                    ],
                    "antonyms": [
                        "sick",
                        "ill",
                        "unwell"
                    ],
                    "common_phrases": [
                        "get well",
                        "feel well",
                        "stay well"
                    ],
                    "target_lang_definitions": {
                        "ru": "В добром здравии; не больной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровый",
                                "target_lang_sense_clarification": "не больной"
                            },
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "о самочувствии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9498d60-d031-3257-9556-6d8ed36c08c7",
                    "sense_definition": "In a satisfactory, favorable, or good state.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All is <w>well</w> with the family back home.",
                            "target_lang_translations": {
                                "ru": "С семьёй дома всё <w>в порядке</w>."
                            }
                        },
                        {
                            "text": "It is <w>well</w> that you arrived before the rain started.",
                            "target_lang_translations": {
                                "ru": "<w>Хорошо</w>, что вы приехали до начала дождя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "all is well"
                    ],
                    "target_lang_definitions": {
                        "ru": "В удовлетворительном, благоприятном или хорошем состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в порядке",
                                "target_lang_sense_clarification": "благополучно"
                            },
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "удачно, благоприятно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdd3a235-b7fc-3df9-b40e-5818ef199ab5",
                    "sense_definition": "Prudent, advisable, or proper.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_prudent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It would be <w>well</w> to remember the rules before you start.",
                            "target_lang_translations": {
                                "ru": "Было бы <w>разумно</w> вспомнить правила перед началом."
                            }
                        },
                        {
                            "text": "It is <w>well</w> for you to check the details one more time.",
                            "target_lang_translations": {
                                "ru": "Тебе <w>полезно</w> проверить детали ещё раз."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Благоразумный, целесообразный или правильный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разумно",
                                "target_lang_sense_clarification": "целесообразно"
                            },
                            {
                                "target_lang_word": "правильно",
                                "target_lang_sense_clarification": "верно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "23975afa-cd1e-329d-aed3-bd4e1c451912",
                    "sense_definition": "A deep hole sunk into the ground to obtain water, oil, or gas.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers draw fresh water from the <w>well</w> every morning.",
                            "target_lang_translations": {
                                "ru": "Жители деревни каждое утро берут свежую воду из <w>колодца</w>."
                            }
                        },
                        {
                            "text": "They discovered a new oil <w>well</w> in the desert.",
                            "target_lang_translations": {
                                "ru": "Они обнаружили новую нефтяную <w>скважину</w> в пустыне."
                            }
                        }
                    ],
                    "synonyms": [
                        "shaft"
                    ],
                    "common_phrases": [
                        "water well",
                        "oil well"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокая яма в земле для добычи воды, нефти или газа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колодец",
                                "target_lang_sense_clarification": "для воды"
                            },
                            {
                                "target_lang_word": "скважина",
                                "target_lang_sense_clarification": "для нефти или газа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2832ed6-49d1-3ba5-b94d-f8003826f092",
                    "sense_definition": "An open space or vertical shaft in a building, such as for a staircase.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cat fell down the stair <w>well</w> but landed safely.",
                            "target_lang_translations": {
                                "ru": "Кот упал в лестничный <w>пролёт</w>, но благополучно приземлился."
                            }
                        },
                        {
                            "text": "Light flooded into the basement through the window <w>well</w>.",
                            "target_lang_translations": {
                                "ru": "Свет залил подвал через световой <w>приямок</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stair well"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытое пространство или вертикальная шахта в здании, например, для лестницы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пролёт",
                                "target_lang_sense_clarification": "лестничный"
                            },
                            {
                                "target_lang_word": "шахта",
                                "target_lang_sense_clarification": "лифта или вентиляции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b527a775-1e81-3809-8515-4939e76fe7c8",
                    "sense_definition": "A plentiful source or supply of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a <w>well</w> of information on local history.",
                            "target_lang_translations": {
                                "ru": "Она — <w>кладезь</w> информации по местной истории."
                            }
                        },
                        {
                            "text": "The library was a deep <w>well</w> of knowledge for the students.",
                            "target_lang_translations": {
                                "ru": "Библиотека была глубоким <w>источником</w> знаний для студентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "well of knowledge"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обильный источник или запас чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кладезь",
                                "target_lang_sense_clarification": "перен. богатый источник"
                            },
                            {
                                "target_lang_word": "источник",
                                "target_lang_sense_clarification": "начало, основа"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da4bdc29-f083-39da-89f9-68cf2fcff767",
                    "sense_definition": "To rise to the surface and flow out, often used regarding liquids or emotions.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tears began to <w>well</w> up in her eyes as she heard the news.",
                            "target_lang_translations": {
                                "ru": "Слёзы начали <w>наворачиваться</w> ей на глаза, когда она услышала новости."

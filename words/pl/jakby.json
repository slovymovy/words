                    "frequency": "High",
                    "semantic_group_id": "conditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daj mi znać, <w>jakbyś</w> zmienił zdanie w tej sprawie."
                        },
                        {
                            "text": "<w>Jakby</w> co, będę czekać na ciebie w domu."
                        }
                    ],
                    "synonyms": [
                        "gdyby",
                        "jeśliby"
                    ],
                    "common_phrases": [
                        "jakby co",
                        "co by było jakby"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane zamiast „gdyby”"

                {
                    "sense_id": "3ac26809-0f32-3460-8fcd-39fa010983a7",
                    "sense_definition": "Het handelen namens een andere partij, persoon of organisatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "act_of_representing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De advocaat nam de juridische <w>vertegenwoordiging</w> van de verdachte op zich.",
                            "target_lang_translations": {
                                "en": "The lawyer took on the legal <w>representation</w> of the suspect.",
                                "ru": "Адвокат взял на себя юридическое <w>представительство</w> подозреваемого."
                            }
                        },
                        {
                            "text": "Goede <w>vertegenwoordiging</w> van de belangen van de leden is onze prioriteit.",
                            "target_lang_translations": {
                                "en": "Good <w>representation</w> of the members' interests is our priority.",
                                "ru": "Качественное <w>представительство</w> интересов членов — наш приоритет."
                            }
                        }
                    ],
                    "synonyms": [
                        "representatie"
                    ],
                    "common_phrases": [
                        "juridische vertegenwoordiging",
                        "diplomatieke vertegenwoordiging"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of acting on behalf of another party, person, or organization.",
                        "ru": "Действие от имени другой стороны, лица или организации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "representation",
                                "target_lang_sense_clarification": "act of representing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "представительство",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c46b1ba-0e1a-3a48-b88f-650d5b41deca",
                    "sense_definition": "Een groep mensen of een instantie die optreedt namens een land, organisatie of groep personen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_of_representatives",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De permanente <w>vertegenwoordiging</w> van ons land is gevestigd in Brussel.",
                            "target_lang_translations": {
                                "en": "Our country's permanent <w>representation</w> is located in Brussels.",
                                "ru": "Постоянное <w>представительство</w> нашей страны находится в Брюсселе."
                            }
                        },
                        {
                            "text": "Er is een officiële <w>vertegenwoordiging</w> van het bedrijf aanwezig op de beurs.",
                            "target_lang_translations": {
                                "en": "An official <w>representation</w> of the company is present at the trade fair.",
                                "ru": "На выставке присутствует официальное <w>представительство</w> компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "afvaardiging",
                        "delegatie",
                        "missie"
                    ],
                    "common_phrases": [
                        "permanente vertegenwoordiging",
                        "volksvertegenwoordiging"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people or an entity acting on behalf of a country, organization, or group of persons.",
                        "ru": "Группа людей или учреждение, действующие от имени страны, организации или группы лиц."
                    },

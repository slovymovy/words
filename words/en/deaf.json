{
    "word_family": [
        "deafen",
        "deafness",
        "deafly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "898cbf43-d699-346c-a60a-5c10b0d81284",
                    "sense_definition": "Unable or partially unable to hear sounds.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather is <w>deaf</w> and needs a hearing aid to communicate.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <w>глухой</w>, и ему нужен слуховой аппарат для общения."
                            }
                        },
                        {
                            "text": "She started to go <w>deaf</w> after a severe illness in her childhood.",
                            "target_lang_translations": {
                                "ru": "Она начала <w>глохнуть</w> после тяжелой болезни в детстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "hard of hearing",
                        "hearing-impaired"
                    ],
                    "antonyms": [
                        "hearing"
                    ],
                    "common_phrases": [
                        "go deaf",
                        "stone deaf",
                        "totally deaf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неспособный или частично неспособный слышать звуки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "о слухе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e9e11b8-e12b-3844-8033-74295cba931f",
                    "sense_definition": "Unwilling to listen to advice or arguments; determined to ignore something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager turned a <w>deaf</w> ear to the workers' complaints about safety.",
                            "target_lang_translations": {
                                "ru": "Менеджер остался <w>глух</w> к жалобам рабочих на безопасность."
                            }
                        },
                        {
                            "text": "Her repeated pleas for forgiveness fell on <w>deaf</w> ears.",
                            "target_lang_translations": {
                                "ru": "Ее неоднократные мольбы о прощении наткнулись на <w>глухую</w> стену непонимания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "turn a deaf ear",
                        "fall on deaf ears"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не желающий слушать советы или аргументы; намеренно игнорирующий что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "перен.: невосприимчивый к просьбам"
                            },
                            {
                                "target_lang_word": "равнодушный",
                                "target_lang_sense_clarification": "игнорирующий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "590494b6-1b6b-359b-9243-f88abbf42bae",
                    "sense_definition": "Relating to the community and culture of deaf people, often involving the use of sign language.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sociocultural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is an active member of the local <w>Deaf</w> community.",
                            "target_lang_translations": {
                                "ru": "Он активный член местного сообщества <w>глухих</w>."
                            }
                        },
                        {
                            "text": "The university offers courses on <w>Deaf</w> culture and history.",
                            "target_lang_translations": {
                                "ru": "Университет предлагает курсы по культуре и истории <w>глухих</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Deaf community",
                        "Deaf culture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к сообществу и культуре глухих людей, часто подразумевающий использование жестового языка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "относящийся к сообществу глухих"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
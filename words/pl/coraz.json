{
    "word_family": [],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ccefd8ef-4d73-38ce-a5f3-6ac598833c6a",
                    "sense_definition": "Słowo używane w połączeniu z przymiotnikami i przysłówkami w stopniu wyższym, wyrażające stopniowe nasilanie się lub zmianę danej cechy w czasie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gradual_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z każdym dniem czuję się <w>coraz</w> lepiej i mam więcej siły.",
                            "target_lang_translations": {
                                "ru": "С каждым днём я чувствую себя <w>всё</w> лучше, и у меня больше сил."
                            }
                        },
                        {
                            "text": "Zima stawała się <w>coraz</w> sroższa i trudniejsza do zniesienia.",
                            "target_lang_translations": {
                                "ru": "Зима становилась <w>всё</w> суровее и невыносимее."
                            }
                        }
                    ],
                    "synonyms": [
                        "stopniowo"
                    ],
                    "common_phrases": [
                        "coraz lepiej",
                        "coraz częściej",
                        "coraz więcej",
                        "coraz gorzej",
                        "coraz mniej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в сочетании с прилагательными и наречиями в сравнительной степени, выражающее постепенное усиление или изменение данного признака во времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всё",
                                "target_lang_sense_clarification": "с прилагательными и наречиями в сравнительной степени"
                            },
                            {
                                "target_lang_word": "всё более",
                                "target_lang_sense_clarification": "постепенно, с нарастанием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afd68634-391b-3f31-8be0-5c1e085407f2",
                    "sense_definition": "Słowo oznaczające powtarzanie się czynności co pewien czas, używane zazwyczaj w konstrukcji „coraz to”.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "repetition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez otwarte okno <w>coraz</w> to wpadały do pokoju muchy.",
                            "target_lang_translations": {
                                "ru": "Через открытое окно в комнату <w>то и дело</w> залетали мухи."
                            }
                        },
                        {
                            "text": "Dziecko biegało po ogrodzie i <w>coraz</w> to przynosiło nowe kwiatki.",
                            "target_lang_translations": {
                                "ru": "Ребёнок бегал по саду и <w>то и дело</w> приносил новые цветы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coraz to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, обозначающее повторение действия через определенные промежутки времени; обычно используется в конструкции «coraz to»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то и дело",
                                "target_lang_sense_clarification": "фразеологизм: часто, постоянно"
                            },
                            {
                                "target_lang_word": "время от времени",
                                "target_lang_sense_clarification": "периодически"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "03ad1c12-6dac-3ab9-b20b-03608fdcc514",
                    "sense_definition": "Sportowiec, którego głównym zadaniem podczas gry jest obrona bramki przed strzałem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sports_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <w>bramkarz</w> obronił rzut karny w ostatniej minucie meczu.",
                            "target_lang_translations": {
                                "ru": "Наш <w>вратарь</w> отбил пенальти на последней минуте матча."
                            }
                        },
                        {
                            "text": "W drużynie piłkarskiej jeden z jedenastu graczy jest <w>bramkarzem</w>.",
                            "target_lang_translations": {
                                "ru": "В футбольной команде один из одиннадцати игроков — <w>вратарь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "golkiper",
                        "goalkeeper"
                    ],
                    "common_phrases": [
                        "dobry bramkarz",
                        "stać na bramce",
                        "bramkarz reprezentacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортсмен, чья главная задача во время игры — защищать ворота от попадания мяча или шайбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вратарь",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "голкипер",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b4036e6-e5a6-3cf2-9195-bfe6f4320624",
                    "sense_definition": "Ochroniarz stojący przy wejściu do lokalu, który kontroluje wchodzących gości.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "security_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Bramkarz</w> nie wpuścił nas do klubu, ponieważ nie mieliśmy przy sobie dokumentów.",
                            "target_lang_translations": {
                                "ru": "<w>Вышибала</w> не пустил нас в клуб, так как у нас не было с собой документов."
                            }
                        },
                        {
                            "text": "Przed wejściem do dyskoteki stał wysoki i szeroki w barach <w>bramkarz</w>.",
                            "target_lang_translations": {
                                "ru": "Перед входом на дискотеку стоял высокий и широкоплечий <w>вышибала</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykidajło",
                        "selekcjoner"
                    ],
                    "common_phrases": [
                        "pracować jako bramkarz",
                        "bramkarz w klubie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охранник, стоящий у входа в заведение (клуб, бар) и контролирующий посетителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вышибала",
                                "target_lang_sense_clarification": "разг., в клубе"
                            },
                            {
                                "target_lang_word": "охранник",
                                "target_lang_sense_clarification": "на входе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "killer",
        "killing",
        "overkill",
        "roadkill"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "48ff2bac-d4c9-3e4e-9e1f-83aadf697176",
                    "sense_definition": "To put to death; to extinguish the life of.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cause_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Smoking <w>kills</w> many people every year.",
                            "target_lang_translations": {
                                "ru": "Курение <w>убивает</w> много людей каждый год."
                            }
                        },
                        {
                            "text": "The mysterious disease <w>killed</w> thousands of cattle in the region.",
                            "target_lang_translations": {
                                "ru": "Таинственная болезнь <w>убила</w> тысячи голов скота в этом регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "murder",
                        "slay",
                        "slaughter",
                        "assassinate",
                        "eliminate"
                    ],
                    "antonyms": [
                        "save",
                        "rescue",
                        "revive",
                        "resurrect"
                    ],
                    "common_phrases": [
                        "kill off",
                        "kill two birds with one stone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать жизни; умерщвлять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убивать",
                                "target_lang_sense_clarification": "лишать жизни (несов. вид)"
                            },
                            {
                                "target_lang_word": "убить",
                                "target_lang_sense_clarification": "лишить жизни (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2db2b5b-e4b4-39b5-a0fd-e5c20d245792",
                    "sense_definition": "To cause great pain, discomfort, or distress to.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These new shoes are <w>killing</w> my feet.",
                            "target_lang_translations": {
                                "ru": "Эти новые туфли <w>убивают</w> мои ноги."
                            }
                        },
                        {
                            "text": "My back is <w>killing</w> me after lifting those heavy boxes.",
                            "target_lang_translations": {
                                "ru": "Моя спина меня просто <w>убивает</w> после поднятия этих тяжелых коробок."
                            }
                        }
                    ],
                    "synonyms": [
                        "hurt"
                    ],
                    "common_phrases": [
                        "killing me"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to express physical pain or intense discomfort"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинять сильную боль, дискомфорт или страдание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убивать",
                                "target_lang_sense_clarification": "перен. причинять сильную боль или мучения"
                            },
                            {
                                "target_lang_word": "доканывать",
                                "target_lang_sense_clarification": "разг. изводить, мучить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b789d0dc-0857-330b-9198-370821c002ec",
                    "sense_definition": "To punish severely (used hyperbolically).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "punish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents are going to <w>kill</w> me when they see this mess.",
                            "target_lang_translations": {
                                "ru": "Родители меня <w>убьют</w>, когда увидят этот беспорядок."
                            }
                        },
                        {
                            "text": "I will <w>kill</w> you if you tell anyone my secret.",
                            "target_lang_translations": {
                                "ru": "Я тебя <w>убью</w>, если ты кому-нибудь расскажешь мой секрет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "going to kill me"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "hyperbolic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сурово наказать (используется как гипербола)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убить",
                                "target_lang_sense_clarification": "перен. сильно ругать или наказать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f296614a-c9d0-3954-882f-7e15eb52c4cd",
                    "sense_definition": "To render inoperative; to turn off (a machine, engine, or light).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stop_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>killed</w> the engine and stepped out of the car.",
                            "target_lang_translations": {
                                "ru": "Он <w>заглушил</w> двигатель и вышел из машины."
                            }
                        },
                        {
                            "text": "Please <w>kill</w> the lights before you leave the room.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>погаси</w> свет, перед тем как выйти из комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "turn off",
                        "disable",
                        "deactivate",
                        "cut"
                    ],
                    "common_phrases": [
                        "kill the engine",
                        "kill the lights"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выводить из строя; выключать (машину, двигатель или свет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглушить",
                                "target_lang_sense_clarification": "о двигателе или моторе"
                            },
                            {
                                "target_lang_word": "погасить",
                                "target_lang_sense_clarification": "о свете"
                            },
                            {
                                "target_lang_word": "выключить",
                                "target_lang_sense_clarification": "общие приборы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b3da13e-07e7-348a-a80a-6df3b5286521",
                    "sense_definition": "To stop, cease, or render void; to terminate a plan or process.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stop_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The editor decided to <w>kill</w> the story before it was published.",
                            "target_lang_translations": {
                                "ru": "Редактор решил <w>зарубить</w> статью до её публикации."
                            }
                        },
                        {
                            "text": "The sudden storm <w>killed</w> our plans for a beach picnic.",
                            "target_lang_translations": {
                                "ru": "Внезапный шторм <w>погубил</w> наши планы на пикник на пляже."
                            }
                        }
                    ],
                    "synonyms": [
                        "end",
                        "terminate",
                        "cancel",
                        "veto"
                    ],
                    "common_phrases": [
                        "kill a project",
                        "kill the deal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановить, прекратить или аннулировать; завершить план или процесс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погубить",
                                "target_lang_sense_clarification": "разрушить планы"
                            },
                            {
                                "target_lang_word": "зарубить",
                                "target_lang_sense_clarification": "разг. отклонить (статью, проект)"
                            },
                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "устранить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b74f1a3-838c-3cda-a4e7-3f880046d5fb",
                    "sense_definition": "To use up or to waste time while waiting for something else.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "waste_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We played cards to <w>kill</w> time while waiting for the train.",
                            "target_lang_translations": {
                                "ru": "Мы играли в карты, чтобы <w>убить</w> время в ожидании поезда."
                            }
                        },
                        {
                            "text": "I have an hour to <w>kill</w> before my appointment starts.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>убить</w> час до начала встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "while away",
                        "waste"
                    ],
                    "common_phrases": [
                        "kill time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тратить или проводить время в ожидании чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убить",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "коротать",
                                "target_lang_sense_clarification": "проводить время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b82dfb44-6dc3-3178-a5c4-29a46c135a01",
                    "sense_definition": "To succeed impressively with an audience or in a performance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "succeed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The comedian really <w>killed</w> at the club last night.",
                            "target_lang_translations": {
                                "ru": "Комик просто <w>порвал</w> зал в клубе вчера вечером."
                            }
                        },
                        {
                            "text": "You absolutely <w>killed</w> it during your presentation today.",
                            "target_lang_translations": {
                                "ru": "Ты просто <w>сразил</w> всех своей презентацией сегодня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "killed it"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Добиться впечатляющего успеха перед аудиторией; выступить потрясающе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сразить",
                                "target_lang_sense_clarification": "произвести сильное впечатление"
                            },
                            {
                                "target_lang_word": "порвать",
                                "target_lang_sense_clarification": "слэнг: выступить очень успешно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4a97a14-b9b7-3df7-9a84-741d62e8b890",
                    "sense_definition": "To amaze, stun, or overwhelm with emotion (often laughter).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "amaze",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That joke always <w>kills</w> me whenever I hear it.",
                            "target_lang_translations": {
                                "ru": "Эта шутка всегда <w>убивает</w> меня, когда я её слышу."
                            }
                        },
                        {
                            "text": "She arrived at the party dressed to <w>kill</w>.",
                            "target_lang_translations": {
                                "ru": "Она пришла на вечеринку, одетая <w>сногсшибательно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stun",
                        "amuse"
                    ],
                    "common_phrases": [
                        "dressed to kill",
                        "kills me"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поражать, ошеломлять или переполнять эмоциями (часто смехом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убивать",
                                "target_lang_sense_clarification": "перен. вызывать сильный смех"
                            },
                            {
                                "target_lang_word": "сражать",
                                "target_lang_sense_clarification": "ошеломлять (напр. внешним видом)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

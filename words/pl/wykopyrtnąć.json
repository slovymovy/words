    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2b403a6a-5975-3b67-ac57-f6d63b424204",
                    "sense_definition": "Przewrócić się, upaść na ziemię, zazwyczaj w sposób gwałtowny, śmieszny lub niezgrabny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko biegło zbyt szybko i nagle <w>wykopyrtnęło</w> się na środku chodnika.",
                            "target_lang_translations": {
                                "ru": "Ребёнок бежал слишком быстро и вдруг <w>чебурахнулся</w> посреди тротуара."
                            }
                        },
                        {
                            "text": "Musisz uważać na tym śliskim parkiecie, żeby się nie <w>wykopyrtnąć</w> przy gościach.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен на этом скользком паркете, чтобы не <w>навернуться</w> при гостях."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewrócić się",
                        "wywrócić się",
                        "wywinąć orła",
                        "wyrżnąć"
                    ],
                    "antonyms": [
                        "wstać",
                        "podnieść się"
                    ],
                    "common_phrases": [
                        "wykopyrtnąć się na lodzie",
                        "wykopyrtnąć się na drodze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Упасть, повалиться на землю, обычно резко, смешно или неуклюже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грохнуться",
                                "target_lang_sense_clarification": "разг. сильно упасть с шумом"
                            },
                            {
                                "target_lang_word": "чебурахнуться",
                                "target_lang_sense_clarification": "разг. упасть тяжело или неуклюже"
                            },
                            {
                                "target_lang_word": "навернуться",
                                "target_lang_sense_clarification": "прост. упасть, потеряв равновесие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "985deb6b-7c6b-3c64-8862-05a20f005d3a",
                    "sense_definition": "Umrzeć, zakończyć życie (określenie o zabarwieniu lekceważącym lub żartobliwym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "die",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiad bał się, że lada dzień <w>wykopyrtnie</w> i nikt się o tym nie dowie.",
                            "target_lang_translations": {
                                "ru": "Сосед боялся, что со дня на день <w>окочурится</w>, и никто об этом не узнает."
                            }
                        },
                        {
                            "text": "Jeśli będziesz dalej tak ryzykować, możesz szybko <w>wykopyrtnąć</w> na zawał serca.",
                            "target_lang_translations": {
                                "ru": "Если будешь продолжать так рисковать, можешь быстро <w>откинуть копыта</w> от инфаркта."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykitować",
                        "przekręcić się",
                        "wyciągnąć kopyta",
                        "kopnąć w kalendarz"
                    ],
                    "antonyms": [
                        "urodzić się",
                        "przeżyć"
                    ],
                    "common_phrases": [
                        "nagle wykopyrtnąć",
                        "wykopyrtnąć na serce"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo nacechowane emocjonalnie, potoczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умереть (разговорное, часто с пренебрежительным или шутливым оттенком)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окочуриться",

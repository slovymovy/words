                        {
                            "text": "The committee <w>passed</w> the budget after a long debate.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>утвердил</w> бюджет после долгих дебатов."
                            }
                        }
                    ],
                    "synonyms": [
                        "enact",
                        "approve",
                        "ratify"
                    ],
                    "common_phrases": [
                        "pass a law",
                        "pass a bill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально одобрять закон, предложение или законопроект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "закон"
                            },
                            {
                                "target_lang_word": "утверждать",
                                "target_lang_sense_clarification": "решение"
                            }
                        ]
                    }

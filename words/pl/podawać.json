            "senses": [
                {
                    "sense_id": "5f7be576-0f03-3fe4-9c96-6f556eff4ed9",
                    "sense_definition": "Wręczać coś komuś, przekazywać przedmiot z ręki do ręki.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możesz <podawać> mi cegły jedna po drugiej?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <подавать> мне кирпичи один за другим?"
                            }
                        },
                        {
                            "text": "Proszę <podawać> bilety do kontroli przy wejściu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <передавайте> билеты для проверки на входе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wręczać",
                        "dawać"
                    ],
                    "antonyms": [
                        "odbierać",
                        "brać"
                    ],
                    "common_phrases": [
                        "podać rękę",
                        "podać płaszcz",
                        "podać coś komuś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вручать что-либо кому-либо, передавать предмет из рук в руки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать",
                                "target_lang_sense_clarification": "давать в руки"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "вручать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ce43543-e667-3cdb-bf49-b407891e3d20",
                    "sense_definition": "Serwować jedzenie lub napoje; stawiać posiłek na stole.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transfer_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej restauracji kelnerzy <podają> wyśmienite desery.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане официанты <подают> отменные десерты."
                            }
                        },
                        {
                            "text": "Moja babcia zawsze <podaje> obiad o godzinie trzynastej.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда <подаёт> обед в час дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "serwować"
                    ],
                    "common_phrases": [
                        "podać do stołu",
                        "podać obiad",
                        "podać kolację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подавать еду или напитки; накрывать на стол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать",
                                "target_lang_sense_clarification": "на стол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52cff334-cffa-39f4-9f55-4b720f07d15e",
                    "sense_definition": "Przekazywać informację, komunikować fakty, liczby lub nazwiska.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transfer_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spiker w radiu właśnie <podaje> aktualną prognozę pogody.",
                            "target_lang_translations": {
                                "ru": "Диктор по радио как раз <передаёт> свежий прогноз погоды."
                            }
                        },
                        {
                            "text": "Musisz <podawać> prawdziwe dane przy wypełnianiu wniosku.",
                            "target_lang_translations": {
                                "ru": "Ты должен <указывать> правдивые данные при заполнении заявления."
                            }
                        }
                    ],
                    "synonyms": [
                        "informować",
                        "komunikować",
                        "ogłaszać"

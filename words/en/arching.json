                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The intricate stone <w>archings</w> were the highlight of the cathedral's design.",
                            "target_lang_translations": {
                                "ru": "Замысловатые каменные <w>своды</w> были изюминкой дизайна собора."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db317304-3007-32ab-b7e5-1996ce968fa7",
                    "sense_definition": "A kind or helpful act that you do for someone.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you do me a <w>favour</w> and help me carry these heavy bags?",
                            "target_lang_translations": {
                                "ru": "Ты не мог бы сделать мне <w>одолжение</w> и помочь донести эти тяжелые сумки?"
                            }
                        },
                        {
                            "text": "I need to ask a <w>favour</w> of you regarding the meeting tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мне нужно попросить тебя об <w>одолжении</w> насчет завтрашней встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "kindness",
                        "good turn",
                        "service",
                        "courtesy"
                    ],
                    "antonyms": [
                        "disservice"
                    ],
                    "common_phrases": [
                        "do a favour",
                        "ask a favour",
                        "owe a favour",
                        "return the favour"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British spelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одобрение, поддержка или предпочтение кого-либо или чего-либо."

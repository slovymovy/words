    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd87b599-7f1a-3068-8242-beb599539815",
                    "sense_definition": "Орган слуха и равновесия у человека и животных, а также его наружная часть.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня сильно болит левое <w>ухо</w>.",
                            "target_lang_translations": {
                                "en": "My left <w>ear</w> hurts badly."
                            }
                        },
                        {
                            "text": "Ребёнок закрыл <w>уши</w> руками, чтобы не слышать громкий шум.",
                            "target_lang_translations": {
                                "en": "The child covered his <w>ears</w> with his hands so as not to hear the loud noise."
                            }
                        },
                        {
                            "text": "Она решила проколоть <w>уши</w>, чтобы носить красивые серьги.",
                            "target_lang_translations": {
                                "en": "She decided to pierce her <w>ears</w> to wear beautiful earrings."
                            }
                        }
                    ],
                    "common_phrases": [
                        "болит ухо",
                        "закрыть уши",
                        "проколоть уши",
                        "среднее ухо",
                        "внутреннее ухо"
                    ],
                    "target_lang_definitions": {
                        "en": "The organ of hearing and balance in humans and animals, as well as its outer part."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ear",
                                "target_lang_sense_clarification": "organ of hearing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b9fcf2a-b36e-3e08-97a8-9e517c568105",
                    "sense_definition": "Способность различать звуки, мелодии; музыкальный слух.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого музыканта очень тонкое <w>ухо</w>.",
                            "target_lang_translations": {
                                "en": "This musician has a very fine <w>ear</w>."
                            }
                        },
                        {
                            "text": "Фальшивая нота в песне сразу резала <w>ухо</w>.",
                            "target_lang_translations": {
                                "en": "The false note in the song immediately grated on the <w>ear</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "слух"
                    ],
                    "common_phrases": [
                        "резать ухо",
                        "ласкать ухо",
                        "медведь на ухо наступил"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to distinguish sounds and melodies; musical ear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ear",
                                "target_lang_sense_clarification": "ability to distinguish sounds"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14df4f76-efa7-3b93-ac42-9a118a3fd6f1",
                    "sense_definition": "Наушники; устройство для прослушивания музыки или звука.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он надел <w>уши</w> и включил любимый плейлист.",
                            "target_lang_translations": {
                                "en": "He put on his <w>headphones</w> and turned on his favorite playlist."
                            }
                        },
                        {
                            "text": "Геймер купил новые беспроводные <w>уши</w> с микрофоном.",
                            "target_lang_translations": {
                                "en": "The gamer bought new wireless <w>headphones</w> with a microphone."
                            }
                        }
                    ],
                    "synonyms": [
                        "наушник"
                    ],
                    "common_phrases": [
                        "надеть уши",
                        "беспроводные уши"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное название наушников"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Headphones; a device for listening to music or sound."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "headphones",
                                "target_lang_sense_clarification": "electronic device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bd9fee1-6dcf-34f0-b105-84cc1f245cd6",
                    "sense_definition": "Опускающаяся часть зимней шапки, закрывающая уши от холода.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На улице был сильный мороз, поэтому он опустил <w>уши</w> шапки.",
                            "target_lang_translations": {
                                "en": "It was freezing outside, so he lowered the <w>earflaps</w> of his hat."
                            }
                        },
                        {
                            "text": "Ветер развевал длинные <w>уши</w> его мехового головного убора.",
                            "target_lang_translations": {
                                "en": "The wind blew the long <w>earflaps</w> of his fur headgear."
                            }
                        }
                    ],
                    "common_phrases": [
                        "шапка с ушами",
                        "опустить уши",
                        "завязать уши"
                    ],
                    "target_lang_definitions": {
                        "en": "The lowering part of a winter hat that covers the ears from the cold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earflap",
                                "target_lang_sense_clarification": "part of a hat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6ab16fd-be6e-3c38-b93d-5e2073f7b1b4",
                    "sense_definition": "Выступающая деталь предмета с отверстием для крепления или подвешивания.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочий зацепил трос крана за стальное <w>ухо</w> бетонной плиты.",
                            "target_lang_translations": {
                                "en": "The worker hooked the crane cable to the steel <w>lug</w> of the concrete slab."
                            }
                        },
                        {
                            "text": "У старого ведра отломилось одно <w>ухо</w>, и ручка упала.",
                            "target_lang_translations": {
                                "en": "One <w>ear</w> of the old bucket broke off, and the handle fell."
                            }
                        }
                    ],
                    "synonyms": [
                        "проушина"
                    ],
                    "common_phrases": [
                        "ухо колокола",
                        "приварить ухо"
                    ],
                    "target_lang_definitions": {
                        "en": "A protruding part of an object with a hole for fastening or hanging."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lug",
                                "target_lang_sense_clarification": "projection for attachment"
                            },
                            {
                                "target_lang_word": "ear",
                                "target_lang_sense_clarification": "handle or projection"
                            },
                            {
                                "target_lang_word": "eye",
                                "target_lang_sense_clarification": "loop or hole"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "oparcie",
        "opór",
        "oporny",
        "podpierać",
        "wspierać",
        "zapierać",
        "opierunek"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0a2ab527-c6b5-3007-a4fa-90e2f2e019a2",
                    "sense_definition": "Przechylać się i dotykać czegoś plecami, ręką lub bokiem, aby utrzymać równowagę lub odpocząć (zwykle z zaimkiem „się”)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_leaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony turysta <opierał> się o wielki głaz.",
                            "target_lang_translations": {
                                "ru": "Уставший турист <опирался> на большой валун."
                            }
                        },
                        {
                            "text": "Nie <opieraj> się o drzwi, bo mogą się otworzyć.",
                            "target_lang_translations": {
                                "ru": "Не <прислоняйся> к двери, так как она может открыться."
                            }
                        }
                    ],
                    "synonyms": [
                        "podpierać się"
                    ],
                    "common_phrases": [
                        "opierać się o ścianę",
                        "opierać się łokciami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклоняться и касаться чего-либо спиной, рукой или боком для сохранения равновесия или отдыха"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опираться",
                                "target_lang_sense_clarification": "использовать как опору, переносить вес"
                            },
                            {
                                "target_lang_word": "прислоняться",
                                "target_lang_sense_clarification": "касаться спиной или боком (стены, двери)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec2dc25e-849f-32bc-a3f5-b9fb6a59a62b",
                    "sense_definition": "Traktować coś jako podstawę, fundament lub dowód dla swoich twierdzeń, działań lub teorii (często z zaimkiem „się”)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swoją opinię <opieram> na wieloletnim doświadczeniu w branży.",
                            "target_lang_translations": {
                                "ru": "Свое мнение я <основываю> на многолетнем опыте работы в отрасли."
                            }
                        },
                        {
                            "text": "Scenariusz tego filmu <opiera> się na faktach historycznych.",
                            "target_lang_translations": {
                                "ru": "Сценарий этого фильма <основывается> на исторических фактах."
                            }
                        }
                    ],
                    "synonyms": [
                        "bazować",
                        "polegać"
                    ],
                    "common_phrases": [
                        "opierać się na faktach",
                        "opierać się na założeniach",
                        "opierać wiedzę na czymś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать что-либо в качестве основы, фундамента или доказательства для своих утверждений, действий или теорий"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основывать",
                                "target_lang_sense_clarification": "строить мнение или теорию на чем-либо"
                            },
                            {
                                "target_lang_word": "опираться",
                                "target_lang_sense_clarification": "использовать факты как довод"
                            },
                            {
                                "target_lang_word": "базироваться",
                                "target_lang_sense_clarification": "иметь в основе (о теории, методе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cdcc882-db70-3b79-8be6-9103a252ee06",
                    "sense_definition": "Ustawiać przedmiot w pozycji pochyłej tak, aby jego górna część dotykała podpory i dzięki temu się nie przewróciła",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_leaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malarz ostrożnie <opierał> drabinę o ścianę budynku.",
                            "target_lang_translations": {
                                "ru": "Маляр осторожно <прислонял> лестницу к стене здания."
                            }
                        },
                        {
                            "text": "Rowerzysta <opiera> rower o drzewo, żeby zawiązać buta.",
                            "target_lang_translations": {
                                "ru": "Велосипедист <прислоняет> велосипед к дереву, чтобы завязать шнурок."
                            }
                        }
                    ],
                    "synonyms": [
                        "przystawiać",
                        "podpierać"
                    ],
                    "common_phrases": [
                        "opierać drabinę",
                        "opierać głowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставить предмет в наклонном положении так, чтобы его верхняя часть касалась опоры и он не падал"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прислонять",
                                "target_lang_sense_clarification": "ставить наклонно к стене или опоре"
                            },
                            {
                                "target_lang_word": "опирать",
                                "target_lang_sense_clarification": "приставлять для опоры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "494d2ccd-8360-3115-8615-4b3bc5be8a0a",
                    "sense_definition": "Stawiać czynny opór, nie zgadzać się na coś lub walczyć z czymś (zawsze z zaimkiem „się”)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko długo <opierało> się przed pójściem spać.",
                            "target_lang_translations": {
                                "ru": "Ребенок долго <сопротивлялся> тому, чтобы идти спать."
                            }
                        },
                        {
                            "text": "Żołnierze dzielnie <opierali> się atakom przeważających sił wroga.",
                            "target_lang_translations": {
                                "ru": "Солдаты храбро <сопротивлялись> атакам превосходящих сил врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwstawiać się",
                        "bronić się"
                    ],
                    "antonyms": [
                        "poddawać się",
                        "ulegać"
                    ],
                    "common_phrases": [
                        "opierać się pokusie",
                        "opierać się zmianom",
                        "opierać się namowom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать активное сопротивление, не соглашаться с чем-либо или бороться с чем-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопротивляться",
                                "target_lang_sense_clarification": "противодействовать силе, атаке или воле"
                            },
                            {
                                "target_lang_word": "противиться",
                                "target_lang_sense_clarification": "выступать против чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3ce66b1-7f0f-3401-b9f8-e9dd23cdb563",
                    "sense_definition": "Prać komuś ubrania lub bieliznę, dbać o czyjąś czystość",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "washing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia całe życie <opierała> męża i synów.",
                            "target_lang_translations": {
                                "ru": "Бабушка всю жизнь <обстирывала> мужа и сыновей."
                            }
                        },
                        {
                            "text": "Muszę <opierać> całą rodzinę, bo pralka się zepsuła.",
                            "target_lang_translations": {
                                "ru": "Мне приходится <обстирывать> всю семью, так как сломалась стиральная машина."
                            }
                        }
                    ],
                    "synonyms": [
                        "prać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane rzadziej niż standardowe 'prać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стирать кому-либо одежду или белье, заботиться о чьей-либо чистоте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обстирывать",
                                "target_lang_sense_clarification": "стирать белье для кого-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
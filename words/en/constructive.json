                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конструктивный",
                                "target_lang_sense_clarification": "полезный, плодотворный"
                            },
                            {
                                "target_lang_word": "созидательный",
                                "target_lang_sense_clarification": "направленный на создание"

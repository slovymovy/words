{
    "word_family": [
        "vreugde",
        "verheugd",
        "verheugend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab9e3f00-480d-3c4b-95f5-dfd26e29c42e",
                    "sense_definition": "Met plezier vooruitkijken naar iets leuks dat gaat gebeuren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anticipation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>verheug</w> me erg op onze vakantie naar Spanje.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>предвкушаю</w> наш отпуск в Испании."
                            }
                        },
                        {
                            "text": "De kinderen <w>verheugen</w> zich al weken op het verjaardagsfeestje.",
                            "target_lang_translations": {
                                "ru": "Дети уже несколько недель <w>предвкушают</w> вечеринку по случаю дня рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitkijken naar",
                        "uitzien naar"
                    ],
                    "antonyms": [
                        "opzien tegen"
                    ],
                    "common_phrases": [
                        "zich verheugen op",
                        "ik verheug me erop"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "С нетерпением и удовольствием ожидать предстоящего события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предвкушать",
                                "target_lang_sense_clarification": "ожидать с удовольствием"
                            },
                            {
                                "target_lang_word": "ждать с нетерпением",
                                "target_lang_sense_clarification": "сильно желать наступления чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "258c64de-5f4c-3dbc-8cb3-bdddc58e7d7a",
                    "sense_definition": "Blijdschap voelen of plezier hebben over iets.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>verheugde</w> zich enorm toen hij het goede nieuws hoorde.",
                            "target_lang_translations": {
                                "ru": "Он очень <w>обрадовался</w>, когда услышал хорошие новости."
                            }
                        },
                        {
                            "text": "Zij <w>verheugt</w> zich in de rust en stilte van de natuur.",
                            "target_lang_translations": {
                                "ru": "Она <w>радуется</w> покою и тишине природы."
                            }
                        }
                    ],
                    "synonyms": [
                        "blij zijn",
                        "genieten"
                    ],
                    "antonyms": [
                        "treuren"
                    ],
                    "common_phrases": [
                        "zich ergens over verheugen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать чувство радости или удовольствия по поводу чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радоваться",
                                "target_lang_sense_clarification": "испытывать веселье, радость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d61a928c-143c-38e7-bedf-87a2a0d42568",
                    "sense_definition": "Iemand blij maken of vreugde geven.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "causative_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>verheugt</w> mij zeer dat u vandaag aanwezig kunt zijn.",
                            "target_lang_translations": {
                                "ru": "Меня очень <w>радует</w>, что вы смогли сегодня присутствовать."
                            }
                        },
                        {
                            "text": "Je snelle herstel heeft ons allemaal enorm <w>verheugd</w>.",
                            "target_lang_translations": {
                                "ru": "Твое быстрое выздоровление нас всех очень <w>обрадовало</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verblijden",
                        "plezieren"
                    ],
                    "antonyms": [
                        "bedroeven",
                        "teleurstellen"
                    ],
                    "common_phrases": [
                        "het verheugt mij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доставлять радость, делать кого-то счастливым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радовать",
                                "target_lang_sense_clarification": "вызывать у кого-то чувство радости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
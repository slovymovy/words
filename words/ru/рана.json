            "senses": [
                {
                    "sense_id": "5b790c8e-0691-33d2-89c9-5fc5df4ba766",
                    "sense_definition": "Повреждение тканей тела, вызванное внешним воздействием, обычно с нарушением целостности кожи.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач внимательно осмотрел глубокую <w>рану</w> на ноге пациента.",
                            "target_lang_translations": {
                                "en": "The doctor carefully examined the deep <w>wound</w> on the patient's leg."
                            }
                        },
                        {
                            "text": "Солдат получил лёгкую <w>рану</w> и быстро вернулся в строй.",
                            "target_lang_translations": {
                                "en": "The soldier received a minor <w>wound</w> and quickly returned to duty."
                            }
                        }
                    ],
                    "synonyms": [
                        "ранение"
                    ],
                    "common_phrases": [
                        "глубокая рана",
                        "открытая рана",
                        "перевязать рану",
                        "смертельная рана",
                        "заживление раны"
                    ],
                    "target_lang_definitions": {
                        "en": "Damage to body tissues caused by external impact, usually involving a break in the skin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wound",
                                "target_lang_sense_clarification": "physical injury, especially a cut or break in the skin"
                            },
                            {
                                "target_lang_word": "injury",
                                "target_lang_sense_clarification": "general physical damage or harm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5ae0781-219e-3159-bb6c-312e74dc1f24",
                    "sense_definition": "Сильное душевное переживание, моральная боль или страдание (переносное значение).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Время постепенно лечит даже самые тяжёлые душевные <w>раны</w>.",
                            "target_lang_translations": {
                                "en": "Time gradually heals even the most severe emotional <w>wounds</w>."
                            }
                        },
                        {
                            "text": "Её жестокие слова нанесли ему глубокую сердечную <w>рану</w>.",
                            "target_lang_translations": {
                                "en": "Her cruel words inflicted a deep <w>wound</w> in his heart."
                            }
                        }
                    ],
                    "synonyms": [
                        "боль"
                    ],
                    "antonyms": [
                        "радость"
                    ],
                    "common_phrases": [
                        "душевная рана",
                        "сердечная рана",
                        "старые раны",
                        "бередить рану"
                    ],
                    "target_lang_definitions": {
                        "en": "Intense emotional distress, moral pain, or suffering (figurative meaning)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wound",
                                "target_lang_sense_clarification": "emotional hurt or psychological trauma"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a7b9c39-ea7a-342d-9940-23319be63cea",
                    "sense_definition": "Серьёзный вред или ущерб, причинённый чему-либо (стране, экономике, природе).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Долгая война нанесла страшную <w>рану</w> экономике государства.",
                            "target_lang_translations": {
                                "en": "The long war inflicted a terrible <w>wound</w> on the state's economy."
                            }
                        },
                        {
                            "text": "Стране потребовались годы, чтобы залечить <w>раны</w> прошлого кризиса.",
                            "target_lang_translations": {
                                "en": "It took the country years to heal the <w>wounds</w> of the past crisis."
                            }
                        }
                    ],
                    "synonyms": [
                        "ущерб",
                        "урон"
                    ],
                    "antonyms": [
                        "польза"
                    ],
                    "common_phrases": [
                        "нанести рану",
                        "залечивать раны"
                    ],
                    "target_lang_definitions": {
                        "en": "Serious harm or damage caused to something (e.g., a country, economy, or nature)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wound",
                                "target_lang_sense_clarification": "metaphorical damage or harm"
                            },
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "harm or detriment to a system"
                            }
                        ]
                    }
                }
            ]
        }
    ]

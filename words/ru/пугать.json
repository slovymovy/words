    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8a405801-a6f6-3570-9f41-a1b56afc0e7a",
                    "sense_definition": "Вызывать чувство страха или испуга.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит <w>пугать</w> собаку, она может укусить.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>scare</w> the dog, it might bite."
                            }
                        },
                        {
                            "text": "Темнота в комнате начала <w>пугать</w> маленького ребёнка.",
                            "target_lang_translations": {
                                "en": "The darkness in the room began to <w>frighten</w> the small child."
                            }
                        }
                    ],
                    "synonyms": [
                        "страшить",
                        "устрашать",
                        "ужасать",
                        "нагонять страх"
                    ],
                    "antonyms": [
                        "успокаивать"
                    ],
                    "common_phrases": [
                        "пугать детей",
                        "пугать до смерти",
                        "не надо пугать"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a feeling of fear or fright."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scare",
                                "target_lang_sense_clarification": "general term for causing fear"
                            },
                            {
                                "target_lang_word": "frighten",
                                "target_lang_sense_clarification": "standard synonym for scare"
                            },
                            {
                                "target_lang_word": "startle",
                                "target_lang_sense_clarification": "to scare suddenly or by surprise"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f7bdd3c-f140-3255-8982-3668458929b8",
                    "sense_definition": "Вызывать тревогу, беспокойство или опасения.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Меня <w>пугает</w> твоё равнодушие к учёбе.",
                            "target_lang_translations": {
                                "en": "Your indifference to your studies <w>scares</w> me."
                            }
                        },
                        {
                            "text": "Экономическая ситуация в стране продолжает <w>пугать</w> инвесторов.",
                            "target_lang_translations": {
                                "en": "The economic situation in the country continues to <w>alarm</w> investors."
                            }
                        }
                    ],
                    "synonyms": [
                        "тревожить",
                        "беспокоить",
                        "вызывать опасения"
                    ],
                    "antonyms": [
                        "успокаивать"
                    ],
                    "common_phrases": [
                        "меня это пугает",
                        "пугать неизвестностью"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause anxiety, worry, or apprehension."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worry",
                                "target_lang_sense_clarification": "to cause anxiety or concern"
                            },
                            {
                                "target_lang_word": "alarm",
                                "target_lang_sense_clarification": "to cause a feeling of danger or concern"
                            },
                            {
                                "target_lang_word": "dismay",
                                "target_lang_sense_clarification": "to cause distress or sadness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1d80926-c500-3283-a129-986f9fb0ea32",
                    "sense_definition": "Намеренно внушать страх, угрожать неприятностями или наказанием.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник любил <w>пугать</w> сотрудников штрафами за опоздания.",
                            "target_lang_translations": {
                                "en": "The boss loved to <w>threaten</w> employees with fines for lateness."
                            }
                        },
                        {
                            "text": "Хулиганы пытались <w>пугать</w> прохожих в парке.",
                            "target_lang_translations": {
                                "en": "Hooligans tried to <w>intimidate</w> passers-by in the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "запугивать",
                        "угрожать",
                        "стращать",
                        "шугать"
                    ],
                    "common_phrases": [
                        "пугать увольнением",
                        "пугать судом",
                        "пугать расправой"
                    ],
                    "target_lang_definitions": {
                        "en": "To intentionally instill fear, threaten with trouble or punishment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "threaten",
                                "target_lang_sense_clarification": "to promise punishment or harm"
                            },
                            {
                                "target_lang_word": "intimidate",
                                "target_lang_sense_clarification": "to frighten in order to compel action"
                            },
                            {
                                "target_lang_word": "browbeat",
                                "target_lang_sense_clarification": "to bully or intimidate with words"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46761f3b-4ec3-3e23-8d38-63d007a42b85",
                    "sense_definition": "Предостерегать, сообщая о возможной, часто преувеличенной опасности.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи любят <w>пугать</w> пациентов страшными диагнозами, которые потом не подтверждаются.",
                            "target_lang_translations": {
                                "en": "Doctors love to <w>scare</w> patients with terrible diagnoses that are not later confirmed."
                            }
                        },
                        {
                            "text": "Газеты продолжают <w>пугать</w> население глобальным потеплением.",
                            "target_lang_translations": {
                                "en": "Newspapers continue to <w>scare</w> the population with global warming."
                            }
                        }
                    ],
                    "synonyms": [
                        "предостерегать"
                    ],
                    "common_phrases": [
                        "пугать последствиями",
                        "пугать кризисом"
                    ],
                    "target_lang_definitions": {
                        "en": "To warn or caution, often about a possible or exaggerated danger."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warn",
                                "target_lang_sense_clarification": "to inform of impending danger"
                            },
                            {
                                "target_lang_word": "scare",
                                "target_lang_sense_clarification": "to frighten with information"
                            },
                            {
                                "target_lang_word": "caution",
                                "target_lang_sense_clarification": "to advise prudence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "31a32817-c6b9-3ebd-a511-026f61778de6",
                    "sense_definition": "Stan fizyczny, w którym ciało lub przedmiot zachowuje stabilną pozycję i nie przewraca się.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_balance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko uczy się chodzić i z trudem próbuje utrzymać <w>równowagę</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок учится ходить и с трудом пытается удержать <w>равновесие</w>."
                            }
                        },
                        {
                            "text": "Poślizgnąłem się na mokrej podłodze i na chwilę straciłem <w>równowagę</w>.",
                            "target_lang_translations": {
                                "ru": "Я поскользнулся на мокром полу и на мгновение потерял <w>равновесие</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stabilność"
                    ],
                    "common_phrases": [
                        "utrzymać równowagę",
                        "stracić równowagę",
                        "zachować równowagę",
                        "odzyskać równowagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое состояние, при котором тело или предмет сохраняет устойчивое положение и не падает."
                    },
                    "translations": {

                    "frequency": "Middle",
                    "semantic_group_id": "tech_abbrev",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания закупила новый <w>ПАК</w> для защиты корпоративных данных.",
                            "target_lang_translations": {
                                "en": "The company purchased a new <w>software and hardware complex</w> to protect corporate data."
                            }
                        },
                        {
                            "text": "Отечественный <w>ПАК</w> успешно заменил иностранное оборудование.",
                            "target_lang_translations": {
                                "en": "The domestic <w>software and hardware complex</w> successfully replaced foreign equipment."
                            }
                        }
                    ],
                    "synonyms": [
                        "программно-аппаратный комплекс"
                    ],
                    "common_phrases": [
                        "внедрение ПАК",
                        "реестр ПАК",
                        "сертифицированный ПАК"

                },
                {
                    "sense_id": "29622691-1f83-32d8-bf58-c64e8089f20c",
                    "sense_definition": "Een hekwerk, leuning of balustrade, vaak ter afscheiding bij een brug of trap.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "railing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude brug heeft een sierlijke ijzeren <w>balie</w>.",
                            "target_lang_translations": {
                                "en": "The old bridge has an ornate iron <w>railing</w>."
                            }
                        },
                        {
                            "text": "Leun niet te ver over de <w>balie</w>, dat is gevaarlijk.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3cd14432-33da-3100-87a2-bfecba356df0",
                    "sense_definition": "To make someone or something ready for a specific activity or situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action_prepare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef needs to <w>prep</w> the vegetables before the dinner service begins.",
                            "target_lang_translations": {
                                "ru": "Шеф-повару нужно <w>подготовить</w> овощи до начала ужина."
                            }
                        },
                        {
                            "text": "We spent the whole afternoon helping to <w>prep</w> for the surprise party.",
                            "target_lang_translations": {
                                "ru": "Мы провели весь день, помогая <w>готовиться</w> к вечеринке-сюрпризу."
                            }
                        }
                    ],
                    "synonyms": [
                        "prep up"

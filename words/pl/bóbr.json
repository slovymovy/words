            "senses": [
                {
                    "sense_id": "c52c954f-68df-301f-b4cf-babf9aadf4c6",
                    "sense_definition": "Ziemnowodny ssak z rzędu gryzoni, posiadający gęste futro i charakterystyczny płaski ogon, znany z budowania tam i żeremi na rzekach.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad brzegiem rzeki siedział <w>bóbr</w> i ogryzał gałąź wierzby.",
                            "target_lang_translations": {
                                "ru": "На берегу реки сидел <w>бобр</w> и грыз ивовую ветку."
                            }
                        },
                        {
                            "text": "Kiedy dziewczyna usłyszała smutną historię, zaczęła płakać jak <w>bóbr</w>.",
                            "target_lang_translations": {
                                "ru": "Когда девушка услышала грустную историю, она начала реветь как <w>белуга</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płakać jak bóbr",
                        "pracowity jak bóbr",
                        "tama bobra",
                        "żeremie bobra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полуводное млекопитающее из отряда грызунов, обладающее густым мехом и характерным плоским хвостом, известное строительством плотин и хаток на реках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бобр",
                                "target_lang_sense_clarification": "животное"
                            },
                            {
                                "target_lang_word": "бобёр",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a36c5be8-b13f-3421-a0ef-25921e869fde",
                    "sense_definition": "Wyprawione futro z bobra lub uszyty z niego element odzieży, na przykład kołnierz.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W szafie babci wisiały stare, ale wciąż eleganckie <w>bobry</w>.",
                            "target_lang_translations": {

                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "severity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest <w>krytyczny</w> moment dla naszej firmy, który zadecyduje o jej przyszłości.",
                            "target_lang_translations": {
                                "ru": "Это <w>критический</w> момент для нашей компании, который решит её будущее."
                            }

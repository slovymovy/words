    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b1d7be3-bead-306a-87da-c6e5569797ef",
                    "sense_definition": "The area in a hotel, office, or hospital where guests or clients are greeted and dealt with.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please leave your room key at <w>reception</w> when you go out.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оставьте ключ от номера на <w>ресепшене</w>, когда будете уходить."
                            }
                        },
                        {
                            "text": "The woman at the <w>reception</w> desk was very helpful with our questions.",
                            "target_lang_translations": {
                                "ru": "Женщина за <w>стойкой администратора</w> очень помогла нам с нашими вопросами."
                            }
                        }
                    ],
                    "synonyms": [
                        "front desk"
                    ],
                    "common_phrases": [
                        "at reception",
                        "reception desk",
                        "hotel reception"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зона в отеле, офисе или больнице, где встречают и обслуживают гостей или клиентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ресепшен",
                                "target_lang_sense_clarification": "в отеле или офисе"
                            },
                            {
                                "target_lang_word": "стойка администратора",
                                "target_lang_sense_clarification": "место работы администратора"
                            },
                            {
                                "target_lang_word": "приёмная",
                                "target_lang_sense_clarification": "комната ожидания"
                            },
                            {
                                "target_lang_word": "регистратура",
                                "target_lang_sense_clarification": "в медицинском учреждении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "427dcea2-295f-3827-a3ab-735ef26ee56a",
                    "sense_definition": "A formal social gathering to welcome guests, often held after a wedding or official event.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They invited over two hundred guests to their wedding <w>reception</w>.",
                            "target_lang_translations": {
                                "ru": "Они пригласили более двухсот гостей на свой свадебный <w>банкет</w>."
                            }
                        },
                        {
                            "text": "The ambassador held a formal <w>reception</w> for the visiting diplomats.",
                            "target_lang_translations": {
                                "ru": "Посол устроил официальный <w>приём</w> для приезжих дипломатов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wedding reception",
                        "hold a reception",
                        "attend a reception"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное общественное мероприятие для встречи гостей, часто проводимое после свадьбы или официального события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "официальное мероприятие"
                            },
                            {
                                "target_lang_word": "банкет",
                                "target_lang_sense_clarification": "торжественный обед или ужин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "323f94f1-3782-3fdb-9ac9-1da826e48771",
                    "sense_definition": "The quality or ability to receive radio, television, or mobile phone signals.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot call you right now because the mobile <w>reception</w> here is terrible.",
                            "target_lang_translations": {
                                "ru": "Я не могу позвонить тебе прямо сейчас, потому что мобильный <w>приём</w> здесь ужасный."
                            }
                        },
                        {
                            "text": "We get very clear radio <w>reception</w> thanks to the new antenna.",
                            "target_lang_translations": {
                                "ru": "У нас очень чистый радио<w>приём</w> благодаря новой антенне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poor reception",
                        "good reception",
                        "mobile reception"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество или способность принимать радио-, теле- или мобильные сигналы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "сигнала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5a786fa-1f9d-35e5-a63c-1a6e17c95f2d",
                    "sense_definition": "The way in which someone or something is treated or reacted to upon arrival or introduction.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new company policy got a mixed <w>reception</w> from the employees.",
                            "target_lang_translations": {
                                "ru": "Новая политика компании встретила неоднозначный <w>приём</w> со стороны сотрудников."
                            }
                        },
                        {
                            "text": "The famous band received a very warm <w>reception</w> from the crowd.",
                            "target_lang_translations": {
                                "ru": "Известная группа получила очень теплый <w>приём</w> от толпы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "warm reception",
                        "cold reception",
                        "mixed reception"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, как кого-то или что-то встречают или как реагируют на появление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "реакция на кого-то или что-то"
                            },
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "то, как встретили"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3d12639-9367-3a42-80ac-9839ee1c9976",
                    "sense_definition": "The formal act of receiving or accepting something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>reception</w> of the stolen goods was considered a serious crime.",
                            "target_lang_translations": {
                                "ru": "<w>Получение</w> краденых товаров считалось серьезным преступлением."
                            }
                        },
                        {
                            "text": "They organized the <w>reception</w> of the refugees at the border.",
                            "target_lang_translations": {
                                "ru": "Они организовали <w>приём</w> беженцев на границе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reception of goods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формальный акт получения или принятия чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получение",
                                "target_lang_sense_clarification": "приобретение чего-либо"
                            },
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "действие по принятию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73aa2e40-581a-36b3-bc69-c6873d930be6",
                    "sense_definition": "The first year of primary school in the UK, for children aged four to five.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My son is excited to start <w>reception</w> next September.",
                            "target_lang_translations": {
                                "ru": "Мой сын с нетерпением ждет начала занятий в <w>подготовительном классе</w> в следующем сентябре."
                            }
                        },
                        {
                            "text": "The children in the <w>reception</w> class are learning to read and write.",
                            "target_lang_translations": {
                                "ru": "Дети в <w>подготовительном классе</w> учатся читать и писать."
                            }
                        }
                    ],
                    "common_phrases": [

                            },
                            {
                                "target_lang_word": "changeable",
                                "target_lang_sense_clarification": "tending to change frequently"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переменный",
                                "target_lang_sense_clarification": "склонный к частым изменениям"
                            },
                            {
                                "target_lang_word": "чередующийся",
                                "target_lang_sense_clarification": "повторяющийся по очереди"
                            }
                        ]
                    }
                }
            ]
        },

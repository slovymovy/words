            "senses": [
                {
                    "sense_id": "9592b229-4a75-3795-b348-b7ef4b77dc9a",
                    "sense_definition": "Wulgarne określenie męskiego narządu płciowego.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_vulgar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W toalecie na ścianie ktoś narysował wielkiego <w>kutasa</w>.",
                            "target_lang_translations": {
                                "ru": "В туалете на стене кто-то нарисовал огромный <w>хуй</w>."
                            }
                        },
                        {
                            "text": "Chłopak zasłonił rękami swojego <w>kutasa</w>, gdy ktoś wszedł do szatni.",
                            "target_lang_translations": {
                                "ru": "Парень прикрыл руками свой <w>хер</w>, когда кто-то вошел в раздевалку."
                            }
                        }
                    ],
                    "synonyms": [
                        "chuj",
                        "fiut",
                        "pała",
                        "fujara",
                        "siusiak"
                    ],
                    "common_phrases": [
                        "twardy kutas",
                        "mały kutas"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgarne"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное название мужского полового органа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хуй",
                                "target_lang_sense_clarification": "вульгарно, нецензурно"
                            },
                            {
                                "target_lang_word": "хер",
                                "target_lang_sense_clarification": "грубое просторечие"
                            },
                            {
                                "target_lang_word": "член",
                                "target_lang_sense_clarification": "анатомическое, общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "216afe4a-1f6b-33e9-990d-a94589846179",
                    "sense_definition": "Wulgarne i obraźliwe określenie mężczyzny, którego się nie lubi, uważa za podłego lub głupiego.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "insults",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>kutas</w> znowu zajął moje miejsce parkingowe pod blokiem.",
                            "target_lang_translations": {
                                "ru": "Этот <w>мудак</w> снова занял мое парковочное место у дома."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>kutasem</w> i oddaj mi wreszcie te pieniądze.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>козлом</w> и верни мне наконец эти деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "chuj",
                        "złamas",
                        "dupek",
                        "gnój",
                        "skurwiel"
                    ],
                    "common_phrases": [
                        "kawał kutasa",
                        "głupi kutas",
                        "złamany kutas"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgarne"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "obraźliwe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное и оскорбительное слово для обозначения неприятного, подлого или глупого мужчины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мудак",
                                "target_lang_sense_clarification": "о глупом или подлом человеке"
                            },
                            {
                                "target_lang_word": "козёл",
                                "target_lang_sense_clarification": "ругательство"
                            },
                            {
                                "target_lang_word": "урод",
                                "target_lang_sense_clarification": "моральный урод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d90b8fec-ed3e-37ab-a47e-a5a863c9a5df",
                    "sense_definition": "Ozdobne zakończenie sznura w postaci pęku nici lub frędzli, używane dawniej jako element dekoracyjny ubioru lub zasłon.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_decor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachcic nosił pas ozdobiony jedwabnym <w>kutasem</w>, który zwisał mu u boku.",
                            "target_lang_translations": {
                                "ru": "Шляхтич носил пояс, украшенный шелковой <w>кистью</w>, которая свисала у него сбоку."
                            }
                        },
                        {
                            "text": "Przy starych, aksamitnych zasłonach wisiał ciężki, złoty <w>kutas</w>.",
                            "target_lang_translations": {
                                "ru": "У старых бархатных штор висела тяжелая золотая <w>кисть</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwost",
                        "frędzel",
                        "pompon"
                    ],
                    "common_phrases": [
                        "pas z kutasami",
                        "czapka z kutasem"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie słowo kojarzone głównie wulgarnie"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "termin rzemieślniczy lub historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративное окончание шнура в виде пучка нитей или бахромы, использовавшееся в старину как элемент украшения одежды или штор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кисть",
                                "target_lang_sense_clarification": "украшение из нитей"
                            },
                            {
                                "target_lang_word": "кисточка",
                                "target_lang_sense_clarification": "уменьшительно"
                            }
                        ]
                    }
                }
            ]
        }

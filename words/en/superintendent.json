{
    "word_family": [
        "superintend",
        "superintendence",
        "superintendency"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7418889c-7906-3d42-be1b-b3671fcf25bc",
                    "sense_definition": "A person who is authorized to supervise, direct, or administer an organization, department, or operation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_supervisor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school <superintendent> announced that classes would be cancelled due to the storm.",
                            "target_lang_translations": {
                                "ru": "Школьный <управляющий> объявил, что занятия будут отменены из-за шторма."
                            }
                        },
                        {
                            "text": "She was promoted to <superintendent> of the construction site after five years of hard work.",
                            "target_lang_translations": {
                                "ru": "После пяти лет упорной работы её повысили до <начальника> строительной площадки."
                            }
                        }
                    ],
                    "synonyms": [
                        "supervisor",
                        "overseer",
                        "manager",
                        "head",
                        "chief",
                        "controller",
                        "head man"
                    ],
                    "common_phrases": [
                        "school superintendent",
                        "general superintendent",
                        "district superintendent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, уполномоченное руководить, управлять или администрировать организацию, отдел или процесс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руководитель",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "управляющий",
                                "target_lang_sense_clarification": "администратор"
                            },
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "на производстве или стройке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e59e4d47-452d-30e9-b51a-4de11ff40429",
                    "sense_definition": "The manager of a building, usually a residential apartment block, who is responsible for maintenance, repairs, and sometimes rent collection.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "building_manager",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We called the building <superintendent> when the heating system stopped working in our apartment.",
                            "target_lang_translations": {
                                "ru": "Мы позвонили <коменданту> здания, когда в нашей квартире перестало работать отопление."
                            }
                        },
                        {
                            "text": "The <superintendent> keeps a spare set of keys for every unit in the complex.",
                            "target_lang_translations": {
                                "ru": "<Комендант> хранит запасной комплект ключей от каждой квартиры в комплексе."
                            }
                        }
                    ],
                    "synonyms": [
                        "manager",
                        "overseer",
                        "chief"
                    ],
                    "common_phrases": [
                        "building superintendent",
                        "apartment superintendent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управляющий зданием (обычно жилым), отвечающий за техническое обслуживание, ремонт и иногда сбор арендной платы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комендант",
                                "target_lang_sense_clarification": "общежития или здания"
                            },
                            {
                                "target_lang_word": "управдом",
                                "target_lang_sense_clarification": "разговорное, управляющий домом"
                            },
                            {
                                "target_lang_word": "управляющий",
                                "target_lang_sense_clarification": "зданием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7ae4874-c85a-37b8-822f-9ce54deefeaf",
                    "sense_definition": "A high-ranking police officer in Commonwealth countries, ranking above a chief inspector and below a chief superintendent.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "police_rank",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "The detective <superintendent> led the press conference regarding the major investigation.",
                            "target_lang_translations": {
                                "ru": "Детектив-<суперинтендант> возглавил пресс-конференцию по поводу крупного расследования."
                            }
                        },
                        {
                            "text": "After twenty years on the force, he finally achieved the rank of <superintendent>.",
                            "target_lang_translations": {
                                "ru": "После двадцати лет службы в полиции он наконец получил звание <суперинтенданта>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "detective superintendent",
                        "police superintendent",
                        "chief superintendent"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Commonwealth countries"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий полицейский чин в странах Содружества, рангом выше старшего инспектора и ниже главного суперинтенданта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суперинтендант",
                                "target_lang_sense_clarification": "полицейское звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8faf986-67d2-3b72-830a-0ce7246d4869",
                    "sense_definition": "A person employed to take care of a large building, such as a school or office block, and who deals with cleaning and minor repairs.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "building_manager",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <superintendent> mopped the hallway floors every evening after the staff went home.",
                            "target_lang_translations": {
                                "ru": "<Смотритель> мыл полы в коридоре каждый вечер после того, как сотрудники уходили домой."
                            }
                        },
                        {
                            "text": "Students were asked to help the <superintendent> by picking up their trash in the cafeteria.",
                            "target_lang_translations": {
                                "ru": "Студентов попросили помочь <смотрителю>, убрав за собой мусор в столовой."
                            }
                        }
                    ],
                    "synonyms": [
                        "manager"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, нанятый для ухода за большим зданием (школой или офисом), занимающийся уборкой и мелким ремонтом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотритель",
                                "target_lang_sense_clarification": "за зданием"
                            },
                            {
                                "target_lang_word": "завхоз",
                                "target_lang_sense_clarification": "заведующий хозяйством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0fa49d9-92d9-3e7f-b889-8f7476410cc9",
                    "sense_definition": "The person in charge of organizing and running a Sunday school.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <superintendent> organized the children's Christmas play for the church congregation.",
                            "target_lang_translations": {
                                "ru": "<Руководитель> организовал детский рождественский спектакль для церковной общины."
                            }
                        },
                        {
                            "text": "She volunteered as the Sunday school <superintendent> for over ten years.",
                            "target_lang_translations": {
                                "ru": "Она была волонтером в качестве <директора> воскресной школы более десяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "head",
                        "manager"
                    ],
                    "common_phrases": [
                        "Sunday school superintendent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, отвечающий за организацию и работу воскресной школы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директор",
                                "target_lang_sense_clarification": "школы"
                            },
                            {
                                "target_lang_word": "руководитель",
                                "target_lang_sense_clarification": "организатор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccebde3d-2002-308b-82bb-26e04de07a8a",
                    "sense_definition": "A clergyman in some Protestant churches who has oversight of the clergy and churches within a specific district.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_role",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "The district <superintendent> visited our parish to discuss the appointment of a new pastor.",
                            "target_lang_translations": {
                                "ru": "Окружной <суперинтендант> посетил наш приход, чтобы обсудить назначение нового пастора."
                            }
                        },
                        {
                            "text": "He reported directly to the <superintendent> regarding the financial status of the local church.",
                            "target_lang_translations": {
                                "ru": "Он отчитывался непосредственно <суперинтенданту> о финансовом состоянии местной церкви."
                            }
                        }
                    ],
                    "synonyms": [
                        "overseer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Священнослужитель в некоторых протестантских церквях, осуществляющий надзор за духовенством и приходами в определенном округе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суперинтендант",
                                "target_lang_sense_clarification": "церковный сан"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "df15b4f1-d7e3-3d8b-94ea-639fa97eaaa0",
                    "sense_definition": "Relating to the act of overseeing, directing, or managing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee held <superintendent> authority over the entire development project.",
                            "target_lang_translations": {
                                "ru": "Комитет обладал <надзорными> полномочиями над всем проектом разработки."
                            }
                        },
                        {
                            "text": "His <superintendent> role required him to inspect every department twice a year.",
                            "target_lang_translations": {
                                "ru": "Его <руководящая> роль требовала от него инспектировать каждый отдел дважды в год."
                            }
                        }
                    ],
                    "synonyms": [
                        "overseer",
                        "supervisor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к акту надзора, руководства или управления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надзорный",
                                "target_lang_sense_clarification": "о полномочиях"
                            },
                            {
                                "target_lang_word": "руководящий",
                                "target_lang_sense_clarification": "о роли или функции"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
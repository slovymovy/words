                {
                    "sense_id": "adebfad8-7346-3b5c-89ab-a70607ed4aca",
                    "sense_definition": "Osobnik, który zachowuje się podejrzanie, dziwnie lub nie budzi zaufania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W bramie stała jakaś podejrzana <w>figura</w> w ciemnym płaszczu.",
                            "target_lang_translations": {
                                "ru": "В воротах стояла какая-то подозрительная <w>фигура</w> в темном плаще."
                            }
                        },
                        {
                            "text": "Nie ufam mu, to jest jakaś ciemna <w>figura</w>.",
                            "target_lang_translations": {

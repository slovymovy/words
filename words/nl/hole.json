                    "sense_id": "17224802-2e94-3cb6-a276-705e63de5e55",
                    "sense_definition": "Het putje in de grond op de green waar de golfbal in geslagen moet worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "golf_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bal rolde langzaam over het gras en viel precies in de <w>hole</w>.",
                            "target_lang_translations": {
                                "en": "The ball rolled slowly over the grass and fell exactly into the <w>hole</w>."
                            }
                        },
                        {
                            "text": "Hij probeerde de bal in één slag in de <w>hole</w> te krijgen.",
                            "target_lang_translations": {
                                "en": "He tried to get the ball into the <w>hole</w> in one shot."
                            }
                        }
                    ],
                    "synonyms": [
                        "putje"
                    ],
                    "common_phrases": [
                        "de bal in de hole slaan",
                        "de vlag uit de hole halen"
                    ],
                    "target_lang_definitions": {
                        "en": "The small cup in the ground on the green into which the golf ball must be hit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hole",
                                "target_lang_sense_clarification": "golf target"
                            },
                            {
                                "target_lang_word": "cup",
                                "target_lang_sense_clarification": "golf hole liner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fb58543-026d-3219-b116-f7bf07c535c9",
                    "sense_definition": "Het gedeelte van een golfbaan vanaf de afslagplaats tot en met de green met het putje.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "golf_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze golfbaan heeft achttien uitdagende <w>holes</w>.",
                            "target_lang_translations": {
                                "en": "This golf course has eighteen challenging <w>holes</w>."
                            }
                        },
                        {
                            "text": "Bij de derde <w>hole</w> sloeg hij de bal per ongeluk in het water.",
                            "target_lang_translations": {
                                "en": "At the third <w>hole</w>, he accidentally hit the ball into the water."
                            }
                        }
                    ],
                    "common_phrases": [
                        "achttien holes",
                        "de negende hole",
                        "een hole spelen"
                    ],
                    "target_lang_definitions": {
                        "en": "The section of a golf course from the teeing ground to the green."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hole",
                                "target_lang_sense_clarification": "unit of a golf course"
                            }
                        ]
                    }
                }

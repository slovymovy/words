        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec6f1db6-c1b2-3b85-9b00-4d8931cf446d",
                    "sense_definition": "The disposal of something no longer needed, or of no value.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "disposal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city has increased fines to stop the illegal <w>dumping</w> of household waste.",
                            "target_lang_translations": {
                                "ru": "Город увеличил штрафы, чтобы остановить незаконный <w>сброс</w> бытовых отходов."
                            }
                        },
                        {
                            "text": "We saw a sign that clearly stated no <w>dumping</w> was allowed in the park.",
                            "target_lang_translations": {
                                "ru": "Мы увидели знак, на котором было четко написано, что <w>сваливание</w> мусора в парке запрещено."
                            }
                        }
                    ],
                    "synonyms": [
                        "disposal",
                        "discarding"
                    ],
                    "antonyms": [
                        "retention",
                        "preservation"
                    ],
                    "common_phrases": [
                        "illegal dumping",
                        "no dumping",
                        "waste dumping"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утилизация или избавление от чего-либо ненужного или не имеющего ценности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сброс",
                                "target_lang_sense_clarification": "отходов, мусора"
                            },
                            {
                                "target_lang_word": "сваливание",
                                "target_lang_sense_clarification": "выгрузка мусора в неположенном месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58bccea3-4fd7-38e4-ac50-89d9f072fb07",
                    "sense_definition": "Selling goods at less than their normal price, or even at a loss, especially in the export market as a means of securing a monopoly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "economics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government imposed tariffs to protect local industries from foreign <w>dumping</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело тарифы для защиты местной промышленности от иностранного <w>демпинга</w>."
                            }
                        },
                        {
                            "text": "Accusations of price <w>dumping</w> have caused tension between the two trading partners.",
                            "target_lang_translations": {
                                "ru": "Обвинения в ценовом <w>демпинге</w> вызвали напряженность между двумя торговыми партнерами."
                            }
                        }
                    ],
                    "synonyms": [
                        "underselling"
                    ],
                    "common_phrases": [
                        "anti-dumping duty",
                        "price dumping",
                        "anti-dumping measures"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "business/economics context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Продажа товаров по цене ниже обычной, часто даже в убыток, особенно на экспортном рынке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демпинг",
                                "target_lang_sense_clarification": "экономический термин"

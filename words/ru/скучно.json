    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ac2c3362-1930-32ab-b191-7927252831e7",
                    "sense_definition": "О чувстве скуки, отсутствии интереса или душевной пустоты, испытываемом кем-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_boredom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <скучно> сидеть дома в дождливую погоду.",
                            "target_lang_translations": {
                                "en": "I am <bored> sitting at home in rainy weather."
                            }
                        },
                        {
                            "text": "Детям стало <скучно> слушать взрослые разговоры.",
                            "target_lang_translations": {
                                "en": "The children became <bored> listening to adult conversations."
                            }
                        }
                    ],
                    "synonyms": [
                        "тоскливо",
                        "неинтересно"
                    ],
                    "antonyms": [
                        "весело",
                        "интересно"
                    ],
                    "common_phrases": [
                        "мне скучно",
                        "нам скучно",
                        "ужасно скучно"
                    ],
                    "target_lang_definitions": {
                        "en": "About a feeling of boredom, lack of interest, or inner emptiness experienced by someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bored",
                                "target_lang_sense_clarification": "feeling"
                            },
                            {
                                "target_lang_word": "feel bored",
                                "target_lang_sense_clarification": "state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd4fab40-2b6d-332f-b2e4-2b139574b6e9",
                    "sense_definition": "О состоянии окружающей обстановки или ситуации, в которой царит скука и нет ничего интересного.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_boredom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На этой лекции было так <скучно>, что многие студенты уснули.",
                            "target_lang_translations": {
                                "en": "It was so <boring> at this lecture that many students fell asleep."
                            }
                        },
                        {
                            "text": "В маленьком городе зимой бывает очень <скучно>.",
                            "target_lang_translations": {
                                "en": "It can be very <boring> in a small town in winter."
                            }
                        }
                    ],
                    "synonyms": [
                        "неинтересно",
                        "тоскливо"
                    ],
                    "antonyms": [
                        "весело",
                        "интересно"
                    ],
                    "common_phrases": [
                        "здесь скучно",
                        "без тебя скучно",
                        "стало скучно"
                    ],
                    "target_lang_definitions": {
                        "en": "About the state of the environment or a situation characterized by boredom and a lack of anything interesting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boring",
                                "target_lang_sense_clarification": "situation"
                            },
                            {

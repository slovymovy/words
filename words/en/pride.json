                },
                {
                    "sense_id": "32f6c651-9165-3e10-83d2-6713bca4dfbe",
                    "sense_definition": "A group of lions forming a social unit.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We saw a large <w>pride</w> of lions resting under the acacia tree.",
                            "target_lang_translations": {
                                "ru": "Мы увидели большой <w>прайд</w> львов, отдыхающий под акацией."
                            }

                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "пешеходный"
                            },
                            {
                                "target_lang_word": "переправа",
                                "target_lang_sense_clarification": "через реку"
                            },
                            {
                                "target_lang_word": "переезд",
                                "target_lang_sense_clarification": "железнодорожный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d13d868-3916-3aca-8260-36e239ca493e",
                    "sense_definition": "The act of traveling across something, especially a journey across a large body of water.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "act_of_crossing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had a very rough <w>crossing</w> on the ferry due to the storm.",
                            "target_lang_translations": {
                                "ru": "У нас была очень тяжелая <w>переправа</w> на пароме из-за шторма."
                            }
                        },
                        {
                            "text": "The Atlantic <w>crossing</w> took several days by ship.",
                            "target_lang_translations": {
                                "ru": "Трансатлантический <w>переход</w> на корабле занял несколько дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "passage",
                        "transit",
                        "journey"
                    ],
                    "common_phrases": [
                        "rough crossing",
                        "smooth crossing",
                        "Atlantic crossing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по пересечению чего-либо, особенно путешествие через большой водоем"

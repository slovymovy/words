                {
                    "sense_id": "ab421072-e075-3d93-9f56-65d4a9e4f874",
                    "sense_definition": "Het proces van het nadoen of imiteren van gedrag, geluiden of een werkwijze.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "act_of_imitating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel dieren leren overleven door middel van <w>nabootsing</w> van hun ouders.",
                            "target_lang_translations": {
                                "ru": "Многие животные учатся выживать путем <w>подражания</w> своим родителям."
                            }
                        },
                        {
                            "text": "Zijn perfecte <w>nabootsing</w> van de leraar liet de hele klas lachen.",
                            "target_lang_translations": {
                                "ru": "Его идеальное <w>подражание</w> учителю рассмешило весь класс."
                            }
                        }
                    ],
                    "synonyms": [
                        "imitatie",
                        "nadoen"
                    ],
                    "common_phrases": [
                        "door nabootsing leren",
                        "ter nabootsing van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс подражания поведению, звукам или методам работы."
                    },
                    "translations": {
                        "ru": [
                            {

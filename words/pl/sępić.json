            "senses": [
                {
                    "sense_id": "0d62987c-c6be-39e1-8eb1-60572b24f236",
                    "sense_definition": "Natrętnie prosić o coś, próbować uzyskać coś od kogoś za darmo, często w sposób uciążliwy dla otoczenia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój młodszy brat ciągle <sępi> ode mnie pieniądze na nowe gry komputerowe.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат постоянно <клянчит> у меня деньги на новые компьютерные игры."
                            }
                        },
                        {
                            "text": "Nie lubię wychodzić z nim na miasto, bo zawsze zapomina portfela i <sępi> darmowe drinki.",
                            "target_lang_translations": {
                                "ru": "Я не люблю ходить с ним в город, потому что он всегда забывает кошелек и <выпрашивает> бесплатные напитки."
                            }
                        }
                    ],
                    "synonyms": [
                        "żebrać",
                        "nagabywać",
                        "napraszać się",
                        "naprzykrzać się"
                    ],
                    "common_phrases": [
                        "sępić papierosa",
                        "sępić kasę",
                        "sępić o drobne",
                        "sępić jedzenie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday, informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Назойливо выпрашивать что-либо, пытаться получить что-то бесплатно, часто досаждая окружающим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клянчить",
                                "target_lang_sense_clarification": "назойливо просить, выпрашивать"
                            },
                            {
                                "target_lang_word": "стрелять",
                                "target_lang_sense_clarification": "просить сигареты или мелочь (сленг)"
                            },
                            {
                                "target_lang_word": "попрошайничать",
                                "target_lang_sense_clarification": "заниматься попрошайничеством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afe4710f-2fca-3599-a707-298b7e4b9037",
                    "sense_definition": "Nadawać komuś lub czemuś smutny, ponury wygląd; wprawiać w przygnębienie lub czynić ciemniejszym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złe wieści z frontu <sępiły> twarze wszystkich zgromadzonych w sali żołnierzy.",
                            "target_lang_translations": {
                                "ru": "Плохие новости с фронта <омрачали> лица всех собравшихся в зале солдат."
                            }
                        },
                        {
                            "text": "Gęste, burzowe chmury powoli <sępiły> krajobraz, zapowiadając nadejście gwałtownej ulewy.",
                            "target_lang_translations": {
                                "ru": "Густые грозовые тучи медленно <делали мрачным> пейзаж, предвещая скорый ливень."
                            }
                        }
                    ],
                    "synonyms": [
                        "zasępiać",
                        "mroczyć",
                        "zaciemniać",
                        "zaćmiewać"
                    ],
                    "common_phrases": [
                        "sępić czoło",
                        "sępić wzrok",
                        "sępić twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать кому-либо или чему-либо грустный, мрачный вид; омрачать или делать темнее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "омрачать",
                                "target_lang_sense_clarification": "делать мрачным, печальным"
                            },
                            {
                                "target_lang_word": "хмурить",
                                "target_lang_sense_clarification": "делать угрюмым (обычно о лице, бровях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

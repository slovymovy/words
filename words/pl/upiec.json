    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "729d663b-8258-3175-a74e-817320f8b983",
                    "sense_definition": "Przygotować potrawę, poddając ją działaniu wysokiej temperatury, najczęściej w piekarniku lub nad ogniem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia obiecała <upiec> na święta swój słynny makowiec.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка пообещала <испечь> на праздники свой знаменитый маковый рулет."
                            }
                        },
                        {
                            "text": "Latem lubimy rozpalić ognisko i <upiec> kiełbaski na patykach.",
                            "target_lang_translations": {
                                "ru": "Летом мы любим разжечь костер и <пожарить> колбаски на палочках."
                            }
                        }
                    ],
                    "common_phrases": [
                        "upiec ciasto",
                        "upiec chleb",
                        "upiec mięso",
                        "upiec kurczaka",
                        "upiec dwie pieczenie na jednym ogniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приготовить пищу, подвергая её воздействию высокой температуры, чаще всего в духовке или на открытом огне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испечь",
                                "target_lang_sense_clarification": "о хлебе, пирогах, тесте"
                            },
                            {
                                "target_lang_word": "запечь",
                                "target_lang_sense_clarification": "о мясе, рыбе, овощах"
                            },
                            {
                                "target_lang_word": "пожарить",
                                "target_lang_sense_clarification": "на костре (например, колбаски)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92e32bbd-aba3-36bf-879b-b21b7124d678",
                    "sense_definition": "O jedzeniu: stać się gotowym do spożycia po procesie pieczenia (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy poczekać jeszcze chwilę, aż chleb dobrze się <upiecze>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно подождать еще немного, пока хлеб хорошо <пропечется>."
                            }
                        },
                        {
                            "text": "Ciasto <upiekło> się szybciej niż myślałam i jest lekko przypalone.",
                            "target_lang_translations": {
                                "ru": "Пирог <испекся> быстрее, чем я думала, и слегка подгорел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobrze się upiec",
                        "ciasto się upiekło"
                    ],
                    "target_lang_definitions": {
                        "ru": "О еде: стать готовым к употреблению в процессе выпекания или жарки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испечься",
                                "target_lang_sense_clarification": "стать готовым (о выпечке)"
                            },
                            {
                                "target_lang_word": "пропечься",
                                "target_lang_sense_clarification": "хорошо прожариться внутри"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c71c715e-5b1d-372e-89ce-0f65e476190b",
                    "sense_definition": "Uniknąć kary lub nieprzyjemnych konsekwencji za swoje zachowanie (używane w formie bezosobowej z „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "idiom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapomniałem odrobić pracę domową, ale <upiekło> mi się, bo nauczyciel nie sprawdzał.",
                            "target_lang_translations": {
                                "ru": "Я забыл сделать домашнее задание, но мне <сошло с рук>, потому что учитель не проверял."
                            }
                        },
                        {
                            "text": "Tym razem kłamstwo mu się nie <upiecze> i poniesie odpowiedzialność.",
                            "target_lang_translations": {
                                "ru": "В этот раз ложь ему с рук не <сойдет>, и он понесет ответственность."
                            }
                        }
                    ],
                    "synonyms": [
                        "ujść na sucho",
                        "ujść bezkarnie"
                    ],
                    "common_phrases": [
                        "upiekło mu się",
                        "tym razem się nie upiecze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Избежать наказания или неприятных последствий за свои поступки (разговорное выражение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сойти с рук",
                                "target_lang_sense_clarification": "идиома: остаться безнаказанным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
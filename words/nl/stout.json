    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "06f36c7b-b8a8-38b9-b931-4390459db310",
                    "sense_definition": "Ongehoorzaam of ondeugend, meestal gebruikt om het gedrag van kinderen te beschrijven die regels overtreden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je <w>stout</w> bent, krijg je vanavond geen toetje.",
                            "target_lang_translations": {
                                "ru": "Если ты будешь <w>непослушным</w>, ты не получишь десерт сегодня вечером."
                            }
                        },
                        {
                            "text": "De leraar moest streng zijn tegen de <w>stoute</w> leerlingen in de klas.",
                            "target_lang_translations": {
                                "ru": "Учителю пришлось быть строгим с <w>непослушными</w> учениками в классе."
                            }
                        }
                    ],
                    "synonyms": [
                        "ondeugend",
                        "ongehoorzaam"
                    ],
                    "antonyms": [
                        "braaf",
                        "lief"
                    ],
                    "common_phrases": [
                        "stoute kinderen",
                        "stout zijn",
                        "niet stout"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непослушный или озорной, обычно используется для описания поведения детей, нарушающих правила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непослушный",
                                "target_lang_sense_clarification": "о детях, не слушающихся взрослых"
                            },
                            {
                                "target_lang_word": "озорной",
                                "target_lang_sense_clarification": "склонный к шалостям"
                            },
                            {
                                "target_lang_word": "плохой",
                                "target_lang_sense_clarification": "о поведении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c7b711f-e14e-3bbd-b75a-b230789dbec4",
                    "sense_definition": "Uitdagend, gewaagd of met een seksuele ondertoon.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij maakte een <w>stoute</w> grap die iedereen deed blozen.",
                            "target_lang_translations": {
                                "ru": "Он отпустил <w>неприличную</w> шутку, от которой все покраснели."
                            }
                        },
                        {
                            "text": "Ze stuurde hem een <w>stout</w> berichtje via de telefoon.",
                            "target_lang_translations": {
                                "ru": "Она отправила ему <w>игривое</w> сообщение по телефону."
                            }
                        }
                    ],
                    "synonyms": [
                        "erotisch",
                        "gewaagd"
                    ],
                    "common_phrases": [
                        "stoute grap",
                        "stout berichtje"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in playful or flirtatious contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий, рискованный или с сексуальным подтекстом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пикантный",
                                "target_lang_sense_clarification": "с эротическим намеком"
                            },
                            {
                                "target_lang_word": "игривый",
                                "target_lang_sense_clarification": "кокетливый"
                            },
                            {
                                "target_lang_word": "неприличный",
                                "target_lang_sense_clarification": "выходящий за рамки приличия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6caf3b4e-ae29-3cad-97f3-6bb902c0897e",
                    "sense_definition": "Dapper, moedig of zonder angst; vaak gebruikt in specifieke uitdrukkingen of oudere teksten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lang twijfelen trok hij de <w>stoute</w> schoenen aan en vroeg haar mee uit.",
                            "target_lang_translations": {
                                "ru": "После долгих сомнений он набрался <w>смелости</w> и пригласил ее на свидание."
                            }
                        },
                        {
                            "text": "Het was een <w>stout</w> plan om de berg in de winter te beklimmen.",
                            "target_lang_translations": {
                                "ru": "Это был <w>дерзкий</w> план — подняться на гору зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "dapper",
                        "gewaagd"
                    ],
                    "antonyms": [
                        "laf"
                    ],
                    "common_phrases": [
                        "stoute schoenen",
                        "stoute dromen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly survives in the idiom 'de stoute schoenen aantrekken'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Храбрый, смелый или бесстрашный; часто встречается в устойчивых выражениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смелый",
                                "target_lang_sense_clarification": "храбрый"
                            },
                            {
                                "target_lang_word": "дерзкий",
                                "target_lang_sense_clarification": "отважный, рискованный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "06e125c6-277a-3b09-aebd-88ed0d4deb90",
                    "sense_definition": "Een zeer donker bier met een gebrande smaak, oorspronkelijk afkomstig uit Groot-Brittannië.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de Ierse pub bestelde hij een groot glas <w>stout</w>.",
                            "target_lang_translations": {
                                "ru": "В ирландском пабе он заказал большой бокал <w>стаута</w>."
                            }
                        },
                        {
                            "text": "Deze brouwerij maakt een heerlijke <w>stout</w> met tonen van koffie.",
                            "target_lang_translations": {
                                "ru": "Эта пивоварня варит вкусный <w>стаут</w> с нотками кофе."
                            }
                        }
                    ],

    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d904fff1-4e5b-3f3c-9455-5a1537207b0e",
                    "sense_definition": "Чрезвычайно сильный по степени проявления, интенсивности или воздействию.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "force_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера на побережье обрушился <w>мощный</w> ураган, который повалил много старых деревьев.",
                            "target_lang_translations": {
                                "en": "Yesterday a <w>powerful</w> hurricane hit the coast, which knocked down many old trees."
                            }
                        },
                        {
                            "text": "Боксёр нанёс сопернику <w>мощный</w> удар правой рукой и сразу выиграл этот бой.",
                            "target_lang_translations": {
                                "en": "The boxer dealt his opponent a <w>powerful</w> blow with his right hand and immediately won the fight."
                            }
                        }
                    ],
                    "synonyms": [
                        "сильный",
                        "могучий"
                    ],
                    "antonyms": [
                        "слабый"
                    ],
                    "common_phrases": [
                        "мощный удар",
                        "мощный взрыв",
                        "мощный поток",
                        "мощный импульс"
                    ],
                    "target_lang_definitions": {
                        "en": "Extremely strong in degree, intensity, or impact."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "powerful",
                                "target_lang_sense_clarification": "general intensity or impact"
                            },
                            {
                                "target_lang_word": "mighty",
                                "target_lang_sense_clarification": "often used for natural forces"
                            },
                            {
                                "target_lang_word": "strong",
                                "target_lang_sense_clarification": "general strength"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8baaaaa-d45b-3c21-8c11-172e8e539842",
                    "sense_definition": "Обладающий высокой производительностью, скоростью работы или вырабатывающий много энергии (о механизмах, двигателях, компьютерах).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "technical_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для работы с трёхмерной графикой и видеомонтажа мне нужен очень <w>мощный</w> компьютер.",
                            "target_lang_translations": {
                                "en": "For working with 3D graphics and video editing, I need a very <w>powerful</w> computer."
                            }
                        },
                        {
                            "text": "Под капотом этого спортивного автомобиля установлен <w>мощный</w> двигатель нового поколения.",
                            "target_lang_translations": {
                                "en": "Under the hood of this sports car is a <w>powerful</w> new-generation engine."
                            }
                        }
                    ],
                    "antonyms": [
                        "слабый"
                    ],
                    "common_phrases": [
                        "мощный двигатель",
                        "мощный компьютер",
                        "мощный процессор",
                        "мощный мотор"
                    ],
                    "target_lang_definitions": {
                        "en": "Possessing high performance, speed, or energy output (of machines, computers)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "powerful",
                                "target_lang_sense_clarification": "high-performance"
                            },
                            {
                                "target_lang_word": "high-powered",
                                "target_lang_sense_clarification": "having a powerful engine or motor"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3f10b6e-2bc5-3b27-94f4-62c20175b4aa",
                    "sense_definition": "Физически очень сильный, крепкого телосложения (о человеке или животном).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого спортсмена была <w>мощная</w> шея и широкие плечи, что выдавало в нём борца.",
                            "target_lang_translations": {
                                "en": "This athlete had a <w>powerful</w> neck and broad shoulders, which gave him away as a wrestler."
                            }
                        },
                        {
                            "text": "На арену цирка медленно вышел <w>мощный</w> слон и поприветствовал зрителей.",
                            "target_lang_translations": {
                                "en": "A <w>mighty</w> elephant slowly walked out into the circus arena and greeted the audience."
                            }
                        }
                    ],
                    "synonyms": [
                        "крепкий",
                        "могучий"
                    ],
                    "antonyms": [
                        "слабый"
                    ],
                    "common_phrases": [
                        "мощная фигура",
                        "мощный торс",
                        "мощная спина"
                    ],
                    "target_lang_definitions": {
                        "en": "Physically very strong or sturdy (of a person or animal)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "powerful",
                                "target_lang_sense_clarification": "having great physical strength"
                            },
                            {
                                "target_lang_word": "sturdy",
                                "target_lang_sense_clarification": "strongly and solidly built"
                            },
                            {
                                "target_lang_word": "strong",
                                "target_lang_sense_clarification": "general physical capability"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8b161e0-4b84-3211-9031-534836818057",
                    "sense_definition": "Обладающий большим авторитетом, властью, влиянием или значительными ресурсами (о государстве, организации, армии).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За последние десять лет эта страна превратилась в <w>мощную</w> индустриальную державу.",
                            "target_lang_translations": {
                                "en": "Over the last ten years, this country has turned into a <w>powerful</w> industrial power."
                            }
                        },
                        {
                            "text": "Нам потребовалась <w>мощная</w> поддержка со стороны инвесторов для реализации этого проекта.",
                            "target_lang_translations": {
                                "en": "We needed <w>strong</w> support from investors to implement this project."
                            }
                        }
                    ],
                    "synonyms": [
                        "могущественный",
                        "величественный"
                    ],
                    "antonyms": [
                        "слабый"
                    ],
                    "common_phrases": [
                        "мощная держава",
                        "мощная экономика",
                        "мощная поддержка",
                        "мощная армия"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Possessing great authority, influence, or resources (of a state or organization)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "powerful",
                                "target_lang_sense_clarification": "having great influence or authority"
                            },
                            {
                                "target_lang_word": "mighty",
                                "target_lang_sense_clarification": "possessing might (often rhetorical or historical)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e743455f-7df8-3e38-914b-cfe66659aac2",
                    "sense_definition": "Производящий впечатление силы своей величиной, толщиной или массивностью; грандиозный.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "massive_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старинный замок окружали <w>мощные</w> каменные стены, которые веками защищали его от врагов.",
                            "target_lang_translations": {
                                "en": "The ancient castle was surrounded by <w>massive</w> stone walls that protected it from enemies for centuries."
                            }
                        },
                        {
                            "text": "В центре парка рос <w>мощный</w> дуб с раскидистой кроной, дающий тень в жаркий день.",
                            "target_lang_translations": {
                                "en": "In the center of the park grew a <w>mighty</w> oak with a spreading crown, providing shade on a hot day."
                            }
                        }
                    ],
                    "synonyms": [
                        "массивный",
                        "грандиозный",
                        "величественный"
                    ],
                    "common_phrases": [
                        "мощные стены",
                        "мощный фундамент",
                        "мощные корни",
                        "мощный слой"
                    ],
                    "target_lang_definitions": {
                        "en": "Impressive in size, thickness, or massiveness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "massive",
                                "target_lang_sense_clarification": "large, heavy, and solid"
                            },
                            {
                                "target_lang_word": "mighty",
                                "target_lang_sense_clarification": "impressive and imposing (e.g., trees)"
                            },
                            {
                                "target_lang_word": "thick",
                                "target_lang_sense_clarification": "having a large distance between opposite sides (e.g., walls)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

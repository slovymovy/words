    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "db742685-3f7c-332d-819f-6b4762536692",
                    "sense_definition": "A non-standard spelling of <w>you</w>, representing its pronunciation when blended with a preceding word ending in 't' or 'd'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't <w>cha</w> want to come to the movies with us?",
                            "target_lang_translations": {
                                "ru": "Разве <w>ты</w> не хочешь пойти с нами в кино?"
                            }
                        },
                        {
                            "text": "I bet <w>cha</w> can't guess what I bought for dinner.",
                            "target_lang_translations": {
                                "ru": "Спорим, <w>ты</w> не угадаешь, что я купил на ужин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "don't cha",
                        "bet cha",
                        "won't cha"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "representing spoken pronunciation"
                        },
                        {
                            "trait_type": "form",
                            "comment": "eye dialect"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное написание местоимения \"you\" (ты/вы), отражающее произношение при слиянии с предыдущим словом, оканчивающимся на 't' или 'd'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "местоимение"
                            },
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "местоимение (вежливое или мн. ч.)"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e728d680-1639-3342-84dc-57cce962cc49",
                    "sense_definition": "Op een bepaalde plaats aankomen of terechtkomen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "terechtkomen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zonder auto kan ik daar moeilijk <w>geraken</w>.",
                            "target_lang_translations": {
                                "en": "Without a car, it is difficult for me to <w>get</w> there.",
                                "ru": "Без машины мне туда трудно <w>добраться</w>."
                            }
                        },
                        {
                            "text": "Hoe ben je gisteren zo snel thuis <w>geraakt</w>?",
                            "target_lang_translations": {
                                "en": "How did you <w>get</w> home so quickly yesterday?",
                                "ru": "Как ты вчера так быстро <w>добрался</w> домой?"
                            }
                        }
                    ],
                    "synonyms": [
                        "komen",
                        "arriveren",
                        "belanden",
                        "terechtkomen"
                    ],
                    "antonyms": [
                        "vertrekken",
                        "weggaan"
                    ],
                    "common_phrases": [
                        "ergens geraken",
                        "binnen geraken",
                        "thuis geraken"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België, in Nederland vaak als formeel of ouderwets ervaren"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To arrive or end up at a specific place",
                        "ru": "Прибыть или оказаться в определенном месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "reach a destination"
                            },
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "arrive at a location"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "добраться",
                                "target_lang_sense_clarification": "прибыть в место, часто с усилием"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "оказаться в каком-либо месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71a2cd30-8d02-3875-a300-385bb27c9dd6",
                    "sense_definition": "In een bepaalde toestand of situatie belanden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "terechtkomen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is bij dat ernstige ongeval zwaargewond <w>geraakt</w>.",
                            "target_lang_translations": {
                                "en": "He <w>got</w> seriously injured in that severe accident.",
                                "ru": "В той серьезной аварии он <w>получил</w> тяжелые ранения."
                            }
                        },
                        {
                            "text": "Door de vele vragen <w>geraakte</w> de student in de war.",
                            "target_lang_translations": {
                                "en": "Due to the many questions, the student <w>got</w> confused.",
                                "ru": "Из-за множества вопросов студент <w>запутался</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "raken",
                        "terechtkomen",
                        "belanden"
                    ],
                    "common_phrases": [
                        "gewond geraken",
                        "in de war geraken",
                        "slaags geraken",
                        "aan de praat geraken"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België, in Nederland vaak als formeel of ouderwets ervaren"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To end up in a certain state or situation",
                        "ru": "Оказаться в определенном состоянии или ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "change of state (e.g., get injured, get confused)"
                            },
                            {
                                "target_lang_word": "become",
                                "target_lang_sense_clarification": "enter a specified state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оказаться",
                                "target_lang_sense_clarification": "очутиться в каком-либо положении"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "угодить в ситуацию (часто неприятную)"
                            },
                            {
                                "target_lang_word": "стать",
                                "target_lang_sense_clarification": "перейти в другое состояние"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
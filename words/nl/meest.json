{
    "word_family": [
        "veel",
        "meer"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e70b465a-862a-3a16-b9bd-454da5564268",
                    "sense_definition": "In de hoogste mate; gebruikt voor een bijvoeglijk naamwoord om de overtreffende trap te vormen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is de <w>meest</w> efficiënte oplossing voor het probleem.",
                            "target_lang_translations": {
                                "en": "This is the <w>most</w> efficient solution to the problem.",
                                "ru": "Это <w>самое</w> эффективное решение проблемы."
                            }
                        },
                        {
                            "text": "Zij is de <w>meest</w> geschikte persoon voor deze functie.",
                            "target_lang_translations": {
                                "en": "She is the <w>most</w> suitable person for this position.",
                                "ru": "Она — <w>наиболее</w> подходящий человек на эту должность."
                            }
                        }
                    ],
                    "antonyms": [
                        "minst"
                    ],
                    "common_phrases": [
                        "het meest belangrijk",
                        "meest voorkomend",
                        "meest recente"
                    ],
                    "target_lang_definitions": {
                        "en": "In the highest degree; used before an adjective to form the superlative.",
                        "ru": "В высшей степени; используется перед прилагательным для образования превосходной степени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "most",
                                "target_lang_sense_clarification": "forming the superlative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "самый",
                                "target_lang_sense_clarification": "служебное слово для образования превосходной степени прилагательных"
                            },
                            {
                                "target_lang_word": "наиболее",
                                "target_lang_sense_clarification": "наречие степени, более формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e773e08-4962-32c1-9ac5-673c62754352",
                    "sense_definition": "Het grootste deel van de tijd.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij volgde <w>meest</w> een langere route om de files te vermijden.",
                            "target_lang_translations": {
                                "en": "He <w>mostly</w> took a longer route to avoid traffic jams.",
                                "ru": "Он <w>большей частью</w> выбирал более длинный маршрут, чтобы избежать пробок."
                            }
                        },
                        {
                            "text": "Omdat de postbezorging <w>meest</w> in de ochtend plaatsvindt, waren we al vroeg wakker.",
                            "target_lang_translations": {
                                "en": "Because mail delivery <w>usually</w> takes place in the morning, we were awake early.",
                                "ru": "Поскольку доставка почты происходит <w>преимущественно</w> утром, мы проснулись рано."
                            }
                        }
                    ],
                    "synonyms": [
                        "meestal"
                    ],
                    "target_lang_definitions": {
                        "en": "The majority of the time.",
                        "ru": "Большую часть времени; как правило."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mostly",
                                "target_lang_sense_clarification": "for the greatest part"
                            },
                            {
                                "target_lang_word": "usually",
                                "target_lang_sense_clarification": "under normal conditions"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преимущественно",
                                "target_lang_sense_clarification": "в основном, главным образом"
                            },
                            {
                                "target_lang_word": "большей частью",
                                "target_lang_sense_clarification": "по большей части"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "5cb2ab8f-8520-312c-85f5-1a8558946db2",
                    "sense_definition": "De overtreffende trap van <w>veel</w>; het grootste aantal of de grootste hoeveelheid.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>meeste</w> mensen gaan in de zomer op vakantie naar het buitenland.",
                            "target_lang_translations": {
                                "en": "<w>Most</w> people go on holiday abroad in the summer.",
                                "ru": "<w>Большинство</w> людей едут летом в отпуск за границу."
                            }
                        },
                        {
                            "text": "Hij heeft het <w>meeste</w> werk verzet van ons allemaal.",
                            "target_lang_translations": {
                                "en": "He did the <w>most</w> work of us all.",
                                "ru": "Он сделал <w>больше всего</w> работы из всех нас."
                            }
                        }
                    ],
                    "antonyms": [
                        "minste"
                    ],
                    "common_phrases": [
                        "de meeste mensen",
                        "de meeste tijd",
                        "het meeste geld"
                    ],
                    "target_lang_definitions": {
                        "en": "The superlative of 'much' or 'many'; the greatest number or quantity.",
                        "ru": "Превосходная степень от «много»; наибольшее количество."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "most",
                                "target_lang_sense_clarification": "the greatest amount or number"
                            },
                            {
                                "target_lang_word": "the most",
                                "target_lang_sense_clarification": "superlative form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "большинство",
                                "target_lang_sense_clarification": "наибольшая часть группы (людей, предметов)"
                            },
                            {
                                "target_lang_word": "больше всего",
                                "target_lang_sense_clarification": "наибольшее количество (часто неисчисляемое)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
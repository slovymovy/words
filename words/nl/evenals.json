    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "53e50e50-603b-3ea9-a9dd-7ed7abcc0443",
                    "sense_definition": "Net zoals; op dezelfde manier als.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze man is <w>evenals</w> zijn vader een directeur.",
                            "target_lang_translations": {
                                "ru": "Этот человек, <w>как и</w> его отец, — директор."
                            }
                        },
                        {
                            "text": "Zij houdt van klassieke muziek, <w>evenals</w> haar zus.",
                            "target_lang_translations": {
                                "ru": "Она любит классическую музыку, <w>так же, как и</w> её сестра."
                            }
                        }
                    ],
                    "antonyms": [
                        "evenmin"
                    ],
                    "common_phrases": [
                        "evenals zijn vader",
                        "evenals de rest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точно так же, как; таким же образом, как."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как и",
                                "target_lang_sense_clarification": "сравнение"
                            },
                            {
                                "target_lang_word": "так же, как и",
                                "target_lang_sense_clarification": "усиленное сравнение"
                            },
                            {
                                "target_lang_word": "подобно",
                                "target_lang_sense_clarification": "схожесть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05605ca5-dc69-3045-84c9-f49dc8a91755",
                    "sense_definition": "En ook; naast het genoemde ook.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester was aanwezig, <w>evenals</w> de wethouders.",
                            "target_lang_translations": {
                                "ru": "Мэр присутствовал, <w>а также</w> члены городского совета."
                            }
                        },
                        {
                            "text": "Hij bestelde koffie, <w>evenals</w> een stuk taart.",
                            "target_lang_translations": {
                                "ru": "Он заказал кофе, <w>а также</w> кусок торта."
                            }
                        }
                    ],
                    "synonyms": [
                        "alsook",
                        "en ook"
                    ],
                    "common_phrases": [
                        "evenals vele anderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "А также; в дополнение к упомянутому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а также",
                                "target_lang_sense_clarification": "добавление"
                            },
                            {
                                "target_lang_word": "равно как и",
                                "target_lang_sense_clarification": "формальное перечисление"
                            }
                        ]
                    }
                }
            ]
        }
    ]

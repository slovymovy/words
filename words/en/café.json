            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a6d3376b-3c20-3e0e-bfc2-ce2e3170c8c8",
                    "sense_definition": "An establishment selling coffee, other beverages, and simple meals or snacks to be consumed on the premises.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "coffee_shop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to meet at the <w>café</w> for a hot drink.",
                            "target_lang_translations": {
                                "ru": "Мы решили встретиться в <w>кафе</w>, чтобы выпить чего-нибудь горячего."
                            }
                        },
                        {
                            "text": "She sat at an outdoor <w>café</w> watching the people go by.",
                            "target_lang_translations": {
                                "ru": "Она сидела в уличном <w>кафе</w>, наблюдая за прохожими."
                            }
                        }
                    ],
                    "synonyms": [
                        "coffee shop",
                        "coffeehouse",
                        "coffee bar"
                    ],
                    "common_phrases": [
                        "sidewalk café",
                        "internet café",
                        "outdoor café"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заведение, в котором подают кофе, другие напитки и легкие закуски для употребления на месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафе",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "кофейня",
                                "target_lang_sense_clarification": "акцент на кофе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f4f05c9-3b13-390c-91bc-6b551785ed71",
                    "sense_definition": "A small restaurant, often informal, that serves simple meals.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "restaurant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The truck drivers stopped at a roadside <w>café</w> for breakfast.",
                            "target_lang_translations": {
                                "ru": "Водители грузовиков остановились в придорожном <w>кафе</w> на завтрак."
                            }
                        },
                        {
                            "text": "They found a cheap <w>café</w> that served excellent local food.",
                            "target_lang_translations": {
                                "ru": "Они нашли недорогое <w>кафе</w>, где подавали отличную местную еду."
                            }
                        }
                    ],
                    "synonyms": [
                        "caff",
                        "beanery"
                    ],
                    "common_phrases": [
                        "roadside café",
                        "local café"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой ресторан, часто с простой обстановкой, где подают простые блюда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафе",
                                "target_lang_sense_clarification": "небольшой ресторан"
                            },
                            {
                                "target_lang_word": "закусочная",
                                "target_lang_sense_clarification": "простое заведение"
                            }
                        ]
                    }
                }
            ]
        }

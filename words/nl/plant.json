    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fcf5e6f0-bb82-33f9-8afd-a15753911101",
                    "sense_definition": "Een levend organisme dat kooldioxide opneemt en zuurstof afgeeft, zoals een bloem, boom of struik.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik geef de <w>plant</w> elke week een beetje water.",
                            "target_lang_translations": {
                                "ru": "Я каждую неделю немного поливаю <w>растение</w>."
                            }
                        },
                        {
                            "text": "Deze <w>plant</w> heeft veel zonlicht nodig om te groeien.",
                            "target_lang_translations": {
                                "ru": "Этому <w>растению</w> нужно много солнечного света для роста."
                            }
                        }
                    ],
                    "common_phrases": [
                        "groene plant",
                        "plant water geven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живой организм, который поглощает углекислый газ и выделяет кислород, например, цветок, дерево или кустарник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растение",
                                "target_lang_sense_clarification": "биологический организм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7b751ae-9be2-3ac2-b934-3016d7e4a340",
                    "sense_definition": "Een fabriek of industriële installatie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "industry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De chemische <w>plant</w> werd gesloten voor groot onderhoud.",
                            "target_lang_translations": {
                                "ru": "Химический <w>завод</w> был закрыт на капитальный ремонт."
                            }
                        },
                        {
                            "text": "Er werken honderden mensen in de nieuwe <w>plant</w>.",
                            "target_lang_translations": {
                                "ru": "На новом <w>заводе</w> работают сотни людей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фабрика или промышленная установка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завод",
                                "target_lang_sense_clarification": "промышленное предприятие"
                            },
                            {
                                "target_lang_word": "фабрика",
                                "target_lang_sense_clarification": "производство"
                            },
                            {
                                "target_lang_word": "установка",
                                "target_lang_sense_clarification": "техническое сооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56ad940d-3d29-3112-93e7-cf7fd8ad5665",
                    "sense_definition": "Iemand die door ernstig hersenletsel niet meer kan reageren of bewegen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het zware ongeluk leefde hij verder als een <w>plant</w>.",
                            "target_lang_translations": {
                                "ru": "После тяжелой аварии он жил как <w>овощ</w>."
                            }
                        },
                        {
                            "text": "De artsen vreesden dat de patiënt een <w>plant</w> zou blijven.",
                            "target_lang_translations": {
                                "ru": "Врачи опасались, что пациент останется <w>овощем</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejoratief"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который из-за тяжелого повреждения мозга больше не может реагировать или двигаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "овощ",
                                "target_lang_sense_clarification": "переносное значение, сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3b16920-70a2-307e-944c-b4d45925ef60",
                    "sense_definition": "De onderkant van de voet.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij voelde een scherp steentje onder de <w>plant</w> van zijn voet.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал острый камешек под <w>подошвой</w> ноги."
                            }
                        },
                        {
                            "text": "De blaar zat precies op de <w>plant</w>.",
                            "target_lang_translations": {
                                "ru": "Волдырь был прямо на <w>подошве</w>."
                            }
                        }
                    ],
                    "synonyms": [

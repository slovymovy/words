{
    "word_family": [
        "infiltratie",
        "infiltrant"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "90510548-a528-32d9-9128-125ee678b9d3",
                    "sense_definition": "Heimelijk binnendringen in een organisatie, groep of gebied, vaak om informatie te verzamelen of invloed uit te oefenen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politieagent wist succesvol de drugsbende te <w>infiltreren</w>.",
                            "target_lang_translations": {
                                "en": "The police officer managed to successfully <w>infiltrate</w> the drug gang."
                            }
                        },
                        {
                            "text": "Ze waren diep in vijandelijk gebied <w>geïnfiltreerd</w>.",
                            "target_lang_translations": {
                                "en": "They had <w>infiltrated</w> deep into enemy territory."
                            }
                        }
                    ],
                    "synonyms": [
                        "binnendringen"
                    ],
                    "common_phrases": [
                        "een organisatie infiltreren",
                        "de vijand infiltreren"
                    ],
                    "target_lang_definitions": {
                        "en": "To secretly enter an organization, group, or area, often to gather information or exert influence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "infiltrate",
                                "target_lang_sense_clarification": "enter secretly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "603de7b3-eec7-3a58-8ceb-d733d8232351",
                    "sense_definition": "Langzaam ergens in doordringen of doorheen sijpelen, zoals een vloeistof in de bodem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het regenwater kan op deze plek makkelijk in de grond <w>infiltreren</w>.",
                            "target_lang_translations": {
                                "en": "Rainwater can easily <w>infiltrate</w> the ground at this spot."
                            }
                        },
                        {
                            "text": "Giftige stoffen zijn in het grondwater <w>geïnfiltreerd</w>.",
                            "target_lang_translations": {
                                "en": "Toxic substances have <w>infiltrated</w> the groundwater."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorsijpelen",
                        "doordringen"
                    ],
                    "target_lang_definitions": {
                        "en": "To slowly penetrate or seep through something, such as liquid into the soil."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "infiltrate",
                                "target_lang_sense_clarification": "permeate or seep through"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
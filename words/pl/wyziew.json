    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "542b7f1d-634d-357e-ac5d-c220e5f3d462",
                    "sense_definition": "Substancja lotna, taka jak gaz, para lub dym, często o nieprzyjemnym zapachu, wydobywająca się z określonego źródła.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trujące <w>wyziewy</w> z komina fabrycznego zanieczyszczają powietrze w całym mieście.",
                            "target_lang_translations": {
                                "ru": "Ядовитые <w>испарения</w> из заводской трубы загрязняют воздух во всем городе."
                            }
                        },
                        {
                            "text": "Podczas erupcji wulkanu wydobywały się z niego gorące <w>wyziewy</w> siarkowe.",
                            "target_lang_translations": {
                                "ru": "Во время извержения вулкана из него вырывались горячие серные <w>испарения</w>."
                            }
                        },
                        {
                            "text": "Poczułem od niego nieprzyjemne <w>wyziewy</w> alkoholu, gdy tylko wszedł do pokoju.",
                            "target_lang_translations": {
                                "ru": "Я почувствовал от него неприятный <w>перегар</w>, как только он вошел в комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "opar",
                        "wypar",
                        "smród"
                    ],
                    "common_phrases": [
                        "trujące wyziewy",
                        "wyziewy wulkaniczne",
                        "wyziewy alkoholu",
                        "szkodliwe wyziewy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Летучее вещество, такое как газ, пар или дым, часто с неприятным запахом, исходящее из определенного источника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испарение",
                                "target_lang_sense_clarification": "газообразное выделение, пар"
                            },
                            {
                                "target_lang_word": "миазм",
                                "target_lang_sense_clarification": "зловонное испарение, обычно во мн. ч."
                            },
                            {
                                "target_lang_word": "перегар",
                                "target_lang_sense_clarification": "запах алкоголя изо рта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0135cc3f-333b-3d6d-a4e5-d3f66ba2dccc",
                    "sense_definition": "Gwałtowne uzewnętrznienie negatywnych emocji, poglądów lub cech charakteru.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego przemówienie było pełne <w>wyziewów</w> nienawiści i nietolerancji.",
                            "target_lang_translations": {
                                "ru": "Его речь была полна <w>излияний</w> ненависти и нетерпимости."
                            }
                        },
                        {
                            "text": "Staram się ignorować internetowe <w>wyziewy</w> złości anonimowych komentatorów.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь игнорировать интернет-<w>выплески</w> злости анонимных комментаторов."
                            }
                        }
                    ],
                    "synonyms": [
                        "emanacja",
                        "eksplozja"
                    ],
                    "common_phrases": [
                        "wyziewy nienawiści",
                        "wyziewy złości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурное проявление негативных эмоций, взглядов или черт характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "излияние",
                                "target_lang_sense_clarification": "поток слов или чувств"
                            },
                            {
                                "target_lang_word": "выплеск",
                                "target_lang_sense_clarification": "внезапный выброс эмоций"
                            }
                        ]
                    }
                }
            ]
        }
    ]

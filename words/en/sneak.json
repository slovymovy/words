{
    "word_family": [
        "sneaker",
        "sneaky",
        "sneakily",
        "sneaking"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b823ff8c-9776-3d5f-b2de-599f0f703280",
                    "sense_definition": "To move somewhere quietly and secretly in order to avoid being seen or heard.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_stealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He managed to <w>sneak</w> out of the house while his parents were sleeping.",
                            "target_lang_translations": {
                                "ru": "Ему удалось <w>выскользнуть</w> из дома, пока родители спали."
                            }
                        },
                        {
                            "text": "The cat tried to <w>sneak</w> up on the bird in the garden.",
                            "target_lang_translations": {
                                "ru": "Кот пытался <w>подкрасться</w> к птице в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "creep",
                        "tiptoe",
                        "slink",
                        "steal"
                    ],
                    "antonyms": [
                        "stomp",
                        "march"
                    ],
                    "common_phrases": [
                        "sneak in",
                        "sneak out",
                        "sneak up on",
                        "sneak away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться куда-либо тихо и скрытно, чтобы не быть замеченным или услышанным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красться",
                                "target_lang_sense_clarification": "двигаться бесшумно и осторожно"
                            },
                            {
                                "target_lang_word": "пробираться",
                                "target_lang_sense_clarification": "двигаться скрытно через препятствия"
                            },
                            {
                                "target_lang_word": "проскальзывать",
                                "target_lang_sense_clarification": "быстро и незаметно проходить куда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2a93d77-9f14-3880-993c-999cd429ce95",
                    "sense_definition": "To take, bring, or put something secretly without permission or notice.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_stealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I tried to <w>sneak</w> a candy bar into the movie theater in my pocket.",
                            "target_lang_translations": {
                                "ru": "Я пытался <w>пронести</w> шоколадный батончик в кинотеатр в кармане."
                            }
                        },
                        {
                            "text": "She managed to <w>sneak</w> a quick look at the answers during the test.",
                            "target_lang_translations": {
                                "ru": "Ей удалось <w>подсмотреть</w> ответы во время теста."
                            }
                        }
                    ],
                    "synonyms": [
                        "smuggle",
                        "slip",
                        "filch"
                    ],
                    "common_phrases": [
                        "sneak a look",
                        "sneak a peek",
                        "sneak a glance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать, приносить или класть что-либо тайно, без разрешения или незаметно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стащить",
                                "target_lang_sense_clarification": "незаметно украсть"
                            },
                            {
                                "target_lang_word": "пронести",
                                "target_lang_sense_clarification": "тайно доставить что-то внутрь"
                            },
                            {
                                "target_lang_word": "делать украдкой",
                                "target_lang_sense_clarification": "совершать действие незаметно (например, взглянуть)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1885c4c-003a-3710-b2a4-2bcbff2d206c",
                    "sense_definition": "To tell an authority figure, such as a teacher or parent, about something wrong another person has done.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "inform",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He threatened to <w>sneak</w> on us if we didn't let him play.",
                            "target_lang_translations": {
                                "ru": "Он грозился <w>наябедничать</w> на нас, если мы не дадим ему поиграть."
                            }
                        },
                        {
                            "text": "I would never <w>sneak</w> to the teacher about what happened.",
                            "target_lang_translations": {
                                "ru": "Я бы никогда не стал <w>доносить</w> учителю о том, что случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "snitch",
                        "grass",
                        "tell tales",
                        "tattle"
                    ],
                    "common_phrases": [
                        "sneak on"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used by children"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассказывать старшим или начальству о проступках другого человека; ябедничать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ябедничать",
                                "target_lang_sense_clarification": "разг., обычно используется детьми"
                            },
                            {
                                "target_lang_word": "доносить",
                                "target_lang_sense_clarification": "сообщать о нарушениях властям или старшим"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "жарг., доносить на кого-то"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0553cb25-3ef0-3660-9c7a-61ff53d1d2cd",
                    "sense_definition": "A person who acts in a secret, dishonest, or underhanded way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_bad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother is such a <w>sneak</w>; he read my diary without asking.",
                            "target_lang_translations": {
                                "ru": "Мой брат такой <w>тихушник</w>; он прочитал мой дневник без спроса."
                            }
                        },
                        {
                            "text": "Don't trust him with your secrets because he is a total <w>sneak</w>.",
                            "target_lang_translations": {
                                "ru": "Не доверяй ему свои секреты, потому что он настоящий <w>подлец</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cheat",
                        "deceiver",
                        "trickster"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, действующий скрытно, нечестно или подло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тихушник",
                                "target_lang_sense_clarification": "разг., скрытный человек"
                            },
                            {
                                "target_lang_word": "подлец",
                                "target_lang_sense_clarification": "нечестный, низкий человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23fa0f33-a97e-386a-824a-d4a5de3c3a8f",
                    "sense_definition": "A person who informs on others; a telltale.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "person_informer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nobody in the class liked the <w>sneak</w> who told the teacher everything.",
                            "target_lang_translations": {
                                "ru": "Никто в классе не любил <w>ябеду</w>, который всё рассказывал учителю."
                            }
                        },
                        {
                            "text": "Don't be a <w>sneak</w> and just keep the secret between us.",
                            "target_lang_translations": {
                                "ru": "Не будь <w>стукачом</w> и просто сохрани секрет между нами."
                            }
                        }
                    ],
                    "synonyms": [
                        "snitch",
                        "tattletale",
                        "informer",
                        "squealer"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used by children"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто доносит на других; ябеда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ябеда",
                                "target_lang_sense_clarification": "разг., тот, кто жалуется старшим"
                            },
                            {
                                "target_lang_word": "стукач",
                                "target_lang_sense_clarification": "груб., доносчик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ed20e99-b2ea-3669-b922-e0ce7ae38c86",
                    "sense_definition": "A soft shoe with a rubber sole worn for sports or casual occasions; a sneaker.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shoe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He put on his old <w>sneaks</w> and went for a run in the park.",
                            "target_lang_translations": {
                                "ru": "Он надел свои старые <w>кроссовки</w> и отправился на пробежку в парк."
                            }
                        },
                        {
                            "text": "We usually wear comfortable <w>sneaks</w> when walking around the city.",
                            "target_lang_translations": {
                                "ru": "Мы обычно носим удобные <w>кеды</w>, когда гуляем по городу."
                            }
                        }
                    ],
                    "synonyms": [
                        "sneaker",
                        "tennis shoe",
                        "trainer"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for sneakers"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкая обувь на резиновой подошве для спорта или отдыха; кроссовка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроссовки",
                                "target_lang_sense_clarification": "спортивная обувь"
                            },
                            {
                                "target_lang_word": "кеды",
                                "target_lang_sense_clarification": "легкая спортивная обувь"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "797ab428-1501-3ee6-8407-a9a613ba866c",
                    "sense_definition": "Done or happening secretly, unexpectedly, or before an official announcement.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adj_secret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The studio gave the fans a <w>sneak</w> peek of the upcoming movie.",
                            "target_lang_translations": {
                                "ru": "Студия устроила фанатам <w>предварительный</w> показ предстоящего фильма."
                            }
                        },
                        {
                            "text": "The enemy launched a <w>sneak</w> attack while the soldiers were sleeping.",
                            "target_lang_translations": {
                                "ru": "Враг начал <w>внезапную</w> атаку, пока солдаты спали."
                            }
                        }
                    ],
                    "synonyms": [
                        "secret",
                        "surprise",
                        "stealthy"
                    ],
                    "antonyms": [
                        "public",
                        "announced"
                    ],
                    "common_phrases": [
                        "sneak peek",
                        "sneak preview",
                        "sneak attack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный или происходящий тайно, неожиданно или до официального объявления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тайный",
                                "target_lang_sense_clarification": "секретный"
                            },
                            {
                                "target_lang_word": "внезапный",
                                "target_lang_sense_clarification": "неожиданный (об атаке)"
                            },
                            {
                                "target_lang_word": "предварительный",
                                "target_lang_sense_clarification": "до официального релиза (о показе)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
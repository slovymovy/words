    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e325654b-ce20-3664-820e-53fe95cef47c",
                    "sense_definition": "The continuous body of salt water that covers most of the Earth's surface.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We spent the whole afternoon swimming in the warm <w>sea</w>.",
                            "target_lang_translations": {
                                "ru": "Мы провели весь день, плавая в теплом <w>море</w>."
                            }
                        },
                        {
                            "text": "Many large ships travel across the <w>sea</w> to transport goods between continents.",
                            "target_lang_translations": {
                                "ru": "Многие большие корабли путешествуют по <w>морю</w>, перевозя товары между континентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogin",
                        "Mediterranean Sea"
                    ],
                    "antonyms": [
                        "land"
                    ],
                    "common_phrases": [
                        "at sea",
                        "by the sea",
                        "open sea",
                        "deep blue sea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывная масса соленой воды, покрывающая большую часть поверхности Земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "водное пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b999ecda-8ae4-35f2-8af3-f093448c7eb0",
                    "sense_definition": "A specific large body of salt water, usually smaller than an ocean and partially enclosed by land.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Mediterranean <w>Sea</w> is a popular destination for tourists in the summer.",
                            "target_lang_translations": {
                                "ru": "Средиземное <w>море</w> — популярное направление для туристов летом."
                            }
                        },
                        {
                            "text": "Many different types of fish live in the coral reefs of the Red <w>Sea</w>.",
                            "target_lang_translations": {
                                "ru": "В коралловых рифах Красного <w>моря</w> обитает множество различных видов рыб."
                            }
                        }
                    ],
                    "common_phrases": [
                        "North Sea",
                        "Red Sea",
                        "Caribbean Sea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный большой водоем с соленой водой, обычно меньше океана и частично окруженный сушей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "часть океана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e18a458-a650-38e0-8945-82e1fac0f2fb",
                    "sense_definition": "The condition of the ocean surface, especially when it is rough or turbulent with large waves.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "water_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain steered the boat carefully through the rough <w>sea</w>.",
                            "target_lang_translations": {
                                "ru": "Капитан осторожно вел лодку через бурное <w>море</w>."
                            }
                        },
                        {
                            "text": "There was a heavy <w>sea</w> yesterday, so we could not go fishing.",
                            "target_lang_translations": {
                                "ru": "Вчера было сильное волнение на <w>море</w>, поэтому мы не смогли пойти на рыбалку."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogin"
                    ],
                    "common_phrases": [
                        "rough sea",
                        "calm sea",
                        "heavy sea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние поверхности океана, особенно когда она неспокойна или есть большие волны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "волнение"
                            },
                            {
                                "target_lang_word": "волны",
                                "target_lang_sense_clarification": "состояние воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5048b55-d968-3f7a-a0d1-78ea7f2a933f",
                    "sense_definition": "A vast or overwhelming quantity of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The singer looked out from the stage at a <w>sea</w> of smiling faces.",
                            "target_lang_translations": {
                                "ru": "Певец смотрел со сцены на <w>море</w> улыбающихся лиц."
                            }
                        },
                        {
                            "text": "He felt lost in a <w>sea</w> of paperwork and bureaucracy.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя потерянным в <w>море</w> бумажной работы и бюрократии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sea of faces",
                        "sea of troubles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Огромное или подавляющее количество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "изобилие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "702e0b60-d051-34e8-8cbb-5768601a71ef",
                    "sense_definition": "A large, dark plain of rock on the moon.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Apollo 11 mission successfully landed in the <w>Sea</w> of Tranquility.",
                            "target_lang_translations": {
                                "ru": "Миссия «Аполлон-11» успешно приземлилась в <w>Море</w> Спокойствия."
                            }
                        },
                        {
                            "text": "Early astronomers called the dark patch a <w>sea</w> because they thought it was water.",
                            "target_lang_translations": {
                                "ru": "Ранние астрономы называли это темное пятно <w>морем</w>, потому что думали, что это вода."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая темная равнина на поверхности Луны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "лунная равнина"
                            }
                        ]
                    }
                }
            ]
        },

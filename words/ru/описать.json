    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "82a46fd2-c5b3-38ef-b3fd-8b31fab34c18",
                    "sense_definition": "Рассказать о чём-либо или ком-либо, передать словами внешность, характер, события или детали.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "description",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свидетель смог подробно <w>описать</w> внешность преступника полиции.",
                            "target_lang_translations": {
                                "en": "The witness was able to <w>describe</w> the criminal's appearance to the police in detail."
                            }
                        },
                        {
                            "text": "В своём письме я хочу <w>описать</w> наши удивительные приключения.",
                            "target_lang_translations": {
                                "en": "In my letter, I want to <w>describe</w> our amazing adventures."
                            }
                        }
                    ],
                    "synonyms": [
                        "изобразить",
                        "обрисовать"
                    ],
                    "common_phrases": [
                        "подробно описать",
                        "описать ситуацию",
                        "описать внешность",
                        "красочно описать"
                    ],
                    "target_lang_definitions": {
                        "en": "To tell about something or someone, conveying appearance, character, events, or details in words."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "describe",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "depict",
                                "target_lang_sense_clarification": "to describe vividly or artistically"
                            },
                            {
                                "target_lang_word": "portray",
                                "target_lang_sense_clarification": "to describe a character or scene"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "515f0a14-f9e9-3397-b400-36666e5d375b",
                    "sense_definition": "Испустить мочу на что-либо или кого-либо, запачкать мочой (ударение на второй слог: опи́сать).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький мальчик заигрался и случайно <w>описал</w> свои штаны.",
                            "target_lang_translations": {
                                "en": "The little boy got carried away playing and accidentally <w>wet</w> his pants."
                            }
                        },
                        {
                            "text": "Соседский кот <w>описал</w> наш новый ковёр в прихожей.",
                            "target_lang_translations": {
                                "en": "The neighbor's cat <w>peed on</w> our new rug in the hallway."
                            }
                        }
                    ],
                    "synonyms": [
                        "обоссать"
                    ],
                    "common_phrases": [
                        "описать штаны",
                        "описать всё вокруг",
                        "описать от страха"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To urinate on something or someone, to stain with urine."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pee on",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "wet",
                                "target_lang_sense_clarification": "used with clothing or bed"
                            },
                            {
                                "target_lang_word": "soil",
                                "target_lang_sense_clarification": "polite/formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b23a6b64-248b-30c8-88c8-5285a73351b2",
                    "sense_definition": "Составить официальный список предметов или имущества для учёта, оценки или конфискации.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "inventory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судебные приставы пришли <w>описать</w> имущество должника за неуплату кредита.",
                            "target_lang_translations": {
                                "en": "The bailiffs came to <w>inventory</w> the debtor's property for non-payment of the loan."
                            }
                        },
                        {
                            "text": "Комиссия должна <w>описать</w> все старые документы в городском архиве.",
                            "target_lang_translations": {
                                "en": "The commission must <w>inventory</w> all the old documents in the city archive."
                            }
                        }
                    ],
                    "common_phrases": [
                        "описать имущество",
                        "описать вещи",
                        "описать за долги"
                    ],
                    "target_lang_definitions": {
                        "en": "To compile an official list of items or property for accounting, assessment, or confiscation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inventory",
                                "target_lang_sense_clarification": "to make a list of property"
                            },
                            {
                                "target_lang_word": "list",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9117eaf1-8af5-37f8-a878-479ba5a92d00",
                    "sense_definition": "Совершить движение по круговой или кривой линии, а также очертить геометрическую фигуру.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "trajectory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Самолёт <w>описал</w> широкий круг над городом и пошёл на посадку.",
                            "target_lang_translations": {
                                "en": "The plane <w>described</w> a wide circle over the city and came in for a landing."
                            }
                        },
                        {
                            "text": "Опытный фигурист <w>описал</w> на льду красивую дугу.",
                            "target_lang_translations": {
                                "en": "The experienced figure skater <w>traced</w> a beautiful arc on the ice."
                            }
                        }
                    ],
                    "common_phrases": [
                        "описать круг",
                        "описать дугу",
                        "описать траекторию"
                    ],
                    "target_lang_definitions": {
                        "en": "To move along a circular or curved line, or to outline a geometric figure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "describe",
                                "target_lang_sense_clarification": "geometry or trajectory"
                            },
                            {
                                "target_lang_word": "trace",
                                "target_lang_sense_clarification": "to follow a path or line"
                            }
                        ]
                    }
                }
            ]
        }

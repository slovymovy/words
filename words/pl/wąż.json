{
    "word_family": [
        "wężowy",
        "wężowisko",
        "wężownica",
        "wężownik"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "54ab5603-98e9-327c-8132-a972d41b3b74",
                    "sense_definition": "Beznogi gad o długim, walcowatym ciele, poruszający się poprzez pełzanie",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wysokiej trawie ukrył się mały <wąż>.",
                            "target_lang_translations": {
                                "ru": "В высокой траве спряталась маленькая <змея>."
                            }
                        },
                        {
                            "text": "Wczoraj w zoo widzieliśmy ogromnego <węża> boa.",
                            "target_lang_translations": {
                                "ru": "Вчера в зоопарке мы видели огромного <удава> (змею) боа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jadowity wąż",
                        "ukąszenie węża",
                        "skóra węża",
                        "wąż boa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Безногое пресмыкающееся с длинным цилиндрическим телом, передвигающееся ползком"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "змея",
                                "target_lang_sense_clarification": "пресмыкающееся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e20decbd-9d40-3b0d-b6a3-d964f563c02a",
                    "sense_definition": "Elastyczna rura, zwykle gumowa lub plastikowa, służąca do doprowadzania cieczy lub gazów",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ojciec podłączył <wąż> do kranu, żeby umyć samochód.",
                            "target_lang_translations": {
                                "ru": "Отец подключил <шланг> к крану, чтобы помыть машину."
                            }
                        },
                        {
                            "text": "Strażacy szybko rozwinęli <wąż>, aby ugasić pożar.",
                            "target_lang_translations": {
                                "ru": "Пожарные быстро размотали <рукав> (шланг), чтобы потушить пожар."
                            }
                        }
                    ],
                    "synonyms": [
                        "szlauch"
                    ],
                    "common_phrases": [
                        "wąż ogrodowy",
                        "wąż strażacki",
                        "podlewać wężem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гибкая труба, обычно резиновая или пластиковая, служащая для подачи жидкостей или газов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шланг",
                                "target_lang_sense_clarification": "гибкая труба"
                            },
                            {
                                "target_lang_word": "рукав",
                                "target_lang_sense_clarification": "пожарный или технический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cea6a18e-0904-3c82-8ee5-58331abd909c",
                    "sense_definition": "Osoba fałszywa, podstępna i niegodna zaufania",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie ufaj mu, to straszny <wąż>, który tylko czeka na twoje potknięcie.",
                            "target_lang_translations": {
                                "ru": "Не доверяй ему, это страшная <змея>, которая только и ждет твоей ошибки."
                            }
                        },
                        {
                            "text": "Okazała się <wężem>, zdradzając nasze sekrety konkurencji.",
                            "target_lang_translations": {
                                "ru": "Она оказалась <змеёй> (гадюкой), выдав наши секреты конкурентам."
                            }
                        }
                    ],
                    "synonyms": [
                        "żmija",
                        "bazyliszek"
                    ],
                    "common_phrases": [
                        "zdradziecki wąż",
                        "wyhodować węża na piersi"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лживый, коварный и не заслуживающий доверия человек"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "змея",
                                "target_lang_sense_clarification": "коварный человек"
                            },
                            {
                                "target_lang_word": "гадюка",
                                "target_lang_sense_clarification": "подлый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01ae1220-c22b-3581-91b2-be5c391f59e0",
                    "sense_definition": "Długi szereg ludzi, pojazdów lub obiektów, często poruszający się krętą drogą",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długi <wąż> turystów czekał cierpliwie na wejście do muzeum.",
                            "target_lang_translations": {
                                "ru": "Длинная <вереница> туристов терпеливо ждала входа в музей."
                            }
                        },
                        {
                            "text": "Na autostradzie utworzył się świetlny <wąż> z reflektorów samochodów.",
                            "target_lang_translations": {
                                "ru": "На автостраде образовалась светящаяся <змейка> из фар автомобилей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wąż samochodów",
                        "tańczyć węża"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный ряд людей, транспортных средств или объектов, часто движущийся извилистым путем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вереница",
                                "target_lang_sense_clarification": "ряд движущихся объектов"
                            },
                            {
                                "target_lang_word": "змейка",
                                "target_lang_sense_clarification": "извилистая линия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d83d2c46-6d3d-3764-9dd8-a8085f1adfd3",
                    "sense_definition": "Żartobliwe określenie męża",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "family_humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <wąż> znowu zapomniał o naszej rocznicy ślubu.",
                            "target_lang_translations": {
                                "ru": "Мой <благоверный> снова забыл о нашей годовщине свадьбы."
                            }
                        },
                        {
                            "text": "Muszę zapytać, co mój <wąż> sądzi o tym pomyśle.",
                            "target_lang_translations": {
                                "ru": "Я должна спросить, что мой <благоверный> думает об этой идее."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "humorous"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шутливое название мужа (игра слов)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благоверный",
                                "target_lang_sense_clarification": "иронично о муже"
                            },
                            {
                                "target_lang_word": "супруг",
                                "target_lang_sense_clarification": "шутливо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "18f5445b-fa49-3400-9cd6-fae00c6c49fa",
                    "sense_definition": "Gwiazdozbiór nieba równikowego",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Gwiazdozbiór <Węża> jest jedynym gwiazdozbiorem podzielonym na dwie niesąsiadujące ze sobą części.",
                            "target_lang_translations": {
                                "ru": "Созвездие <Змеи> — единственное созвездие, разделенное на две несмежные части."
                            }
                        },
                        {
                            "text": "Astronomowie obserwowali nowe gwiazdy w konstelacji <Węża>.",
                            "target_lang_translations": {
                                "ru": "Астрономы наблюдали новые звезды в созвездии <Змеи>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gwiazdozbiór Węża"
                    ],
                    "target_lang_definitions": {
                        "ru": "Созвездие экваториального неба"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Змея",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03cf0bdd-40be-3d86-9ade-36aabfc62b77",
                    "sense_definition": "Nazwisko polskie",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Wąż> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <Вонж> был избран председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <Wąż>.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <Вонж>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вонж",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
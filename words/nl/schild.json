                },
                {
                    "sense_id": "debc705b-a900-3f24-abcc-92d7ae49014c",
                    "sense_definition": "Een beschermende balustrade achter en aan weerszijden van de boegspriet op een schip",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matroos leunde tegen het <w>schild</w> terwijl hij over de zee uitkeek.",
                            "target_lang_translations": {

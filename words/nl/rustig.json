                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "calm_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liep <w>rustig</w> naar de deur om open te doen.",
                            "target_lang_translations": {
                                "en": "He walked <w>calmly</w> to the door to open it.",
                                "ru": "Он <w>спокойно</w> подошел к двери, чтобы открыть."
                            }
                        },
                        {
                            "text": "We moeten dit probleem <w>rustig</w> bespreken.",
                            "target_lang_translations": {
                                "en": "We need to discuss this problem <w>calmly</w>.",
                                "ru": "Мы должны <w>спокойно</w> обсудить эту проблему."
                            }
                        }

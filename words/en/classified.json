                {
                    "sense_id": "b14c4122-3f0d-30bc-a0d0-9d201fc2107c",
                    "sense_definition": "Officially designated by a government or organization as secret and restricted to authorized personnel.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "secrecy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journalist was arrested for publishing <w>classified</w> information about the military operation.",
                            "target_lang_translations": {
                                "ru": "Журналиста арестовали за публикацию <w>секретной</w> информации о военной операции."
                            }
                        },
                        {
                            "text": "Access to these <w>classified</w> documents requires a high-level security clearance.",
                            "target_lang_translations": {
                                "ru": "Доступ к этим <w>засекреченным</w> документам требует высокого уровня допуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "secret",
                        "confidential",
                        "restricted"
                    ],
                    "antonyms": [
                        "public",
                        "unclassified",
                        "open"
                    ],
                    "common_phrases": [
                        "classified information",
                        "classified documents",
                        "highly classified",
                        "classified material"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально признанный правительством или организацией секретным и доступный только уполномоченному персоналу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засекреченный",
                                "target_lang_sense_clarification": "с ограниченным доступом"

            "senses": [
                {
                    "sense_id": "31e11e09-c7ef-318a-905e-7f9e96f3795a",
                    "sense_definition": "The day immediately before today.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Yesterday</w> was a very cold and rainy day.",
                            "target_lang_translations": {

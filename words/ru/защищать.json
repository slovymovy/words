    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "33258756-a6b6-31b8-9bb4-0f3de2075477",
                    "sense_definition": "Оберегать, охранять от нападения, опасности, вреда или враждебных действий.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "protection_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тёплая одежда хорошо <w>защищает</w> от сильного ветра и мороза.",
                            "target_lang_translations": {
                                "en": "Warm clothes <w>protect</w> well against strong wind and frost."
                            }
                        },
                        {
                            "text": "Каждый солдат даёт клятву верно <w>защищать</w> родину.",
                            "target_lang_translations": {
                                "en": "Every soldier takes an oath to faithfully <w>defend</w> the homeland."
                            }
                        }
                    ],
                    "synonyms": [
                        "охранять",
                        "оберегать",
                        "оборонять"
                    ],
                    "antonyms": [
                        "нападать"
                    ],
                    "common_phrases": [
                        "защищать родину",
                        "защищать от холода",
                        "защищать права",
                        "защищать природу"
                    ],
                    "target_lang_definitions": {
                        "en": "To guard, protect from attack, danger, harm, or hostile actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "protect",
                                "target_lang_sense_clarification": "keep safe from harm or danger"
                            },
                            {
                                "target_lang_word": "defend",
                                "target_lang_sense_clarification": "ward off attacks or enemies"
                            },
                            {
                                "target_lang_word": "guard",
                                "target_lang_sense_clarification": "watch over to prevent harm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5da1a526-08fc-38dc-8434-2cb415a460a7",
                    "sense_definition": "Отстаивать чьи-либо интересы, выступать в чью-либо пользу, поддерживать словами.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "protection_verbal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он всегда <w>защищает</w> своих друзей, даже если они не правы.",
                            "target_lang_translations": {
                                "en": "He always <w>defends</w> his friends, even if they are wrong."
                            }
                        },
                        {
                            "text": "Мы должны смело <w>защищать</w> свои убеждения в любом споре.",
                            "target_lang_translations": {
                                "en": "We must boldly <w>defend</w> our beliefs in any dispute."
                            }
                        }
                    ],
                    "synonyms": [
                        "оберегать",
                        "оборонять"
                    ],
                    "common_phrases": [
                        "защищать интересы",
                        "защищать друга",
                        "защищать мнение",
                        "защищать честь"
                    ],
                    "target_lang_definitions": {
                        "en": "To stand up for someone's interests, speak in someone's favor, or support with words."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defend",
                                "target_lang_sense_clarification": "support by words or arguments"
                            },
                            {
                                "target_lang_word": "stand up for",
                                "target_lang_sense_clarification": "advocate for someone or something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7042e58b-004a-3ec7-909d-cc9700ccda91",
                    "sense_definition": "Отражать атаки соперника в спортивной игре.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "protection_sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш вратарь отлично <w>защищал</w> ворота на протяжении всего матча.",
                            "target_lang_translations": {
                                "en": "Our goalkeeper <w>defended</w> the goal excellently throughout the match."
                            }
                        },
                        {
                            "text": "Чемпион мира будет <w>защищать</w> свой титул в следующем бою.",
                            "target_lang_translations": {
                                "en": "The world champion will <w>defend</w> his title in the next fight."
                            }
                        }
                    ],
                    "synonyms": [
                        "оборонять"
                    ],
                    "antonyms": [
                        "нападать"
                    ],
                    "common_phrases": [

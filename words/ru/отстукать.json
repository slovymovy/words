    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f247249a-1103-3da5-a70b-bcf9de8b63b9",
                    "sense_definition": "Воспроизвести какой-либо ритм или сигнал с помощью стука.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Барабанщик уверенно <отстукал> сложный ритм на краю сцены.",
                            "target_lang_translations": {
                                "en": "The drummer confidently <tapped out> a complex rhythm on the edge of the stage."
                            }
                        },
                        {
                            "text": "Сосед через стену <отстукал> нам секретное сообщение.",
                            "target_lang_translations": {
                                "en": "The neighbor <tapped out> a secret message to us through the wall."
                            }
                        }
                    ],
                    "synonyms": [
                        "отбить"
                    ],
                    "common_phrases": [
                        "отстукать ритм",
                        "отстукать дробь",
                        "отстукать сообщение"
                    ],
                    "target_lang_definitions": {
                        "en": "To reproduce a rhythm or signal by tapping or knocking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tap out",
                                "target_lang_sense_clarification": "a rhythm or message"
                            },
                            {
                                "target_lang_word": "beat out",
                                "target_lang_sense_clarification": "a rhythm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c8c1f0f-a742-39cd-8219-2aae17a0695c",
                    "sense_definition": "Напечатать текст на пишущей машинке или клавиатуре.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь успела <отстукать> приказ до начала совещания.",
                            "target_lang_translations": {
                                "en": "The secretary managed to <type out> the order before the meeting started."
                            }
                        },
                        {
                            "text": "Писатель <отстукал> последнюю главу романа и выдохнул.",
                            "target_lang_translations": {
                                "en": "The writer <banged out> the last chapter of the novel and exhaled."
                            }
                        }
                    ],
                    "synonyms": [
                        "напечатать",
                        "набрать"
                    ],
                    "common_phrases": [

                {
                    "sense_id": "5b5f3b26-df44-3c36-bcc8-9496d3c27086",
                    "sense_definition": "Umieścić przedmiot wewnątrz jakiejś przestrzeni, urządzenia lub mebla.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "placement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>wstawić</w> brudne naczynia do zmywarki, zanim pójdę spać.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>поставить</w> грязную посуду в посудомойку, прежде чем пойду спать."
                            }
                        },
                        {
                            "text": "Pamiętaj, żeby <w>wstawić</w> mleko do lodówki, bo się zepsuje.",
                            "target_lang_translations": {
                                "ru": "Помни, что нужно <w>поставить</w> молоко в холодильник, иначе оно испортится."
                            }
                        }
                    ],
                    "synonyms": [
                        "umieścić",
                        "włożyć"
                    ],
                    "antonyms": [
                        "wyjąć"
                    ],
                    "common_phrases": [
                        "wstawić do lodówki",
                        "wstawić do piekarnika",
                        "wstawić pranie",
                        "wstawić wodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить предмет внутрь какого-либо пространства, устройства или мебели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поставить",
                                "target_lang_sense_clarification": "разместить вертикально или внутрь чего-либо"
                            },
                            {
                                "target_lang_word": "убрать",
                                "target_lang_sense_clarification": "положить на место хранения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "809d0af5-558a-3f35-83b8-c0da0e37df60",
                    "sense_definition": "Zamontować nowy element w konstrukcji lub dodać fragment do tekstu czy pliku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "placement_constructive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro przyjdzie fachowiec, aby <w>wstawić</w> nową szybę w oknie.",
                            "target_lang_translations": {
                                "ru": "Завтра придет мастер, чтобы <w>вставить</w> новое стекло в окно."
                            }
                        },
                        {
                            "text": "Zapomniałem <w>wstawić</w> tabelę z wynikami do mojego raportu.",
                            "target_lang_translations": {
                                "ru": "Я забыл <w>вставить</w> таблицу с результатами в свой отчет."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamontować",
                        "wpisać",
                        "dodać"
                    ],
                    "antonyms": [
                        "usunąć",
                        "wyciąć"
                    ],
                    "common_phrases": [

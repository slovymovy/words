    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6e96088-e0e1-3ed5-a61e-64b797e954ef",
                    "sense_definition": "Поспешный уход или отъезд, спасение бегом от опасности или преследования.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "flight_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мирные жители спасались <бегством> из разрушенного города.",
                            "target_lang_translations": {
                                "en": "Civilians saved themselves by <flight> from the destroyed city."
                            }
                        },
                        {
                            "text": "Внезапный пожар заставил всех обратиться в <бегство>.",
                            "target_lang_translations": {
                                "en": "The sudden fire forced everyone to take to <flight>."
                            }
                        }
                    ],
                    "synonyms": [
                        "побег"
                    ],
                    "antonyms": [
                        "преследование",
                        "погоня"
                    ],
                    "common_phrases": [
                        "обратиться в бегство",
                        "спасаться бегством",
                        "поспешное бегство"
                    ],
                    "target_lang_definitions": {
                        "en": "A hasty departure or leaving, saving oneself by running from danger or persecution."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flight",
                                "target_lang_sense_clarification": "act of fleeing from danger"
                            },
                            {
                                "target_lang_word": "escape",
                                "target_lang_sense_clarification": "act of getting away from danger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bb46ee3-3801-370f-a57c-0b02feda6478",
                    "sense_definition": "Быстрое и беспорядочное отступление войск под натиском противника.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "flight_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Атака нашей кавалерии закончилась паническим <бегством> врага.",
                            "target_lang_translations": {
                                "en": "Our cavalry's attack ended in the enemy's panic <flight>."
                            }
                        },
                        {
                            "text": "Генерал пытался остановить позорное <бегство> своих солдат с поля боя.",
                            "target_lang_translations": {
                                "en": "The general tried to stop the shameful <flight> of his soldiers from the battlefield."
                            }
                        }
                    ],
                    "synonyms": [
                        "отступление"
                    ],
                    "antonyms": [
                        "наступление",
                        "атака"
                    ],
                    "common_phrases": [
                        "паническое бегство",
                        "позорное бегство",
                        "беспорядочное бегство"
                    ],
                    "target_lang_definitions": {
                        "en": "A rapid and disorderly retreat of troops under enemy pressure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flight",
                                "target_lang_sense_clarification": "running away in panic"
                            },
                            {
                                "target_lang_word": "rout",
                                "target_lang_sense_clarification": "disorderly retreat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d058c94-fa72-3941-83c2-3d1aee9d2f36",
                    "sense_definition": "Тайный самовольный уход, побег откуда-либо (из плена, тюрьмы, дома).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "escape_secret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пленники долго готовили ночное <бегство> через подкоп.",
                            "target_lang_translations": {
                                "en": "The captives spent a long time preparing their night <escape> through a tunnel."
                            }
                        },
                        {
                            "text": "Его неожиданное <бегство> за границу вызвало громкий скандал.",
                            "target_lang_translations": {
                                "en": "His unexpected <flight> abroad caused a loud scandal."
                            }
                        }
                    ],
                    "synonyms": [
                        "побег"
                    ],
                    "antonyms": [
                        "возвращение"
                    ],
                    "common_phrases": [
                        "готовить бегство",
                        "совершить бегство",
                        "бегство из плена"
                    ],
                    "target_lang_definitions": {
                        "en": "A secret unauthorized departure, an escape from somewhere (captivity, prison, home)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "escape",
                                "target_lang_sense_clarification": "breaking out of confinement"
                            },
                            {
                                "target_lang_word": "flight",
                                "target_lang_sense_clarification": "act of fleeing a place or jurisdiction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ab23f5d-761c-3f6b-b6b9-267c475ed3b8",
                    "sense_definition": "Стремление уйти от реальности, отказ от решения жизненных проблем или участия в чём-либо неприятном.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_withdrawal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтение фантастики стало для него своеобразным <бегством> от скучной работы.",
                            "target_lang_translations": {
                                "en": "Reading sci-fi became a kind of <escape> from boring work for him."
                            }
                        },
                        {
                            "text": "Психолог назвал такое поведение <бегством> от ответственности.",
                            "target_lang_translations": {
                                "en": "The psychologist called such behavior a <flight> from responsibility."
                            }
                        }
                    ],
                    "synonyms": [
                        "уход",
                        "отстранённость"
                    ],
                    "antonyms": [
                        "столкновение",
                        "борьба"
                    ],
                    "common_phrases": [
                        "бегство от реальности",
                        "бегство от себя",
                        "бегство от свободы"
                    ],
                    "target_lang_definitions": {
                        "en": "A desire to leave reality, a refusal to solve life problems or participate in something unpleasant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "escape",
                                "target_lang_sense_clarification": "mental diversion or distraction"
                            },
                            {
                                "target_lang_word": "flight",
                                "target_lang_sense_clarification": "avoidance of reality or responsibility"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
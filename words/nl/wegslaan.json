            "pos": "verb",
            "senses": [
                {
                    "sense_id": "764dcd97-8d98-39ff-992c-b3223cd7224f",
                    "sense_definition": "Iets of iemand verwijderen of afweren door een slaande beweging te maken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij probeerde de vervelende vlieg met zijn hand <w>weg</w> te <w>slaan</w>.",
                            "target_lang_translations": {
                                "en": "He tried to <w>swat</w> the annoying fly <w>away</w> with his hand.",
                                "ru": "Он пытался <w>смахнуть</w> назойливую муху рукой."
                            }
                        },
                        {
                            "text": "De keeper kon de bal nog net op tijd uit het doel <w>wegslaan</w>.",
                            "target_lang_translations": {
                                "en": "The goalkeeper was able to <w>punch</w> the ball <w>away</w> from the goal just in time.",
                                "ru": "Вратарь смог <w>отбить</w> мяч из ворот как раз вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegmeppen",
                        "afslaan"
                    ],
                    "common_phrases": [
                        "een vlieg wegslaan",
                        "de bal wegslaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove or ward off something or someone by making a hitting movement.",
                        "ru": "Удалять или отгонять что-либо или кого-либо, делая ударяющее движение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swat away",
                                "target_lang_sense_clarification": "specifically for insects or using an open hand"
                            },
                            {
                                "target_lang_word": "punch away",
                                "target_lang_sense_clarification": "often used in sports, e.g., by a goalkeeper"
                            },
                            {
                                "target_lang_word": "beat off",
                                "target_lang_sense_clarification": "to ward off an attack or persistent annoyance"
                            },
                            {
                                "target_lang_word": "knock away",
                                "target_lang_sense_clarification": "to remove an object by hitting it"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отбивать",
                                "target_lang_sense_clarification": "мяч, атаку"
                            },
                            {
                                "target_lang_word": "смахивать",
                                "target_lang_sense_clarification": "насекомое, рукой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06ce0323-a71a-3fd5-8767-3d17cbf109e8",
                    "sense_definition": "Met grote kracht van zijn plaats rukken of vernietigen, vaak door water of wind.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "force_of_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude houten brug werd door het wilde water volledig <w>weggeslagen</w>.",
                            "target_lang_translations": {
                                "en": "The old wooden bridge was completely <w>washed away</w> by the wild water.",
                                "ru": "Старый деревянный мост был полностью <w>снесён</w> бурным потоком."
                            }
                        },
                        {
                            "text": "Tijdens de zware storm werden er verschillende daken van de huizen <w>weggeslagen</w>.",
                            "target_lang_translations": {
                                "en": "During the severe storm, several roofs were <w>blown off</w> the houses.",
                                "ru": "Во время сильного шторма с домов <w>сорвало</w> несколько крыш."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegvagen",
                        "meesleuren",
                        "verwoesten"
                    ],
                    "common_phrases": [
                        "weggeslagen door de storm",
                        "een brug wegslaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To be torn from its place or destroyed by great force, often by water or wind.",
                        "ru": "Срывать с места или разрушать с большой силой, часто под действием воды или ветра."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wash away",
                                "target_lang_sense_clarification": "when caused by water"
                            },
                            {
                                "target_lang_word": "blow away",
                                "target_lang_sense_clarification": "when caused by wind"
                            },
                            {
                                "target_lang_word": "sweep away",
                                "target_lang_sense_clarification": "by a powerful force"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сносить",
                                "target_lang_sense_clarification": "разрушать силой стихии"
                            },
                            {
                                "target_lang_word": "смывать",
                                "target_lang_sense_clarification": "водой"
                            },
                            {
                                "target_lang_word": "срывать",
                                "target_lang_sense_clarification": "ветром"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cdaccb6d-7334-336a-a749-7858c8c497a6",
                    "sense_definition": "Gekenmerkt door diepe lijnen of rimpels, vaak gezegd van een gezicht of handen als teken van ouderdom of een zwaar leven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude visser had een diep <w>doorgroefd</w> gezicht door jarenlang werken in weer en wind.",
                            "target_lang_translations": {
                                "ru": "У старого рыбака было глубоко <w>изборождённое</w> лицо из-за многолетней работы на ветру и в непогоду."
                            }
                        },
                        {
                            "text": "Zijn <w>doorgroefde</w> handen getuigden van tientallen jaren zware arbeid op het land.",
                            "target_lang_translations": {
                                "ru": "Его <w>изборождённые</w> руки свидетельствовали о десятилетиях тяжёлого труда на земле."
                            }
                        }
                    ],
                    "synonyms": [
                        "gerimpeld",
                        "verweerd"
                    ],
                    "antonyms": [
                        "glad",
                        "egaal"
                    ],
                    "common_phrases": [
                        "een doorgroefd gezicht",
                        "doorgroefde handen",
                        "een doorgroefd gelaat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризуется глубокими линиями или морщинами; часто говорится о лице или руках как о признаке старости или тяжелой жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изборождённый",
                                "target_lang_sense_clarification": "покрытый глубокими морщинами или бороздами"
                            },
                            {
                                "target_lang_word": "морщинистый",
                                "target_lang_sense_clarification": "имеющий много морщин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d48000ef-4252-31d4-adfb-3365bab2de15",
                    "sense_definition": "Voltooid deelwoord van <w>doorgroeven</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het landschap was <w>doorgroefd</w> door talloze kleine stroompjes die naar de rivier liepen.",
                            "target_lang_translations": {
                                "ru": "Ландшафт был <w>изрезан</w> бесчисленными маленькими ручейками, бегущими к реке."
                            }
                        },
                        {
                            "text": "De zware regenval had de zandweg diep <w>doorgroefd</w>.",
                            "target_lang_translations": {
                                "ru": "Сильный ливень глубоко <w>изрыл</w> песчаную дорогу."
                            }
                        }
                    ],

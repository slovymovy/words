                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "технический персонал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7b86e77-ec23-3be9-b696-0252a6d3166b",
                    "sense_definition": "A close group of friends or associates.",

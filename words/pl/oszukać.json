            "senses": [
                {
                    "sense_id": "818b41cd-8ce0-3485-b73d-04148c308d3d",
                    "sense_definition": "Świadomie wprowadzić kogoś w błąd, skłamać lub zataić prawdę w celu osiągnięcia korzyści.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieuczciwy sprzedawca <oszukał> klienta, wydając mu za mało reszty.",
                            "target_lang_translations": {
                                "ru": "Нечестный продавец <обманул> клиента, дав ему слишком мало сдачи."
                            }
                        },
                        {
                            "text": "Musisz uważać, aby nie dać się <oszukać> internetowym naciągaczom.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, чтобы не дать интернет-мошенникам себя <обмануть>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wprowadzić w błąd",
                        "okłamać",
                        "nabrać",
                        "wykiwać",
                        "zrobić w balona",
                        "wyprowadzić w pole"
                    ],
                    "antonyms": [
                        "być szczerym",
                        "powiedzieć prawdę"
                    ],
                    "common_phrases": [
                        "dać się oszukać",
                        "oszukać przeznaczenie",
                        "oszukać system",
                        "oszukać głód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сознательно ввести кого-то в заблуждение, солгать или скрыть правду с целью получения выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмануть",
                                "target_lang_sense_clarification": "ввести в заблуждение"
                            },
                            {
                                "target_lang_word": "провести",
                                "target_lang_sense_clarification": "перехитрить"
                            },
                            {
                                "target_lang_word": "надуть",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5be7df23-4177-3a4d-916f-36a28331456c",
                    "sense_definition": "Błędnie ocenić sytuację lub osobę, ponosząc tego negatywne konsekwencje (zazwyczaj w formie zwrotnej).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myślałem, że to będzie łatwe zadanie, ale srogo się <oszukałem>.",
                            "target_lang_translations": {
                                "ru": "Я думал, что это будет легкая задача, но жестоко <ошибся>."
                            }
                        },
                        {
                            "text": "Wielu inwestorów <oszukało> się na gwałtownym spadku cen akcji.",
                            "target_lang_translations": {
                                "ru": "Многие инвесторы <просчитались> на резком падении цен акций."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomylić się",
                        "zawieść się",
                        "przeliczyć się"
                    ],
                    "antonyms": [
                        "mieć rację"
                    ],
                    "common_phrases": [
                        "oszukać się na kimś",
                        "srogo się oszukać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ошибочно оценить ситуацию или человека, столкнувшись с негативными последствиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ошибиться",
                                "target_lang_sense_clarification": "сделать ошибку"
                            },
                            {
                                "target_lang_word": "обмануться",
                                "target_lang_sense_clarification": "в ожиданиях"
                            },
                            {
                                "target_lang_word": "просчитаться",
                                "target_lang_sense_clarification": "неверно рассчитать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

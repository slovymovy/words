                {
                    "sense_id": "e58b125a-45aa-310b-bac0-9b3e551bc7fa",
                    "sense_definition": "W sposób pozbawiony światła; w warunkach, gdzie panuje mrok.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą bardzo szybko robi się <w>ciemno</w> na dworze.",
                            "target_lang_translations": {
                                "ru": "Зимой на улице очень быстро становится <w>темно</w>."
                            }
                        },
                        {
                            "text": "W pokoju było tak <w>ciemno</w>, że musiałem zapalić lampę.",
                            "target_lang_translations": {
                                "ru": "В комнате было так <w>темно</w>, что мне пришлось зажечь лампу."
                            }
                        }
                    ],
                    "synonyms": [
                        "mrocznie"
                    ],
                    "antonyms": [
                        "jasno"
                    ],
                    "common_phrases": [
                        "robi się ciemno",
                        "jest ciemno"
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "armijny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b4e0768-59cf-3e69-90f8-b293e9b1ca7d",
                    "sense_definition": "Ogół sił zbrojnych danego państwa, powołanych do obrony jego granic i interesów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "military_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska chce wprowadzić w pełni zawodową <w>armię</w>.",
                            "target_lang_translations": {
                                "ru": "Польша хочет создать полностью профессиональную <w>армию</w>."
                            }
                        },
                        {
                            "text": "Mój starszy brat postanowił wstąpić do <w>armii</w> zaraz po ukończeniu szkoły.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат решил пойти в <w>армию</w> сразу после окончания школы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wojsko",
                        "oręż"
                    ],
                    "common_phrases": [
                        "wstąpić do armii",
                        "armia zawodowa",
                        "służyć w armii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность вооруженных сил государства, призванных защищать его границы и интересы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "армия",
                                "target_lang_sense_clarification": "вооруженные силы государства"
                            },
                            {
                                "target_lang_word": "вооружённые силы",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13b724d1-a582-3e9c-b3d2-7d2b4aeb5b7f",
                    "sense_definition": "Bardzo liczna grupa ludzi lub zwierząt, często działająca wspólnie lub przemieszczająca się razem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej kuchni pojawiła się prawdziwa <w>armia</w> mrówek.",
                            "target_lang_translations": {
                                "ru": "На нашей кухне появилась настоящая <w>армия</w> муравьёв."
                            }
                        },
                        {
                            "text": "Cała <w>armia</w> urzędników pracuje nad nowymi przepisami podatkowymi.",
                            "target_lang_translations": {
                                "ru": "Целая <w>армия</w> чиновников работает над новыми налоговыми правилами."
                            }
                        }
                    ],
                    "synonyms": [
                        "tłum",
                        "legion"
                    ],
                    "common_phrases": [
                        "armia ludzi",
                        "armia bezrobotnych",
                        "cała armia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большая группа людей или животных, часто действующая сообща или перемещающаяся вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "армия",
                                "target_lang_sense_clarification": "огромное множество"
                            },
                            {
                                "target_lang_word": "полчище",
                                "target_lang_sense_clarification": "большое скопление (часто с негативным оттенком)"
                            },
                            {
                                "target_lang_word": "легион",
                                "target_lang_sense_clarification": "бесчисленное множество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34c23a76-4e9f-3aa5-b4ae-507e83dd4910",
                    "sense_definition": "Część sił zbrojnych przeznaczona do działań na lądzie, w odróżnieniu od lotnictwa i marynarki wojennej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marynarka wojenna i <w>armia</w> przeprowadziły wspólne manewry na wybrzeżu.",
                            "target_lang_translations": {
                                "ru": "Военно-морской флот и <w>армия</w> провели совместные учения на побережье."
                            }
                        },
                        {
                            "text": "Generał podkreślił, że lądowa <w>armia</w> potrzebuje wsparcia z powietrza.",
                            "target_lang_translations": {
                                "ru": "Генерал подчеркнул, что сухопутная <w>армия</w> нуждается в поддержке с воздуха."
                            }
                        }
                    ],
                    "common_phrases": [
                        "generał armii",
                        "armia lądowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть вооруженных сил, предназначенная для действий на суше, в отличие от авиации и флота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сухопутные войска",
                                "target_lang_sense_clarification": "вид вооруженных сил"
                            },
                            {
                                "target_lang_word": "армия",
                                "target_lang_sense_clarification": "сухопутные силы (в противопоставлении флоту)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f91f95f9-39ab-33f0-93e5-b8c615994bce",
                    "sense_definition": "Duża jednostka organizacyjna wojska (związek operacyjny), składająca się zazwyczaj z kilku korpusów lub dywizji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pododdziały trzeciej <w>armii</w> zaatakowały miasto o świcie.",
                            "target_lang_translations": {
                                "ru": "Подразделения третьей <w>армии</w> атаковали город на рассвете."
                            }
                        },
                        {
                            "text": "Dowódca <w>armii</w> wydał rozkaz przegrupowania sił na wschód.",
                            "target_lang_translations": {
                                "ru": "Командующий <w>армией</w> отдал приказ о перегруппировке сил на восток."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sztab armii",
                        "dowódca armii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное оперативное войсковое объединение, обычно состоящее из нескольких корпусов или дивизий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "армия",
                                "target_lang_sense_clarification": "оперативное объединение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
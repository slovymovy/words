    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a37f2d23-c1c4-3d1a-97ea-69382bf99a73",
                    "sense_definition": "Relating to, showing, or depending on a difference or distinction between things.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company has a policy of <w>differential</w> pricing based on the region.",
                            "target_lang_translations": {
                                "ru": "У компании есть политика <w>дифференцированного</w> ценообразования в зависимости от региона."
                            }
                        },
                        {
                            "text": "Doctors perform a <w>differential</w> diagnosis to distinguish between diseases with similar symptoms.",
                            "target_lang_translations": {
                                "ru": "Врачи проводят <w>дифференциальную</w> диагностику, чтобы различить болезни со схожими симптомами."
                            }
                        }
                    ],
                    "synonyms": [
                        "distinctive",
                        "discriminatory",
                        "distinguishing"
                    ],
                    "antonyms": [
                        "uniform",
                        "identical"
                    ],
                    "common_phrases": [
                        "differential diagnosis",
                        "differential treatment",
                        "differential pricing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к различию, показывающий различие или зависящий от него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дифференциальный",
                                "target_lang_sense_clarification": "специальный термин, напр. диагноз"
                            },
                            {
                                "target_lang_word": "дифференцированный",
                                "target_lang_sense_clarification": "основанный на различии, напр. тарифы"
                            },
                            {
                                "target_lang_word": "различительный",
                                "target_lang_sense_clarification": "служащий для отличия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5c795eb-f965-3e61-bca6-875d7e5510dd",
                    "sense_definition": "Relating to the branch of mathematics dealing with derivatives, rates of change, and slopes of curves.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Solving a <w>differential</w> equation is essential for modeling physical systems.",
                            "target_lang_translations": {
                                "ru": "Решение <w>дифференциального</w> уравнения необходимо для моделирования физических систем."
                            }
                        },
                        {
                            "text": "The student struggled with the complex concepts of <w>differential</w> calculus.",
                            "target_lang_translations": {
                                "ru": "Студент с трудом понимал сложные концепции <w>дифференциального</w> исчисления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "differential equation",
                        "differential calculus",
                        "differential geometry"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к разделу математики, имеющему дело с производными, скоростью изменений и наклонами кривых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дифференциальный",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "57746c41-4157-319a-bb8c-c093b17b909c",
                    "sense_definition": "A difference in amount, value, or degree between similar or comparable things, often referring to wages or prices.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "gap_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wage <w>differential</w> between the two departments caused some dissatisfaction among employees.",
                            "target_lang_translations": {
                                "ru": "<w>Разница</w> в заработной плате между двумя отделами вызвала недовольство среди сотрудников."
                            }
                        },
                        {
                            "text": "Economists are studying the price <w>differential</w> of oil across various international markets.",
                            "target_lang_translations": {
                                "ru": "Экономисты изучают <w>разницу</w> в ценах на нефть на различных международных рынках."
                            }
                        }
                    ],
                    "synonyms": [
                        "disparity",
                        "gap",
                        "discrepancy"
                    ],
                    "antonyms": [
                        "equality",
                        "parity"
                    ],
                    "common_phrases": [
                        "wage differential",
                        "price differential",
                        "pay differential"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "business/economics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разница в количестве, стоимости или степени между схожими или сопоставимыми вещами, часто в отношении заработной платы или цен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разница",
                                "target_lang_sense_clarification": "отличие в величине или значении"
                            },
                            {
                                "target_lang_word": "различие",
                                "target_lang_sense_clarification": "несходство"
                            },
                            {
                                "target_lang_word": "дифференциал",
                                "target_lang_sense_clarification": "экон. разрыв в ставках или ценах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58fd3f33-9a93-3e96-8659-1dde9433cb75",
                    "sense_definition": "A gear assembly in a vehicle that allows the wheels to rotate at different speeds, used especially when turning corners.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mech_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic said the noise was coming from the rear <w>differential</w>.",
                            "target_lang_translations": {
                                "ru": "Механик сказал, что шум исходит от заднего <w>дифференциала</w>."
                            }
                        },
                        {
                            "text": "A locking <w>differential</w> helps the truck maintain traction on muddy roads.",
                            "target_lang_translations": {
                                "ru": "Блокируемый <w>дифференциал</w> помогает грузовику сохранять сцепление на грязных дорогах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rear differential",
                        "locking differential",
                        "limited-slip differential"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "automotive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм в транспортном средстве, позволяющий колесам вращаться с разной скоростью, особенно при поворотах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дифференциал",
                                "target_lang_sense_clarification": "автомобильный механизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ab9eac0-5738-3ac5-945a-a544bcb6ac3a",
                    "sense_definition": "An infinitesimal difference or change in a variable, used in calculus.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the equation, the term dx represents the <w>differential</w> of x.",
                            "target_lang_translations": {
                                "ru": "В уравнении член dx представляет собой <w>дифференциал</w> x."
                            }
                        },
                        {
                            "text": "Students learned how to calculate the total <w>differential</w> of the function.",
                            "target_lang_translations": {
                                "ru": "Студенты учились вычислять полный <w>дифференциал</w> функции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "total differential",
                        "partial differential",
                        "exact differential"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бесконечно малое изменение переменной, используемое в исчислении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дифференциал",
                                "target_lang_sense_clarification": "математическое понятие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

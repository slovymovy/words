    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fc267ca8-e193-3ee9-a62e-05e62df8dad1",
                    "sense_definition": "Worthy of attention; notable or interesting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "notable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She showed <w>remarkable</w> talent for music at a very young age.",
                            "target_lang_translations": {
                                "ru": "Она проявила <w>выдающийся</w> музыкальный талант в очень юном возрасте."
                            }
                        },
                        {
                            "text": "The scientist made a <w>remarkable</w> discovery that changed history.",
                            "target_lang_translations": {
                                "ru": "Учёный сделал <w>замечательное</w> открытие, которое изменило историю."
                            }
                        }
                    ],
                    "synonyms": [
                        "notable",
                        "noteworthy",
                        "extraordinary",
                        "outstanding",
                        "significant",
                        "impressive"
                    ],
                    "antonyms": [
                        "unremarkable"
                    ],
                    "common_phrases": [
                        "remarkable achievement",
                        "truly remarkable",
                        "quite remarkable",
                        "remarkable success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заслуживающий внимания; выдающийся или интересный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдаю́щийся",
                                "target_lang_sense_clarification": "исключительный, выделяющийся среди других"
                            },
                            {
                                "target_lang_word": "замеча́тельный",
                                "target_lang_sense_clarification": "достойный внимания, очень хороший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05e73428-5b2f-382a-a499-43342531371e",
                    "sense_definition": "Uncommon, unusual, or surprising.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unusual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>remarkable</w> that the two strangers had the exact same name.",
                            "target_lang_translations": {
                                "ru": "<w>Удивительно</w>, что у двух незнакомцев были совершенно одинаковые имена."
                            }
                        },
                        {
                            "text": "There is a <w>remarkable</w> similarity between the two paintings.",
                            "target_lang_translations": {
                                "ru": "Между двумя картинами существует <w>поразительное</w> сходство."
                            }
                        }
                    ],
                    "synonyms": [
                        "unusual",
                        "strange",
                        "rare",
                        "singular",
                        "surprising"
                    ],
                    "antonyms": [
                        "normal",
                        "unremarkable"
                    ],
                    "common_phrases": [
                        "remarkable coincidence",
                        "remarkable similarity",
                        "remarkable difference"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычный, редкий или вызывающий удивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порази́тельный",
                                "target_lang_sense_clarification": "вызывающий сильное удивление"
                            },
                            {
                                "target_lang_word": "удиви́тельный",
                                "target_lang_sense_clarification": "странный или неожиданный"
                            }
                        ]
                    }
                }
            ]
        }

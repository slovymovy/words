            "senses": [
                {
                    "sense_id": "dae1b46f-7db1-3ae4-b7c9-de4666de9b8f",
                    "sense_definition": "Opletten dat er niets ergs gebeurt; voorzichtig zijn.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "watch_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet goed <w>oppassen</w> als je deze drukke straat oversteekt.",
                            "target_lang_translations": {
                                "en": "You have to <w>be careful</w> when you cross this busy street.",
                                "ru": "Тебе нужно быть очень <w>осторожным</w>, когда переходишь эту оживленную улицу."
                            }
                        },
                        {
                            "text": "<w>Pas op</w>, de vloer is net gedweild en erg glad!",
                            "target_lang_translations": {
                                "en": "<w>Watch out</w>, the floor has just been mopped and is very slippery!",
                                "ru": "<w>Осторожно</w>, пол только что помыли, и он очень скользкий!"
                            }
                        }
                    ],
                    "synonyms": [
                        "opletten",
                        "uitkijken"
                    ],
                    "common_phrases": [
                        "pas op",
                        "goed oppassen"
                    ],
                    "target_lang_definitions": {
                        "en": "To pay attention to ensure nothing bad happens; to be careful.",
                        "ru": "Следить за тем, чтобы не случилось ничего плохого; быть внимательным и осторожным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch out",
                                "target_lang_sense_clarification": "to be alert to danger"
                            },
                            {

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dee4e740-2021-3232-9c55-34e7217b8e2e",
                    "sense_definition": "Чувство огорчения, горечи или досады, вызванное несправедливостью, оскорблением или неприятным событием.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_grievance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она старалась не показывать свою <обиду> на резкие слова начальника.",
                            "target_lang_translations": {
                                "en": "She tried not to show her <resentment> at the boss's harsh words."
                            }
                        },
                        {
                            "text": "Мы договорились говорить друг другу правду и жить без <обид>.",
                            "target_lang_translations": {
                                "en": "We agreed to tell each other the truth and live without <hard feelings>."
                            }
                        }
                    ],
                    "synonyms": [
                        "огорчение",
                        "досада",
                        "горечь"
                    ],
                    "antonyms": [
                        "радость"
                    ],
                    "common_phrases": [
                        "без обид",
                        "затаить обиду",
                        "чувство обиды",
                        "глотать обиду",
                        "слёзы обиды"
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of distress, bitterness, or annoyance caused by injustice, an insult, or an unpleasant event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resentment",
                                "target_lang_sense_clarification": "feeling of bitterness"
                            },
                            {
                                "target_lang_word": "offense",
                                "target_lang_sense_clarification": "feeling of being insulted"
                            },
                            {
                                "target_lang_word": "grudge",
                                "target_lang_sense_clarification": "persistent ill will"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6efeb686-9c0f-39e5-810b-a54a0ec00f89",
                    "sense_definition": "Несправедливо причинённое огорчение, оскорбительный поступок или слова.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_grievance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старший брат обещал никогда не давать младшего в <обиду> школьным хулиганам.",
                            "target_lang_translations": {
                                "en": "The older brother promised never to let the younger one be <hurt> by school bullies."
                            }
                        },
                        {
                            "text": "Он случайно нанёс ей <обиду>, забыв поздравить с днём рождения.",
                            "target_lang_translations": {
                                "en": "He accidentally caused her <offense> by forgetting to wish her a happy birthday."
                            }
                        }
                    ],
                    "synonyms": [
                        "обижда",
                        "огорчение"
                    ],
                    "antonyms": [
                        "похвала",
                        "комплимент"
                    ],
                    "common_phrases": [
                        "не дать в обиду",
                        "нанести обиду",
                        "стерпеть обиду"
                    ],
                    "target_lang_definitions": {
                        "en": "Unjustly caused distress, an insulting act or words."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "insult",
                                "target_lang_sense_clarification": "disrespectful act or remark"
                            },
                            {
                                "target_lang_word": "offense",
                                "target_lang_sense_clarification": "act of offending"
                            },
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "unjust action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76281a8b-1128-3cd6-94f4-76eaeffd4902",
                    "sense_definition": "О том, что вызывает сожаление, досаду или разочарование (обычно в значении «жаль»).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_grievance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Какая <обида>, что билеты в театр закончились прямо перед нами!",
                            "target_lang_translations": {
                                "en": "What a <shame> that the theater tickets ran out right in front of us!"
                            }
                        },
                        {
                            "text": "Мы проиграли важный матч на последней минуте, вот так <обида>!",
                            "target_lang_translations": {
                                "en": "We lost an important match in the last minute, what a <pity>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "досада",
                        "огорчение"
                    ],
                    "common_phrases": [
                        "какая обида",
                        "вот так обида",
                        "вот обида"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Something that causes regret, annoyance, or disappointment (usually meaning \"what a pity\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pity",
                                "target_lang_sense_clarification": "cause for regret"
                            },
                            {
                                "target_lang_word": "shame",
                                "target_lang_sense_clarification": "disappointing situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7703b17-498c-3cfc-a928-c5cb3c7f0ab7",
                    "sense_definition": "Материальный ущерб или убыток.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "material_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный град нанёс большую <обиду> посевам пшеницы в этом году.",
                            "target_lang_translations": {
                                "en": "Heavy hail caused great <damage> to the wheat crops this year."
                            }
                        },
                        {
                            "text": "Купец долго подсчитывал <обиду>, причинённую внезапным пожаром на складе.",
                            "target_lang_translations": {
                                "en": "The merchant spent a long time calculating the <loss> caused by the sudden fire in the warehouse."
                            }
                        }
                    ],
                    "synonyms": [
                        "огорчение"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "областное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Material damage or loss."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "physical harm"
                            },
                            {
                                "target_lang_word": "loss",
                                "target_lang_sense_clarification": "financial detriment"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c82d3db7-e936-3287-9897-70a23756b41e",
                    "sense_definition": "A round piece of metal or other material worn around the finger, often as jewelry or a symbol of marriage.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave her a beautiful gold <w>ring</w> for their engagement.",
                            "target_lang_translations": {
                                "ru": "Он подарил ей красивое золотое <w>кольцо</w> на помолвку."
                            }
                        },
                        {
                            "text": "She wears a diamond <w>ring</w> on her left hand.",
                            "target_lang_translations": {
                                "ru": "Она носит <w>кольцо</w> с бриллиантом на левой руке."
                            }
                        }
                    ],
                    "synonyms": [
                        "annulus",
                        "hoop"
                    ],
                    "common_phrases": [
                        "wedding ring",
                        "engagement ring",
                        "diamond ring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглый предмет из металла или другого материала, который носят на пальце, часто как украшение или символ брака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "украшение"
                            },
                            {
                                "target_lang_word": "перстень",
                                "target_lang_sense_clarification": "кольцо с камнем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa0f57ee-50bb-36d7-8c8b-5b17fe2b567c",
                    "sense_definition": "A telephone call.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will give you a <w>ring</w> when I get home.",
                            "target_lang_translations": {
                                "ru": "Я <w>позвоню</w> тебе, когда доберусь до дома."
                            }
                        },
                        {
                            "text": "She promised to give her mother a <w>ring</w> tomorrow.",
                            "target_lang_translations": {
                                "ru": "Она обещала <w>позвонить</w> матери завтра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give someone a ring"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in British English and informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Телефонный звонок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "по телефону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38467d5c-c576-3072-808c-54ca44e69d8a",
                    "sense_definition": "A solid object or arrangement in the shape of a circle.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children sat in a <w>ring</w> on the floor to play the game.",
                            "target_lang_translations": {
                                "ru": "Дети сели в <w>круг</w> на полу, чтобы поиграть в игру."
                            }
                        },
                        {
                            "text": "Smoke rose in a perfect <w>ring</w> from the chimney.",
                            "target_lang_translations": {
                                "ru": "Дым поднимался из трубы идеальным <w>кольцом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoop",
                        "annulus",
                        "torus"
                    ],
                    "common_phrases": [
                        "onion rings",
                        "ring of fire",
                        "smoke ring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый предмет или расположение объектов в форме круга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7379d766-fecc-36a4-8df5-d43c108fcbd2",
                    "sense_definition": "The clear, resonant sound made by a bell or a similar object.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could hear the loud <w>ring</w> of the church bells from the valley.",
                            "target_lang_translations": {
                                "ru": "Из долины мы слышали громкий <w>звон</w> церковных колоколов."
                            }
                        },
                        {
                            "text": "There was a sudden <w>ring</w> at the door that startled us.",
                            "target_lang_translations": {
                                "ru": "В дверь раздался внезапный <w>звонок</w>, который напугал нас."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чистый, резонирующий звук, издаваемый колоколом или похожим предметом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звон",
                                "target_lang_sense_clarification": "звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "164c8533-2ad7-3c04-9df6-5c03c64a0611",
                    "sense_definition": "An enclosed space, typically circular or square, used for sports, exhibitions, or performances.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two boxers stepped into the <w>ring</w> to begin the fight.",
                            "target_lang_translations": {
                                "ru": "Два боксера вышли на <w>ринг</w>, чтобы начать бой."
                            }
                        },
                        {
                            "text": "Everyone watched the acrobats perform in the circus <w>ring</w>.",
                            "target_lang_translations": {
                                "ru": "Все смотрели, как акробаты выступают на цирковой <w>арене</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "boxing ring",
                        "circus ring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Огороженное пространство, обычно круглое или квадратное, используемое для спорта, выставок или представлений."

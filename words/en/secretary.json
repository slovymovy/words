{
    "word_family": [
        "secretarial",
        "secretariat"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ba288c0-4d4c-3387-8fc4-1f9f60a8e11f",
                    "sense_definition": "A person employed to handle correspondence, keep records, and perform general clerical work for an individual or an organization.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clerical_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>secretary</w> scheduled the meeting for next Tuesday morning.",
                            "target_lang_translations": {
                                "ru": "<w>Секретарь</w> назначил встречу на утро следующего вторника."
                            }
                        },
                        {
                            "text": "She works as a legal <w>secretary</w> at a large firm in the city.",
                            "target_lang_translations": {
                                "ru": "Она работает юридическим <w>секретарём</w> в крупной фирме в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "assistant",
                        "administrative assistant",
                        "clerk"
                    ],
                    "common_phrases": [
                        "private secretary",
                        "legal secretary",
                        "medical secretary",
                        "school secretary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник, нанятый для ведения переписки, делопроизводства и выполнения общей канцелярской работы для частного лица или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секретарь",
                                "target_lang_sense_clarification": "должность"
                            },
                            {
                                "target_lang_word": "секретарша",
                                "target_lang_sense_clarification": "разг., женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d71f62d-4295-330f-ae66-26f199184a09",
                    "sense_definition": "An official who heads a government department or holds a high-ranking position in state administration.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leadership_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The press <w>secretary</w> answered questions from the journalists regarding the new policy.",
                            "target_lang_translations": {
                                "ru": "Пресс-<w>секретарь</w> ответил на вопросы журналистов касательно новой политики."
                            }
                        },
                        {
                            "text": "He was appointed as the <w>secretary</w> of defense last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году он был назначен <w>министром</w> обороны."
                            }
                        }
                    ],
                    "synonyms": [
                        "administrator"
                    ],
                    "common_phrases": [
                        "press secretary",
                        "foreign secretary",
                        "cabinet secretary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должностное лицо, возглавляющее правительственное ведомство или занимающее высокий пост в государственном управлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "министр",
                                "target_lang_sense_clarification": "глава министерства (в контексте США и др.)"
                            },
                            {
                                "target_lang_word": "секретарь",
                                "target_lang_sense_clarification": "в названиях должностей (напр. пресс-секретарь)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a4ec4a0-5e59-3616-b2d7-6d83622301e6",
                    "sense_definition": "A person holding a managerial or leading position in certain organizations, such as political parties or trade unions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "leadership_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general <w>secretary</w> of the union announced the start of the strike.",
                            "target_lang_translations": {
                                "ru": "Генеральный <w>секретарь</w> профсоюза объявил о начале забастовки."
                            }
                        },
                        {
                            "text": "She served as the executive <w>secretary</w> of the committee for five years.",
                            "target_lang_translations": {
                                "ru": "Она прослужила исполнительным <w>секретарём</w> комитета пять лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "administrator"
                    ],
                    "common_phrases": [
                        "general secretary",
                        "executive secretary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, занимающее руководящую должность в определенных организациях, таких как политические партии или профсоюзы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секретарь",
                                "target_lang_sense_clarification": "партийная или профсоюзная должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "608f1f75-88d5-3649-98aa-0fe925e0d71a",
                    "sense_definition": "A person employed to write letters, copy documents, or take dictation, especially in a historical context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clerical_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king dictated his decree to his faithful <w>secretary</w>.",
                            "target_lang_translations": {
                                "ru": "Король продиктовал указ своему верному <w>секретарю</w>."
                            }
                        },
                        {
                            "text": "He acted as a <w>secretary</w>, carefully copying the manuscripts by hand.",
                            "target_lang_translations": {
                                "ru": "Он выполнял обязанности <w>писца</w>, тщательно переписывая рукописи от руки."
                            }
                        }
                    ],
                    "synonyms": [
                        "scribe",
                        "amanuensis",
                        "scrivener"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, нанятый для написания писем, копирования документов или записи под диктовку, особенно в историческом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секретарь",
                                "target_lang_sense_clarification": "историческое"
                            },
                            {
                                "target_lang_word": "писец",
                                "target_lang_sense_clarification": "переписчик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3dd2e03-b9c1-35e6-a96d-58bbbc50ace7",
                    "sense_definition": "A piece of furniture with a writing surface and drawers, often with a top section for books.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sat at the antique <w>secretary</w> to write her letters.",
                            "target_lang_translations": {
                                "ru": "Она села за старинный <w>секретер</w>, чтобы написать письма."
                            }
                        },
                        {
                            "text": "The mahogany <w>secretary</w> has been in the family for generations.",
                            "target_lang_translations": {
                                "ru": "Этот <w>секретер</w> из красного дерева находится в семье уже несколько поколений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "secretary desk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет мебели с откидной доской для письма и ящиками, часто с верхней секцией для книг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секретер",
                                "target_lang_sense_clarification": "мебель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34a8cff7-9155-304a-a710-4f6046157012",
                    "sense_definition": "A large, long-legged bird of prey found in Africa, known as the secretary bird.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We saw a <w>secretary</w> bird hunting snakes in the tall grass.",
                            "target_lang_translations": {
                                "ru": "Мы видели, как <w>птица-секретарь</w> охотилась на змей в высокой траве."
                            }
                        },
                        {
                            "text": "The <w>secretary</w> is easily recognized by the crest of feathers on its head.",
                            "target_lang_translations": {
                                "ru": "<w>Птицу-секретаря</w> легко узнать по хохолку из перьев на голове."
                            }
                        }
                    ],
                    "common_phrases": [
                        "secretary bird"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная длинноногая хищная птица, обитающая в Африке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "птица-секретарь",
                                "target_lang_sense_clarification": "зоология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f95bbe9b-ca48-3d36-ba7c-01855dc6bd58",
                    "sense_definition": "Someone entrusted with a secret or private matters; a confidant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "confidant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was the king's closest <w>secretary</w>, knowing all the affairs of the state.",
                            "target_lang_translations": {
                                "ru": "Он был ближайшим <w>доверенным лицом</w> короля и знал все государственные дела."
                            }
                        },
                        {
                            "text": "She acted as a <w>secretary</w> to her friend's hidden plans.",
                            "target_lang_translations": {
                                "ru": "Она была <w>поверенной</w> в тайных планах своей подруги."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "original meaning"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кому доверены секреты или личные дела; доверенное лицо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доверенное лицо",
                                "target_lang_sense_clarification": "конфидент"
                            },
                            {
                                "target_lang_word": "поверенный",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3f3df513-f3f4-3ff1-9844-320dbe3c3eaf",
                    "sense_definition": "A town located in the state of Maryland.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited the small town of <w>Secretary</w> last summer.",
                            "target_lang_translations": {
                                "ru": "Прошлым летом они посетили маленький городок <w>Секретери</w>."
                            }
                        },
                        {
                            "text": "<w>Secretary</w> is situated on the Choptank River in Maryland.",
                            "target_lang_translations": {
                                "ru": "<w>Секретери</w> расположен на реке Чоптанк в Мэриленде."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город, расположенный в штате Мэриленд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Секретери",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "597c9e7d-bbdf-38e8-8ce6-1fe934705e55",
                    "sense_definition": "To work or serve in the capacity of a secretary.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>secretaried</w> for the local club for many years.",
                            "target_lang_translations": {
                                "ru": "Она <w>секретарствовала</w> в местном клубе много лет."
                            }
                        },
                        {
                            "text": "He has <w>secretaried</w> for several authors during his career.",
                            "target_lang_translations": {
                                "ru": "За свою карьеру он <w>поработал секретарём</w> у нескольких авторов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работать или служить в качестве секретаря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секретарствовать",
                                "target_lang_sense_clarification": "работать секретарём"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "e6d2c6d1-d84f-3787-9a28-25c01c938037",
                    "sense_definition": "Pogardliwe lub lekceważące określenie księdza katolickiego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "priest_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie dam temu chciwemu <w>popowi</w> ani grosza na tacę.",
                            "target_lang_translations": {
                                "ru": "Я не дам этому жадному <w>попу</w> ни гроша на поднос."
                            }
                        },
                        {
                            "text": "Ludzie we wsi narzekali na nowego <w>popa</w>.",
                            "target_lang_translations": {
                                "ru": "Люди в деревне жаловались на нового <w>попа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "klecha",
                        "katabas"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pogardliwie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное или пренебрежительное название католического священника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поп",
                                "target_lang_sense_clarification": "пренебр."
                            }
                        ]

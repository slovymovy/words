    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a2af9bb-6155-3e3f-b575-67110d4d6407",
                    "sense_definition": "The process of becoming pregnant involving fertilization.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The moment of <w>conception</w> marks the beginning of biological development.",
                            "target_lang_translations": {
                                "ru": "Момент <w>зачатия</w> знаменует начало биологического развития."
                            }
                        },
                        {
                            "text": "Doctors can estimate the date of <w>conception</w> based on the size of the fetus.",
                            "target_lang_translations": {
                                "ru": "Врачи могут определить дату <w>зачатия</w> по размеру плода."
                            }
                        }
                    ],
                    "synonyms": [
                        "fertilization"
                    ],
                    "common_phrases": [
                        "moment of conception",
                        "prevention of conception"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс наступления беременности, включающий оплодотворение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачатие",
                                "target_lang_sense_clarification": "биологический процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69469130-763e-3f6d-8350-7055ee8d9234",
                    "sense_definition": "An idea, belief, or plan formed in the mind.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_construct",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>conception</w> of a successful life is very different from mine.",
                            "target_lang_translations": {
                                "ru": "Её <w>представление</w> об успешной жизни сильно отличается от моего."
                            }
                        },
                        {
                            "text": "The architect explained his original <w>conception</w> for the new museum design.",
                            "target_lang_translations": {
                                "ru": "Архитектор объяснил свою первоначальную <w>концепцию</w> дизайна нового музея."
                            }
                        }
                    ],
                    "synonyms": [
                        "idea",
                        "notion",
                        "concept",
                        "plan"
                    ],
                    "antonyms": [
                        "misconception"
                    ],
                    "common_phrases": [
                        "original conception",
                        "conception of the world"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идея, убеждение или план, сформированные в уме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концепция",
                                "target_lang_sense_clarification": "система взглядов, замысел"
                            },
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "взгляд, мнение, образ"
                            },
                            {
                                "target_lang_word": "идея",
                                "target_lang_sense_clarification": "мысль, план"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "034de16e-015d-3724-8eb2-b262553e95eb",
                    "sense_definition": "The ability to understand or imagine something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has absolutely no <w>conception</w> of how much work is required for this project.",
                            "target_lang_translations": {
                                "ru": "Он не имеет абсолютно никакого <w>понятия</w> о том, сколько работы требуется для этого проекта."
                            }
                        },
                        {
                            "text": "The vast distances in the universe are almost beyond human <w>conception</w>.",
                            "target_lang_translations": {
                                "ru": "Огромные расстояния во вселенной почти за гранью человеческого <w>понимания</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "understanding",
                        "grasp"
                    ],
                    "common_phrases": [
                        "no conception of",
                        "beyond conception"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность понять или представить что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понятие",
                                "target_lang_sense_clarification": "осведомлённость, знание"
                            },
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "понимание, способность вообразить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
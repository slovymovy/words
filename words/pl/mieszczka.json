            "senses": [
                {
                    "sense_id": "652cce6f-b57f-340e-8a91-de83b1b42571",
                    "sense_definition": "Żeńska forma słowa <w>mieszczuch</w>; kobieta mieszkająca w mieście, przywiązana do miejskiego trybu życia i wygód, często nielubiąca wsi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "city_dweller",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako typowa <w>mieszczka</w>, narzekała na błoto i brak chodników na wsi.",
                            "target_lang_translations": {
                                "ru": "Как типичная <w>горожанка</w>, она жаловалась на грязь и отсутствие тротуаров в деревне."
                            }
                        },
                        {
                            "text": "Moja ciocia to wygodna <w>mieszczka</w>, która nie wyobraża sobie życia bez galerii handlowych.",
                            "target_lang_translations": {
                                "ru": "Моя тетя — любящая комфорт <w>горожанка</w>, которая не представляет себе жизни без торговых центров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "typowa mieszczka",
                        "wygodna mieszczka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "female equivalent of mieszczuch"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская форма слова mieszczuch; жительница города, привязанная к городскому образу жизни и удобствам, часто не любящая деревню."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горожанка",
                                "target_lang_sense_clarification": "жительница города"
                            },
                            {
                                "target_lang_word": "городская жительница",
                                "target_lang_sense_clarification": "нейтральное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86e2b93c-1008-34d7-8cf4-d1b72c1e2815",
                    "sense_definition": "Kobieta należąca do mieszczaństwa (historycznej warstwy społecznej mieszkańców miast).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_class",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bogata <w>mieszczka</w> ufundowała nowy ołtarz w miejscowym kościele.",
                            "target_lang_translations": {
                                "ru": "Богатая <w>мещанка</w> пожертвовала средства на новый алтарь в местном костеле."
                            }
                        },
                        {
                            "text": "Strój tej <w>mieszczki</w> świadczył o jej wysokiej pozycji w mieście.",
                            "target_lang_translations": {
                                "ru": "Наряд этой <w>горожанки</w> свидетельствовал о её высоком положении в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieszczanka"
                    ],
                    "common_phrases": [
                        "bogata mieszczka",
                        "prawa mieszczki"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, принадлежащая к мещанству (историческому социальному слою жителей городов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мещанка",
                                "target_lang_sense_clarification": "сословие"
                            },
                            {
                                "target_lang_word": "горожанка",
                                "target_lang_sense_clarification": "исторический контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a9de7db-c49b-37b4-bfaf-607f94ff700a",
                    "sense_definition": "Kobieta niewykształcona, o wąskich horyzontach, a mająca o sobie wysokie mniemanie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pejorative_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zachowywała się jak ograniczona <w>mieszczka</w>, która gardzi wszystkim, czego nie rozumie.",
                            "target_lang_translations": {
                                "ru": "Она вела себя как ограниченная <w>мещанка</w>, презирающая всё, чего не понимает."
                            }
                        },
                        {
                            "text": "To była zwykła, zadufana w sobie <w>mieszczka</w> bez krzty ogłady.",
                            "target_lang_translations": {
                                "ru": "Это была обычная, самодовольная <w>мещанка</w> без капли воспитания."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Необразованная женщина с узким кругозором, но высокого мнения о себе."
                    },

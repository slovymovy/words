                {
                    "sense_id": "49079421-d2d2-322b-ac85-f510d61d14d3",
                    "sense_definition": "Pozyskiwać nowych ludzi do wojska, organizacji lub do pracy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Armia musiała aktywnie <werbować> ochotników, aby uzupełnić braki kadrowe w jednostkach.",
                            "target_lang_translations": {
                                "ru": "Армии приходилось активно <вербовать> добровольцев, чтобы восполнить нехватку личного состава в подразделениях."
                            }
                        },
                        {
                            "text": "Nasza firma planuje <werbować> najlepszych specjalistów z branży informatycznej.",
                            "target_lang_translations": {
                                "ru": "Наша компания планирует <вербовать> лучших специалистов из IT-индустрии."
                            }
                        }
                    ],
                    "synonyms": [
                        "rekrutować",
                        "zaciągać",
                        "zapisywać"
                    ],
                    "antonyms": [
                        "zwalniać"
                    ],
                    "common_phrases": [
                        "werbować ochotników",
                        "werbować do wojska",
                        "werbować nowych członków",
                        "werbować pracowników"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанимать или привлекать новых людей в армию, организацию или на работу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вербовать",
                                "target_lang_sense_clarification": "на службу или работу"
                            },
                            {
                                "target_lang_word": "набирать",
                                "target_lang_sense_clarification": "персонал, людей"
                            },
                            {
                                "target_lang_word": "рекрутировать",
                                "target_lang_sense_clarification": "официально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eaf78458-dca7-323d-87ab-cba0680325a8",
                    "sense_definition": "Usilnie namawiać kogoś do poparcia jakiejś idei, udziału w spisku lub do tajnej współpracy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tajne służby próbowały <werbować> urzędników państwowych do współpracy wywiadowczej.",
                            "target_lang_translations": {
                                "ru": "Спецслужбы пытались <вербовать> государственных чиновников для сотрудничества с разведкой."
                            }
                        },
                        {
                            "text": "Politycy jeżdżą po całym kraju, aby <werbować> zwolenników dla swojej partii.",
                            "target_lang_translations": {
                                "ru": "Политики ездят по всей стране, чтобы <вербовать> сторонников для своей партии."
                            }
                        }
                    ],
                    "synonyms": [

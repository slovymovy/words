            "senses": [
                {
                    "sense_id": "ce784b1d-a05d-38eb-a756-55b3420e40f4",
                    "sense_definition": "Het lichaam van een mens of dier zonder ledematen, kop of staart.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "lichaamsdeel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleet traint zijn <w>romp</w> voor meer stabiliteit.",
                            "target_lang_translations": {
                                "ru": "Атлет тренирует свой <w>торс</w> для большей устойчивости."
                            }
                        },
                        {
                            "text": "Op de foto was alleen de <w>romp</w> van het standbeeld overgebleven.",
                            "target_lang_translations": {
                                "ru": "На фотографии остался только <w>торс</w> статуи."
                            }
                        }
                    ],
                    "synonyms": [
                        "torso"
                    ],
                    "common_phrases": [
                        "de menselijke romp",
                        "romp en ledematen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тело человека или животного без конечностей, головы и хвоста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туловище",
                                "target_lang_sense_clarification": "анатомический термин"
                            },
                            {
                                "target_lang_word": "торс",
                                "target_lang_sense_clarification": "человеческое тело, особенно в искусстве или спорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7df8de5c-de42-3cb4-8838-d81579dc08b2",
                    "sense_definition": "Het hoofdgedeelte van een schip of vliegtuig.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "voertuigonderdeel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>romp</w> van het schip is gemaakt van sterk staal.",
                            "target_lang_translations": {
                                "ru": "<w>Корпус</w> корабля изготовлен из прочной стали."
                            }
                        },
                        {
                            "text": "Monteurs inspecteren de <w>romp</w> van het vliegtuig op scheuren.",
                            "target_lang_translations": {
                                "ru": "Механики осматривают <w>фюзеляж</w> самолета на наличие трещин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de romp van een schip",
                        "een stalen romp"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "fa9f29ea-75ba-3785-8aaf-21632bbad563",
                    "sense_definition": "bóg słońca w mitologii rzymskiej, utożsamiany z greckim Heliosem",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mythology_roman",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Rzymianie oddawali cześć bogu <w>Sol</w>, prosząc go o pomyślność i urodzaj.",
                            "target_lang_translations": {
                                "ru": "Римляне поклонялись богу <w>Сол</w>, прося его о благополучии и урожае."
                            }
                        },
                        {
                            "text": "Świątynia poświęcona bóstwu <w>Sol</w> Invictus była ważnym miejscem kultu w cesarstwie.",
                            "target_lang_translations": {
                                "ru": "Храм, посвященный божеству <w>Сол</w> Инвиктус, был важным местом культа в империи."
                            }
                        }
                    ],
                    "synonyms": [
                        "Helios"
                    ],
                    "antonyms": [
                        "Luna"
                    ],
                    "common_phrases": [
                        "Sol Invictus"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mitologia rzymska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бог Солнца в римской мифологии, отождествляемый с греческим Гелиосом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сол",
                                "target_lang_sense_clarification": "римский бог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87c16005-ad81-3c52-99cd-e2f85a4f4e21",
                    "sense_definition": "bogini słońca w mitologii nordyckiej, powożąca rydwanem słońca",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mythology_norse",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "W dawnych wierzeniach nordyckich <w>Sol</w> ucieka przed wilkiem, który pragnie ją pożreć.",
                            "target_lang_translations": {
                                "ru": "В древних скандинавских верованиях <w>Соль</w> убегает от волка, который хочет её пожрать."
                            }
                        },
                        {
                            "text": "Bratem bogini <w>Sol</w> jest Mani, który włada księżycem.",
                            "target_lang_translations": {
                                "ru": "Братом богини <w>Соль</w> является Мани, который управляет луной."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mitologia nordycka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Богиня Солнца в скандинавской мифологии, управляющая солнечной колесницей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Соль",
                                "target_lang_sense_clarification": "скандинавская богиня"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7a1398ae-0d07-3696-b785-001a06a4d5aa",
                    "sense_definition": "nazwa solmizacyjna piątego stopnia gamy C-dur, odpowiadająca dźwiękowi G",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_note",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas lekcji muzyki uczyliśmy się śpiewać gamę, kładąc nacisk na czyste <w>sol</w>.",
                            "target_lang_translations": {
                                "ru": "На уроке музыки мы учились петь гамму, делая акцент на чистой <w>соль</w>."
                            }
                        },
                        {
                            "text": "W tym utworze po nucie fa następuje <w>sol</w>, co tworzy ciekawe napięcie.",
                            "target_lang_translations": {
                                "ru": "В этом произведении за нотой фа следует <w>соль</w>, что создает интересное напряжение."
                            }
                        }
                    ],
                    "synonyms": [
                        "G"
                    ],
                    "common_phrases": [
                        "klucz wiolinowy",
                        "gama C-dur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сольмизационное название пятой ступени гаммы до мажор, соответствующее ноте G"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соль",
                                "target_lang_sense_clarification": "нота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15194876-1ba1-3a1b-b5b1-1e34f2be7ff2",
                    "sense_definition": "jednostka czasu odpowiadająca dobie na Marsie, trwająca nieco dłużej niż doba ziemska",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy_mars",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Łazik marsjański przetrwał już setny <w>sol</w> swojej misji badawczej.",
                            "target_lang_translations": {
                                "ru": "Марсоход пережил уже сотый <w>сол</w> своей исследовательской миссии."
                            }
                        },
                        {
                            "text": "Jeden <w>sol</w> trwa około dwudziestu czterech godzin i trzydziestu dziewięciu minut.",
                            "target_lang_translations": {
                                "ru": "Один <w>сол</w> длится около двадцати четырех часов и тридцати девяти минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "marsjański sol"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "astronomia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица времени, соответствующая суткам на Марсе, длящаяся немного дольше земных суток"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сол",
                                "target_lang_sense_clarification": "марсианские сутки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d878ae7-4209-32c0-b7d1-4cb198b1757f",
                    "sense_definition": "jednostka monetarna używana w Peru",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wymieniłem dolary na lokalną walutę, aby mieć przy sobie każdy potrzebny <w>sol</w>.",
                            "target_lang_translations": {
                                "ru": "Я обменял доллары на местную валюту, чтобы иметь при себе каждый необходимый <w>соль</w>."
                            }
                        },
                        {
                            "text": "Cena tej pamiątki wynosiła zaledwie jeden <w>sol</w>, co było bardzo tanio.",
                            "target_lang_translations": {
                                "ru": "Цена этого сувенира составляла всего один <w>соль</w>, что было очень дешево."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nowy sol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная единица, используемая в Перу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соль",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                }
            ]
        }
    ]

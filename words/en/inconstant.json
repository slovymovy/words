    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "33f34dcf-686f-3be1-9579-53e2a59b3ba6",
                    "sense_definition": "Likely to change frequently without apparent reason; not steady or reliable.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "changeable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>inconstant</w> weather made it difficult to plan an outdoor picnic.",
                            "target_lang_translations": {
                                "ru": "<w>Изменчивая</w> погода затруднила планирование пикника на свежем воздухе."
                            }
                        },
                        {
                            "text": "Because of his <w>inconstant</w> nature, he struggles to keep a steady job.",
                            "target_lang_translations": {
                                "ru": "Из-за своего <w>непостоянного</w> характера ему трудно удержаться на постоянной работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "changeable",
                        "variable",
                        "unstable",
                        "fickle",
                        "erratic"
                    ],
                    "antonyms": [
                        "constant",
                        "steady",
                        "stable",
                        "reliable"
                    ],
                    "common_phrases": [
                        "inconstant nature",
                        "inconstant weather",
                        "inconstant moon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный часто меняться без видимой причины; неустойчивый или ненадежный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изменчивый",
                                "target_lang_sense_clarification": "часто меняющийся (о погоде, обстоятельствах)"
                            },
                            {
                                "target_lang_word": "непостоянный",
                                "target_lang_sense_clarification": "склонный к переменам (о характере, привычках)"
                            },
                            {
                                "target_lang_word": "неустойчивый",
                                "target_lang_sense_clarification": "лишенный стабильности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc227a65-8569-3365-87a9-f843cec250de",
                    "sense_definition": "Not faithful or loyal to a romantic partner.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "disloyal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The novel tells the tragic story of a devoted wife and her <w>inconstant</w> husband.",
                            "target_lang_translations": {
                                "ru": "Роман рассказывает трагическую историю преданной жены и её <w>непостоянного</w> мужа."
                            }
                        },
                        {
                            "text": "He was accused of being an <w>inconstant</w> lover who broke many hearts.",
                            "target_lang_translations": {
                                "ru": "Его обвиняли в том, что он <w>непостоянный</w> любовник, разбивший много сердец."
                            }
                        }
                    ],
                    "synonyms": [
                        "unfaithful",
                        "disloyal",
                        "fickle"
                    ],
                    "antonyms": [
                        "faithful",
                        "loyal",
                        "constant"
                    ],
                    "common_phrases": [
                        "inconstant lover",
                        "inconstant heart",
                        "inconstant affections"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не хранящий верность или преданность романтическому партнеру."
                    },
                    "translations": {
                        "ru": [
                            {

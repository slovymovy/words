        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c90fbb9-522c-310b-92b6-2665b9ed91f0",
                    "sense_definition": "A loud alarm or signaling device, especially one on a motor vehicle.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The driver honked his <w>horn</w> to warn the pedestrians.",
                            "target_lang_translations": {
                                "ru": "Водитель посигналил в <w>клаксон</w>, чтобы предупредить пешеходов."
                            }
                        },
                        {
                            "text": "I heard the blast of a car <w>horn</w> outside my window.",
                            "target_lang_translations": {
                                "ru": "Я услышал звук автомобильного <w>гудка</w> за окном."
                            }
                        }
                    ],
                    "synonyms": [
                        "hooter",
                        "klaxon"
                    ],
                    "common_phrases": [
                        "honk the horn",
                        "car horn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство для подачи громких звуковых сигналов, особенно на автомобиле"
                    },
                    "translations": {

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уход",
                                "target_lang_sense_clarification": "прекращение деятельности или участия"
                            },
                            {
                                "target_lang_word": "удаление",
                                "target_lang_sense_clarification": "уход в отдельное помещение или уединение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "rajca",
        "rajcowny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2845cc6a-450e-3fd8-9360-0df166307ccc",
                    "sense_definition": "Wywoływać u kogoś silne zainteresowanie, ekscytację lub podniecenie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "excitement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy model samochodu strasznie go <w>rajcuje</w>.",
                            "target_lang_translations": {
                                "ru": "Эта новая модель машины жутко его <w>заводит</w>."
                            }
                        },
                        {
                            "text": "Nie wiem, co cię tak <w>rajcuje</w> w tej głośnej muzyce.",
                            "target_lang_translations": {
                                "ru": "Не знаю, что тебя так <w>заводит</w> в этой громкой музыке."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekscytować",
                        "podniecać",
                        "podniecać się"
                    ],
                    "antonyms": [
                        "nudzić"
                    ],
                    "common_phrases": [
                        "to mnie rajcuje",
                        "nic go nie rajcuje"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-то сильный интерес, волнение или возбуждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заводить",
                                "target_lang_sense_clarification": "вызывать сильный интерес или возбуждение (разг.)"
                            },
                            {
                                "target_lang_word": "возбуждать",
                                "target_lang_sense_clarification": "вызывать эмоциональный или физический подъем"
                            },
                            {
                                "target_lang_word": "драйвить",
                                "target_lang_sense_clarification": "давать энергию, сильно увлекать (сленг)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "784df51f-8ddd-3497-af2f-e4745b4db5eb",
                    "sense_definition": "Odczuwać wielką radość, zachwyt lub ekscytację z jakiegoś powodu (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "excitement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat ciągle <w>rajcuje się</w> nowymi grami komputerowymi.",
                            "target_lang_translations": {
                                "ru": "Мой брат постоянно <w>тащится</w> от новых компьютерных игр."
                            }
                        },
                        {
                            "text": "Nie ma czym <w>się rajcować</w>, to tylko zwykły telefon.",
                            "target_lang_translations": {
                                "ru": "Тут не с чего <w>тащиться</w>, это просто обычный телефон."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekscytować się",
                        "podniecać się"
                    ],
                    "antonyms": [
                        "nudzić się"
                    ],
                    "common_phrases": [
                        "rajcować się czymś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать огромную радость, восторг или возбуждение по какому-либо поводу (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащиться",
                                "target_lang_sense_clarification": "получать огромное удовольствие от чего-либо (сленг)"
                            },
                            {
                                "target_lang_word": "фанатеть",
                                "target_lang_sense_clarification": "быть в восторге, сильно увлекаться"
                            },
                            {
                                "target_lang_word": "кайфовать",
                                "target_lang_sense_clarification": "наслаждаться чем-либо (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbf6bc81-2edd-35c4-9bf7-677570e639ac",
                    "sense_definition": "Głośno i z ożywieniem rozmawiać, dyskutować lub opowiadać o czymś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "talking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi wyszli przed dom i <w>rajcowali</w> do późnego wieczora.",
                            "target_lang_translations": {
                                "ru": "Соседи вышли к дому и <w>болтали</w> до позднего вечера."
                            }
                        },
                        {
                            "text": "Przekupki na targu <w>rajcowały</w> o najnowszych plotkach z okolicy.",
                            "target_lang_translations": {
                                "ru": "Торговки на рынке <w>судачили</w> о последних местных сплетнях."
                            }
                        }
                    ],
                    "synonyms": [
                        "dyskutować",
                        "rozprawiać"
                    ],
                    "antonyms": [
                        "milczeć"
                    ],
                    "common_phrases": [
                        "rajcować o polityce"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "często spotykane w gwarze poznańskiej lub śląskiej"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко и оживленно разговаривать, дискутировать или рассказывать о чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болтать",
                                "target_lang_sense_clarification": "оживленно беседовать"
                            },
                            {
                                "target_lang_word": "судачить",
                                "target_lang_sense_clarification": "сплетничать, обсуждать новости"
                            },
                            {
                                "target_lang_word": "трепаться",
                                "target_lang_sense_clarification": "долго разговаривать ни о чем (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ca9aebb-78e3-34ee-83a3-f6994fa7a73b",
                    "sense_definition": "Robić hałas, głośno się zachowywać (często o ptakach lub grupie ludzi).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "talking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stadko wróbli głośno <w>rajcuje</w> w krzakach za domem.",
                            "target_lang_translations": {
                                "ru": "Стайка воробьев громко <w>галдит</w> в кустах за домом."
                            }
                        },
                        {
                            "text": "Dzieciaki <w>rajcowały</w> na podwórku, aż sąsiedzi zwrócili im uwagę.",
                            "target_lang_translations": {
                                "ru": "Дети <w>галдели</w> во дворе, пока соседи не сделали им замечание."
                            }
                        }
                    ],
                    "synonyms": [
                        "hałasować"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шуметь, громко вести себя (часто о птицах или группе людей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галдеть",
                                "target_lang_sense_clarification": "громко и беспорядочно кричать (о птицах или людях)"
                            },
                            {
                                "target_lang_word": "шуметь",
                                "target_lang_sense_clarification": "издавать громкие звуки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1d07d58f-1de3-3abe-9a08-fddf6a88e0ca",
                    "sense_definition": "Consisting of small broken fragments or loose pieces, especially referring to floating ice.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "texture_composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ship moved slowly through the <w>brashy</w> waters filled with small chunks of ice.",
                            "target_lang_translations": {
                                "ru": "Корабль медленно двигался сквозь воды, полные <w>мелкобитого</w> льда и небольших ледяных глыб."
                            }
                        },
                        {
                            "text": "We encountered <w>brashy</w> ice that made navigation difficult near the shore.",
                            "target_lang_translations": {
                                "ru": "Мы столкнулись с <w>мелкобитым</w> льдом, который затруднял навигацию у берега."
                            }
                        }
                    ],
                    "synonyms": [
                        "fragmented",
                        "broken",
                        "slushy"
                    ],
                    "common_phrases": [
                        "brashy ice",
                        "brashy floes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из мелких обломков или разрозненных кусков, особенно применительно к плавучему льду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелкобитый",
                                "target_lang_sense_clarification": "о льде"
                            },
                            {
                                "target_lang_word": "обломочный",
                                "target_lang_sense_clarification": "состоящий из обломков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "932f321b-d6b4-3f4c-963b-7681d336841a",
                    "sense_definition": "Having a brittle or crumbly texture, often describing soil or ground mixed with loose stones.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "texture_composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer found the <w>brashy</w> soil difficult to plow due to the many loose stones.",
                            "target_lang_translations": {
                                "ru": "Фермер обнаружил, что <w>щебнистую</w> почву трудно пахать из-за множества камней."
                            }
                        },
                        {
                            "text": "Walking up the <w>brashy</w> slope was dangerous because the ground crumbled underfoot.",
                            "target_lang_translations": {
                                "ru": "Подниматься по <w>щебнистому</w> склону было опасно, так как земля крошилась под ногами."
                            }
                        }
                    ],
                    "synonyms": [
                        "crumbly",
                        "brittle",
                        "friable",
                        "stony"
                    ],
                    "antonyms": [
                        "solid",
                        "compact"
                    ],
                    "common_phrases": [
                        "brashy soil",
                        "brashy ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий ломкую или крошащуюся структуру; часто описывает почву, смешанную с мелкими камнями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щебнистый",
                                "target_lang_sense_clarification": "о почве с камнями"
                            },
                            {
                                "target_lang_word": "ломкий",
                                "target_lang_sense_clarification": "хрупкий, крошащийся"
                            }
                        ]
                    }
                }
            ]
        }
    ]

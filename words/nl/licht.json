                    "antonyms": [
                        "zwaar"
                    ],
                    "common_phrases": [
                        "lichte griep",
                        "lichte twijfel"
                    ],
                    "target_lang_definitions": {
                        "ru": "В незначительной степени; не серьезный или сильный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "незначительный"
                            },
                            {
                                "target_lang_word": "слабый",
                                "target_lang_sense_clarification": "малой интенсивности"
                            },
                            {
                                "target_lang_word": "небольшой",
                                "target_lang_sense_clarification": "малый по размеру или силе"
                            }
                        ]
                    }

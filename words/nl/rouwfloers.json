            "senses": [
                {
                    "sense_id": "8176d66f-ac31-3cce-9cc3-b457d2ee308b",
                    "sense_definition": "Een zwarte doek, sluier of crêpestof die als teken van rouw wordt gedragen of over iets heen wordt gelegd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De weduwe droeg een hoed met een lange <w>rouwfloers</w> tijdens de ceremonie.",
                            "target_lang_translations": {
                                "ru": "Вдова была в шляпе с длинной <w>траурной вуалью</w> во время церемонии.",
                                "en": "The widow wore a hat with a long <w>mourning veil</w> during the ceremony."
                            }
                        },
                        {
                            "text": "Als teken van respect was het portret van de overledene bedekt met een <w>rouwfloers</w>.",
                            "target_lang_translations": {
                                "ru": "В знак уважения портрет покойного был накрыт <w>траурным крепом</w>.",
                                "en": "As a sign of respect, the portrait of the deceased was covered with <w>mourning crepe</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rouwsluier",
                        "krip",
                        "pleureuse"
                    ],
                    "common_phrases": [
                        "met rouwfloers omhangen",
                        "een zware rouwfloers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чёрная ткань, вуаль или креп, которые носят или которыми накрывают что-либо в знак траура.",
                        "en": "A black cloth, veil, or crepe fabric worn or draped over something as a sign of mourning."
                    },
                    "translations": {
                        "ru": [
                            {

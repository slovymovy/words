    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a0e9fcb-891a-3d7e-9499-63401436911a",
                    "sense_definition": "Een gebogen voorwerp, meestal van metaal, waaraan men jassen of andere voorwerpen kan ophangen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_hook",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hang je natte jas maar aan de <w>haak</w> in de gang.",
                            "target_lang_translations": {
                                "ru": "Повесь своё мокрое пальто на <w>крючок</w> в коридоре."
                            }
                        },
                        {
                            "text": "De handdoek viel op de grond omdat de <w>haak</w> loszat.",
                            "target_lang_translations": {
                                "ru": "Полотенце упало на пол, потому что <w>крючок</w> разболтался."
                            }
                        }
                    ],
                    "synonyms": [
                        "hanger"
                    ],
                    "common_phrases": [
                        "aan de haak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изогнутый предмет, обычно металлический, на который вешают пальто или другие предметы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крючок",
                                "target_lang_sense_clarification": "для одежды"
                            },
                            {
                                "target_lang_word": "вешалка",
                                "target_lang_sense_clarification": "крючок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a64a8ea8-e37b-3054-b603-05380c6ec68c",
                    "sense_definition": "Een leesteken, zoals ( ) of [ ], dat wordt gebruikt om tekst of getallen in te sluiten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de extra informatie in de zin tussen <w>haken</w>.",
                            "target_lang_translations": {
                                "ru": "Помести дополнительную информацию в предложении в <w>скобки</w>."
                            }
                        },
                        {
                            "text": "Je bent vergeten de sluitende <w>haak</w> te typen.",
                            "target_lang_translations": {
                                "ru": "Ты забыл напечатать закрывающую <w>скобку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "haakje"
                    ],
                    "common_phrases": [
                        "tussen haken",
                        "openende haak",
                        "sluitende haak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак препинания, такой как ( ) или [ ], используемый для выделения текста или чисел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скобка",
                                "target_lang_sense_clarification": "знак препинания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00da3861-8a52-3633-811e-626f9c94edbb",
                    "sense_definition": "Een scherp, gebogen stukje metaal dat aan een vislijn wordt bevestigd om vissen te vangen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_hook",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De visser deed voorzichtig een worm aan de scherpe <w>haak</w>.",
                            "target_lang_translations": {
                                "ru": "Рыбак осторожно насадил червяка на острый <w>крючок</w>."
                            }
                        },
                        {
                            "text": "Er zat een grote snoek aan de <w>haak</w> van zijn hengel.",
                            "target_lang_translations": {
                                "ru": "На <w>крючке</w> его удочки была большая щука."
                            }
                        }
                    ],
                    "synonyms": [
                        "vishaak"
                    ],
                    "common_phrases": [
                        "aan de haak slaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый изогнутый кусок металла, прикрепляемый к леске для ловли рыбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крючок",
                                "target_lang_sense_clarification": "рыболовный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2101502c-bb99-356c-b635-b7c1a672da66",
                    "sense_definition": "Een gereedschap in de vorm van een rechte hoek, gebruikt om hoeken te meten of af te tekenen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De timmerman gebruikte een <w>haak</w> om de plank recht af te zagen.",
                            "target_lang_translations": {
                                "ru": "Плотник использовал <w>угольник</w>, чтобы ровно отпилить доску."
                            }
                        },
                        {
                            "text": "Leg de <w>haak</w> langs de rand om te zien of het haaks is.",
                            "target_lang_translations": {
                                "ru": "Приложи <w>угольник</w> к краю, чтобы проверить, прямой ли угол."
                            }
                        }
                    ],
                    "synonyms": [
                        "winkelhaak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент в форме прямого угла, используемый для измерения или разметки углов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угольник",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                }
            ]
        },
        {

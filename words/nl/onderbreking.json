{
    "word_family": [
        "onderbreken"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "778bda23-299c-350a-956c-2371a60262c7",
                    "sense_definition": "Het tijdelijk stoppen van een activiteit of proces, vaak door een onverwachte gebeurtenis of storing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door een technische storing was er een korte <w>onderbreking</w> van de uitzending.",
                            "target_lang_translations": {
                                "ru": "Из-за технического сбоя произошло кратковременное <w>прерывание</w> трансляции.",
                                "en": "Due to a technical failure, there was a short <w>interruption</w> of the broadcast."
                            }
                        },
                        {
                            "text": "Hij werkte urenlang door zonder enige <w>onderbreking</w>.",
                            "target_lang_translations": {
                                "ru": "Он работал часами напролет без какого-либо <w>перерыва</w>.",
                                "en": "He worked for hours without any <w>interruption</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verstoring",
                        "interruptie",
                        "stagnatie"
                    ],
                    "antonyms": [
                        "voortgang",
                        "continuering"
                    ],
                    "common_phrases": [
                        "zonder onderbreking",
                        "tijdelijke onderbreking",
                        "plotselinge onderbreking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временная приостановка деятельности или процесса, часто вызванная неожиданным событием или сбоем.",
                        "en": "The temporary stopping of an activity or process, often caused by an unexpected event or failure."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прерывание",
                                "target_lang_sense_clarification": "действие по остановке процесса, часто техническое или вынужденное"
                            },
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "прекращение движения или работы"
                            },
                            {
                                "target_lang_word": "сбой",
                                "target_lang_sense_clarification": "нарушение нормального хода работы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "interruption",
                                "target_lang_sense_clarification": "the act of stopping a process or activity"
                            },
                            {
                                "target_lang_word": "disruption",
                                "target_lang_sense_clarification": "a disturbance that interrupts an event or process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc51a832-3ab7-3bef-b7c6-0559dd821796",
                    "sense_definition": "Een korte periode van rust of stilstand tussen twee delen van een activiteit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een korte <w>onderbreking</w> ging de vergadering weer verder.",
                            "target_lang_translations": {
                                "ru": "После небольшого <w>перерыва</w> совещание продолжилось.",
                                "en": "After a short <w>break</w>, the meeting continued."
                            }
                        },
                        {
                            "text": "De spelers namen een <w>onderbreking</w> om wat water te drinken.",
                            "target_lang_translations": {
                                "ru": "Игроки сделали <w>паузу</w>, чтобы попить воды.",
                                "en": "The players took a <w>break</w> to drink some water."
                            }
                        }
                    ],
                    "synonyms": [
                        "pauze",
                        "rustpoos",
                        "rusttijd"
                    ],
                    "common_phrases": [
                        "korte onderbreking",
                        "na de onderbreking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий период отдыха или пауза между двумя частями деятельности.",
                        "en": "A short period of rest or pause between two parts of an activity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перерыв",
                                "target_lang_sense_clarification": "промежуток времени для отдыха или между действиями"
                            },
                            {
                                "target_lang_word": "пауза",
                                "target_lang_sense_clarification": "кратковременная остановка"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "a short rest period during an activity"
                            },
                            {
                                "target_lang_word": "pause",
                                "target_lang_sense_clarification": "a temporary stop or hesitation"
                            },
                            {
                                "target_lang_word": "intermission",
                                "target_lang_sense_clarification": "an interval between parts of a performance or event"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
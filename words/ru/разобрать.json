            "senses": [
                {
                    "sense_id": "dfad86b0-a2dd-3d90-b2f6-dad94603d827",
                    "sense_definition": "Разделить что-либо на составные части, разъединить детали.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец решил <разобрать> старый велосипед на запчасти.",
                            "target_lang_translations": {
                                "en": "Father decided to <w>take apart</w> the old bicycle for spare parts."
                            }
                        },
                        {
                            "text": "Нам нужно <разобрать> этот шкаф, чтобы вынести его из комнаты.",
                            "target_lang_translations": {
                                "en": "We need to <w>disassemble</w> this wardrobe to take it out of the room."
                            }
                        }
                    ],
                    "antonyms": [
                        "собрать"
                    ],
                    "common_phrases": [
                        "разобрать на запчасти",
                        "разобрать двигатель",
                        "разобрать конструкцию"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate an object into its component parts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take apart",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "disassemble",
                                "target_lang_sense_clarification": "technical/formal"
                            },
                            {
                                "target_lang_word": "dismantle",
                                "target_lang_sense_clarification": "structures/mechanisms"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1757dbd-7491-378b-89a5-dd9c5a18138d",
                    "sense_definition": "Рассортировать вещи, разложить их по местам, наводя порядок.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После поездки мне пришлось долго <разобрать> чемоданы.",
                            "target_lang_translations": {
                                "en": "After the trip, I had to <w>unpack</w> my suitcases for a long time."
                            }
                        },
                        {
                            "text": "Мама попросила меня <разобрать> вещи в шкафу.",
                            "target_lang_translations": {
                                "en": "Mom asked me to <w>sort out</w> the things in the closet."
                            }
                        }
                    ],
                    "antonyms": [
                        "собрать"
                    ],
                    "common_phrases": [
                        "разобрать вещи",
                        "разобрать сумки",
                        "разобрать документы"
                    ],
                    "target_lang_definitions": {
                        "en": "To organize items or remove contents from a container."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unpack",
                                "target_lang_sense_clarification": "suitcases/bags"
                            },
                            {
                                "target_lang_word": "sort out",
                                "target_lang_sense_clarification": "organize/tidy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b69549a4-017c-3906-8d3b-799d3ed7292a",
                    "sense_definition": "Приготовить постель ко сну, сняв покрывало или разложив её.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я так устал, что хочу скорее <разобрать> постель и уснуть.",
                            "target_lang_translations": {
                                "en": "I am so tired that I want to <w>turn down</w> the bed and fall asleep soon."
                            }
                        },
                        {
                            "text": "Бабушка помогла внуку <разобрать> кровать.",
                            "target_lang_translations": {
                                "en": "Grandma helped her grandson <w>prepare</w> the bed."
                            }
                        }
                    ],
                    "antonyms": [
                        "собрать"
                    ],
                    "common_phrases": [
                        "разобрать постель",
                        "разобрать кровать"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare a bed for sleeping by removing the cover."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn down",
                                "target_lang_sense_clarification": "prepare for sleep"
                            },
                            {
                                "target_lang_word": "prepare",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bedadf36-f730-3d56-8e2f-16bdf174cb51",
                    "sense_definition": "С трудом понять или распознать (написанное, услышанное, увиденное).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач написал рецепт так неразборчиво, что я не смог <разобрать> ни слова.",
                            "target_lang_translations": {
                                "en": "The doctor wrote the prescription so illegibly that I couldn't <w>make out</w> a single word."
                            }
                        },
                        {
                            "text": "Из-за сильного шума на улице было трудно <разобрать> голос собеседника.",
                            "target_lang_translations": {
                                "en": "Due to the loud noise outside, it was difficult to <w>distinguish</w> the interlocutor's voice."
                            }
                        }
                    ],
                    "synonyms": [
                        "понять",
                        "различить",
                        "распознать"
                    ],
                    "common_phrases": [
                        "с трудом разобрать",
                        "не разобрать ни слова",
                        "разобрать почерк"
                    ],
                    "target_lang_definitions": {
                        "en": "To manage to see, hear, or understand something unclear."

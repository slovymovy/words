                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Used by fans of the series."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращенное название телесериала «Доктор Кто»."
                    },
                    "translations": {

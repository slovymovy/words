        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "54ef6efa-7714-337c-bb56-a1b93aca0362",
                    "sense_definition": "To cut something short or trim it, usually with scissors or shears.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cutting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>clipped</w> the coupon out of the newspaper.",

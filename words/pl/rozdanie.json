            "senses": [
                {
                    "sense_id": "ca6e2ae3-9f89-3244-b629-a1aa64c9c4ae",
                    "sense_definition": "Uroczystość, podczas której wręczane są nagrody, dyplomy lub wyróżnienia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na tegorocznym <w>rozdaniu</w> Oscarów uhonorowano jednego Polaka.",
                            "target_lang_translations": {
                                "ru": "На сегодняшнем <w>вручении</w> «Оскаров» наградили одного поляка."
                            }
                        },
                        {
                            "text": "Uroczyste <w>rozdanie</w> dyplomów odbędzie się w głównej auli uniwersytetu.",
                            "target_lang_translations": {
                                "ru": "Торжественное <w>вручение</w> дипломов состоится в главной аудитории университета."
                            }
                        }
                    ],
                    "synonyms": [
                        "wręczenie"
                    ],
                    "common_phrases": [
                        "rozdanie nagród",
                        "rozdanie dyplomów",
                        "uroczyste rozdanie",
                        "gala rozdania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Церемония, во время которой вручаются награды, дипломы или знаки отличия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вручение",
                                "target_lang_sense_clarification": "церемония"
                            },
                            {
                                "target_lang_word": "церемония вручения",
                                "target_lang_sense_clarification": "полная фраза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a54d6d0-f94d-33f0-9eb2-a068300bdc7e",
                    "sense_definition": "Czynność polegająca na przekazaniu czegoś wielu osobom; rzeczownik odczasownikowy od: <w>rozdać</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Rozdanie</w> słodyczy przed obiadem było błędem, bo dzieci nie chciały jeść zupy.",
                            "target_lang_translations": {
                                "ru": "<w>Раздача</w> сладостей перед обедом была ошибкой, так как дети не хотели есть суп."
                            }
                        },
                        {
                            "text": "Nauczyciel poprosił dyżurnego o szybkie <w>rozdanie</w> kartek z testem.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил дежурного о быстрой <w>раздаче</w> листков с тестом."
                            }
                        }
                    ],
                    "antonyms": [
                        "zebranie"
                    ],
                    "common_phrases": [
                        "rozdanie majątku",
                        "rozdanie ulotek",
                        "rozdanie prezentów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по передаче чего-либо многим людям; отглагольное существительное от «раздать»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздача",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17e8bf3e-a026-3c8c-a02f-db48fcb70a76",
                    "sense_definition": "Konkurs internetowy lub akcja promocyjna, w której uczestnicy mogą wygrać darmowe produkty.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wzięłam udział w świątecznym <w>rozdaniu</w> na jej profilu i wygrałam zestaw kosmetyków.",
                            "target_lang_translations": {
                                "ru": "Я приняла участие в праздничном <w>розыгрыше</w> в её профиле и выиграла набор косметики."
                            }
                        },
                        {
                            "text": "Wyniki <w>rozdania</w> zostaną ogłoszone jutro wieczorem w relacji na żywo.",
                            "target_lang_translations": {
                                "ru": "Результаты <w>розыгрыша</w> будут объявлены завтра вечером в прямом эфире."
                            }
                        }
                    ],
                    "synonyms": [
                        "konkurs"
                    ],
                    "common_phrases": [
                        "wziąć udział w rozdaniu",
                        "wygrać rozdanie",
                        "szybkie rozdanie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "internet"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Интернет-конкурс или рекламная акция, в которой участники могут выиграть бесплатные продукты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розыгрыш",
                                "target_lang_sense_clarification": "конкурс"
                            },
                            {
                                "target_lang_word": "гивэвей",
                                "target_lang_sense_clarification": "интернет-сленг"
                            },
                            {
                                "target_lang_word": "раздача",
                                "target_lang_sense_clarification": "бесплатных предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4f7b6a1-5fc9-3512-9120-56847f3f634a",
                    "sense_definition": "Rozdzielenie kart do gry pomiędzy graczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W brydżu po <w>rozdaniu</w> kart następuje licytacja.",
                            "target_lang_translations": {
                                "ru": "В бридже после <w>сдачи</w> карт следует торговля."
                            }
                        },
                        {
                            "text": "To twoja kolej na <w>rozdanie</w>, więc potasuj dobrze talię.",
                            "target_lang_translations": {
                                "ru": "Твоя очередь на <w>сдачу</w>, так что хорошенько перетасуй колоду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ponowne rozdanie",
                        "kolej na rozdanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распределение игральных карт между игроками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдача",
                                "target_lang_sense_clarification": "карт"
                            },
                            {
                                "target_lang_word": "раздача",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d33ddcce-50ad-36d8-89c8-bb83cfa9eee1",
                    "sense_definition": "Pojedyncza partia gry w karty lub układ kart, jaki gracz otrzymał w jednej turze.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym <w>rozdaniu</w> atu są piki.",
                            "target_lang_translations": {
                                "ru": "В этой <w>раздаче</w> козыри — пики."
                            }
                        },
                        {
                            "text": "Mimo słabego początku, wygrałem to <w>rozdanie</w> dzięki dobrej strategii.",
                            "target_lang_translations": {
                                "ru": "Несмотря на слабое начало, я выиграл эту <w>раздачу</w> благодаря хорошей стратегии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobre rozdanie",
                        "wygrać rozdanie",
                        "kolejne rozdanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная партия в карты или набор карт, полученный игроком за один раз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздача",
                                "target_lang_sense_clarification": "партия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5f3f860-5c5b-31d8-9724-c82afc64c45f",
                    "sense_definition": "Rozdzielenie stanowisk, funkcji lub wpływów, zazwyczaj w kontekście politycznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zwycięskich wyborach zawsze przychodzi <w>rozdanie</w> posad między swoich.",
                            "target_lang_translations": {
                                "ru": "После победы на выборах всегда происходит <w>раздача</w> должностей своим."
                            }
                        },
                        {
                            "text": "Nowe <w>rozdanie</w> w rządzie zaskoczyło wielu komentatorów politycznych.",
                            "target_lang_translations": {
                                "ru": "Новый <w>расклад</w> в правительстве удивил многих политических обозревателей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nowe rozdanie",
                        "polityczne rozdanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распределение должностей, функций или влияния, обычно в политическом контексте."
                    },
                    "translations": {

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8644f185-5325-39d5-ab2a-50325a72df27",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kappen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kap</w> ermee, want ik ben veel te moe om door te gaan.",
                            "target_lang_translations": {
                                "en": "I <w>quit</w>, because I am way too tired to go on.",
                                "ru": "Я <w>завязываю</w> с этим, потому что я слишком устал, чтобы продолжать."
                            }
                        },
                        {
                            "text": "Elke zaterdag <w>kap</w> ik hout voor de open haard.",
                            "target_lang_translations": {
                                "en": "Every Saturday I <w>chop</w> wood for the fireplace.",
                                "ru": "Каждую субботу я <w>рублю</w> дрова для камина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik kap ermee"
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'kappen' (to chop, to quit).",
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола kappen (рубить, прекращать)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quit",
                                "target_lang_sense_clarification": "stop doing something"
                            },
                            {
                                "target_lang_word": "chop",
                                "target_lang_sense_clarification": "cut wood"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "завязываю",
                                "target_lang_sense_clarification": "прекращаю делать что-то (разг.)"
                            },
                            {
                                "target_lang_word": "рублю",
                                "target_lang_sense_clarification": "дрова или деревья"
                            },
                            {
                                "target_lang_word": "бросаю",
                                "target_lang_sense_clarification": "перестаю заниматься чем-то"
                            }
                        ]
                    }
                },

                            },
                            {
                                "target_lang_word": "совсем",
                                "target_lang_sense_clarification": "при отрицании (совсем не)"
                            },
                            {
                                "target_lang_word": "вообще",
                                "target_lang_sense_clarification": "разг. усиление (вообще не, вообще игнорировал)"
                            }
                        ]

                },
                {
                    "sense_id": "18c786d3-fc74-3045-98d3-2f62505089a7",
                    "sense_definition": "nagłe, przykre wydarzenie powodujące cierpienie psychiczne lub negatywny zwrot w jakiejś sprawie",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_misfortune",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagła śmierć ojca była dla niego ogromnym <w>ciosem</w>.",
                            "target_lang_translations": {
                                "ru": "Внезапная смерть отца стала для него огромным <w>ударом</w>."
                            }
                        },
                        {
                            "text": "Zwolnienie z pracy to bolesny <w>cios</w>, po którym trudno się podnieść.",
                            "target_lang_translations": {

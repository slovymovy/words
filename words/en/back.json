    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f14aa534-4a07-36f9-93c5-b06def9c3800",
                    "sense_definition": "To or in a previous condition, place, or situation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "return_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will come <w>back</w> to see you tomorrow.",
                            "target_lang_translations": {
                                "ru": "Я <w>вернусь</w> к тебе завтра."
                            }
                        },
                        {
                            "text": "Please give <w>back</w> the pen you borrowed.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отдай <w>назад</w> ручку, которую ты одолжил."
                            }
                        }
                    ],
                    "common_phrases": [
                        "come back",
                        "go back",
                        "give back",
                        "get back"
                    ],
                    "target_lang_definitions": {
                        "ru": "В прежнее состояние, место или ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наза́д",
                                "target_lang_sense_clarification": "возвращение в прежнее место или состояние"
                            },
                            {
                                "target_lang_word": "обра́тно",
                                "target_lang_sense_clarification": "возвращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad335ade-4bd2-3e16-b5b5-af0371affc96",
                    "sense_definition": "In a direction opposite to the usual forward movement; backwards.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rear_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please step <w>back</w> from the edge of the platform.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отойдите <w>назад</w> от края платформы."
                            }
                        },
                        {
                            "text": "He sat <w>back</w> in his comfortable chair.",
                            "target_lang_translations": {
                                "ru": "Он откинулся <w>назад</w> в своем удобном кресле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "step back",
                        "stand back",
                        "sit back",
                        "look back"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении, противоположном обычному движению вперед; вспять."

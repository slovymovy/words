    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b74be8e3-d3db-3e75-91fa-284594f67cbd",
                    "sense_definition": "Zasłaniać lub osłaniać kogoś lub coś, zapewniając ochronę przed czymś, na przykład przed wiatrem lub słońcem.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cover_protect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielki dąb skutecznie <w>słonił</w> nasz dom przed porywistym wiatrem.",
                            "target_lang_translations": {
                                "ru": "Большой дуб эффективно <w>заслонял</w> наш дом от порывистого ветра."
                            }
                        },
                        {
                            "text": "Matka instynktownie <w>słoniła</w> swoje dziecko przed niebezpieczeństwem.",
                            "target_lang_translations": {
                                "ru": "Мать инстинктивно <w>заслоняла</w> своего ребенка от опасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "osłaniać",
                        "zasłaniać",
                        "zakrywać",
                        "przysłaniać"
                    ],
                    "antonyms": [
                        "odsłaniać",
                        "odkrywać"
                    ],
                    "common_phrases": [
                        "słonić oczy",
                        "słonić twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Закрывать или заслонять кого-либо или что-либо, обеспечивая защиту, например, от ветра или солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслонять",
                                "target_lang_sense_clarification": "закрывать собой или чем-то от воздействия"
                            },
                            {
                                "target_lang_word": "прикрывать",
                                "target_lang_sense_clarification": "защищать, закрывая от чего-либо"
                            },
                            {
                                "target_lang_word": "защищать",
                                "target_lang_sense_clarification": "оберегать от опасности или воздействия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e800c03a-4f57-3f7c-8026-73e3ea2f9f7a",
                    "sense_definition": "Przebywać w nasłonecznionym miejscu, wystawiając ciało na działanie promieni słonecznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sun_exposure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem turyści całymi dniami <w>słonią się</w> na piaszczystej plaży.",
                            "target_lang_translations": {
                                "ru": "Летом туристы целыми днями <w>загорают</w> на песчаном пляже."
                            }
                        },
                        {
                            "text": "Nie <w>słoń się</w> zbyt długo, jeśli nie posmarowałeś się kremem z filtrem.",
                            "target_lang_translations": {
                                "ru": "Не <w>грейся на солнце</w> слишком долго, если не намазался солнцезащитным кремом."
                            }
                        }
                    ],
                    "synonyms": [
                        "przysłaniać"
                    ],
                    "common_phrases": [
                        "słonić się na słońcu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в солнечном месте, подставляя тело воздействию солнечных лучей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "греться на солнце",
                                "target_lang_sense_clarification": "находиться под теплыми лучами солнца"
                            },
                            {
                                "target_lang_word": "загорать",
                                "target_lang_sense_clarification": "принимать солнечные ванны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7e627aa-dd87-3976-b3ff-d88ec3b1f60b",
                    "sense_definition": "Chwiać się na nogach i tracić równowagę, zazwyczaj z powodu osłabienia, zmęczenia lub choroby.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stagger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ranny żołnierz <w>słonił się</w> na nogach, próbując dotrzeć do obozu.",
                            "target_lang_translations": {
                                "ru": "Раненый солдат <w>шатался</w> на ногах, пытаясь добраться до лагеря."
                            }
                        },
                        {
                            "text": "Ze zmęczenia ledwo szedł i co chwilę <w>słonił się</w> na boki.",
                            "target_lang_translations": {
                                "ru": "От усталости он едва шел и то и дело <w>качался</w> из стороны в сторону."
                            }
                        }
                    ],
                    "synonyms": [
                        "słaniać się"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "obecnie powszechna forma to 'słaniać się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шататься на ногах и терять равновесие, как правило, из-за слабости, усталости или болезни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шататься",
                                "target_lang_sense_clarification": "терять устойчивость при ходьбе"
                            },
                            {
                                "target_lang_word": "качаться",
                                "target_lang_sense_clarification": "быть неустойчивым, клониться в стороны"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
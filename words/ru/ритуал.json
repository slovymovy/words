    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bf030a23-4a4f-322e-b866-ae975c604f22",
                    "sense_definition": "Порядок обрядовых действий, совершаемых по определённым традиционным или религиозным правилам.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Древний <w>ритуал</w> требовал от участников полного молчания.",
                            "target_lang_translations": {
                                "en": "The ancient <w>ritual</w> required complete silence from the participants."
                            }
                        },
                        {
                            "text": "Мы с интересом наблюдали за тем, как местный шаман проводит свой таинственный <w>ритуал</w>.",
                            "target_lang_translations": {
                                "en": "We watched with interest as the local shaman performed his mysterious <w>ritual</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "обряд"
                    ],
                    "common_phrases": [
                        "древний ритуал",
                        "магический ритуал",
                        "ритуал погребения",
                        "совершить ритуал"
                    ],
                    "target_lang_definitions": {
                        "en": "A sequence of ceremonial actions performed according to certain traditional or religious rules."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ritual",
                                "target_lang_sense_clarification": "general term for a set of fixed actions"
                            },
                            {
                                "target_lang_word": "rite",
                                "target_lang_sense_clarification": "often refers to a specific religious act"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdb6560b-9170-3a87-8bfd-fab3185afb0e",
                    "sense_definition": "Строго установленный порядок проведения какой-либо официальной или торжественной церемонии.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Воинский <w>ритуал</w> смены караула всегда привлекает множество туристов.",
                            "target_lang_translations": {
                                "en": "The military <w>ritual</w> of the changing of the guard always attracts many tourists."
                            }
                        },
                        {
                            "text": "Дипломатический <w>ритуал</w> встречи посла был соблюдён до мельчайших деталей.",
                            "target_lang_translations": {
                                "en": "The diplomatic <w>ritual</w> of meeting the ambassador was observed to the smallest detail."
                            }
                        }
                    ],
                    "synonyms": [
                        "церемониал"
                    ],
                    "common_phrases": [
                        "воинский ритуал",
                        "соблюдать ритуал",
                        "ритуал встречи"
                    ],
                    "target_lang_definitions": {
                        "en": "A strictly established order for conducting an official or solemn ceremony."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ritual",
                                "target_lang_sense_clarification": "formal procedure or protocol"
                            },
                            {
                                "target_lang_word": "ceremonial",
                                "target_lang_sense_clarification": "system of rules and ceremonies"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3cdfd911-1736-35f3-a8ad-484542d7e18f",
                    "sense_definition": "Мнение, взгляд на что-либо или кого-либо, эмоциональная оценка.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "opinion_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <отношение> к работе всегда было очень серьёзным.",
                            "target_lang_translations": {
                                "en": "His <w>attitude</w> towards work was always very serious."
                            }
                        },
                        {
                            "text": "Я хочу изменить своё <отношение> к этой проблеме.",
                            "target_lang_translations": {
                                "en": "I want to change my <w>attitude</w> towards this problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "мнение",
                        "взгляд",
                        "позиция"
                    ],
                    "common_phrases": [
                        "моё отношение",
                        "хорошее отношение",
                        "отношение к жизни",
                        "изменить отношение"
                    ],
                    "target_lang_definitions": {
                        "en": "A settled way of thinking or feeling about someone or something, typically one that is reflected in a person's behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attitude",
                                "target_lang_sense_clarification": "opinion or mindset"
                            },
                            {
                                "target_lang_word": "view",
                                "target_lang_sense_clarification": "perspective or opinion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9d3ce11-620f-397f-9487-13e52709dbdc",
                    "sense_definition": "Взаимная связь, общение и взаимодействия между людьми, группами или странами (обычно во множественном числе).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Между нашими странами сложились дружественные <отношения>.",
                            "target_lang_translations": {
                                "en": "Friendly <w>relations</w> have developed between our countries."
                            }
                        },
                        {
                            "text": "Они решили прекратить свои <отношения> после долгой ссоры.",
                            "target_lang_translations": {
                                "en": "They decided to end their <w>relationship</w> after a long quarrel."
                            }
                        }
                    ],
                    "synonyms": [
                        "связи",
                        "контакты",
                        "взаимоотношения"
                    ],
                    "antonyms": [
                        "изоляция"
                    ],
                    "common_phrases": [
                        "международные отношения",
                        "выяснять отношения",
                        "семейные отношения",
                        "дипломатические отношения"
                    ],
                    "target_lang_definitions": {
                        "en": "The way in which two or more people, groups, or countries talk to, behave toward, and deal with each other."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relations",
                                "target_lang_sense_clarification": "diplomatic or social connections (plural)"
                            },
                            {
                                "target_lang_word": "relationship",
                                "target_lang_sense_clarification": "interpersonal connection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce4fb99f-dee3-3d27-bbcf-4f255bce30e6",
                    "sense_definition": "Взаимная связь, касательство к чему-либо; точка зрения или аспект, в котором рассматривается предмет.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "connection_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это событие не имеет никакого <отношения> к нашему делу.",
                            "target_lang_translations": {
                                "en": "This event has no <w>relation</w> to our case."
                            }
                        },
                        {
                            "text": "В этом <отношении> ты абсолютно прав.",
                            "target_lang_translations": {
                                "en": "In this <w>respect</w> you are absolutely right."
                            }
                        }
                    ],
                    "synonyms": [
                        "связь",
                        "касательство"
                    ],
                    "common_phrases": [
                        "иметь отношение",
                        "во всех отношениях",
                        "в этом отношении",
                        "не имеет отношения"
                    ],
                    "target_lang_definitions": {
                        "en": "A connection or link between things; a particular aspect or detail."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relation",
                                "target_lang_sense_clarification": "connection or relevance"
                            },
                            {
                                "target_lang_word": "respect",
                                "target_lang_sense_clarification": "aspect or regard (e.g., in this respect)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "131f6a7f-d32b-38e4-aee4-d71689a5253c",
                    "sense_definition": "Характер обращения с кем-либо или чем-либо, поведение по отношению к кому-то.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель требовал внимательного <отношения> к каждому ученику.",
                            "target_lang_translations": {
                                "en": "The teacher demanded attentive <w>treatment</w> of every student."
                            }
                        },
                        {
                            "text": "Такое небрежное <отношение> к технике приведёт к поломке.",
                            "target_lang_translations": {
                                "en": "Such careless <w>handling</w> of the equipment will lead to a breakdown."
                            }
                        }
                    ],
                    "synonyms": [
                        "обращение",
                        "подход"
                    ],
                    "common_phrases": [
                        "человеческое отношение",
                        "бережное отношение",
                        "отношение к клиентам"
                    ],
                    "target_lang_definitions": {
                        "en": "The manner in which someone behaves towards or deals with someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "treatment",
                                "target_lang_sense_clarification": "manner of dealing with someone"
                            },
                            {
                                "target_lang_word": "attitude",
                                "target_lang_sense_clarification": "behavioral stance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d446f38-99c8-3199-8571-f40ba689016d",
                    "sense_definition": "Результат деления одной величины на другую; взаимная зависимость величин.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Смешайте эти жидкости в <отношении> два к одному.",
                            "target_lang_translations": {
                                "en": "Mix these liquids in a <w>ratio</w> of two to one."
                            }
                        },
                        {
                            "text": "Процентное <отношение> прибыли к затратам было высоким.",
                            "target_lang_translations": {
                                "en": "The percentage <w>ratio</w> of profit to costs was high."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропорция",
                        "соотношение"
                    ],
                    "common_phrases": [

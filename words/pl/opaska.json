            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e098357a-6ac4-38dd-8645-ebf8bfe593d2",
                    "sense_definition": "Pasek z materiału, gumy lub tworzywa noszony na ciele, służący do podtrzymywania włosów, opatrunku lub jako ozdoba.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczynka założyła kolorową <w>opaskę</w> na włosy przed wyjściem do szkoły.",
                            "target_lang_translations": {
                                "ru": "Девочка надела цветную <w>повязку</w> на волосы перед выходом в школу."
                            }
                        },
                        {
                            "text": "Lekarz zalecił noszenie elastycznej <w>opaski</w> na bolący nadgarstek.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал носить эластичную <w>повязку</w> на больном запястье."
                            }
                        }
                    ],
                    "synonyms": [
                        "taśma"
                    ],
                    "common_phrases": [
                        "opaska na włosy",
                        "opaska uciskowa",
                        "opaska odblaskowa",
                        "opaska elastyczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоска из ткани, резины или пластика, надеваемая на тело для удержания волос, повязки или в качестве украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повязка",
                                "target_lang_sense_clarification": "медицинская, декоративная или спортивная"
                            },
                            {
                                "target_lang_word": "ободок",
                                "target_lang_sense_clarification": "обруч для волос"
                            },
                            {
                                "target_lang_word": "жгут",
                                "target_lang_sense_clarification": "медицинский, для остановки кровотечения"
                            },
                            {
                                "target_lang_word": "бандаж",
                                "target_lang_sense_clarification": "эластичный, для фиксации суставов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "235f3ceb-f77f-35c7-804a-54e6f7888557",
                    "sense_definition": "Papierowy pasek umieszczany na opakowaniu, książce lub pliku banknotów, pełniący funkcję informacyjną lub zabezpieczającą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownik banku spiął przeliczone pieniądze papierową <w>opaską</w>.",

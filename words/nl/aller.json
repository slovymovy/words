{
    "word_family": [
        "al",
        "alles",
        "allen",
        "alom"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3b6e1536-3529-3f65-be5f-c67eee3d09aa",
                    "sense_definition": "Versterkend woord (of voorvoegsel) dat vóór een overtreffende trap wordt geplaatst om de hoogst mogelijke graad aan te geven; betekent letterlijk 'van allemaal'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is het <w>aller</w>mooiste cadeau dat ik ooit heb gekregen.",
                            "target_lang_translations": {
                                "en": "That is the most beautiful gift <w>of all</w> that I have ever received."
                            }
                        },
                        {
                            "text": "Zij is mijn <w>aller</w>beste vriendin.",
                            "target_lang_translations": {
                                "en": "She is my <w>very</w> best friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "absoluut"
                    ],
                    "common_phrases": [
                        "het allerbeste",
                        "de allermooiste",
                        "het allerlaatste"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "meestal vast geschreven aan het bijvoeglijk naamwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Intensifying word (or prefix) placed before a superlative to indicate the highest possible degree; literally means 'of all'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "very",
                                "target_lang_sense_clarification": "used to intensify superlatives (e.g., the very best)"
                            },
                            {
                                "target_lang_word": "of all",
                                "target_lang_sense_clarification": "literal meaning, indicating the absolute highest degree"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "a4f99172-8ad7-3325-ad34-fa99ca439924",
                    "sense_definition": "Van alle; een oude naamvalsvorm (genitief meervoud) van 'al', die aangeeft dat iets tot de gehele groep behoort, voornamelijk gebruikt in vaste uitdrukkingen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "genitive_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is de succesvolste film <w>aller</w> tijden.",
                            "target_lang_translations": {
                                "en": "This is the most successful film <w>of all</w> time."
                            }
                        },
                        {
                            "text": "Hij wordt gezien als de grootste schrijver <w>aller</w> tijden.",
                            "target_lang_translations": {
                                "en": "He is seen as the greatest writer <w>of all</w> time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aller tijden",
                        "aller zielen",
                        "aller heiligen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "grammaticale vorm"
                        },
                        {
                            "trait_type": "form",
                            "comment": "in vaste uitdrukkingen"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Of all; an old case form (genitive plural) of 'al', indicating that something belongs to the entire group, mainly used in fixed expressions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of all",
                                "target_lang_sense_clarification": "used in fixed expressions like 'of all time'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Понять, разобраться в чем-либо (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскусить",
                                "target_lang_sense_clarification": "понять суть"
                            },
                            {
                                "target_lang_word": "разобраться",
                                "target_lang_sense_clarification": "понять устройство или ситуацию"
                            }

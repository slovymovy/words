    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "efc0dda2-3636-38e5-a2ad-bccc3923cda6",
                    "sense_definition": "To make someone feel unhappy because something they hoped for did not happen or was not as good as expected.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really hate to <w>disappoint</w> you, but I cannot come to the party tonight.",
                            "target_lang_translations": {
                                "ru": "Мне очень не хочется тебя <w>разочаровывать</w>, но я не смогу прийти на вечеринку сегодня вечером."
                            }
                        },
                        {
                            "text": "The team played poorly and began to <w>disappoint</w> the fans in the stadium.",
                            "target_lang_translations": {
                                "ru": "Команда играла плохо и начала <w>разочаровывать</w> болельщиков на стадионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "thwart"
                    ],
                    "antonyms": [
                        "satisfy"
                    ],
                    "common_phrases": [
                        "hate to disappoint",
                        "deeply disappoint",
                        "disappoint the fans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать чувство неудовлетворенности или огорчения из-за того, что ожидаемое не произошло или оказалось хуже, чем предполагалось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разочаровывать",
                                "target_lang_sense_clarification": "вызывать чувство разочарования"
                            },
                            {
                                "target_lang_word": "огорчать",
                                "target_lang_sense_clarification": "расстраивать кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2242e1b9-2e65-377f-b46b-0231aea4f949",
                    "sense_definition": "To fail to fulfill a hope, promise, or expectation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new movie did not <w>disappoint</w> the high expectations of the audience.",
                            "target_lang_translations": {
                                "ru": "Новый фильм <w>не обманул</w> высоких ожиданий зрителей."
                            }
                        },
                        {
                            "text": "We must work very hard to not <w>disappoint</w> the hopes of our supporters.",
                            "target_lang_translations": {
                                "ru": "Мы должны усердно работать, чтобы <w>не подвести</w> надежды наших сторонников."
                            }
                        }
                    ],
                    "antonyms": [
                        "satisfy"
                    ],
                    "common_phrases": [
                        "disappoint expectations",
                        "disappoint hopes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не суметь оправдать надежд, обещаний или ожиданий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подводить",
                                "target_lang_sense_clarification": "не оправдывать доверия, ставить в трудное положение"
                            },
                            {
                                "target_lang_word": "не оправдывать",
                                "target_lang_sense_clarification": "ожиданий или надежд"
                            },
                            {
                                "target_lang_word": "обманывать",
                                "target_lang_sense_clarification": "надежды или ожидания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "760a355a-8867-36e8-9eb5-82eeb95f680e",
                    "sense_definition": "To prevent a plan or attempt from succeeding.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden heavy rain arrived to <w>disappoint</w> their plans for a summer picnic.",
                            "target_lang_translations": {
                                "ru": "Внезапный проливной дождь <w>расстроил</w> их планы на летний пикник."
                            }
                        },
                        {
                            "text": "His clever strategies helped to <w>disappoint</w> the enemy's attack on the city.",
                            "target_lang_translations": {
                                "ru": "Его хитроумные стратегии помогли <w>сорвать</w> атаку врага на город."
                            }
                        }
                    ],
                    "synonyms": [
                        "thwart",
                        "frustrate"
                    ],
                    "traits": [

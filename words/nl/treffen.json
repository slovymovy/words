                        "militair treffen"
                    ],
                    "target_lang_definitions": {
                        "en": "A struggle, fight, or match between two parties.",
                        "ru": "Борьба, бой или состязание между двумя сторонами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clash",
                                "target_lang_sense_clarification": "a conflict or fight"
                            },
                            {
                                "target_lang_word": "encounter",
                                "target_lang_sense_clarification": "a match or hostile meeting"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "столкновение",
                                "target_lang_sense_clarification": "конфликт или битва"
                            },
                            {
                                "target_lang_word": "поединок",
                                "target_lang_sense_clarification": "состязание или матч"
                            }
                        ]
                    }
                }
            ]
        }
    ]

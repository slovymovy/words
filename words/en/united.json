                {
                    "sense_id": "d2e2b080-5df0-31d5-8356-3b8097870cdf",
                    "sense_definition": "Joined together as a single group or entity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {

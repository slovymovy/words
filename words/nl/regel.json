    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9bfa531d-4ebe-3ea8-8800-7387acdd1754",
                    "sense_definition": "Een voorschrift, afspraak of richtlijn die bepaalt wat wel of niet mag.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rule_norm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op deze school hebben we de <w>regel</w> dat we niet rennen in de gangen.",
                            "target_lang_translations": {
                                "en": "At this school we have the <w>rule</w> that we do not run in the hallways.",
                                "ru": "В этой школе у нас есть <w>правило</w>, что мы не бегаем по коридорам."
                            }
                        },
                        {
                            "text": "Het is tegen de regels om hier je auto te parkeren.",
                            "target_lang_translations": {
                                "en": "It is against the <w>rules</w> to park your car here.",
                                "ru": "Парковать здесь машину — против <w>правил</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorschrift",
                        "gebod"
                    ],
                    "common_phrases": [
                        "zich aan de regels houden",
                        "tegen de regels",
                        "de regels overtreden",
                        "uitzondering op de regel"
                    ],
                    "target_lang_definitions": {
                        "en": "A rule, agreement, or guideline that determines what is allowed or not.",
                        "ru": "Предписание, договоренность или руководство, определяющее, что разрешено, а что нет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rule",
                                "target_lang_sense_clarification": "regulation or principle"
                            },
                            {
                                "target_lang_word": "regulation",
                                "target_lang_sense_clarification": "official rule"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правило",
                                "target_lang_sense_clarification": "предписание, норма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36adeca1-7093-3ab1-bbb8-71fd68beb181",
                    "sense_definition": "Een horizontale rij geschreven of gedrukte woorden op een bladzijde of scherm.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "line_text",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lees de eerste <w>regel</w> van de tekst hardop voor aan de klas.",
                            "target_lang_translations": {
                                "en": "Read the first <w>line</w> of the text out loud to the class.",
                                "ru": "Прочитай классу вслух первую <w>строчку</w> текста."
                            }
                        },
                        {
                            "text": "Hij schreef slechts enkele regels in zijn dagboek voordat hij ging slapen.",
                            "target_lang_translations": {
                                "en": "He wrote only a few <w>lines</w> in his diary before going to sleep.",
                                "ru": "Он написал всего несколько <w>строк</w> в своем дневнике, прежде чем лечь спать."
                            }
                        }
                    ],
                    "synonyms": [
                        "lijn"
                    ],
                    "common_phrases": [
                        "tussen de regels door lezen",
                        "op een nieuwe regel beginnen"
                    ],
                    "target_lang_definitions": {
                        "en": "A horizontal row of written or printed words on a page or screen.",
                        "ru": "Горизонтальный ряд написанных или напечатанных слов на странице или экране."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "line",
                                "target_lang_sense_clarification": "row of text"
                            }
                        ],
                        "ru": [

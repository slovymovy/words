        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "68a70d36-34f9-39ef-974d-afefe0461e15",
                    "sense_definition": "Used to call attention to something surprising, interesting, or important.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We opened the box, and lo and behold, it was empty.",
                            "target_lang_translations": {
                                "ru": "Мы открыли коробку, и <w>о чудо</w>, она была пуста."
                            }
                        },
                        {
                            "text": "And <w>lo</w>, the sun finally appeared after days of rain.",
                            "target_lang_translations": {
                                "ru": "И <w>вот</w>, после нескольких дней дождя наконец выглянуло солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "behold",
                        "look",
                        "see"
                    ],
                    "common_phrases": [
                        "lo and behold"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или литературное слово, используемое для привлечения внимания к чему-то удивительному или важному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вот",
                                "target_lang_sense_clarification": "указание на что-то, часто с оттенком удивления"
                            },
                            {
                                "target_lang_word": "глядь",
                                "target_lang_sense_clarification": "разговорное, выражает внезапность обнаружения"
                            },
                            {
                                "target_lang_word": "о чудо",
                                "target_lang_sense_clarification": "в составе устойчивых выражений"
                            }
                        ]
                    }
                }
            ]
        },

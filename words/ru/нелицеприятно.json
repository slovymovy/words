    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "06cca9bb-4bef-31f4-9a63-6384e428a507",
                    "sense_definition": "Справедливо и беспристрастно, не стараясь угодить кому-либо и не взирая на чины или звания.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impartiality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья обязан рассматривать каждое дело <w>нелицеприятно</w>, опираясь исключительно на факты и закон.",
                            "target_lang_translations": {
                                "en": "The judge is obliged to examine every case <w>impartially</w>, relying exclusively on facts and the law."
                            }
                        },
                        {
                            "text": "Историки будущего будут судить о наших поступках <w>нелицеприятно</w> и строго.",
                            "target_lang_translations": {
                                "en": "Future historians will judge our actions <w>impartially</w> and strictly."
                            }
                        }
                    ],
                    "synonyms": [
                        "справедливо",
                        "объективно",
                        "непредвзято",
                        "непредубеждённо",
                        "невзирая на лица"
                    ],
                    "antonyms": [
                        "предвзято",
                        "субъективно",
                        "предубеждённо"
                    ],
                    "common_phrases": [
                        "судить нелицеприятно",
                        "действовать нелицеприятно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a fair and unbiased manner, without trying to please anyone or regard their rank."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "impartially",
                                "target_lang_sense_clarification": "without bias or favoritism"
                            },
                            {
                                "target_lang_word": "unbiasedly",
                                "target_lang_sense_clarification": "fairly and objectively"
                            },
                            {
                                "target_lang_word": "without prejudice",
                                "target_lang_sense_clarification": "disregarding personal feelings or status"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9781a190-840c-3fb0-82ed-f3a301efe22a",
                    "sense_definition": "Критически, резко, с осуждением или неприязнью.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "criticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Журналист весьма <w>нелицеприятно</w> высказался о качестве ремонта дорог в городе.",
                            "target_lang_translations": {
                                "en": "The journalist spoke quite <w>unflatteringly</w> about the quality of road repairs in the city."
                            }
                        },
                        {
                            "text": "После ошибки на него обрушились с критикой и говорили о нём <w>нелицеприятно</w>.",
                            "target_lang_translations": {
                                "en": "After the mistake, they lashed out at him with criticism and spoke of him <w>harshly</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "резко"
                    ],
                    "common_phrases": [
                        "отозваться нелицеприятно",
                        "высказаться нелицеприятно"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "распространенное современное употребление, отличающееся от исходного значения"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Critically, sharply, or with disapproval (common modern usage)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "harshly",
                                "target_lang_sense_clarification": "critically or severely"
                            },
                            {
                                "target_lang_word": "unflatteringly",
                                "target_lang_sense_clarification": "in a negative or critical light"
                            },
                            {
                                "target_lang_word": "critically",
                                "target_lang_sense_clarification": "expressing disapproval"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            },
                            {
                                "target_lang_word": "clear up",
                                "target_lang_sense_clarification": "a backlog or clutter"
                            },
                            {
                                "target_lang_word": "wade through",
                                "target_lang_sense_clarification": "a large volume of work or paperwork"

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "40d88111-ebf9-38cc-81cd-a45a4f635f91",
                    "sense_definition": "Met veel invloed, kracht of gezag; in staat om macht uit te oefenen over anderen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de middeleeuwen was de kerk een zeer <w>machtig</w> instituut.",
                            "target_lang_translations": {
                                "en": "In the Middle Ages, the church was a very <w>powerful</w> institution.",
                                "ru": "В Средние века церковь была очень <w>могущественным</w> институтом."
                            }
                        },
                        {
                            "text": "De president is de <w>machtigste</w> persoon van het land.",
                            "target_lang_translations": {
                                "en": "The president is the <w>most powerful</w> person in the country.",
                                "ru": "Президент — самый <w>влиятельный</w> человек в стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "invloedrijk",
                        "krachtig",
                        "sterk"
                    ],
                    "antonyms": [
                        "onmachtig",
                        "zwak",
                        "machteloos"
                    ],
                    "common_phrases": [
                        "een machtig man",
                        "een machtig rijk",
                        "machtig zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a lot of influence, strength, or authority; capable of exercising power over others.",
                        "ru": "Обладающий большим влиянием, силой или властью; способный осуществлять власть над другими."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "powerful",
                                "target_lang_sense_clarification": "having great power, influence, or strength"
                            },
                            {
                                "target_lang_word": "mighty",
                                "target_lang_sense_clarification": "possessing great and impressive power or strength"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "могущественный",
                                "target_lang_sense_clarification": "обладающий большой властью или силой"
                            },
                            {
                                "target_lang_word": "влиятельный",
                                "target_lang_sense_clarification": "имеющий большое влияние или авторитет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30d3365d-75c3-30ab-b0e5-87e111c33e6e",
                    "sense_definition": "(van voedsel) Zwaar op de maag liggend en snel een vol gevoel gevend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hoef geen toetje meer, want het hoofdgerecht was erg <w>machtig</w>.",
                            "target_lang_translations": {
                                "en": "I don't want dessert anymore, because the main course was very <w>heavy</w>.",
                                "ru": "Я больше не хочу десерт, так как основное блюдо было очень <w>сытным</w>."
                            }
                        },
                        {
                            "text": "Chocoladetaart met slagroom is vaak erg <w>machtig</w>.",
                            "target_lang_translations": {
                                "en": "Chocolate cake with whipped cream is often very <w>rich</w>.",
                                "ru": "Шоколадный торт со взбитыми сливками часто очень <w>тяжёлый</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwaar",
                        "vullend"
                    ],
                    "antonyms": [
                        "licht"
                    ],
                    "common_phrases": [
                        "te machtig",
                        "machtig eten"
                    ],
                    "target_lang_definitions": {
                        "en": "(of food) Lying heavy on the stomach and quickly giving a full feeling.",
                        "ru": "(о еде) Тяжёлая для желудка и быстро вызывающая чувство сытости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rich",
                                "target_lang_sense_clarification": "containing a large amount of fat, sugar, or spice"
                            },
                            {
                                "target_lang_word": "heavy",
                                "target_lang_sense_clarification": "very filling and difficult to digest"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сытный",
                                "target_lang_sense_clarification": "быстро утоляющий голод, питательный"
                            },
                            {
                                "target_lang_word": "тяжёлый",
                                "target_lang_sense_clarification": "(о еде) трудно перевариваемый, создающий ощущение тяжести"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf0be722-01b4-3e14-8d14-816672447a80",
                    "sense_definition": "Heel mooi, leuk of indrukwekkend.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "impressive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hadden een <w>machtig</w> uitzicht vanaf de top van de berg.",
                            "target_lang_translations": {
                                "en": "We had a <w>magnificent</w> view from the top of the mountain.",
                                "ru": "С вершины горы открывался <w>великолепный</w> вид."
                            }
                        },
                        {
                            "text": "Dat was echt een <w>machtig</w> feest gisteravond.",
                            "target_lang_translations": {
                                "en": "That was really a <w>great</w> party last night.",
                                "ru": "Вчера вечером была просто <w>потрясающая</w> вечеринка."
                            }
                        }
                    ],
                    "synonyms": [
                        "geweldig",
                        "prachtig",
                        "indrukwekkend"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech to express admiration"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very beautiful, fun, or impressive.",
                        "ru": "Очень красивый, приятный или впечатляющий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "magnificent",
                                "target_lang_sense_clarification": "impressively beautiful or elaborate"
                            },
                            {
                                "target_lang_word": "wonderful",
                                "target_lang_sense_clarification": "inspiring delight, pleasure, or admiration"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "великолепный",
                                "target_lang_sense_clarification": "роскошный, производящий сильное впечатление"
                            },
                            {
                                "target_lang_word": "потрясающий",
                                "target_lang_sense_clarification": "вызывающий восторг, изумительный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5219b57d-98c8-3d77-a048-85607db1e322",
                    "sense_definition": "Heel erg; in hoge mate (gebruikt ter versterking van een bijvoeglijk naamwoord).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vind het <w>machtig</w> interessant om te horen hoe dat werkt.",
                            "target_lang_translations": {
                                "en": "I find it <w>mighty</w> interesting to hear how that works.",
                                "ru": "Мне <w>жутко</w> интересно узнать, как это работает."
                            }
                        },
                        {
                            "text": "Het is <w>machtig</w> mooi om te zien hoe de natuur zich herstelt.",
                            "target_lang_translations": {
                                "en": "It is <w>incredibly</w> beautiful to see how nature recovers.",
                                "ru": "Это <w>невероятно</w> красиво — видеть, как природа восстанавливается."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontzettend",
                        "enorm",
                        "zeer"
                    ],
                    "common_phrases": [
                        "machtig mooi",
                        "machtig interessant"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very; to a high degree (used to intensify an adjective).",
                        "ru": "Очень; в высокой степени (используется для усиления прилагательного)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mighty",
                                "target_lang_sense_clarification": "(colloquial) very or extremely"
                            },
                            {
                                "target_lang_word": "awfully",
                                "target_lang_sense_clarification": "very; exceedingly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "жутко",
                                "target_lang_sense_clarification": "(разг.) очень, чрезвычайно"
                            },
                            {
                                "target_lang_word": "чертовски",
                                "target_lang_sense_clarification": "(разг.) в высшей степени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "490f3aeb-f209-3439-a085-e9b70e613f60",
                    "sense_definition": "Vervoeging van <w>machtigen</w>: eerste persoon enkelvoud, gebiedende wijs of bij inversie tweede persoon enkelvoud.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>machtig</w> mijn advocaat om namens mij te spreken.",
                            "target_lang_translations": {
                                "en": "I <w>authorize</w> my lawyer to speak on my behalf.",
                                "ru": "Я <w>уполномочиваю</w> своего адвоката выступать от моего имени."
                            }
                        },
                        {
                            "text": "<w>Machtig</w> jij je broer om de auto te verkopen?",
                            "target_lang_translations": {
                                "en": "Do you <w>authorize</w> your brother to sell the car?",
                                "ru": "Ты <w>уполномочиваешь</w> своего брата продать машину?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection of machtigen"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of <w>machtigen</w>: first person singular, imperative, or second person singular in inversion.",
                        "ru": "Спряжение глагола <w>machtigen</w>: первое лицо единственного числа, повелительное наклонение или второе лицо единственного числа при инверсии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "authorize",
                                "target_lang_sense_clarification": "to give official permission or power to"
                            },
                            {
                                "target_lang_word": "empower",
                                "target_lang_sense_clarification": "to give someone the authority or power to do something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уполномочивать",
                                "target_lang_sense_clarification": "давать кому-либо официальное разрешение или власть"
                            }
                        ]
                    }
                }
            ]
        }
    ]

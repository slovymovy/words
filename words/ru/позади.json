                        "en": [
                            {
                                "target_lang_word": "behind",
                                "target_lang_sense_clarification": "in the past"
                            },
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "finished"
                            },
                            {
                                "target_lang_word": "in the past",
                                "target_lang_sense_clarification": "time"
                            }
                        ]
                    }
                }
            ]
        }
    ]

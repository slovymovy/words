                                        "sense_id": "54848faa-a199-3816-981d-1230691b564b",
                                        "entry_id": "c5989309-5533-3dca-bc2f-9fa72e2a0227",
                                        "glosses": [
                                            "Frage an jemanden, mit der eine Antwort erwartet wird"
                                        ],

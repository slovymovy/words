{
    "word_family": [
        "miserable",
        "miserably",
        "miser"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "16020650-2d61-3e7f-b9f4-e85791bb44fc",
                    "sense_definition": "A state of great unhappiness, emotional distress, or physical suffering.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "suffering_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The refugees faced sheer <w>misery</w> during their long journey to safety.",
                            "target_lang_translations": {
                                "ru": "Беженцы пережили настоящие <w>мучения</w> во время долгого пути в безопасное место."
                            }
                        },
                        {
                            "text": "He lived in <w>misery</w> for years after losing his entire family in the accident.",
                            "target_lang_translations": {
                                "ru": "Он годами жил в <w>горе</w> после потери всей семьи в аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "unhappiness",
                        "distress",
                        "wretchedness",
                        "woe",
                        "sorrow",
                        "grief",
                        "depression",
                        "despondency"
                    ],
                    "antonyms": [
                        "happiness",
                        "joy",
                        "delight",
                        "bliss",
                        "comfort"
                    ],
                    "common_phrases": [
                        "put out of misery",
                        "sheer misery",
                        "life of misery",
                        "die in misery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние сильного несчастья, душевных страданий или физических мучений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страдание",
                                "target_lang_sense_clarification": "физическая или душевная боль"
                            },
                            {
                                "target_lang_word": "мучение",
                                "target_lang_sense_clarification": "сильное страдание, пытка"
                            },
                            {
                                "target_lang_word": "горе",
                                "target_lang_sense_clarification": "глубокая печаль, скорбь"
                            },
                            {
                                "target_lang_word": "несчастье",
                                "target_lang_sense_clarification": "отсутствие счастья, беда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7a59426-a495-304a-83fb-b9e9f7ef795d",
                    "sense_definition": "A specific event, condition, or circumstance that causes suffering or distress.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "suffering_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The flood was just one of the many <w>miseries</w> that the village had to endure that year.",
                            "target_lang_translations": {
                                "ru": "Наводнение было лишь одним из многих <w>несчастий</w>, которые деревне пришлось пережить в том году."
                            }
                        },
                        {
                            "text": "Constant train cancellations heaped further <w>misery</w> on the already frustrated commuters.",
                            "target_lang_translations": {
                                "ru": "Постоянные отмены поездов принесли новые <w>страдания</w> и без того расстроенным пассажирам."
                            }
                        }
                    ],
                    "synonyms": [
                        "hardship",
                        "affliction",
                        "misfortune",
                        "calamity",
                        "adversity",
                        "burden"
                    ],
                    "antonyms": [
                        "blessing",
                        "boon",
                        "benefit"
                    ],
                    "common_phrases": [
                        "heap misery on",
                        "untold misery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное событие, условие или обстоятельство, причиняющее страдания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несчастье",
                                "target_lang_sense_clarification": "горестное событие"
                            },
                            {
                                "target_lang_word": "беда",
                                "target_lang_sense_clarification": "тяжелое происшествие, неприятность"
                            },
                            {
                                "target_lang_word": "невзгоды",
                                "target_lang_sense_clarification": "тяготы жизни, лишения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ac0ebb5-869d-3d1f-bb74-47c6da410d92",
                    "sense_definition": "A state of extreme poverty and lack of essential resources.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "suffering_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite working two jobs, the family continued to live in <w>misery</w> and debt.",
                            "target_lang_translations": {
                                "ru": "Несмотря на работу в двух местах, семья продолжала жить в <w>нищете</w> и долгах."
                            }
                        },
                        {
                            "text": "The novel depicts the <w>misery</w> of the working class in the nineteenth century.",
                            "target_lang_translations": {
                                "ru": "Роман описывает <w>нищету</w> рабочего класса в девятнадцатом веке."
                            }
                        }
                    ],
                    "synonyms": [
                        "poverty",
                        "destitution",
                        "penury",
                        "indigence",
                        "neediness"
                    ],
                    "antonyms": [
                        "wealth",
                        "affluence",
                        "prosperity"
                    ],
                    "common_phrases": [
                        "live in misery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние крайней бедности и нехватки жизненно важных ресурсов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нищета",
                                "target_lang_sense_clarification": "крайняя бедность"
                            },
                            {
                                "target_lang_word": "бедность",
                                "target_lang_sense_clarification": "недостаток средств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e16716ed-2583-3e91-bc56-10329fdeaa1f",
                    "sense_definition": "A physical ache or pain in a specific part of the body.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old woman complained of a <w>misery</w> in her joints whenever the weather turned cold.",
                            "target_lang_translations": {
                                "ru": "Старуха жаловалась на <w>ломоту</w> в суставах всякий раз, когда холодало."
                            }
                        },
                        {
                            "text": "He said he had a <w>misery</w> in his chest that made it difficult to breathe deeply.",
                            "target_lang_translations": {
                                "ru": "Он сказал, что у него <w>боль</w> в груди, из-за которой трудно глубоко дышать."
                            }
                        }
                    ],
                    "synonyms": [
                        "pain",
                        "ache"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "used in some regional dialects to refer to a specific bodily pain"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая боль или ломота в определенной части тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боль",
                                "target_lang_sense_clarification": "физическое ощущение"
                            },
                            {
                                "target_lang_word": "ломота",
                                "target_lang_sense_clarification": "тупая боль в костях или суставах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dcf153a-95c8-32c3-9f4d-da3441ebb613",
                    "sense_definition": "An intense desire to accumulate wealth; greed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "greed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>misery</w> was so great that he refused to buy coal even in the freezing winter.",
                            "target_lang_translations": {
                                "ru": "Его <w>скупость</w> была так велика, что он отказывался покупать уголь даже морозной зимой."
                            }
                        },
                        {
                            "text": "The merchant's <w>misery</w> led him to cheat his customers at every opportunity.",
                            "target_lang_translations": {
                                "ru": "<w>Жадность</w> торговца заставляла его обманывать покупателей при любой возможности."
                            }
                        }
                    ],
                    "synonyms": [
                        "avarice",
                        "greed",
                        "covetousness",
                        "cupidity"
                    ],
                    "antonyms": [
                        "generosity",
                        "charity"
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "modern English uses 'miserliness' or 'avarice' instead"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное желание накапливать богатство; жадность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скупость",
                                "target_lang_sense_clarification": "чрезмерная бережливость"
                            },
                            {
                                "target_lang_word": "жадность",
                                "target_lang_sense_clarification": "алчность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d39f729b-6700-32c8-803f-161be0fe278b",
                    "sense_definition": "A derogatory nickname for the U.S. state of Missouri.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place_nickname",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "He jokingly referred to his home state as <w>Misery</w> because of the oppressive summer humidity.",
                            "target_lang_translations": {
                                "ru": "Он в шутку называл свой родной штат <w>Мизери</w> из-за гнетущей летней влажности."
                            }
                        },
                        {
                            "text": "They were not looking forward to driving back through <w>Misery</w> on their way home.",
                            "target_lang_translations": {
                                "ru": "Им не хотелось ехать обратно через <w>Мизери</w> по пути домой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "derogatory play on words"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уничижительное прозвище американского штата Миссури."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мизери",
                                "target_lang_sense_clarification": "игра слов: Missouri и misery (страдание)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
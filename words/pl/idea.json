    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e20425e-16fa-3e14-bf58-1d84ac761e10",
                    "sense_definition": "Ogólny zarys, projekt lub koncepcja czegoś, co ma zostać zrealizowane lub stworzone.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "concept_plan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twoja <idea> założenia własnej firmy bardzo mi się podoba.",
                            "target_lang_translations": {
                                "ru": "Мне очень нравится твоя <идея> открыть собственную фирму."
                            }
                        },
                        {
                            "text": "To jest bardzo ciekawa <idea>, ale trudna do wykonania.",
                            "target_lang_translations": {
                                "ru": "Это очень интересная <идея>, но её трудно осуществить."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomysł",
                        "koncepcja"
                    ],
                    "common_phrases": [
                        "dobra idea",
                        "ciekawa idea",
                        "mieć ideę",
                        "nowatorska idea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общий план, замысел или концепция того, что должно быть создано или осуществлено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идея",
                                "target_lang_sense_clarification": "мысль, план"
                            },
                            {
                                "target_lang_word": "замысел",
                                "target_lang_sense_clarification": "намерение, проект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9370fd01-9ee1-36c5-8d6a-44f7d59ff1e8",
                    "sense_definition": "Główna myśl, cel lub wyższa wartość, która nadaje sens działaniom, twórczości lub postawie życiowej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "guiding_principle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główną <ideą> tej książki jest walka o wolność człowieka.",
                            "target_lang_translations": {
                                "ru": "Главной <идеей> этой книги является борьба за свободу человека."
                            }
                        },
                        {
                            "text": "Oni poświęcili całe swoje życie dla słusznej <idei>.",
                            "target_lang_translations": {
                                "ru": "Они посвятили всю свою жизнь праведной <идее>."
                            }
                        }
                    ],
                    "synonyms": [
                        "myśl",
                        "ideologia"
                    ],
                    "common_phrases": [
                        "idea przewodnia",
                        "w imię idei",
                        "wierny idei",
                        "idea narodowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главная мысль, цель или высшая ценность, придающая смысл действиям, творчеству или жизненной позиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идея",
                                "target_lang_sense_clarification": "главная мысль, принцип"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7086fc80-1389-3ccd-8896-42d61999207a",
                    "sense_definition": "W filozofii: niematerialny, wieczny i doskonały wzorzec rzeczy materialnych, istniejący niezależnie od świata zmysłów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "philosophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według Platona każda rzecz jest tylko cieniem swojej doskonałej <idei>.",
                            "target_lang_translations": {
                                "ru": "Согласно Платону, каждая вещь — это лишь тень своей совершенной <идеи>."
                            }
                        },
                        {
                            "text": "Filozofowie od wieków spierają się o to, czym jest <idea> dobra.",
                            "target_lang_translations": {
                                "ru": "Философы веками спорят о том, что такое <идея> блага."
                            }
                        }
                    ],
                    "common_phrases": [
                        "świat idei",
                        "idea dobra"
                    ],
                    "target_lang_definitions": {
                        "ru": "В философии: нематериальный, вечный и совершенный прообраз материальных вещей, существующий независимо от мира чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идея",
                                "target_lang_sense_clarification": "философское понятие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
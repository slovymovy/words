                            {
                                "target_lang_word": "заклёпка",
                                "target_lang_sense_clarification": "декоративная деталь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb5c2e2f-2682-340b-8bc6-53f181c19a51",
                    "sense_definition": "A small piece of jewelry worn in the ear that is attached with a straight post.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protrusion_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a simple pair of diamond <w>studs</w> to the wedding.",

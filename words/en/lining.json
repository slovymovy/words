                            {
                                "target_lang_word": "футеровка",
                                "target_lang_sense_clarification": "промышленность, печи"
                            },
                            {
                                "target_lang_word": "подшивка подкладки",
                                "target_lang_sense_clarification": "шитье"
                            }
                        ]
                    }
                }
            ]
        },

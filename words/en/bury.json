                {
                    "sense_id": "900fef81-6a9c-3947-a7f8-5b083e941376",
                    "sense_definition": "To live longer than someone else; to survive one's relatives or peers.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "outlive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather is so healthy that he will probably <w>bury</w> us all.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка настолько здоров, что, вероятно, <w>похоронит</w> нас всех."
                            }
                        },
                        {
                            "text": "It is a tragedy for a parent to have to <w>bury</w> their child.",
                            "target_lang_translations": {
                                "ru": "Это трагедия для родителя — <w>хоронить</w> своего ребенка."
                            }
                        }
                    ],
                    "synonyms": [
                        "outlive",
                        "survive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жить дольше кого-то другого; пережить своих родственников или сверстников."
                    },
                    "translations": {
                        "ru": [

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7a29d532-2ee3-34e8-9de1-6ba276511a06",
                    "sense_definition": "Used to emphasize that something is true or real, especially when it is surprising or contradicts a previous belief.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reality_emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I thought he was joking, but he <w>actually</w> meant what he said.",
                            "target_lang_translations": {
                                "ru": "Я думал, он шутит, но он <w>действительно</w> имел в виду то, что сказал."
                            }
                        },
                        {
                            "text": "She looks very young, but she is <w>actually</w> over fifty years old.",
                            "target_lang_translations": {
                                "ru": "Она выглядит очень молодо, но <w>на самом деле</w> ей больше пятидесяти."
                            }
                        }
                    ],
                    "synonyms": [
                        "really",
                        "in fact",
                        "in reality",
                        "truly",
                        "as a matter of fact",
                        "indeed",
                        "literally"
                    ],
                    "antonyms": [
                        "ostensibly",
                        "virtually",
                        "figuratively"
                    ],
                    "common_phrases": [
                        "actually I think",
                        "actually it is",
                        "not actually",
                        "did actually"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для того, чтобы подчеркнуть, что что-то является правдой или реальностью, особенно когда это удивительно или противоречит предыдущему убеждению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на самом деле",
                                "target_lang_sense_clarification": "в действительности, вопреки ожиданиям"
                            },
                            {
                                "target_lang_word": "действительно",
                                "target_lang_sense_clarification": "подтверждение истинности факта"
                            },
                            {
                                "target_lang_word": "вообще-то",
                                "target_lang_sense_clarification": "разговорное; используется при возражении или уточнении"
                            },
                            {
                                "target_lang_word": "фактически",
                                "target_lang_sense_clarification": "по сути, практически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc455ce9-8c5c-376a-aaa0-c2feb3a7e814",
                    "sense_definition": "At the present time; currently.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "time_current",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The regulations <w>actually</w> in force differ significantly from the old ones.",
                            "target_lang_translations": {
                                "ru": "Нормы, <w>в настоящее время</w> имеющие силу, существенно отличаются от старых."
                            }
                        },
                        {
                            "text": "She described the fashion <w>actually</w> in vogue during that specific era.",
                            "target_lang_translations": {
                                "ru": "Она описала моду, <w>в то время</w> бывшую популярной в ту конкретную эпоху."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "this usage is obsolete in modern English and often considered a false friend for speakers of Romance languages"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В настоящее время; на данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в настоящее время",
                                "target_lang_sense_clarification": "текущий момент (устаревшее значение оригинала)"
                            },
                            {
                                "target_lang_word": "сейчас",
                                "target_lang_sense_clarification": "в данный момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0806d5e5-340e-32a9-a6b6-b4ed78021614",
                    "sense_definition": "In an active manner; involving action rather than potentiality or passivity.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee was neither <w>actually</w> nor passively involved in the decision.",
                            "target_lang_translations": {
                                "ru": "Комитет не участвовал в принятии решения ни <w>активно</w>, ни пассивно."
                            }
                        },
                        {
                            "text": "He did not just support the idea theoretically, but <w>actually</w> worked to achieve it.",
                            "target_lang_translations": {
                                "ru": "Он не просто поддерживал идею теоретически, но <w>на деле</w> работал над её воплощением."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rarely used in this specific sense in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Активным образом; подразумевает действие, а не потенциальную возможность или пассивность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на деле",
                                "target_lang_sense_clarification": "на практике, в реальности"
                            },
                            {
                                "target_lang_word": "активно",
                                "target_lang_sense_clarification": "деятельно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "442b0b1e-85cc-34de-a6fa-dff56c7bed8e",
                    "sense_definition": "Sztywna konstrukcja nośna z prętów lub belek, stosowana w budownictwie, na przykład w mostach lub dachach",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "construction_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stalowa <w>kratownica</w> podtrzymuje ciężki dach hali fabrycznej.",
                            "target_lang_translations": {
                                "ru": "Стальная <w>ферма</w> поддерживает тяжелую крышу заводского цеха."
                            }
                        },
                        {
                            "text": "Inżynierowie dokładnie sprawdzili stan techniczny mostowej <w>kratownicy</w>.",
                            "target_lang_translations": {
                                "ru": "Инженеры тщательно проверили техническое состояние мостовой <w>фермы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stalowa kratownica",
                        "kratownica mostowa",
                        "konstrukcja kratownicy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жесткая несущая конструкция из стержней или балок, используемая в строительстве, например, в мостах или крышах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ферма",
                                "target_lang_sense_clarification": "строительная конструкция"
                            },
                            {
                                "target_lang_word": "решётчатая конструкция",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "891c1e84-973f-3b64-9e1a-bfb405cde8d3",
                    "sense_definition": "Płaska przegroda lub panel z krzyżujących się listew, często używana w ogrodzie jako podpora dla roślin",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "garden_lattice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gęsty bluszcz całkowicie zarósł drewnianą <w>kratownicę</w> przy tarasie.",
                            "target_lang_translations": {
                                "ru": "Густой плющ полностью зарос деревянную <w>решётку</w> у террасы."
                            }
                        },
                        {
                            "text": "Kupiłem nową <w>kratownicę</w>, aby oddzielić część wypoczynkową ogrodu.",
                            "target_lang_translations": {
                                "ru": "Я купил новую <w>шпалеру</w>, чтобы отделить зону отдыха в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "krata"
                    ],
                    "common_phrases": [
                        "drewniana kratownica",
                        "kratownica ogrodowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская перегородка или панель из перекрещивающихся планок, часто используемая в саду как опора для растений"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпалера",
                                "target_lang_sense_clarification": "опора для растений"
                            },
                            {
                                "target_lang_word": "решётка",
                                "target_lang_sense_clarification": "декоративная перегородка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65be9b11-75a0-3a84-ac8c-aeda36e6aa86",
                    "sense_definition": "Drewniana kratka ułożona na dnie łodzi lub jachtu, chroniąca przed zamoczeniem nóg",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_grating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żeglarz podniósł <w>kratownicę</w>, aby wybrać wodę z zęzy.",
                            "target_lang_translations": {
                                "ru": "Моряк поднял <w>грейтинг</w>, чтобы вычерпать воду из трюма."
                            }
                        },
                        {
                            "text": "Mokra <w>kratownica</w> na pokładzie może być bardzo śliska.",
                            "target_lang_translations": {
                                "ru": "Мокрый <w>грейтинг</w> на палубе может быть очень скользким."
                            }
                        }
                    ],
                    "synonyms": [
                        "greting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянная решётка, уложенная на дне лодки или яхты, защищающая ноги от намокания"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грейтинг",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "решётчатый настил",
                                "target_lang_sense_clarification": "описание конструкции"
                            }
                        ]
                    }
                }
            ]
        }
    ]

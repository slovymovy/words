                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбиваться",
                                "target_lang_sense_clarification": "в комки или кучу"
                            },
                            {
                                "target_lang_word": "сваливаться",
                                "target_lang_sense_clarification": "о шерсти или волосах"
                            }
                        ]
                    }

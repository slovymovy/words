                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число слова speet (прут для копчения или сушки рыбы)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прутья",
                                "target_lang_sense_clarification": "тонкие палки для рыбы"
                            },
                            {
                                "target_lang_word": "вешала",
                                "target_lang_sense_clarification": "жерди для сушки"
                            }
                        ]
                    }

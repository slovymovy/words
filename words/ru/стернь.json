    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4a1ca02a-a7ce-34ae-9d6e-0c7961595a11",
                    "sense_definition": "Остатки нижних частей стеблей злаковых культур, сохранившиеся на корню после уборки урожая.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Босые ноги больно колола сухая и жёсткая <w>стернь</w>.",
                            "target_lang_translations": {
                                "en": "The dry and hard <w>stubble</w> painfully pricked the bare feet."
                            }
                        },
                        {
                            "text": "Из-под первого снега уныло торчала пожелтевшая <w>стернь</w>.",
                            "target_lang_translations": {
                                "en": "Yellowed <w>stubble</w> stuck out gloomily from under the first snow."
                            }
                        }
                    ],
                    "synonyms": [
                        "жнивьё"
                    ],
                    "common_phrases": [
                        "колючая стернь",
                        "жёсткая стернь",
                        "сухая стернь"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "собирательное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The remains of the lower parts of cereal crop stems left rooted in the ground after harvest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stubble",
                                "target_lang_sense_clarification": "cut stalks left in the ground"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3db00c0-9888-345c-afee-fea7d8c87185",
                    "sense_definition": "Поле, на котором сжаты хлеба и остались срезанные стебли растений.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотники медленно брели по <w>стерни</w> в поисках перепёлок.",
                            "target_lang_translations": {
                                "en": "The hunters wandered slowly across the <w>stubble</w> in search of quail."
                            }
                        },
                        {
                            "text": "Трактора вышли в поле, чтобы запахать <w>стернь</w> под озимые культуры.",
                            "target_lang_translations": {
                                "en": "Tractors went out into the field to plow the <w>stubble</w> under for winter crops."
                            }
                        }
                    ],
                    "synonyms": [
                        "жнивьё"
                    ],
                    "common_phrases": [
                        "идти по стерни",
                        "запахивать стернь",
                        "бескрайняя стернь"
                    ],
                    "target_lang_definitions": {
                        "en": "A field where grain crops have been harvested and cut stems remain."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stubble field",
                                "target_lang_sense_clarification": "field after harvest"
                            },
                            {
                                "target_lang_word": "stubble",
                                "target_lang_sense_clarification": "field covered with cut stalks"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26814ffc-b1bb-3aec-8b4c-167e6ab774f7",
                    "sense_definition": "A smaller, self-contained part of a larger entity, often referring to a manufactured object that is part of a larger device or system.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trust is a vital <w>component</w> of any successful relationship.",
                            "target_lang_translations": {
                                "ru": "Доверие — жизненно важный <w>компонент</w> любых успешных отношений."
                            }
                        },
                        {
                            "text": "The factory produces electronic <w>components</w> for the automotive industry.",
                            "target_lang_translations": {
                                "ru": "Завод производит электронные <w>компоненты</w> для автомобильной промышленности."
                            }
                        }
                    ],
                    "synonyms": [
                        "part",
                        "element",
                        "constituent",
                        "unit",
                        "module",
                        "piece",
                        "segment"
                    ],
                    "antonyms": [
                        "whole"
                    ],
                    "common_phrases": [
                        "key component",
                        "essential component",
                        "main component",
                        "electronic component",
                        "core component"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составная часть целого, элемент системы или устройства, часто являющийся самостоятельным изделием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компоне́нт",
                                "target_lang_sense_clarification": "часть целого"
                            },
                            {
                                "target_lang_word": "составна́я часть",
                                "target_lang_sense_clarification": "элемент конструкции"
                            },
                            {
                                "target_lang_word": "дета́ль",
                                "target_lang_sense_clarification": "элемент механизма или устройства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e37bdfc6-c2cc-3ffc-93d1-622542375224",
                    "sense_definition": "A connected subgraph that is not part of any larger connected subgraph.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The algorithm identifies every connected <w>component</w> within the network graph.",
                            "target_lang_translations": {
                                "ru": "Алгоритм определяет каждую связную <w>компоненту</w> в графе сети."
                            }
                        },
                        {
                            "text": "In this model, each <w>component</w> represents a distinct cluster of data points.",
                            "target_lang_translations": {
                                "ru": "В этой модели каждый <w>компонент</w> представляет собой отдельный кластер точек данных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "connected component"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связный подграф, не являющийся частью большего связного подграфа (в теории графов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компоне́нта",
                                "target_lang_sense_clarification": "математический термин (компонента связности)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1db958ff-b412-31d6-ab48-78c157266467",
                    "sense_definition": "Serving as one of the parts that make up a larger whole.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to examine the <w>component</w> parts of the engine to find the fault.",
                            "target_lang_translations": {
                                "ru": "Нам нужно осмотреть <w>составные</w> части двигателя, чтобы найти неисправность."
                            }
                        },
                        {
                            "text": "The <w>component</w> elements of the plan were discussed in detail.",
                            "target_lang_translations": {
                                "ru": "<w>Составные</w> элементы плана были подробно обсуждены."
                            }
                        }
                    ],
                    "synonyms": [
                        "constituent",
                        "integral"
                    ],
                    "common_phrases": [
                        "component part",
                        "component element"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся одной из частей, составляющих большее целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составно́й",
                                "target_lang_sense_clarification": "являющийся частью чего-либо"
                            },
                            {
                                "target_lang_word": "компоне́нтный",
                                "target_lang_sense_clarification": "относящийся к компонентам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53086b11-e6b4-3a33-9ef7-bd3e174b98bf",
                    "sense_definition": "Made up of smaller complete units combined together, particularly regarding audio equipment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He prefers a <w>component</w> stereo system because it offers better sound quality.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает <w>блочную</w> стереосистему, так как она обеспечивает лучшее качество звука."
                            }
                        },
                        {
                            "text": "The <w>component</w> design allows users to upgrade individual pieces of hardware.",
                            "target_lang_translations": {
                                "ru": "<w>Модульная</w> конструкция позволяет пользователям обновлять отдельные части оборудования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "component stereo",
                        "component system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из отдельных функциональных блоков (обычно об аудиоаппаратуре)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бло́чный",
                                "target_lang_sense_clarification": "состоящий из блоков"
                            },
                            {
                                "target_lang_word": "мо́дульный",
                                "target_lang_sense_clarification": "состоящий из модулей"
                            }
                        ]
                    }
                }
            ]
        }
    ]

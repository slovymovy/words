                    "sense_id": "663ce8e2-deee-3fe8-8dbc-1ddc7bb88c82",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>steun</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brug rust op vier betonnen <w>steunen</w> die in het water staan.",
                            "target_lang_translations": {
                                "en": "The bridge rests on four concrete <w>supports</w> standing in the water.",
                                "ru": "Мост покоится на четырех бетонных <w>опорах</w>, стоящих в воде."
                            }
                        },
                        {
                            "text": "We hebben extra <w>steunen</w> nodig om de boekenplank recht te hangen.",
                            "target_lang_translations": {
                                "en": "We need extra <w>supports</w> to hang the bookshelf straight.",
                                "ru": "Нам нужны дополнительные <w>опоры</w>, чтобы ровно повесить книжную полку."
                            }
                        }
                    ],

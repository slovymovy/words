    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c2ed5022-6e67-3c2a-ad5a-60a15dba79dd",
                    "sense_definition": "Effective as proof or evidence; capable of causing someone to believe that something is true or real.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer presented a <w>convincing</w> argument to the jury.",
                            "target_lang_translations": {
                                "ru": "Адвокат представил присяжным <w>убедительный</w> довод."
                            }
                        },
                        {
                            "text": "Her explanation did not sound very <w>convincing</w> to me.",
                            "target_lang_translations": {
                                "ru": "Её объяснение прозвучало для меня не очень <w>убедительно</w>."
                            }

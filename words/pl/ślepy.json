            "senses": [
                {
                    "sense_id": "8a042498-d0b7-3285-a52b-b148b32b6eb8",
                    "sense_definition": "Taki, który nie widzi lub stracił wzrok.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_disability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary pies jest już prawie całkowicie <w>ślepy</w>.",
                            "target_lang_translations": {
                                "ru": "Этот старый пес уже почти полностью <w>слепой</w>."
                            }
                        },
                        {
                            "text": "Mój dziadek był <w>ślepy</w> na jedno oko po wypadku w pracy.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был <w>слеп</w> на один глаз после несчастного случая на работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewidomy",
                        "ociemniały"
                    ],
                    "antonyms": [
                        "widzący"
                    ],
                    "common_phrases": [
                        "ślepy na jedno oko",
                        "ślepy od urodzenia",
                        "całkowicie ślepy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто не видит или потерял зрение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепой",
                                "target_lang_sense_clarification": "лишенный зрения"
                            },
                            {
                                "target_lang_word": "незрячий",
                                "target_lang_sense_clarification": "вежливая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1a08a26-9d8b-30b3-9d15-3e1ede81e35c",
                    "sense_definition": "O drodze, korytarzu lub pomieszczeniu: nieposiadający wylotu, zamknięty z jednej strony.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_dead_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca zorientował się, że wjechał w <w>ślepą</w> uliczkę.",
                            "target_lang_translations": {
                                "ru": "Водитель понял, что заехал в <w>тупиковую</w> улочку."
                            }
                        },
                        {
                            "text": "To śledztwo to <w>ślepy</w> zaułek, nie mamy żadnych nowych dowodów.",
                            "target_lang_translations": {
                                "ru": "Это расследование — <w>тупиковый</w> путь, у нас нет никаких новых улик."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ślepa uliczka",
                        "ślepy zaułek",
                        "ślepy tor"
                    ],
                    "target_lang_definitions": {
                        "ru": "О дороге, коридоре или помещении: не имеющий сквозного прохода, закрытый с одной стороны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тупиковый",
                                "target_lang_sense_clarification": "не имеющий выхода"
                            },
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "закрытый, непроходной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bcf5c3a-a5db-3d56-be1f-6bf9770e7dc1",
                    "sense_definition": "Taki, który nie dostrzega oczywistych faktów, prawdy lub czyichś wad.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_ignorance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Byłem <w>ślepy</w> i nie widziałem, że ona mnie okłamuje.",
                            "target_lang_translations": {
                                "ru": "Я был <w>слеп</w> и не видел, что она меня обманывает."
                            }
                        },
                        {
                            "text": "Politycy często są <w>ślepi</w> na codzienne problemy zwykłych obywateli.",
                            "target_lang_translations": {
                                "ru": "Политики часто <w>слепы</w> к повседневным проблемам обычных граждан."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaślepiony"
                    ],
                    "common_phrases": [
                        "być ślepym na argumenty",
                        "być ślepym i głuchym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто не замечает очевидных фактов, правды или чьих-то недостатков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепой",
                                "target_lang_sense_clarification": "перен. не замечающий очевидного"
                            },
                            {
                                "target_lang_word": "ослепленный",
                                "target_lang_sense_clarification": "введенный в заблуждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fbe506e-7a25-30d9-bb1e-02c92060d820",
                    "sense_definition": "Niekierujący się rozsądkiem, gwałtowny, bezkrytyczny lub przypadkowy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz okazywał <w>ślepe</w> posłuszeństwo swojemu dowódcy.",
                            "target_lang_translations": {
                                "ru": "Солдат проявлял <w>слепое</w> повиновение своему командиру."
                            }
                        },
                        {
                            "text": "O naszym zwycięstwie w tym meczu zadecydował <w>ślepy</w> traf.",
                            "target_lang_translations": {
                                "ru": "Нашу победу в этом матче решил <w>слепой</w> случай."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaślepiony"
                    ],
                    "common_phrases": [
                        "ślepy traf",
                        "ślepe posłuszeństwo",
                        "ślepa furia",
                        "ślepa miłość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не руководствующийся разумом, безотчетный, некритичный или случайный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепой",
                                "target_lang_sense_clarification": "безотчетный, фанатичный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7411ae7c-5aef-3d58-8fd7-693f14fc8266",
                    "sense_definition": "W biologii: taki, który naturalnie nie posiada narządu wzroku.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biological_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jaskiniach żyje specyficzny gatunek <w>ślepej</w> ryby.",
                            "target_lang_translations": {
                                "ru": "В пещерах обитает специфический вид <w>слепой</w> рыбы."
                            }
                        },
                        {
                            "text": "Ewolucja sprawiła, że ten podziemny ssak jest całkowicie <w>ślepy</w>.",
                            "target_lang_translations": {
                                "ru": "Эволюция привела к тому, что это подземное млекопитающее полностью <w>слепое</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В биологии: естественно не имеющий органов зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепой",
                                "target_lang_sense_clarification": "биологический вид"
                            }
                        ]
                    }
                }
            ]
        },

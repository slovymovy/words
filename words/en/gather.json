{
    "word_family": [
        "gathering",
        "gatherer",
        "regather",
        "ingathering"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8dbfdf8c-4f44-3a9a-a6aa-4fe34d3e9df8",
                    "sense_definition": "To bring together things that are scattered or separate into one place or group.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bent down to <w>gather</w> the scattered papers from the floor.",
                            "target_lang_translations": {
                                "ru": "Она наклонилась, чтобы <w>собрать</w> разбросанные по полу бумаги."
                            }
                        },
                        {
                            "text": "We need to <w>gather</w> information before making a decision.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>собрать</w> информацию перед принятием решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "round up",
                        "assemble",
                        "aggroup",
                        "begather"
                    ],
                    "antonyms": [
                        "disperse",
                        "scatter",
                        "distribute"
                    ],
                    "common_phrases": [
                        "gather information",
                        "gather dust",
                        "gather evidence",
                        "gather resources"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать разбросанные или отдельные вещи в одно место или группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "объединять, коллекционировать"
                            },
                            {
                                "target_lang_word": "собрать",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7179108e-391e-3983-9d9f-ecd8327a5a3b",
                    "sense_definition": "To come together as a group; to congregate.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People began to <w>gather</w> around the street performer to watch the show.",
                            "target_lang_translations": {
                                "ru": "Люди начали <w>собираться</w> вокруг уличного артиста, чтобы посмотреть представление."
                            }
                        },
                        {
                            "text": "The whole family will <w>gather</w> for dinner on Sunday.",
                            "target_lang_translations": {
                                "ru": "Вся семья <w>соберётся</w> на ужин в воскресенье."
                            }
                        }
                    ],
                    "synonyms": [
                        "assemble",
                        "aggroup",
                        "begather"
                    ],
                    "antonyms": [
                        "disperse",
                        "scatter"
                    ],
                    "common_phrases": [
                        "gather around",
                        "gather together",
                        "gather round"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собираться вместе группой; сходиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "встречаться, сходиться"
                            },
                            {
                                "target_lang_word": "собраться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b347d48-c914-37ae-a860-2c6dfe032190",
                    "sense_definition": "To collect plants, fruits, or crops from nature or a garden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to the forest to <w>gather</w> mushrooms for dinner.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в лес <w>собирать</w> грибы на ужин."
                            }
                        },
                        {
                            "text": "Farmers work hard to <w>gather</w> the harvest before the rain starts.",
                            "target_lang_translations": {
                                "ru": "Фермеры усердно трудятся, чтобы <w>собрать</w> урожай до начала дождей."
                            }
                        }
                    ],
                    "synonyms": [
                        "round up",
                        "assemble"
                    ],
                    "common_phrases": [
                        "gather crops",
                        "gather wood",
                        "gather flowers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать растения, фрукты или урожай."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "урожай, грибы, ягоды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6ed4ae8-1863-3b5a-953f-73c0c216ccee",
                    "sense_definition": "To gradually increase in amount, magnitude, or force over time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train began to <w>gather</w> speed as it left the station.",
                            "target_lang_translations": {
                                "ru": "Поезд начал <w>набирать</w> скорость, отходя от станции."
                            }
                        },
                        {
                            "text": "Over the years, he had <w>gathered</w> a large collection of rare coins.",
                            "target_lang_translations": {
                                "ru": "За эти годы он <w>собрал</w> большую коллекцию редких монет."
                            }
                        }
                    ],
                    "synonyms": [
                        "accumulate",
                        "accrue",
                        "add up"
                    ],
                    "antonyms": [
                        "dissipate",
                        "lose"
                    ],
                    "common_phrases": [
                        "gather speed",
                        "gather momentum",
                        "gather strength",
                        "gather pace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постепенно увеличиваться в количестве, величине или силе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набирать",
                                "target_lang_sense_clarification": "скорость, силу"
                            },
                            {
                                "target_lang_word": "накапливать",
                                "target_lang_sense_clarification": "аккумулировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3665423b-ff3c-3aa6-8480-d481494bd046",
                    "sense_definition": "To understand or conclude something based on available information or context.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "inference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "From his silence, I <w>gathered</w> that things had not gone well.",
                            "target_lang_translations": {
                                "ru": "По его молчанию я <w>понял</w>, что дела шли не очень хорошо."
                            }
                        },
                        {
                            "text": "I <w>gather</w> you are not interested in joining us for lunch.",
                            "target_lang_translations": {
                                "ru": "Я <w>так понимаю</w>, вам не интересно пообедать с нами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "I gather that",
                        "from what I gather",
                        "as I gather"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать или делать вывод на основе имеющейся информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "делать вывод"
                            },
                            {
                                "target_lang_word": "заключать",
                                "target_lang_sense_clarification": "приходить к заключению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35dceb2a-7188-3cbf-ad4c-3ad725356158",
                    "sense_definition": "To pull or draw parts of a garment or object closer around oneself or together.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "manipulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>gathered</w> the shawl tightly about her shoulders to keep warm.",
                            "target_lang_translations": {
                                "ru": "Она плотно <w>запахнула</w> шаль на плечах, чтобы согреться."
                            }
                        },
                        {
                            "text": "He <w>gathered</w> the loose ends of the rope into a neat coil.",
                            "target_lang_translations": {
                                "ru": "Он <w>собрал</w> свободные концы веревки в аккуратный моток."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gather up",
                        "gather close"
                    ],
                    "target_lang_definitions": {
                        "ru": "Притягивать или подбирать части одежды или предмета ближе к себе или друг к другу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подбирать",
                                "target_lang_sense_clarification": "подтягивать концы"
                            },
                            {
                                "target_lang_word": "запахивать",
                                "target_lang_sense_clarification": "одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ace74618-5643-386e-b7c4-891d0dd35fe4",
                    "sense_definition": "To sew fabric in a way that creates small folds or pleats to reduce its width.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "manipulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>gather</w> the skirt at the waist to make it fit properly.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>присборить</w> юбку на талии, чтобы она сидела правильно."
                            }
                        },
                        {
                            "text": "The sleeves were <w>gathered</w> at the cuffs for a decorative effect.",
                            "target_lang_translations": {
                                "ru": "Рукава были <w>присборены</w> на манжетах для декоративного эффекта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gather the fabric",
                        "gather a seam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сшивать ткань так, чтобы образовались мелкие складки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присбаривать",
                                "target_lang_sense_clarification": "делать складки"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "в сборку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "85cbfb4f-b963-3cf9-8e04-03aac5fcf1bb",
                    "sense_definition": "A fold or pucker in fabric made by pulling a thread to tighten it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sewing_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dress features a small <w>gather</w> at the back for a better fit.",
                            "target_lang_translations": {
                                "ru": "Платье имеет небольшую <w>сборку</w> сзади для лучшей посадки."
                            }
                        },
                        {
                            "text": "Check the <w>gather</w> on the curtain to ensure the pleats are even.",
                            "target_lang_translations": {
                                "ru": "Проверьте <w>сборку</w> на шторе, чтобы убедиться, что складки ровные."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Складка или сборка на ткани, образованная стягиванием нити."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сборка",
                                "target_lang_sense_clarification": "на ткани"
                            },
                            {
                                "target_lang_word": "складка",
                                "target_lang_sense_clarification": "элемент одежды"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "networking",
        "networker",
        "subnetwork",
        "internetwork"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8bb23ee-d993-336a-ba3c-1e9ab1b49f72",
                    "sense_definition": "A system of computers and other devices connected together to share information.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computer_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The printer is connected to the office <w>network</w>.",
                            "target_lang_translations": {
                                "ru": "Принтер подключен к офисной <w>сети</w>."
                            }
                        },
                        {
                            "text": "You need a password to access the secure wireless <w>network</w>.",
                            "target_lang_translations": {
                                "ru": "Вам нужен пароль для доступа к защищенной беспроводной <w>сети</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "system",
                        "LAN",
                        "WAN"
                    ],
                    "common_phrases": [
                        "computer network",
                        "wireless network",
                        "network connection",
                        "local area network"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система компьютеров и других устройств, соединенных для обмена информацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеть",
                                "target_lang_sense_clarification": "компьютерная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bf54e31-9d14-3a02-a2aa-c860acd8556f",
                    "sense_definition": "A large system consisting of many similar parts that are connected together to allow movement or communication.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country has an extensive <w>network</w> of high-speed railways.",
                            "target_lang_translations": {
                                "ru": "В стране имеется обширная <w>сеть</w> скоростных железных дорог."
                            }
                        },
                        {
                            "text": "A complex <w>network</w> of roads covers the entire region.",
                            "target_lang_translations": {
                                "ru": "Сложная <w>сеть</w> дорог покрывает весь регион."
                            }
                        }
                    ],
                    "synonyms": [
                        "system",
                        "grid",
                        "web",
                        "mesh"
                    ],
                    "common_phrases": [
                        "rail network",
                        "road network",
                        "transport network",
                        "communications network"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая система, состоящая из множества соединенных частей (дорог, линий) для обеспечения движения или связи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеть",
                                "target_lang_sense_clarification": "дорог, транспорта, учреждений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25d2d636-0867-3568-ac56-4aca07d485f4",
                    "sense_definition": "A group of people who know each other and work together or share information, often for professional purposes.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She built a strong <w>network</w> of contacts during her time at the university.",
                            "target_lang_translations": {
                                "ru": "Во время учебы в университете она создала надежную <w>сеть</w> контактов."
                            }
                        },
                        {
                            "text": "Having a good support <w>network</w> is important for your mental health.",
                            "target_lang_translations": {
                                "ru": "Наличие хорошей <w>сети</w> поддержки важно для вашего психического здоровья."
                            }
                        }
                    ],
                    "synonyms": [
                        "circle",
                        "community",
                        "web"
                    ],
                    "common_phrases": [
                        "social network",
                        "support network",
                        "business network",
                        "old boy network"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, поддерживающих профессиональные или социальные контакты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеть",
                                "target_lang_sense_clarification": "контактов, социальная"
                            },
                            {
                                "target_lang_word": "связи",
                                "target_lang_sense_clarification": "знакомства"
                            },
                            {
                                "target_lang_word": "круг знакомств",
                                "target_lang_sense_clarification": "группа людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fcbbac9-1634-3333-b356-77f9960d250e",
                    "sense_definition": "A group of television or radio stations that broadcast the same programs.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "media_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The major television <w>network</w> will broadcast the debate live tonight.",
                            "target_lang_translations": {
                                "ru": "Крупная телевизионная <w>сеть</w> будет транслировать дебаты в прямом эфире сегодня вечером."
                            }
                        },
                        {
                            "text": "Several local stations are affiliated with the national news <w>network</w>.",
                            "target_lang_translations": {
                                "ru": "Несколько местных станций являются филиалами национальной новостной <w>сети</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "channel",
                        "station"
                    ],
                    "common_phrases": [
                        "television network",
                        "cable network",
                        "news network",
                        "radio network"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа теле- или радиостанций, вещающих одни и те же программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеть",
                                "target_lang_sense_clarification": "телевещания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37a6150b-f83c-36e6-85a7-f2d1d70f67db",
                    "sense_definition": "A fabric or structure made of cords, threads, or wires that cross each other at regular intervals.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_mesh",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fisherman repaired the intricate <w>network</w> of his nets before heading out to sea.",
                            "target_lang_translations": {
                                "ru": "Рыбак починил запутанную <w>сеть</w> своих снастей перед выходом в море."
                            }
                        },
                        {
                            "text": "Under the microscope, the tissue appeared as a fine <w>network</w> of fibers.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом ткань выглядела как тонкая <w>сеть</w> волокон."
                            }
                        }
                    ],
                    "synonyms": [
                        "mesh",
                        "net",
                        "web",
                        "netting"
                    ],
                    "common_phrases": [
                        "network of veins",
                        "network of nerves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Структура из переплетенных нитей, шнуров или проволоки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеть",
                                "target_lang_sense_clarification": "плетеная структура, паутина"
                            },
                            {
                                "target_lang_word": "сетка",
                                "target_lang_sense_clarification": "материал, ячеистая структура"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f5d2c0a2-5ff0-3022-a591-ef831b8eb6bb",
                    "sense_definition": "To meet and talk to people in order to exchange information and establish professional contacts.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The conference gave us a great opportunity to <w>network</w> with industry leaders.",
                            "target_lang_translations": {
                                "ru": "Конференция предоставила нам отличную возможность <w>наладить связи</w> с лидерами отрасли."
                            }
                        },
                        {
                            "text": "He likes to <w>network</w> at social events to find new clients for his business.",
                            "target_lang_translations": {
                                "ru": "Он любит <w>заводить знакомства</w> на светских мероприятиях, чтобы найти новых клиентов для своего бизнеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "socialize",
                        "mingle",
                        "associate"
                    ],
                    "common_phrases": [
                        "network with people",
                        "network for jobs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заводить знакомства и общаться для установления деловых контактов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налаживать связи",
                                "target_lang_sense_clarification": "устанавливать контакты"
                            },
                            {
                                "target_lang_word": "заводить знакомства",
                                "target_lang_sense_clarification": "общаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a16cbca9-3a53-3454-b13f-6d980ba1b65d",
                    "sense_definition": "To connect computers or other devices together so that they can share information.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computer_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The IT technician will <w>network</w> all the new computers in the office tomorrow.",
                            "target_lang_translations": {
                                "ru": "IT-специалист завтра <w>объединит в сеть</w> все новые компьютеры в офисе."
                            }
                        },
                        {
                            "text": "It can be difficult to <w>network</w> older machines with modern servers.",
                            "target_lang_translations": {
                                "ru": "Может быть трудно <w>соединить в сеть</w> старые машины с современными серверами."
                            }
                        }
                    ],
                    "synonyms": [
                        "connect",
                        "link",
                        "interconnect"
                    ],
                    "antonyms": [
                        "disconnect"
                    ],
                    "common_phrases": [
                        "network computers",
                        "network devices"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять компьютеры или устройства в единую систему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединять в сеть",
                                "target_lang_sense_clarification": "компьютеры"
                            },
                            {
                                "target_lang_word": "подключать к сети",
                                "target_lang_sense_clarification": "устройства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e25c1ce-51dc-35bd-9cbc-77a15a1a535c",
                    "sense_definition": "To broadcast a program across an entire group of stations at the same time.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "media_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The channel decided to <w>network</w> the charity concert to reach a wider audience.",
                            "target_lang_translations": {
                                "ru": "Канал решил <w>транслировать по всей сети</w> благотворительный концерт, чтобы охватить более широкую аудиторию."
                            }
                        },
                        {
                            "text": "The interview was <w>networked</w> across all affiliated stations last night.",
                            "target_lang_translations": {
                                "ru": "Интервью <w>транслировалось по всей сети</w> всех дочерних станций вчера вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "broadcast",
                        "transmit",
                        "air"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транслировать программу одновременно по группе станций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транслировать по сети",
                                "target_lang_sense_clarification": "вещать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
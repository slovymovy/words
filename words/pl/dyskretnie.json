                            {
                                "target_lang_word": "конфиденциально",
                                "target_lang_sense_clarification": "секретно, не для огласки"
                            },
                            {
                                "target_lang_word": "деликатно",
                                "target_lang_sense_clarification": "осторожно, чтобы не доставить неудобств"

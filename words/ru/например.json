    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "702df05d-97f3-35f2-a94e-b327e96f0b49",
                    "sense_definition": "Вводное слово, используемое для пояснения или уточнения сказанного ранее путём приведения одного или нескольких примеров.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В некоторых языках, как <w>например</w> во французском, нет среднего рода.",
                            "target_lang_translations": {
                                "en": "In some languages, like in French <w>for example</w>, there is no neuter gender."
                            }
                        },
                        {
                            "text": "Многие фрукты, <w>например</w> яблоки и груши, очень полезны для здоровья.",
                            "target_lang_translations": {
                                "en": "Many fruits, <w>for example</w> apples and pears, are very healthy."
                            }
                        }
                    ],
                    "synonyms": [
                        "к примеру",
                        "для примера"
                    ],
                    "antonyms": [
                        "в общем",
                        "вообще"
                    ],
                    "common_phrases": [
                        "как например",
                        "вот например"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to introduce an illustrative example or clarification of a previous statement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for example",
                                "target_lang_sense_clarification": "most common standard usage"
                            },
                            {
                                "target_lang_word": "for instance",
                                "target_lang_sense_clarification": "synonymous with 'for example'"
                            },
                            {
                                "target_lang_word": "e.g.",
                                "target_lang_sense_clarification": "abbreviation used in writing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cef509a-0607-3a97-8880-ccb44342b197",
                    "sense_definition": "Употребляется для выделения конкретного лица, предмета или случая из ряда возможных для подтверждения мысли.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я, <w>например</w>, никогда ничего подобного раньше не видел.",
                            "target_lang_translations": {
                                "en": "I, <w>for instance</w>, have never seen anything like this before."
                            }
                        },
                        {
                            "text": "Мой брат, <w>например</w>, совершенно не умеет готовить.",
                            "target_lang_translations": {
                                "en": "My brother, <w>for example</w>, cannot cook at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "в частности"
                    ],
                    "common_phrases": [
                        "я например"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to single out a specific person, object, or case to support a point or argument."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for instance",
                                "target_lang_sense_clarification": "often used when citing a specific case"
                            },
                            {
                                "target_lang_word": "for example",
                                "target_lang_sense_clarification": "general usage"
                            }
                        ]
                    }
                }
            ]
        }

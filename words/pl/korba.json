{
    "word_family": [
        "korbowy",
        "korbowód",
        "korbka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eeb133d8-6984-3758-a30d-e4f0c50b2ebc",
                    "sense_definition": "Zakrzywione ramię lub dźwignia służąca do ręcznego wprawiania mechanizmu w ruch obrotowy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mechanical_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mocno kręcić <korbą>, aby wyciągnąć wiadro wody ze studni.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сильно крутить <рукоятку>, чтобы вытащить ведро воды из колодца."
                            }
                        },
                        {
                            "text": "Stary samochód trzeba było odpalać za pomocą metalowej <korby>.",
                            "target_lang_translations": {
                                "ru": "Старую машину нужно было заводить с помощью металлической <заводной ручки>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kręcić korbą",
                        "korba do studni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изогнутый рычаг или рукоятка для приведения механизма во вращательное движение вручную."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рукоятка",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "кривошип",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "заводная ручка",
                                "target_lang_sense_clarification": "для автомобилей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fb925b1-9362-3ca7-b7a8-d6e8ebfc42f6",
                    "sense_definition": "Potoczne określenie na stan utraty rozsądku, nagłego ataku złości lub obsesji na jakimś punkcie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mojemu sąsiadowi zupełnie odbiła <korba> i zaczął krzyczeć na przechodniów.",
                            "target_lang_translations": {
                                "ru": "У моего соседа совсем поехала <крыша>, и он начал кричать на прохожих."
                            }
                        },
                        {
                            "text": "Marek ma pozytywną <korbę> na punkcie starych motocykli.",
                            "target_lang_translations": {
                                "ru": "У Марека позитивный <сдвиг> на почве старых мотоциклов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odbiła komuś korba",
                        "mieć korbę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное выражение, обозначающее потерю рассудка, приступ гнева или одержимость чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдвиг",
                                "target_lang_sense_clarification": "сумасшествие, странность"
                            },
                            {
                                "target_lang_word": "бзик",
                                "target_lang_sense_clarification": "одержимость, причуда"
                            },
                            {
                                "target_lang_word": "крыша",
                                "target_lang_sense_clarification": "в идиоме 'поехала крыша'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5790f48-0999-376c-aef6-67e2b08acf84",
                    "sense_definition": "Element układu napędowego roweru łączący oś suportu z pedałem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mechanical_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas jazdy w trudnym terenie pękła mi lewa <korba>.",
                            "target_lang_translations": {
                                "ru": "Во время езды по сложной местности у меня сломался левый <шатун>."
                            }
                        },
                        {
                            "text": "Wymiana <korby> w rowerze wymaga użycia specjalnego ściągacza.",
                            "target_lang_translations": {
                                "ru": "Замена <шатуна> на велосипеде требует использования специального съемника."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент велосипедного привода, соединяющий ось каретки с педалью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шатун",
                                "target_lang_sense_clarification": "велосипедная деталь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a2d998a-622e-3b9d-bf9c-f829857d2cde",
                    "sense_definition": "Prostokątny koszyk wykonany z łubu lub wikliny, używany często do zbierania owoców.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia przyniosła z targu pełną <korbę> świeżych truskawek.",
                            "target_lang_translations": {
                                "ru": "Бабушка принесла с рынка полное <лукошко> свежей клубники."
                            }
                        },
                        {
                            "text": "Wzięliśmy dużą <korbę> i poszliśmy do lasu na grzyby.",
                            "target_lang_translations": {
                                "ru": "Мы взяли большое <лукошко> и пошли в лес за грибами."
                            }
                        }
                    ],
                    "synonyms": [
                        "łubianka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямоугольная корзина из луба или лозы, часто используемая для сбора ягод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лукошко",
                                "target_lang_sense_clarification": "корзина из луба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c53ae25f-48e4-3df6-b5b9-eb046472105e",
                    "sense_definition": "Urządzenie sterownicze w dawnych tramwajach służące do regulacji prędkości i hamowania.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Motorniczy sprawnie operował <korbą>, zatrzymując tramwaj na przystanku.",
                            "target_lang_translations": {
                                "ru": "Вагоновожатый ловко управлял <рукояткой контроллера>, останавливая трамвай на остановке."
                            }
                        },
                        {
                            "text": "W muzeum komunikacji można zobaczyć starą <korbę> tramwajową.",
                            "target_lang_translations": {
                                "ru": "В музее транспорта можно увидеть старую трамвайную <рукоятку контроллера>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство управления в старых трамваях для регулирования скорости и торможения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рукоятка контроллера",
                                "target_lang_sense_clarification": "рычаг управления"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
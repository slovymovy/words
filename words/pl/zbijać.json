                            {
                                "target_lang_word": "опровергать",
                                "target_lang_sense_clarification": "доказывать ложность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49b8a715-e871-39fd-87e3-74a772435148",
                    "sense_definition": "Zarabiać bardzo duże pieniądze, często w sposób łatwy lub szybki (używane głównie w zwrotach frazeologicznych).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciele tej firmy zaczęli <w>zbijać</w> kokosy na sprzedaży nowych technologii.",
                            "target_lang_translations": {
                                "ru": "Владельцы этой фирмы начали <w>зашибать</w> бешеные деньги на продаже новых технологий."
                            }
                        },
                        {
                            "text": "Oni <w>zbijają</w> fortunę na handlu nieruchomościami za granicą.",
                            "target_lang_translations": {
                                "ru": "Они <w>сколачивают</w> состояние на торговле недвижимостью за границей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbijać kokosy",
                        "zbijać majątek",
                        "zbijać fortunę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Зарабатывать очень большие деньги, часто легко или быстро (используется в идиомах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сколачивать",
                                "target_lang_sense_clarification": "состояние или капитал"
                            },
                            {
                                "target_lang_word": "зашибать",
                                "target_lang_sense_clarification": "деньги (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30ccdf84-2a81-3d76-9737-e6588647e57f",
                    "sense_definition": "Gromadzić ludzi lub zwierzęta w ciasną grupę; ściskać coś, aby zajmowało mniej miejsca.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasterz <w>zbija</w> owce w stado, aby zaprowadzić je do zagrody.",
                            "target_lang_translations": {
                                "ru": "Пастух <w>сбивает</w> овец в стадо, чтобы отвести их в загон."
                            }
                        },
                        {
                            "text": "Tłum ludzi <w>zbijał</w> się coraz ciaśniej przy wejściu na koncert.",
                            "target_lang_translations": {
                                "ru": "Толпа людей <w>сбивалась</w> все плотнее у входа на концерт."
                            }
                        }
                    ],
                    "synonyms": [
                        "zganiać"
                    ],
                    "antonyms": [
                        "rozganiać"
                    ],
                    "common_phrases": [
                        "zbijać w stado",
                        "zbijać w gromadę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать людей или животных в плотную группу; сжимать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивать",
                                "target_lang_sense_clarification": "в кучу или стадо"
                            },
                            {
                                "target_lang_word": "сгонять",
                                "target_lang_sense_clarification": "собирать в одно место"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cbd489ec-e66f-3957-9ba2-80ed9125c4ed",
                    "sense_definition": "Ulegać rozbiciu na kawałki (o przedmiotach szklanych lub kruchych).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kieliszki w naszej restauracji <w>zbijają się</w> niestety bardzo często.",
                            "target_lang_translations": {
                                "ru": "Бокалы в нашем ресторане, к сожалению, очень часто <w>бьются</w>."
                            }
                        },
                        {
                            "text": "To szkło jest hartowane, więc nie <w>zbija się</w> tak łatwo jak zwykłe.",
                            "target_lang_translations": {
                                "ru": "Это стекло закаленное, поэтому оно не <w>разбивается</w> так легко, как обычное."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łatwo się zbijać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушаться на куски (о стеклянных или хрупких предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "биться",
                                "target_lang_sense_clarification": "иметь свойство разбиваться"
                            },
                            {
                                "target_lang_word": "разбиваться",
                                "target_lang_sense_clarification": "разлетаться на куски"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9448c992-67e3-3079-887a-c9d2d99e49eb",
                    "sense_definition": "Żartować sobie z kogoś, naśmiewać się lub kpić w sposób lekceważący.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bierz tego na poważnie, on tylko <w>zbija się</w> z twojej nowej fryzury.",
                            "target_lang_translations": {
                                "ru": "Не принимай это всерьез, он просто <w>прикалывается</w> над твоей новой прической."
                            }
                        },
                        {
                            "text": "Chłopaki w klasie ciągle <w>zbijają się</w> z młodszego kolegi.",
                            "target_lang_translations": {
                                "ru": "Парни в классе постоянно <w>стебутся</w> над младшим товарищем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbijać się z kogoś"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "youth language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шутить над кем-то, насмехаться или издеваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикалываться",
                                "target_lang_sense_clarification": "шутить (разг.)"
                            },
                            {
                                "target_lang_word": "стебаться",
                                "target_lang_sense_clarification": "насмехаться (сленг)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6e67f28-e10b-34f0-a4d9-6c411586712c",
                    "sense_definition": "Gromadzić się w ciasną, zbitą masę lub skupisko.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mokry śnieg pod butami <w>zbija się</w> w twarde i śliskie bryły.",
                            "target_lang_translations": {
                                "ru": "Мокрый снег под ботинками <w>сбивается</w> в твердые и скользкие комья."
                            }
                        },
                        {
                            "text": "Sierść twojego psa <w>zbija się</w> w kołtuny, musisz go częściej czesać.",
                            "target_lang_translations": {
                                "ru": "Шерсть твоей собаки <w>сваливается</w> в колтуны, ты должен ее чаще расчесывать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbijać się w grudki",
                        "zbijać się w masę"
                    ],
                    "target_lang_definitions": {

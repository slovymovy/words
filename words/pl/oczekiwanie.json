            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b7710be7-942e-301e-991a-bb009b7a440e",
                    "sense_definition": "Rzeczownik odczasownikowy od słowa <w>oczekiwać</w>; czynność czekania na kogoś lub na coś, często z nadzieją lub niecierpliwością.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "waiting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej <oczekiwanie> na spóźniony pociąg trwało ponad dwie godziny.",
                            "target_lang_translations": {
                                "ru": "Её <ожидание> опаздывающего поезда длилось более двух часов."
                            }
                        },
                        {
                            "text": "Wszyscy trwali w napiętym <oczekiwaniu> na ogłoszenie oficjalnych wyników wyborów.",
                            "target_lang_translations": {
                                "ru": "Все находились в напряжённом <ожидании> объявления официальных результатов выборов."
                            }
                        }
                    ],
                    "synonyms": [
                        "czekanie",
                        "wyczekiwanie",
                        "wyglądanie",
                        "wypatrywanie"
                    ],
                    "common_phrases": [
                        "w oczekiwaniu na",
                        "niecierpliwe oczekiwanie",
                        "długie oczekiwanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по глаголу ожидать; состояние, когда кто-то ждет кого-то или что-то, часто с надеждой или нетерпением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ожидание",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66f35e1e-3a85-3808-903d-76846a413cb5",
                    "sense_definition": "To, czego spodziewamy się od kogoś lub czegoś; nadzieja, że coś się wydarzy, lub wymaganie wobec kogoś.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "expectation_requirement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy pracownik szybko spełnił wszystkie <oczekiwania> swojego szefa.",
                            "target_lang_translations": {
                                "ru": "Новый сотрудник быстро оправдал все <ожидания> своего начальника."
                            }
                        },
                        {
                            "text": "Wyniki sprzedaży w tym miesiącu przerosły nasze najśmielsze <oczekiwania>.",
                            "target_lang_translations": {
                                "ru": "Результаты продаж в этом месяце превзошли наши самые смелые <ожидания>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyczekiwanie",
                        "czekanie",
                        "wypatrywanie"
                    ],
                    "common_phrases": [
                        "spełnić oczekiwania",
                        "przerosnąć oczekiwania",
                        "mieć oczekiwania",
                        "powyżej oczekiwań",
                        "wbrew oczekiwaniom"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, на что рассчитывают или чего требуют от кого-либо; надежда на то, что определенное событие произойдет."
                    },
                    "translations": {
                        "ru": [

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9e39057-28c3-38ec-aab9-4dbe70a1bfab",
                    "sense_definition": "Класть в пищу больше соли, чем необходимо, делая её слишком солёной.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "salt_cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Повар старался не <w>пересаливать</w> суп, чтобы каждый мог добавить соль по вкусу.",
                            "target_lang_translations": {
                                "en": "The cook tried not to <w>oversalt</w> the soup so that everyone could add salt to taste."
                            }
                        },
                        {
                            "text": "Если постоянно <w>пересаливать</w> еду, это может негативно сказаться на здоровье.",
                            "target_lang_translations": {
                                "en": "If you constantly <w>put too much salt in</w> your food, it can negatively affect your health."
                            }
                        }
                    ],
                    "antonyms": [
                        "недосаливать"
                    ],
                    "common_phrases": [
                        "пересаливать суп",
                        "пересаливать блюдо",
                        "сильно пересаливать"
                    ],
                    "target_lang_definitions": {
                        "en": "To put more salt in food than necessary, making it too salty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oversalt",
                                "target_lang_sense_clarification": "culinary term"
                            },
                            {
                                "target_lang_word": "put too much salt in",
                                "target_lang_sense_clarification": "descriptive"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "700635fc-25eb-337c-bfbf-d996b4a61ce3",
                    "sense_definition": "Делать что-либо чрезмерно, переходить границы разумного; преувеличивать (переносное значение).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "exaggerate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рассказывая о своих приключениях, он любил немного <w>пересаливать</w> для эффекта.",
                            "target_lang_translations": {
                                "en": "When telling about his adventures, he liked to <w>exaggerate</w> a little for effect."
                            }
                        },
                        {
                            "text": "Критик начал <w>пересаливать</w> с замечаниями, и автор обиделся.",
                            "target_lang_translations": {
                                "en": "The critic started to <w>go too far</w> with his remarks, and the author got offended."
                            }
                        }
                    ],
                    "synonyms": [
                        "перелавливать"
                    ],
                    "common_phrases": [
                        "явно пересаливать",
                        "пересаливать с критикой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To do something excessively or cross reasonable boundaries; to exaggerate (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overdo it",
                                "target_lang_sense_clarification": "general behavior"
                            },
                            {
                                "target_lang_word": "exaggerate",
                                "target_lang_sense_clarification": "in storytelling or description"
                            },
                            {
                                "target_lang_word": "go too far",
                                "target_lang_sense_clarification": "crossing boundaries"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d86c5b0-c51a-3424-ba7d-ee7df7be4118",
                    "sense_definition": "Подвергать солению (консервированию с солью) много продуктов или предметов один за другим.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "salt_cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осенью бабушка начинала <w>пересаливать</w> огурцы и помидоры в больших бочках.",
                            "target_lang_translations": {
                                "en": "In the autumn, grandma would start <w>salting</w> cucumbers and tomatoes in large barrels."
                            }
                        },
                        {
                            "text": "Нам пришлось заново <w>пересаливать</w> всю рыбу, так как первая партия испортилась.",
                            "target_lang_translations": {
                                "en": "We had to <w>re-salt</w> all the fish because the first batch had spoiled."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пересаливать огурцы",
                        "пересаливать рыбу"
                    ],
                    "target_lang_definitions": {
                        "en": "To preserve many food items with salt one after another, or to salt something again."
                    },
                    "translations": {

                    "sense_id": "70d7149a-5e01-3276-a6ec-8605e3cf2072",
                    "sense_definition": "Verbogen vorm van de stellende trap van <w>Brussels</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>Brusselse</w> wafels zijn wereldberoemd om hun lichte en luchtige structuur.",
                            "target_lang_translations": {
                                "ru": "<w>Брюссельские</w> вафли всемирно известны своей легкой и воздушной текстурой."
                            }
                        },
                        {
                            "text": "Tijdens ons bezoek aan de hoofdstad hebben we genoten van de typische <w>Brusselse</w> sfeer.",
                            "target_lang_translations": {
                                "ru": "Во время нашего визита в столицу мы наслаждались типичной <w>брюссельской</w> атмосферой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Brusselse wafels",
                        "Brusselse spruiten",
                        "Brusselse lof",
                        "Brusselse aarde"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflected form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Брюсселю или происходящий из него"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брюссельский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e9e69fe-815b-3a4b-814f-f34503308ae3",
                    "sense_definition": "Een vrouwelijke inwoner van Brussel",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurvrouw is een echte <w>Brusselse</w> en kent alle leuke plekjes in de stad.",
                            "target_lang_translations": {
                                "ru": "Моя соседка — коренная <w>жительница Брюсселя</w>, и она знает все интересные места в городе."
                            }
                        },
                        {
                            "text": "Als geboren <w>Brusselse</w> spreekt zij zowel Nederlands als Frans.",
                            "target_lang_translations": {
                                "ru": "Как урожденная <w>жительница Брюсселя</w>, она говорит и по-нидерландски, и по-французски."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een echte Brusselse",
                        "geboren Brusselse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, проживающая в Брюсселе или родом из него"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жительница Брюсселя",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

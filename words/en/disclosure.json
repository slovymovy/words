{
    "word_family": [
        "disclose",
        "undisclosed"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f437276a-6099-3789-a9bc-9bb237cfc1a4",
                    "sense_definition": "The act of making new or secret information known to others.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "act_of_revealing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract requires full <w>disclosure</w> of all financial assets.",
                            "target_lang_translations": {
                                "ru": "Контракт требует полного <w>раскрытия</w> всех финансовых активов."
                            }
                        },
                        {
                            "text": "Unauthorized <w>disclosure</w> of private data can lead to legal action.",
                            "target_lang_translations": {
                                "ru": "Несанкционированное <w>разглашение</w> личных данных может привести к судебному иску."
                            }
                        }
                    ],
                    "synonyms": [
                        "revelation"
                    ],
                    "common_phrases": [
                        "full disclosure",
                        "public disclosure",
                        "financial disclosure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, делающее новую или секретную информацию известной другим"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрытие",
                                "target_lang_sense_clarification": "процесс обеспечения доступности информации"
                            },
                            {
                                "target_lang_word": "разглашение",
                                "target_lang_sense_clarification": "обычно о секретной или конфиденциальной информации"
                            },
                            {
                                "target_lang_word": "обнародование",
                                "target_lang_sense_clarification": "официальное объявление для всеобщего сведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00f32f8c-1186-3046-b558-cb1802cc1045",
                    "sense_definition": "A fact or piece of information that has been revealed.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "thing_revealed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspaper published a shocking <w>disclosure</w> about the politician's past.",
                            "target_lang_translations": {
                                "ru": "Газета опубликовала шокирующее <w>разоблачение</w> прошлого этого политика."
                            }
                        },
                        {
                            "text": "Recent <w>disclosures</w> have changed public opinion on the matter.",
                            "target_lang_translations": {
                                "ru": "Недавние <w>разоблачения</w> изменили общественное мнение по этому вопросу."
                            }
                        }
                    ],
                    "synonyms": [
                        "revelation"
                    ],
                    "common_phrases": [
                        "shocking disclosure",
                        "recent disclosure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт или сведения, которые стали достоянием гласности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разоблачение",
                                "target_lang_sense_clarification": "раскрытие правды, часто скандальной или скрываемой"
                            },
                            {
                                "target_lang_word": "откровение",
                                "target_lang_sense_clarification": "неожиданная или важная новая информация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "improvement",
        "improver",
        "improvable",
        "unimproved"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6e7ce30f-ccc8-3160-abc6-dca17677bebe",
                    "sense_definition": "To make something better or increase its quality, value, or productivity.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "betterment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regular practice will <w>improve</w> your writing skills significantly.",
                            "target_lang_translations": {
                                "ru": "Регулярная практика значительно <w>улучшит</w> ваши навыки письма."
                            }
                        },
                        {
                            "text": "They want to <w>improve</w> the quality of customer service.",
                            "target_lang_translations": {
                                "ru": "Они хотят <w>улучшить</w> качество обслуживания клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "enhance",
                        "better",
                        "ameliorate",
                        "refine",
                        "amend"
                    ],
                    "antonyms": [
                        "worsen",
                        "aggravate"
                    ],
                    "common_phrases": [
                        "improve quality",
                        "improve performance",
                        "improve skills",
                        "improve standards"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо лучше, повышать качество, ценность или продуктивность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "улучшать",
                                "target_lang_sense_clarification": "делать более качественным или хорошим"
                            },
                            {
                                "target_lang_word": "совершенствовать",
                                "target_lang_sense_clarification": "доводить до совершенства, развивать"
                            },
                            {
                                "target_lang_word": "налаживать",
                                "target_lang_sense_clarification": "приводить в порядок, организовывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b27b7908-3d60-33da-b6e8-53a3919164a0",
                    "sense_definition": "To become better or reach a better state.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "betterment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather is expected to <w>improve</w> by the weekend.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что погода <w>улучшится</w> к выходным."
                            }
                        },
                        {
                            "text": "His health began to <w>improve</w> after he started the new treatment.",
                            "target_lang_translations": {
                                "ru": "Его здоровье начало <w>поправляться</w> после того, как он начал новое лечение."
                            }
                        }
                    ],
                    "synonyms": [
                        "get better",
                        "look up"
                    ],
                    "antonyms": [
                        "deteriorate",
                        "worsen"
                    ],
                    "common_phrases": [
                        "improve significantly",
                        "situation improve",
                        "health improve",
                        "steadily improve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться лучше или приходить в лучшее состояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "улучшаться",
                                "target_lang_sense_clarification": "становиться лучше (общая характеристика)"
                            },
                            {
                                "target_lang_word": "поправляться",
                                "target_lang_sense_clarification": "выздоравливать или восстанавливать положение"
                            },
                            {
                                "target_lang_word": "налаживаться",
                                "target_lang_sense_clarification": "идти на лад (о делах, погоде, отношениях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fa8a278-ff02-339c-8b1a-40b89ac7d105",
                    "sense_definition": "To use or employ something for a good purpose or profit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He failed to <w>improve</w> the opportunity given to him by the director.",
                            "target_lang_translations": {
                                "ru": "Он не сумел <w>воспользоваться</w> возможностью, предоставленной ему директором."
                            }
                        },
                        {
                            "text": "We must <w>improve</w> our time wisely while we are here.",
                            "target_lang_translations": {
                                "ru": "Мы должны мудро <w>использовать</w> наше время, пока мы здесь."
                            }
                        }
                    ],
                    "synonyms": [
                        "batten"
                    ],
                    "common_phrases": [
                        "improve the occasion",
                        "improve the time"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать что-либо с пользой или выгодой (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспользоваться",
                                "target_lang_sense_clarification": "извлечь выгоду из ситуации"
                            },
                            {
                                "target_lang_word": "улучать",
                                "target_lang_sense_clarification": "находить подходящее время (устар.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d49028fa-38e7-3eb8-9aa3-e09b8cbe9a50",
                    "sense_definition": "To disprove, refute, or make void.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer sought to <w>improve</w> the witness's false testimony.",
                            "target_lang_translations": {
                                "ru": "Адвокат стремился <w>опровергнуть</w> ложные показания свидетеля."
                            }
                        },
                        {
                            "text": "No argument could <w>improve</w> the logic of his statement.",
                            "target_lang_translations": {
                                "ru": "Никакой аргумент не мог <w>опровергнуть</w> логику его заявления."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Опровергать, доказывать ложность или аннулировать (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опровергать",
                                "target_lang_sense_clarification": "доказывать ложность утверждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71321633-cc27-390f-91a4-6e256b0c2594",
                    "sense_definition": "To disapprove of, find fault with, or censure.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The elders did <w>improve</w> his negligent conduct.",
                            "target_lang_translations": {
                                "ru": "Старейшины <w>порицали</w> его халатное поведение."
                            }
                        },
                        {
                            "text": "They found nothing to <w>improve</w> in his actions.",
                            "target_lang_translations": {
                                "ru": "Они не нашли, за что <w>порицать</w> его действия."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать неодобрение, находить недостатки или порицать (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порицать",
                                "target_lang_sense_clarification": "выражать осуждение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
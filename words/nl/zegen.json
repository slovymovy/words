    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bf094f49-a079-325d-9a94-46acfdd54744",
                    "sense_definition": "Een bede om goddelijke bescherming of hulp, of de bescherming zelf.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meaning_blessing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De priester sprak de <w>zegen</w> uit over het bruidspaar.",
                            "target_lang_translations": {
                                "en": "The priest pronounced the <w>blessing</w> over the bridal couple.",
                                "ru": "Священник произнес <w>благословение</w> над новобрачными."
                            }
                        },
                        {
                            "text": "Wij wensen jullie veel heil en <w>zegen</w> in het nieuwe huis.",
                            "target_lang_translations": {
                                "en": "We wish you much salvation and <w>blessing</w> in the new house.",
                                "ru": "Мы желаем вам много счастья и <w>благословения</w> в новом доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "zegening"
                    ],
                    "common_phrases": [
                        "de zegen uitspreken",
                        "heil en zegen",
                        "Gods zegen"
                    ],
                    "target_lang_definitions": {
                        "en": "A prayer for divine protection or help, or the protection itself.",
                        "ru": "Молитва о божественной защите или помощи, либо сама эта защита."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blessing",
                                "target_lang_sense_clarification": "general term for divine favor or the act of pronouncing it"
                            },
                            {
                                "target_lang_word": "benediction",
                                "target_lang_sense_clarification": "formal or liturgical act of blessing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "благословение",
                                "target_lang_sense_clarification": "религиозное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96f73578-86f6-3ffe-914c-c990b36a42e5",
                    "sense_definition": "Een groot voordeel of een gelukkige omstandigheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "meaning_boon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een ware <w>zegen</w> dat iedereen gezond is gebleven.",
                            "target_lang_translations": {
                                "en": "It is a true <w>blessing</w> that everyone remained healthy.",
                                "ru": "Это настоящее <w>счастье</w>, что все остались здоровы."
                            }
                        },
                        {
                            "text": "De regen was een <w>zegen</w> voor de droge planten in de tuin.",
                            "target_lang_translations": {
                                "en": "The rain was a <w>blessing</w> for the dry plants in the garden.",
                                "ru": "Дождь стал <w>благом</w> для сухих растений в саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een ware zegen",
                        "tot zegen zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "A great advantage or a happy circumstance.",
                        "ru": "Большое преимущество или счастливое стечение обстоятельств."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blessing",
                                "target_lang_sense_clarification": "something that brings happiness or relief"
                            },
                            {
                                "target_lang_word": "boon",
                                "target_lang_sense_clarification": "a benefit or advantage, often timely"
                            },
                            {
                                "target_lang_word": "godsend",
                                "target_lang_sense_clarification": "an unexpected fortunate event or item"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "благо",
                                "target_lang_sense_clarification": "дар, преимущество"
                            },
                            {
                                "target_lang_word": "счастье",
                                "target_lang_sense_clarification": "удача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bad36fbb-ed41-391a-9fea-4ef1fd1d1ec5",
                    "sense_definition": "De goedkeuring of instemming van een belangrijk persoon of instantie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "meaning_approval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertrok met de <w>zegen</w> van zijn ouders naar het buitenland.",
                            "target_lang_translations": {
                                "en": "He left for abroad with the <w>blessing</w> of his parents.",
                                "ru": "Он уехал за границу с <w>благословения</w> родителей."
                            }
                        },
                        {
                            "text": "Zonder de <w>zegen</w> van de directeur gaat het plan niet door.",
                            "target_lang_translations": {
                                "en": "Without the <w>blessing</w> of the director, the plan will not go ahead.",
                                "ru": "Без <w>одобрения</w> директора план не будет реализован."
                            }
                        }
                    ],
                    "synonyms": [
                        "instemming",
                        "toestemming"
                    ],
                    "common_phrases": [
                        "zijn zegen geven",
                        "met iemands zegen"
                    ],
                    "target_lang_definitions": {
                        "en": "The approval or agreement of an important person or authority.",
                        "ru": "Одобрение или согласие важного лица или инстанции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blessing",
                                "target_lang_sense_clarification": "figurative approval or consent"
                            },
                            {
                                "target_lang_word": "approval",
                                "target_lang_sense_clarification": "formal agreement or permission"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "благословение",
                                "target_lang_sense_clarification": "переносное: одобрение"
                            },
                            {
                                "target_lang_word": "одобрение",
                                "target_lang_sense_clarification": "согласие"
                            }
                        ]
                    }
                }
            ]
        },
        {

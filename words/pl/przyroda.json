            "senses": [
                {
                    "sense_id": "6799a201-041c-3ec2-b77a-7ec9c7e35d91",
                    "sense_definition": "świat fizyczny, ożywiony i nieożywiony, otaczający człowieka, bez wytworów jego działalności",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "nature_world",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W weekendy lubimy wyjeżdżać za miasto, żeby odpocząć na łonie <w>przyrody</w>.",
                            "target_lang_translations": {
                                "ru": "По выходным мы любим выезжать за город, чтобы отдохнуть на лоне <w>природы</w>."
                            }
                        },
                        {
                            "text": "Turyści podziwiali piękną <w>przyrodę</w> Babiogórskiego Parku Narodowego.",
                            "target_lang_translations": {
                                "ru": "Туристы любовались красивой <w>природой</w> Бабьегорского национального парка."
                            }
                        }
                    ],
                    "synonyms": [
                        "natura"
                    ],
                    "common_phrases": [
                        "ochrona przyrody",
                        "na łonie przyrody",
                        "pomnik przyrody",
                        "ożywiona przyroda"
                    ],
                    "target_lang_definitions": {
                        "ru": "естественный мир, всё, что не создано деятельностью человека"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "природа",
                                "target_lang_sense_clarification": "окружающий мир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19db5575-24ad-321c-ba44-458d9b1e2949",
                    "sense_definition": "przedmiot w szkole podstawowej obejmujący wiedzę o świecie ożywionym i nieożywionym",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "school_subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka <w>przyrody</w> zabrała całą naszą klasę na wycieczkę do lasu.",
                            "target_lang_translations": {
                                "ru": "Учительница <w>природоведения</w> повела весь наш класс на экскурсию в лес."
                            }
                        },
                        {
                            "text": "Mój syn bardzo lubi uczyć się <w>przyrody</w> i poznawać nowe zwierzęta.",
                            "target_lang_translations": {
                                "ru": "Мой сын очень любит изучать <w>природоведение</w> и узнавать о новых животных."
                            }
                        }
                    ],
                    "common_phrases": [

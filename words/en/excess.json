            "senses": [
                {
                    "sense_id": "ef891963-f1e1-373f-bfd2-0d95e0447e33",
                    "sense_definition": "An amount or quantity that is more than necessary, permitted, usual, or desirable.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "excess_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The project cost was in <w>excess</w> of the original budget.",
                            "target_lang_translations": {
                                "ru": "Стоимость проекта <w>превысила</w> первоначальный бюджет."
                            }
                        },
                        {
                            "text": "He started drinking to <w>excess</w> after he lost his job.",
                            "target_lang_translations": {
                                "ru": "Он начал пить <w>чрезмерно</w> после того, как потерял работу."

            "senses": [
                {
                    "sense_id": "a8f4f0bf-e373-30f1-b121-9d57b7dbcf2a",
                    "sense_definition": "Een uitdrukking, idee of handeling die door veelvuldig gebruik zijn oorspronkelijke kracht, betekenis of originaliteit heeft verloren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een <w>cliché</w>, maar daarom niet minder waar.",
                            "target_lang_translations": {
                                "en": "It is a <w>cliché</w>, but that makes it no less true."
                            }
                        },
                        {
                            "text": "De film zat vol met elk mogelijk <w>cliché</w> uit het genre, waardoor het verhaal erg voorspelbaar werd.",
                            "target_lang_translations": {
                                "en": "The film was full of every possible <w>cliché</w> from the genre, which made the story very predictable."
                            }
                        }
                    ],
                    "synonyms": [
                        "dooddoener",
                        "gemeenplaats",
                        "platitude"
                    ],
                    "antonyms": [
                        "originaliteit",
                        "vondst"
                    ],
                    "common_phrases": [
                        "een wandelend cliché",
                        "vervallen in clichés",
                        "een groot cliché"
                    ],
                    "target_lang_definitions": {
                        "en": "An expression, idea, or action that has lost its original power, meaning, or originality through frequent use."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cliché",
                                "target_lang_sense_clarification": "overused expression"
                            },
                            {
                                "target_lang_word": "platitude",
                                "target_lang_sense_clarification": "trite remark"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "450d1cf1-f95e-3ef8-a281-32c63c9c7060",
                    "sense_definition": "Een drukvorm, meestal van metaal, die gebruikt wordt om illustraties of tekst te vermenigvuldigen in de boekdrukkunst.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "printing_term",
                    "name_type": "no",
                    "examples": [
                        {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "75271fcf-08f2-3d59-b9be-11d00f7a16ef",
                    "sense_definition": "Основной приём пищи в середине дня (обычно между 12 и 15 часами).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meal_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся семья собралась за столом на <w>обед</w>.",
                            "target_lang_translations": {
                                "en": "The whole family gathered at the table for <w>lunch</w>."
                            }
                        },
                        {
                            "text": "Мы обычно готовим <w>обед</w> дома, но иногда ходим в кафе.",
                            "target_lang_translations": {
                                "en": "We usually cook <w>lunch</w> at home, but sometimes we go to a cafe."
                            }
                        }
                    ],
                    "synonyms": [
                        "ланч"
                    ],
                    "common_phrases": [
                        "вкусный обед",
                        "готовить обед",
                        "пригласить на обед",
                        "время обеда"
                    ],
                    "target_lang_definitions": {
                        "en": "The main meal eaten in the middle of the day (usually between 12 p.m. and 3 p.m.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lunch",
                                "target_lang_sense_clarification": "midday meal"
                            },
                            {
                                "target_lang_word": "dinner",
                                "target_lang_sense_clarification": "main meal of the day"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c41915f-31b0-3bb7-979d-34d4a68a5042",
                    "sense_definition": "Еда, кушанья, приготовленные для дневного приёма пищи (часто состоит из супа и второго блюда).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meal_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама собрала мне <w>обед</w> в школу в пластиковый контейнер.",
                            "target_lang_translations": {
                                "en": "Mom packed my <w>lunch</w> for school in a plastic container."
                            }
                        },
                        {
                            "text": "Этот комплексный <w>обед</w> стоит совсем недорого.",
                            "target_lang_translations": {
                                "en": "This set <w>lunch</w> costs very little."
                            }
                        }
                    ],
                    "common_phrases": [
                        "комплексный обед",
                        "горячий обед",
                        "купить обед",
                        "разогреть обед"
                    ],
                    "target_lang_definitions": {
                        "en": "Food or dishes prepared for the midday meal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lunch",
                                "target_lang_sense_clarification": "food"
                            },
                            {
                                "target_lang_word": "meal",
                                "target_lang_sense_clarification": "prepared food"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2313d016-947c-3eb4-866a-4c38dff0b6b3",
                    "sense_definition": "Время в середине дня, перерыв в работе для принятия пищи.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Магазин закрыт на <w>обед</w> с двух до трёх часов дня.",
                            "target_lang_translations": {
                                "en": "The shop is closed for <w>lunch</w> from two to three in the afternoon."
                            }
                        },
                        {
                            "text": "Давай встретимся завтра в <w>обед</w> и обсудим этот вопрос.",
                            "target_lang_translations": {
                                "en": "Let's meet tomorrow at <w>lunchtime</w> and discuss this issue."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перерыв на обед",
                        "закрыто на обед",
                        "до обеда",
                        "после обеда"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {

                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "supports",
                                "target_lang_sense_clarification": "objects that hold something up"
                            },
                            {
                                "target_lang_word": "props",
                                "target_lang_sense_clarification": "objects used to brace something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "опоры",

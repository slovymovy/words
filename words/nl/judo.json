    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b731118e-2b3c-3c92-9d21-d04cfd649ef3",
                    "sense_definition": "Een Japanse vechtsport waarbij men probeert de tegenstander te werpen of onder controle te houden, zonder te slaan of te schoppen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broertje zit al twee jaar op <w>judo</w> en vindt het erg leuk.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат уже два года ходит на <w>дзюдо</w>, и ему это очень нравится.",
                                "en": "My little brother has been doing <w>judo</w> for two years now and likes it very much."
                            }
                        },
                        {
                            "text": "Tijdens de les <w>judo</w> leerden we hoe we veilig moeten vallen.",
                            "target_lang_translations": {
                                "ru": "На уроке <w>дзюдо</w> мы учились безопасно падать.",
                                "en": "During the <w>judo</w> lesson, we learned how to fall safely."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op judo zitten",
                        "aan judo doen",
                        "les in judo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Японское боевое искусство, в котором пытаются бросить противника или удержать его под контролем, без ударов руками или ногами.",
                        "en": "A Japanese martial art where one tries to throw the opponent or keep them under control, without hitting or kicking."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дзюдо",
                                "target_lang_sense_clarification": "вид спорта"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "judo",
                                "target_lang_sense_clarification": "martial art"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "530cba33-b12b-34de-87aa-65ba2ec65334",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>judoën</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>judo</w> elke woensdagavond in de lokale sporthal.",
                            "target_lang_translations": {
                                "ru": "Я <w>занимаюсь дзюдо</w> каждую среду вечером в местном спортзале.",
                                "en": "I <w>do judo</w> every Wednesday evening in the local sports hall."
                            }
                        },
                        {
                            "text": "Sinds kort <w>judo</w> ik samen met mijn beste vriend.",
                            "target_lang_translations": {
                                "ru": "С недавних пор я <w>занимаюсь дзюдо</w> вместе со своим лучшим другом.",
                                "en": "Recently I have been <w>doing judo</w> together with my best friend."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола <w>judoën</w>.",
                        "en": "First person singular present tense of <w>judoën</w> (to do judo)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занимаюсь дзюдо",
                                "target_lang_sense_clarification": "я занимаюсь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "do judo",
                                "target_lang_sense_clarification": "I do judo"
                            },
                            {
                                "target_lang_word": "practice judo",
                                "target_lang_sense_clarification": "I practice judo"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "416aa9c3-f6f9-33b4-a616-a56720785c43",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>judoën</w> (bij inversie).",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Judo</w> je al lang bij deze vereniging?",
                            "target_lang_translations": {
                                "ru": "Ты давно <w>занимаешься дзюдо</w> в этом клубе?",
                                "en": "Have you been <w>doing judo</w> at this club for a long time?"
                            }
                        },
                        {
                            "text": "Waarom <w>judo</w> je eigenlijk niet meer?",
                            "target_lang_translations": {
                                "ru": "Почему ты, собственно, больше не <w>занимаешься дзюдо</w>?",
                                "en": "Why don't you <w>do judo</w> anymore, actually?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма второго лица единственного числа настоящего времени глагола <w>judoën</w> (при инверсии).",
                        "en": "Second person singular present tense of <w>judoën</w> (to do judo), used in inversions/questions."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занимаешься дзюдо",
                                "target_lang_sense_clarification": "ты занимаешься"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "do judo",
                                "target_lang_sense_clarification": "you do judo"
                            },
                            {
                                "target_lang_word": "practice judo",
                                "target_lang_sense_clarification": "you practice judo"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02e0e8bb-a96b-316e-ab96-2a8a1e8b6ad2",
                    "sense_definition": "Gebiedende wijs van <w>judoën</w>.",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Judo</w> met respect voor je tegenstander en de regels.",
                            "target_lang_translations": {
                                "ru": "<w>Занимайся дзюдо</w> с уважением к противнику и правилам.",
                                "en": "<w>Do judo</w> with respect for your opponent and the rules."
                            }
                        },
                        {
                            "text": "<w>Judo</w> voorzichtig, want de mat is vandaag erg glad.",
                            "target_lang_translations": {
                                "ru": "<w>Занимайся дзюдо</w> осторожно, потому что татами сегодня очень скользкое.",
                                "en": "<w>Do judo</w> carefully, because the mat is very slippery today."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>judoën</w>.",
                        "en": "Imperative mood of <w>judoën</w> (to do judo)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занимайся дзюдо",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "do judo",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2c331a0f-dcd1-3325-864a-ce3aada6ac0d",
                    "sense_definition": "Een officiële functie, baan of maatschappelijke positie innemen en uitoefenen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>bekleedt</w> momenteel een zware functie binnen het ministerie.",
                            "target_lang_translations": {
                                "en": "She currently <w>holds</w> a demanding position within the ministry.",
                                "ru": "В настоящее время она <w>занимает</w> ответственную должность в министерстве."
                            }
                        },
                        {
                            "text": "Mijn vader heeft jarenlang een belangrijk ambt <w>bekleed</w> bij de gemeente.",
                            "target_lang_translations": {
                                "en": "My father <w>held</w> an important office at the municipality for years.",
                                "ru": "Мой отец много лет <w>занимал</w> важный пост в муниципалитете."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervullen",
                        "innemen",
                        "uitoefenen"
                    ],
                    "common_phrases": [
                        "een functie bekleden",
                        "een ambt bekleden",
                        "een positie bekleden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wordt vaak gebruikt in een zakelijke of officiële context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To hold or occupy an official position, job, or office.",
                        "ru": "Занимать официальную должность или общественный пост."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "a position or office"
                            },
                            {
                                "target_lang_word": "occupy",
                                "target_lang_sense_clarification": "a formal post or role"
                            },
                            {
                                "target_lang_word": "fill",
                                "target_lang_sense_clarification": "a vacancy or function"
                            }
                        ],
                        "ru": [
                            {

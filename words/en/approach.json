{
    "word_family": [
        "approachable",
        "unapproachable",
        "approachability"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17b38c52-6970-3cf1-8f03-592ea6ebc8f2",
                    "sense_definition": "To come near or nearer to something or someone in space, time, quality, or amount.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train began to <w>approach</w> the station slowly.",
                            "target_lang_translations": {
                                "ru": "Поезд начал медленно <w>приближаться</w> к станции."
                            }
                        },
                        {
                            "text": "As winter <w>approaches</w>, the days get shorter and colder.",
                            "target_lang_translations": {
                                "ru": "Когда <w>приближается</w> зима, дни становятся короче и холоднее."
                            }
                        },
                        {
                            "text": "We watched the boat <w>approach</w> the shore from a distance.",
                            "target_lang_translations": {
                                "ru": "Мы издали наблюдали, как лодка <w>приближается</w> к берегу."
                            }
                        }
                    ],
                    "synonyms": [
                        "come near",
                        "move toward",
                        "advance",
                        "near"
                    ],
                    "antonyms": [
                        "leave",
                        "depart",
                        "retreat",
                        "withdraw"
                    ],
                    "common_phrases": [
                        "approach the destination",
                        "fast approaching",
                        "approach the end"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приближаться или подходить к чему-либо или кому-либо в пространстве, времени, качестве или количестве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближаться",
                                "target_lang_sense_clarification": "сокращать расстояние или время до чего-либо"
                            },
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "идти по направлению к кому-либо или чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17daf7e1-bfae-377a-be44-6ad4ac58f068",
                    "sense_definition": "To speak to someone for the first time about a proposal, request, or offer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>approach</w> her boss about a pay rise.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>обратиться</w> к начальнику по поводу повышения зарплаты."
                            }
                        },
                        {
                            "text": "We were <w>approached</w> by a student asking for a donation.",
                            "target_lang_translations": {
                                "ru": "К нам <w>подошел</w> студент с просьбой о пожертвовании."
                            }
                        },
                        {
                            "text": "He did not know how to <w>approach</w> the stranger for help.",
                            "target_lang_translations": {
                                "ru": "Он не знал, как <w>обратиться</w> к незнакомцу за помощью."
                            }
                        }
                    ],
                    "synonyms": [
                        "contact",
                        "speak to",
                        "talk to"
                    ],
                    "common_phrases": [
                        "approach someone for help",
                        "approach with an offer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к кому-либо с предложением, просьбой или идеей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "начинать разговор с просьбой или предложением"
                            },
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "вступать в контакт, обычно лично"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "570efe0d-3dcc-330a-8ced-3453dc3dc28d",
                    "sense_definition": "To start dealing with a problem, task, or situation in a particular way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "method_strategy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am not sure how to <w>approach</w> this difficult math problem.",
                            "target_lang_translations": {
                                "ru": "Я не уверен, как <w>подойти</w> к этой сложной математической задаче."
                            }
                        },
                        {
                            "text": "They <w>approached</w> the project with great enthusiasm and energy.",
                            "target_lang_translations": {
                                "ru": "Они <w>подошли</w> к проекту с большим энтузиазмом и энергией."
                            }
                        },
                        {
                            "text": "We need to <w>approach</w> this situation with caution.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>подойти</w> к этой ситуации с осторожностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "tackle",
                        "address",
                        "deal with",
                        "handle"
                    ],
                    "antonyms": [
                        "ignore",
                        "avoid"
                    ],
                    "common_phrases": [
                        "approach a problem",
                        "approach a task"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать заниматься проблемой, задачей или ситуацией определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "выбирать способ решения задачи"
                            },
                            {
                                "target_lang_word": "браться",
                                "target_lang_sense_clarification": "начинать делать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88beccfc-8185-38fc-8369-a8690f9b599f",
                    "sense_definition": "To be almost the same as something; to come close to a particular quality or state.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature is expected to <w>approach</w> freezing tonight.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что сегодня ночью температура <w>приблизится</w> к нулю."
                            }
                        },
                        {
                            "text": "His performance <w>approached</w> perfection during the final act.",
                            "target_lang_translations": {
                                "ru": "Его игра в последнем акте <w>приближалась</w> к совершенству."
                            }
                        }
                    ],
                    "synonyms": [
                        "approximate",
                        "resemble",
                        "border on"
                    ],
                    "common_phrases": [
                        "approach perfection",
                        "approach zero"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть почти таким же, как что-то; приближаться к определенному качеству или состоянию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближаться",
                                "target_lang_sense_clarification": "становиться похожим или почти равным"
                            },
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "достигать определенного уровня"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e775a04c-8d61-3802-a928-25bf6b3e5e55",
                    "sense_definition": "The act of coming closer or drawing near in distance or time.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden <w>approach</w> of the storm caught us by surprise.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>приближение</w> шторма застало нас врасплох."
                            }
                        },
                        {
                            "text": "Our dog always barks at the <w>approach</w> of strangers.",
                            "target_lang_translations": {
                                "ru": "Наша собака всегда лает при <w>приближении</w> незнакомцев."
                            }
                        },
                        {
                            "text": "With the <w>approach</w> of winter, we prepared the house for the cold.",
                            "target_lang_translations": {
                                "ru": "С <w>приближением</w> зимы мы подготовили дом к холодам."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrival",
                        "coming",
                        "advance"
                    ],
                    "antonyms": [
                        "departure",
                        "retreat"
                    ],
                    "common_phrases": [
                        "the approach of winter",
                        "with the approach of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие приближения в пространстве или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближение",
                                "target_lang_sense_clarification": "процесс сокращения расстояния"
                            },
                            {
                                "target_lang_word": "наступление",
                                "target_lang_sense_clarification": "о времени или событиях (например, зимы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f10d43cf-06a8-3600-8ac9-17fb7733e00e",
                    "sense_definition": "A way of dealing with something or doing something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "method_strategy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to try a different <w>approach</w> to solve this puzzle.",
                            "target_lang_translations": {
                                "ru": "Нам нужно попробовать другой <w>подход</w> к решению этой головоломки."
                            }
                        },
                        {
                            "text": "His <w>approach</w> to teaching is very interactive and engaging.",
                            "target_lang_translations": {
                                "ru": "Его <w>подход</w> к преподаванию очень интерактивный и увлекательный."
                            }
                        },
                        {
                            "text": "The company adopted a new <w>approach</w> to customer service.",
                            "target_lang_translations": {
                                "ru": "Компания приняла новый <w>подход</w> к обслуживанию клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "method",
                        "strategy",
                        "technique",
                        "way",
                        "tactic"
                    ],
                    "common_phrases": [
                        "new approach",
                        "different approach",
                        "approach to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ решения задачи или ведения дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подход",
                                "target_lang_sense_clarification": "метод или стратегия действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcdcff82-260a-3731-816c-27c399fe8e05",
                    "sense_definition": "A road, path, or way that leads to a place.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>approach</w> to the castle is steep and winding.",
                            "target_lang_translations": {
                                "ru": "<w>Подъезд</w> к замку крутой и извилистый."
                            }
                        },
                        {
                            "text": "Trees lined the long <w>approach</w> to the main house.",
                            "target_lang_translations": {
                                "ru": "Деревья росли вдоль длинного <w>подъезда</w> к главному дому."
                            }
                        }
                    ],
                    "synonyms": [
                        "access",
                        "driveway",
                        "entrance",
                        "path"
                    ],
                    "common_phrases": [
                        "approach to the bridge",
                        "main approach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорога или путь, ведущий к какому-либо месту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подъезд",
                                "target_lang_sense_clarification": "путь для транспорта"
                            },
                            {
                                "target_lang_word": "подход",
                                "target_lang_sense_clarification": "путь для пешеходов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76c69d0d-9205-39d4-886a-a815513815c3",
                    "sense_definition": "The final part of a flight before the aircraft lands.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "aviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot announced that we were on our final <w>approach</w> to the airport.",
                            "target_lang_translations": {
                                "ru": "Пилот объявил, что мы находимся на финальном <w>заходе на посадку</w> в аэропорту."
                            }
                        },
                        {
                            "text": "Bad weather made the <w>approach</w> very difficult for the crew.",
                            "target_lang_translations": {
                                "ru": "Плохая погода сделала <w>заход на посадку</w> очень трудным для экипажа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "final approach",
                        "landing approach"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "aviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конечная стадия полета перед посадкой самолета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заход на посадку",
                                "target_lang_sense_clarification": "маневр самолета перед приземлением"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
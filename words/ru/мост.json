    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1afdc27-0fae-3ce7-9e36-6bb2266a1c6b",
                    "sense_definition": "Сооружение для перехода или переезда через реку, овраг, дорогу или другое препятствие.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "structure_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы медленно перешли через старый <мост>.",
                            "target_lang_translations": {
                                "en": "We slowly crossed the old <bridge>."
                            }
                        },
                        {
                            "text": "В Санкт-Петербурге каждую ночь разводят <мосты>.",
                            "target_lang_translations": {
                                "en": "In Saint Petersburg, they raise the <bridges> every night."
                            }
                        }
                    ],
                    "synonyms": [
                        "переправа",
                        "виадук"
                    ],
                    "antonyms": [
                        "тоннель"
                    ],
                    "common_phrases": [
                        "разводной мост",
                        "строить мост",
                        "железнодорожный мост",
                        "каменный мост",
                        "стоять на мосту"
                    ],
                    "target_lang_definitions": {
                        "en": "A structure built to span a physical obstacle such as a river, valley, road, or railroad."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bridge",
                                "target_lang_sense_clarification": "structure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3e843c5-8c13-3e38-9720-efb97bd13995",
                    "sense_definition": "Гимнастическая поза, при которой тело выгнуто дугой вверх с опорой на ладони и стопы.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_arch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На уроке физкультуры дети учились вставать на <мост>.",
                            "target_lang_translations": {
                                "en": "In PE class, the children learned to get into a <bridge>."
                            }
                        },
                        {
                            "text": "Гимнастка легко сделала <мост> и замерла в этом положении.",
                            "target_lang_translations": {
                                "en": "The gymnast easily did a <bridge> and froze in that position."
                            }
                        }
                    ],
                    "synonyms": [
                        "мостик"
                    ],
                    "common_phrases": [
                        "встать на мост",
                        "борцовский мост"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A gymnastic position in which the body is arched backwards, supported by the hands and feet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bridge",
                                "target_lang_sense_clarification": "gymnastics"
                            },
                            {
                                "target_lang_word": "backbend",
                                "target_lang_sense_clarification": "pose"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "269dba17-610a-3425-b8fd-636425430825",
                    "sense_definition": "То, что соединяет, служит связью между кем-либо или чем-либо (переносное значение).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "connection_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Культура служит <мостом> между нашими народами.",
                            "target_lang_translations": {
                                "en": "Culture serves as a <bridge> between our nations."
                            }
                        },
                        {
                            "text": "Ему удалось навести <мосты> с новыми партнёрами по бизнесу.",
                            "target_lang_translations": {
                                "en": "He managed to build <bridges> with new business partners."
                            }
                        }
                    ],
                    "synonyms": [
                        "связь",
                        "звено"
                    ],
                    "common_phrases": [
                        "наводить мосты",
                        "сжигать мосты",
                        "воздушный мост",
                        "живой мост"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that provides a connection, contact, or reconciliation between different people, groups, or things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bridge",
                                "target_lang_sense_clarification": "figurative connection"
                            },
                            {
                                "target_lang_word": "link",
                                "target_lang_sense_clarification": "connection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c613dbdf-deb7-334b-92cf-b64fbfb4eb23",
                    "sense_definition": "Зубной протез, заменяющий один или несколько утраченных зубов и крепящийся на соседних зубах.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_arch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стоматолог рекомендовал пациенту поставить <мост>.",
                            "target_lang_translations": {
                                "en": "The dentist recommended that the patient get a <bridge>."
                            }
                        },
                        {
                            "text": "Изготовление золотого <моста> заняло несколько дней.",
                            "target_lang_translations": {
                                "en": "Making the gold <bridge> took several days."
                            }
                        }
                    ],
                    "synonyms": [
                        "протез"
                    ],
                    "common_phrases": [
                        "зубной мост",
                        "поставить мост"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A dental appliance used to replace one or more missing teeth, anchored to the neighboring natural teeth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bridge",
                                "target_lang_sense_clarification": "dentistry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b84ac735-117c-3820-8119-977763a538e6",
                    "sense_definition": "Часть шасси автомобиля или другого транспортного средства, соединяющая колёса одной оси.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_axle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Механик обнаружил неисправность заднего <моста> грузовика.",
                            "target_lang_translations": {
                                "en": "The mechanic discovered a fault in the truck's rear <axle>."
                            }
                        },
                        {
                            "text": "Ведущий <мост> обеспечивает передачу крутящего момента на колёса.",
                            "target_lang_translations": {
                                "en": "The drive <axle> ensures the transmission of torque to the wheels."
                            }
                        }
                    ],
                    "synonyms": [
                        "ось"
                    ],
                    "common_phrases": [
                        "задний мост",
                        "передний мост",
                        "ведущий мост"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A central shaft for a rotating wheel or gear; the part of a vehicle connecting the wheels."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "axle",
                                "target_lang_sense_clarification": "vehicle part"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8df9702-e1e1-3320-8a53-f37ecd24f1d8",
                    "sense_definition": "Линия прямой связи или взаимодействия между удалёнными пунктами.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "connection_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Телевизионный <мост> соединил студии в Москве и Нью-Йорке.",
                            "target_lang_translations": {
                                "en": "A television <link> connected the studios in Moscow and New York."
                            }
                        },
                        {
                            "text": "Спасатели организовали воздушный <мост> для эвакуации пострадавших.",
                            "target_lang_translations": {
                                "en": "Rescuers organized an air <bridge> to evacuate the victims."
                            }
                        }
                    ],
                    "synonyms": [
                        "канал"
                    ],
                    "common_phrases": [
                        "телемост",
                        "видеомост"
                    ],
                    "target_lang_definitions": {
                        "en": "A direct line of communication or transport established between distant points."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "link",
                                "target_lang_sense_clarification": "communication"
                            },
                            {
                                "target_lang_word": "bridge",
                                "target_lang_sense_clarification": "airlift/connection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c704262-d843-38d8-be27-d443b0351fea",
                    "sense_definition": "Настил из досок или брёвен.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "structure_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старой избе прогнил деревянный <мост>.",
                            "target_lang_translations": {
                                "en": "In the old hut, the wooden <floor> had rotted."
                            }
                        },
                        {
                            "text": "Рабочие соорудили временный <мост> из досок, чтобы пройти через грязь.",
                            "target_lang_translations": {

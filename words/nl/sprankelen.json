            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0191f039-8683-395a-88a0-220f5c3af328",
                    "sense_definition": "Korte, heldere lichtflitsjes uitzenden of schitteren, vaak door weerkaatsing van licht of door koolzuur.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "light_visual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sterren <w>sprankelden</w> helder aan de donkere hemel.",
                            "target_lang_translations": {
                                "ru": "Звезды ярко <w>искрились</w> на темном небе."
                            }
                        },
                        {
                            "text": "De champagne bruiste en <w>sprankelde</w> in de glazen toen we proostten.",
                            "target_lang_translations": {
                                "ru": "Шампанское шипело и <w>искрилось</w> в бокалах, когда мы произносили тост."
                            }
                        }
                    ],
                    "synonyms": [
                        "fonkelen",
                        "glinsteren",
                        "schitteren"
                    ],
                    "common_phrases": [
                        "sprankelende ogen",
                        "sprankelend water",
                        "sprankelende wijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать короткие, яркие вспышки света или блестеть, часто из-за отражения света или пузырьков газа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искриться",
                                "target_lang_sense_clarification": "излучать искры или яркий свет; играть (о вине)"
                            },
                            {
                                "target_lang_word": "сверкать",
                                "target_lang_sense_clarification": "ярко блестеть, сиять переливчатым светом"
                            },
                            {
                                "target_lang_word": "мерцать",
                                "target_lang_sense_clarification": "светиться неровным, колеблющимся светом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0220bd6f-b3f4-384a-9575-b76c95a7bd84",
                    "sense_definition": "Levendig, energiek en enthousiast zijn; bruisen van energie of geestigheid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_lively",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>sprankelde</w> van geluk toen ze het goede nieuws vertelde.",
                            "target_lang_translations": {
                                "ru": "Она <w>сияла</w> от счастья, когда рассказывала хорошие новости."
                            }
                        },
                        {
                            "text": "Het gesprek <w>sprankelde</w> vanaf het begin door hun gedeelde humor.",
                            "target_lang_translations": {
                                "ru": "Разговор <w>искрился</w> с самого начала благодаря их общему юмору."
                            }
                        }
                    ],
                    "synonyms": [
                        "bruisen",
                        "stralen"
                    ],
                    "common_phrases": [
                        "een sprankelende persoonlijkheid",
                        "een sprankelend gesprek",
                        "sprankelende humor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть оживленным, полным энергии и энтузиазма; излучать радость или остроумие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искриться",
                                "target_lang_sense_clarification": "перен. быть полным жизни, веселья или остроумия"
                            },
                            {
                                "target_lang_word": "сиять",
                                "target_lang_sense_clarification": "перен. излучать радость или счастье"
                            },
                            {
                                "target_lang_word": "блистать",
                                "target_lang_sense_clarification": "выделяться умом, остроумием или талантом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

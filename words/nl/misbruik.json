    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bdbe0c3e-10eb-3639-953f-698b90a8403d",
                    "sense_definition": "Het verkeerde of slechte gebruik van iets voor een doel waarvoor het niet bedoeld is, vaak met betrekking tot macht, vertrouwen of voorwerpen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verkeerd_gebruik",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politicus moest aftreden vanwege <w>misbruik</w> van zijn machtspositie.",
                            "target_lang_translations": {
                                "ru": "Политик был вынужден уйти в отставку из-за <w>злоупотребления</w> своим служебным положением."
                            }
                        },
                        {
                            "text": "De bankmedewerker werd ontslagen wegens <w>misbruik</w> van vertrouwen.",
                            "target_lang_translations": {
                                "ru": "Сотрудник банка был уволен за <w>злоупотребление</w> доверием."
                            }
                        }
                    ],
                    "synonyms": [
                        "wangedrag",
                        "exploitatie"
                    ],
                    "antonyms": [
                        "gebruik"
                    ],
                    "common_phrases": [
                        "misbruik van macht",
                        "misbruik van vertrouwen",
                        "misbruik maken van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неправильное или дурное использование чего-либо в целях, для которых оно не предназначено, часто в отношении власти или доверия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "злоупотребление",
                                "target_lang_sense_clarification": "властью, доверием, положением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fcba55f-9aa6-34c9-9186-999edbbfb7ee",
                    "sense_definition": "De ernstige mishandeling of uitbuiting van personen, vaak van seksuele aard.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mishandeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stichting biedt hulp aan slachtoffers van seksueel <w>misbruik</w>.",
                            "target_lang_translations": {
                                "ru": "Фонд предлагает помощь жертвам сексуального <w>насилия</w>."
                            }
                        },
                        {
                            "text": "Na jaren van <w>misbruik</w> durfde ze eindelijk haar verhaal te vertellen aan de politie.",
                            "target_lang_translations": {
                                "ru": "После многих лет <w>насилия</w> она наконец осмелилась рассказать свою историю полиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "mishandeling",
                        "uitbuiting"
                    ],
                    "common_phrases": [
                        "seksueel misbruik",
                        "slachtoffer van misbruik",
                        "geestelijk misbruik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серьезное жестокое обращение или эксплуатация людей, часто сексуального характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насилие",
                                "target_lang_sense_clarification": "сексуальное или физическое"
                            },
                            {
                                "target_lang_word": "надругательство",
                                "target_lang_sense_clarification": "унижение, оскорбление действием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68ede547-c948-3e6c-964e-e299f164218b",
                    "sense_definition": "Het overmatige en schadelijke gebruik van middelen zoals alcohol of drugs.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verslaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn jarenlange <w>misbruik</w> van alcohol heeft zijn lever ernstig beschadigd.",
                            "target_lang_translations": {
                                "ru": "Его многолетнее <w>злоупотребление</w> алкоголем серьезно повредило печень."
                            }
                        },
                        {
                            "text": "De kliniek is gespecialiseerd in de behandeling van <w>misbruik</w> van medicijnen.",
                            "target_lang_translations": {
                                "ru": "Клиника специализируется на лечении <w>злоупотребления</w> лекарствами."
                            }

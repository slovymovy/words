{
    "word_family": [
        "beggar",
        "beggarly",
        "begging"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1787b040-7895-3a17-a579-a57f03b1210f",
                    "sense_definition": "To ask for money, food, or help from people, typically on the street.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "request_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The homeless man sat on the corner and <w>begged</w> for change.",
                            "target_lang_translations": {
                                "ru": "Бездомный сидел на углу и <w>просил</w> мелочь."
                            }
                        },
                        {
                            "text": "He was too proud to <w>beg</w> for money even when he was hungry.",
                            "target_lang_translations": {
                                "ru": "Он был слишком горд, чтобы <w>просить</w> деньги, даже когда был голоден."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beg for money",
                        "beg for food",
                        "beg on the street"
                    ],
                    "target_lang_definitions": {
                        "ru": "Просить деньги, еду или помощь у людей, обычно на улице; нищенствовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просить милостыню",
                                "target_lang_sense_clarification": "устойчивое выражение"
                            },
                            {
                                "target_lang_word": "побираться",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "попрошайничать",
                                "target_lang_sense_clarification": "заниматься попрошайничеством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c136a8f3-dee7-3ad3-a7b4-94e14a0c3ce9",
                    "sense_definition": "To ask someone for something in an anxious, urgent, or very polite way.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "plead",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>begged</w> him to stay and listen to her explanation.",
                            "target_lang_translations": {
                                "ru": "Она <w>умоляла</w> его остаться и выслушать её объяснение."
                            }
                        },
                        {
                            "text": "I <w>beg</w> your pardon, but I think you are sitting in my seat.",
                            "target_lang_translations": {
                                "ru": "<w>Прошу</w> прощения, но мне кажется, вы сидите на моем месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "beseech",
                        "entreat",
                        "supplicate",
                        "sue"
                    ],
                    "common_phrases": [
                        "beg your pardon",
                        "beg for mercy",
                        "beg to differ",
                        "beg leave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво или очень вежливо просить кого-то о чем-то; умолять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умолять",
                                "target_lang_sense_clarification": "настойчиво просить"
                            },
                            {
                                "target_lang_word": "просить",
                                "target_lang_sense_clarification": "обращаться с просьбой"
                            },
                            {
                                "target_lang_word": "молить",
                                "target_lang_sense_clarification": "высокий стиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "615da720-ffae-3a3b-94e1-8e495c6c3c17",
                    "sense_definition": "To act in a way that makes a negative or difficult result very likely to happen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "provoke",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Walking alone in that dangerous neighborhood at night is just <w>begging</w> for trouble.",
                            "target_lang_translations": {
                                "ru": "Гулять одной в этом опасном районе ночью — это просто <w>напрашиваться</w> на неприятности."
                            }
                        },
                        {
                            "text": "If you leave your car unlocked, you are practically <w>begging</w> to have it stolen.",
                            "target_lang_translations": {
                                "ru": "Если вы оставляете машину незапертой, вы практически <w>напрашиваетесь</w> на то, чтобы её угнали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beg for it",
                        "beg for trouble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя так, что неприятности или негативные последствия становятся неизбежными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напрашиваться",
                                "target_lang_sense_clarification": "на неприятности"
                            },
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "приключений/неприятностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c349e3c9-65e9-3013-b3dd-ce8b45393769",
                    "sense_definition": "To obviously lack or need something very much.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "need",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old wooden fence is <w>begging</w> for a fresh coat of paint.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный забор просто <w>просится</w>, чтобы его покрасили."
                            }
                        },
                        {
                            "text": "This exciting story simply <w>begs</w> for a movie adaptation.",
                            "target_lang_translations": {
                                "ru": "Эта захватывающая история просто <w>просится</w> на экранизацию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beg for attention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остро нуждаться в чем-либо (обычно о неодушевленных предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проситься",
                                "target_lang_sense_clarification": "требовать внимания/ремонта"
                            },
                            {
                                "target_lang_word": "требовать",
                                "target_lang_sense_clarification": "нуждаться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6cca3986-72ad-30d3-87fb-51b5fbfeddc5",
                    "sense_definition": "Abbreviation of <w>beginning</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "abbr",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Repeat the pattern from the <w>beg</w> until the scarf is long enough.",
                            "target_lang_translations": {
                                "ru": "Повторяйте узор с <w>нач.</w> (начала), пока шарф не станет достаточно длинным."
                            }
                        },
                        {
                            "text": "The instructions stated to measure three inches from the <w>beg</w> of the sleeve.",
                            "target_lang_translations": {
                                "ru": "В инструкции было сказано отмерить три дюйма от <w>нач.</w> (начала) рукава."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова \"beginning\" (начало), используемое в схемах вязания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нач.",
                                "target_lang_sense_clarification": "сокращение от начало"
                            },
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c21124df-fd96-3792-9013-506660a2d668",
                    "sense_definition": "Alternative form of <w>bey</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "title",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "The local <w>beg</w> ruled the province with absolute authority.",
                            "target_lang_translations": {
                                "ru": "Местный <w>бек</w> правил провинцией с абсолютной властью."
                            }
                        },
                        {
                            "text": "Historical records mention Yaqub <w>Beg</w> as a significant leader in the region.",
                            "target_lang_translations": {
                                "ru": "Исторические записи упоминают Якуб-<w>бека</w> как значительного лидера в регионе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alternative form"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical title"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул правителя или знатного лица тюркского происхождения; альтернативная форма слова \"bey\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бек",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cfa2c80f-d5c5-3c7d-9d99-488f5db1c4fa",
                    "sense_definition": "A surname from Hindi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Beg</w> arrived at the conference early this morning.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Бег</w> прибыл на конференцию сегодня рано утром."
                            }
                        },
                        {
                            "text": "We are looking for a file belonging to a student named <w>Beg</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем папку, принадлежащую студенту по фамилии <w>Бег</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "from Hindi"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия индийского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бег",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

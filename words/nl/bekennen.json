{
    "word_family": [
        "bekentenis",
        "bekend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4ea29751-58e0-35eb-abd8-72aa9806520e",
                    "sense_definition": "Toegeven dat men iets gedaan heeft, vaak iets wat verkeerd, geheim of strafbaars is",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte <w>bekende</w> gisteren de diefstal tijdens het verhoor.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый вчера <w>сознался</w> в краже во время допроса."
                            }
                        },
                        {
                            "text": "Ik moet eerlijk <w>bekennen</w> dat ik het antwoord op die vraag niet weet.",
                            "target_lang_translations": {
                                "ru": "Я должен честно <w>признаться</w>, что не знаю ответа на этот вопрос."
                            }
                        },
                        {
                            "text": "Na lang aandringen heeft hij uiteindelijk zijn fout <w>bekend</w>.",
                            "target_lang_translations": {
                                "ru": "После долгих уговоров он в конце концов <w>признал</w> свою ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "toegeven",
                        "opbiechten"
                    ],
                    "antonyms": [
                        "ontkennen"
                    ],
                    "common_phrases": [
                        "schuld bekennen",
                        "kleur bekennen",
                        "eerlijk bekennen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Признавать или сознаваться в совершении чего-либо (часто проступка, преступления или чего-то скрываемого)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признавать",
                                "target_lang_sense_clarification": "подтверждать факт или ошибку"
                            },
                            {
                                "target_lang_word": "сознаваться",
                                "target_lang_sense_clarification": "признавать свою вину"
                            },
                            {
                                "target_lang_word": "признаваться",
                                "target_lang_sense_clarification": "откровенно сообщать о чем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63135e60-8984-38be-94fd-4c3a4752ff49",
                    "sense_definition": "Zichtbaar of aanwezig zijn (vrijwel altijd gebruikt in ontkennende zinnen om aan te geven dat iets of iemand er niet is)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er was op het hele plein geen mens te <w>bekennen</w>.",
                            "target_lang_translations": {
                                "ru": "На всей площади не было <w>видно</w> ни души."
                            }
                        },
                        {
                            "text": "We zochten naar een oplossing, maar er was geen uitweg te <w>bekennen</w>.",
                            "target_lang_translations": {
                                "ru": "Мы искали решение, но выхода не было <w>видно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "te vinden zijn",
                        "aanwezig zijn"
                    ],
                    "common_phrases": [
                        "nergens te bekennen",
                        "niet te bekennen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть видимым или обнаруживаемым (почти всегда используется в отрицательных предложениях, означая отсутствие кого-либо или чего-либо)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видно",
                                "target_lang_sense_clarification": "быть заметным (в контексте 'не видно')"
                            },
                            {
                                "target_lang_word": "найти",
                                "target_lang_sense_clarification": "обнаружить (в контексте 'не найти')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a513e55-96f7-3399-ad4b-d5160e4c68c5",
                    "sense_definition": "Geslachtsgemeenschap hebben met iemand",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude bijbelse teksten leest men dat de man zijn vrouw <w>bekende</w>.",
                            "target_lang_translations": {
                                "ru": "В старых библейских текстах читаем, что муж <w>познал</w> свою жену."
                            }
                        },
                        {
                            "text": "Hij <w>bekende</w> haar en zij werd zwanger van een zoon.",
                            "target_lang_translations": {
                                "ru": "Он <w>познал</w> её, и она зачала сына."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "voornamelijk bijbels taalgebruik"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступать в половую связь (устаревшее, библейское значение)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "познать",
                                "target_lang_sense_clarification": "библейский стиль"
                            }
                        ]
                    }
                }
            ]
        }
    ]

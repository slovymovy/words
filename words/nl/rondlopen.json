    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "456560f4-b3ae-3fdb-af46-9de8504b3ec7",
                    "sense_definition": "Zonder specifiek doel ergens lopen of zich ergens bevinden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De toeristen <w>liepen</w> de hele dag <w>rond</w> in het historische centrum.",
                            "target_lang_translations": {
                                "ru": "Туристы весь день <w>бродили</w> по историческому центру.",
                                "en": "The tourists <w>walked around</w> the historic center all day."
                            }
                        },
                        {
                            "text": "Er <w>lopen</w> in dit bos veel wilde dieren <w>rond</w>.",
                            "target_lang_translations": {
                                "ru": "В этом лесу <w>бродит</w> много диких животных.",
                                "en": "Many wild animals <w>roam</w> this forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwalen",
                        "slenteren",
                        "wandelen"
                    ],
                    "antonyms": [
                        "stilstaan",
                        "zitten"
                    ],
                    "common_phrases": [
                        "vrij rondlopen",
                        "buiten rondlopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ходить без определенной цели или находиться где-либо.",
                        "en": "To walk without a specific destination or to be present somewhere."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродить",
                                "target_lang_sense_clarification": "ходить без цели, блуждать"
                            },
                            {
                                "target_lang_word": "гулять",
                                "target_lang_sense_clarification": "прогуливаться"
                            },
                            {
                                "target_lang_word": "расхаживать",
                                "target_lang_sense_clarification": "ходить туда-сюда"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "walk around",
                                "target_lang_sense_clarification": "general movement without a fixed destination"
                            },
                            {
                                "target_lang_word": "wander",
                                "target_lang_sense_clarification": "to move aimlessly or casually"
                            },
                            {
                                "target_lang_word": "roam",
                                "target_lang_sense_clarification": "to move about freely, often used for animals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e2c9d08-88fc-34dd-98bc-4ee618ca99cd",
                    "sense_definition": "In een kring lopen of een route volgen die terugkeert naar het startpunt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleten hebben al drie keer op de baan <w>rondgelopen</w>.",
                            "target_lang_translations": {
                                "ru": "Атлеты уже три раза <w>прошли</w> круг по дорожке.",
                                "en": "The athletes have already <w>walked around</w> the track three times."
                            }
                        },
                        {
                            "text": "We zijn helemaal om het grote meer <w>rondgelopen</w>.",
                            "target_lang_translations": {
                                "ru": "Мы полностью <w>обошли</w> большое озеро.",
                                "en": "We <w>walked</w> all the way <w>around</w> the large lake."
                            }
                        }
                    ],
                    "synonyms": [
                        "rondgaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ходить по кругу или следовать по маршруту, возвращающемуся к исходной точке.",
                        "en": "To walk in a circle or follow a path around something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обойти",
                                "target_lang_sense_clarification": "пройти вокруг чего-либо"
                            },
                            {
                                "target_lang_word": "ходить по кругу",
                                "target_lang_sense_clarification": "двигаться по замкнутой траектории"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "walk around",
                                "target_lang_sense_clarification": "to move in a circle or circuit"
                            },
                            {
                                "target_lang_word": "circle",
                                "target_lang_sense_clarification": "to move around something in a circular path"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
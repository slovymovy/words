        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "94627410-0713-39bb-aa02-5721f11726ab",
                    "sense_definition": "Roodharig, met name gezegd van mensen of dieren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "color_hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer is <ros> en verbrandt snel in de zon.",
                            "target_lang_translations": {
                                "en": "My brother is <w>ginger</w> and burns quickly in the sun.",
                                "ru": "Мой брат <w>рыжий</w> и быстро обгорает на солнце."
                            }
                        },
                        {
                            "text": "Er liep een grote <rosse> kater door onze tuin.",
                            "target_lang_translations": {
                                "en": "A big <w>ginger</w> tomcat walked through our garden.",
                                "ru": "По нашему саду гулял большой <w>рыжий</w> кот."
                            }
                        }
                    ],
                    "synonyms": [
                        "paard"
                    ],
                    "target_lang_definitions": {
                        "en": "Red-haired, specifically said of people or animals.",
                        "ru": "Рыжий (о цвете волос или шерсти)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ginger",
                                "target_lang_sense_clarification": "hair or fur color"
                            },
                            {
                                "target_lang_word": "red-haired",
                                "target_lang_sense_clarification": "having red hair"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рыжий",
                                "target_lang_sense_clarification": "о волосах или шерсти"
                            }

                    ],
                    "common_phrases": [
                        "szkodliwy nałóg",
                        "stary nałóg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Укоренившаяся привычка или обыкновение, обычно оцениваемое негативно, но не обязательно являющееся болезнью."
                    },

                            {
                                "target_lang_word": "ходи",
                                "target_lang_sense_clarification": "приказ двигаться (вообще/несовершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d693100d-8dad-33e3-996c-77199f98920b",

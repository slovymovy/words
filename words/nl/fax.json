    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e13745f1-db03-3810-81df-d00d8931dd63",
                    "sense_definition": "Een apparaat waarmee documenten via een telefoonlijn verzonden en ontvangen kunnen worden.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op ons kantoor staat nog een oude <w>fax</w> die we zelden gebruiken.",
                            "target_lang_translations": {
                                "en": "There is still an old <w>fax machine</w> in our office that we rarely use.",
                                "ru": "В нашем офисе всё ещё стоит старый <w>факс</w>, которым мы редко пользуемся."
                            }
                        },
                        {
                            "text": "Zorg ervoor dat er genoeg papier in de <w>fax</w> zit voordat je begint.",
                            "target_lang_translations": {
                                "en": "Make sure there is enough paper in the <w>fax</w> before you start.",
                                "ru": "Убедись, что в <w>факсе</w> достаточно бумаги, прежде чем начать."
                            }
                        }
                    ],
                    "synonyms": [
                        "telefax"
                    ],
                    "common_phrases": [
                        "via de fax",
                        "per fax"
                    ],
                    "target_lang_definitions": {
                        "en": "A device used to send and receive documents over a telephone line.",
                        "ru": "Устройство, используемое для отправки и получения документов по телефонной линии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fax machine",
                                "target_lang_sense_clarification": "device"
                            },
                            {
                                "target_lang_word": "fax",
                                "target_lang_sense_clarification": "device"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "факс",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "факсимильный аппарат",
                                "target_lang_sense_clarification": "устройство (официально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08da726f-fda9-372e-be2f-88c576f0af6f",
                    "sense_definition": "Een schriftelijk bericht of document dat via een faxtoestel is verstuurd.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "message",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je de <w>fax</w> met de getekende contracten al ontvangen?",
                            "target_lang_translations": {
                                "en": "Have you already received the <w>fax</w> with the signed contracts?",
                                "ru": "Ты уже получил <w>факс</w> с подписанными контрактами?"
                            }
                        },
                        {
                            "text": "Ik stuur je morgen een <w>fax</w> met alle details van de bestelling.",
                            "target_lang_translations": {
                                "en": "I will send you a <w>fax</w> tomorrow with all the details of the order.",
                                "ru": "Завтра я отправлю тебе <w>факс</w> со всеми деталями заказа."
                            }
                        }
                    ],
                    "synonyms": [
                        "telefax"
                    ],
                    "common_phrases": [
                        "een fax sturen",
                        "een fax ontvangen"
                    ],
                    "target_lang_definitions": {
                        "en": "A written message or document sent via a fax machine.",
                        "ru": "Письменное сообщение или документ, отправленный через факсимильный аппарат."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fax",
                                "target_lang_sense_clarification": "message/document"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "факс",
                                "target_lang_sense_clarification": "сообщение/документ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6d12e6d3-4a37-3a49-ac26-3206ae388d2d",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>faxen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>fax</w> het document meteen naar je toe zodat je het kunt lezen.",
                            "target_lang_translations": {
                                "en": "I will <w>fax</w> the document to you right away so you can read it.",
                                "ru": "Я сейчас же <w>отправлю</w> тебе этот документ <w>по факсу</w>, чтобы ты мог его прочитать."
                            }
                        },
                        {
                            "text": "Als de e-mail niet werkt, <w>fax</w> ik de gegevens wel naar kantoor.",
                            "target_lang_translations": {
                                "en": "If the email doesn't work, I will <w>fax</w> the data to the office.",
                                "ru": "Если электронная почта не сработает, я <w>отправлю</w> данные в офис <w>по факсу</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'faxen' (to fax).",
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'faxen' (отправлять факс)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fax",
                                "target_lang_sense_clarification": "verb action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отправляю по факсу",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "высылаю факсом",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7a8d000-1b8e-3231-9e3a-2e523992387e",
                    "sense_definition": "Gebiedende wijs van <w>faxen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Fax</w> die papieren zo snel mogelijk naar de verzekeringsmaatschappij.",
                            "target_lang_translations": {
                                "en": "<w>Fax</w> those papers to the insurance company as soon as possible.",
                                "ru": "<w>Отправь</w> эти бумаги <w>по факсу</w> в страховую компанию как можно скорее."
                            }
                        },
                        {
                            "text": "<w>Fax</w> me even zodra je de bevestiging binnen hebt.",
                            "target_lang_translations": {
                                "en": "<w>Fax</w> me as soon as you have received the confirmation.",
                                "ru": "<w>Вышли</w> мне <w>факс</w>, как только получишь подтверждение."
                            }
                        }
                    ],

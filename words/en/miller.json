                            {
                                "target_lang_word": "совка",
                                "target_lang_sense_clarification": "энтомология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1797216e-006f-314b-8e97-893beb1b2e5b",
                    "sense_definition": "A floury-smelling mushroom, Clitopilus prunulus.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The forager identified the <w>miller</w> by its distinctive scent of raw flour.",
                            "target_lang_translations": {
                                "ru": "Грибник опознал <w>ивишень</w> по характерному запаху сырой муки."
                            }
                        },
                        {
                            "text": "We found a cluster of the <w>miller</w> mushrooms growing in the damp soil.",
                            "target_lang_translations": {
                                "ru": "Мы нашли группу грибов <w>ивишней</w>, растущих во влажной почве."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гриб Clitopilus prunulus с запахом муки."
                    },
                    "translations": {
                        "ru": [
                            {

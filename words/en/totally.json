    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4b779100-9e74-3ed5-9605-e2360583ee35",
                    "sense_definition": "To the fullest extent or degree.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "completeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>totally</w> agree with your opinion on this matter.",
                            "target_lang_translations": {
                                "ru": "Я <w>полностью</w> согласен с вашим мнением по этому вопросу."
                            }
                        },
                        {
                            "text": "The old building was <w>totally</w> destroyed by the fire.",
                            "target_lang_translations": {
                                "ru": "Старое здание было <w>полностью</w> разрушено пожаром."
                            }
                        }
                    ],
                    "synonyms": [
                        "completely",
                        "entirely",
                        "fully",
                        "wholly"
                    ],
                    "antonyms": [
                        "partially",
                        "partly"
                    ],
                    "common_phrases": [
                        "totally different",
                        "totally wrong",
                        "totally agree"
                    ],
                    "target_lang_definitions": {
                        "ru": "В полной мере или степени; целиком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полностью",
                                "target_lang_sense_clarification": "целиком, без остатка"
                            },
                            {
                                "target_lang_word": "абсолютно",
                                "target_lang_sense_clarification": "безусловно, совершенно"
                            },
                            {
                                "target_lang_word": "совершенно",
                                "target_lang_sense_clarification": "в высшей степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b20f624-7924-39e5-82cd-6c95700aa63c",
                    "sense_definition": "Very; extremely.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The concert we went to last night was <w>totally</w> awesome.",
                            "target_lang_translations": {
                                "ru": "Концерт, на который мы ходили вчера вечером, был <w>просто</w> потрясающим."
                            }
                        },
                        {
                            "text": "She was <w>totally</w> surprised when everyone shouted happy birthday.",
                            "target_lang_translations": {
                                "ru": "Она была <w>совершенно</w> удивлена, когда все закричали «С днем рождения»."
                            }
                        }
                    ],
                    "antonyms": [
                        "slightly",
                        "somewhat"
                    ],
                    "common_phrases": [
                        "totally awesome",
                        "totally cool"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used for emphasis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень; чрезвычайно (используется для эмоционального усиления)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просто",
                                "target_lang_sense_clarification": "усилительная частица (разг.)"
                            },
                            {
                                "target_lang_word": "реально",
                                "target_lang_sense_clarification": "действительно (разг.)"
                            },
                            {
                                "target_lang_word": "жутко",
                                "target_lang_sense_clarification": "очень сильно (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79f7b47b-b6a6-34a7-abca-b46865f544d4",
                    "sense_definition": "Definitely; for sure.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "confirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That is <w>totally</w> not what I meant to say.",
                            "target_lang_translations": {
                                "ru": "Это <w>совсем</w> не то, что я хотел сказать."
                            }
                        },
                        {
                            "text": "He <w>totally</w> ignored my advice despite the warning.",
                            "target_lang_translations": {
                                "ru": "Он <w>вообще</w> проигнорировал мой совет, несмотря на предупреждение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "totally not"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to express agreement or certainty"

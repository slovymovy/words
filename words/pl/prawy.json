                        "prawy obywatel"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке: честный, благородный, следующий моральным принципам"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благородный",
                                "target_lang_sense_clarification": "высокоморальный"
                            },
                            {
                                "target_lang_word": "честный",
                                "target_lang_sense_clarification": "правдивый, добросовестный"
                            },
                            {

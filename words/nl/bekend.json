            "senses": [
                {
                    "sense_id": "4e64c999-035d-3139-b768-9dca70fecdf3",
                    "sense_definition": "Door heel veel mensen gekend en vaak ook gewaardeerd.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fame_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een <w>bekend</w> acteur in Nederland.",
                            "target_lang_translations": {

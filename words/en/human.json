        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "841ba621-e86c-315b-8f1d-7999e54ac893",
                    "sense_definition": "Relating to or belonging to people (the species Homo sapiens).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>human</w> body is made up of many different systems.",
                            "target_lang_translations": {
                                "ru": "<w>Человеческое</w> тело состоит из множества различных систем."
                            }
                        },
                        {
                            "text": "We must work together to protect basic <w>human</w> rights.",
                            "target_lang_translations": {
                                "ru": "Мы должны работать вместе, чтобы защитить основные права <w>человека</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "nonhuman"
                    ],
                    "common_phrases": [
                        "human body",
                        "human rights",
                        "human race",
                        "human nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к людям или принадлежащий им (виду Homo sapiens)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человеческий",
                                "target_lang_sense_clarification": "относящийся к человеку"
                            },
                            {
                                "target_lang_word": "людской",
                                "target_lang_sense_clarification": "принадлежащий людям, свойственный людям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4bf6e67-28f3-3d95-b7fd-91dd6310ae47",
                    "sense_definition": "Having the typical qualities, feelings, or weaknesses of people, as opposed to being divine or perfect.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is only <w>human</w> to make mistakes when you are tired.",
                            "target_lang_translations": {
                                "ru": "Совершать ошибки, когда устал, — это <w>свойственно человеку</w>."
                            }
                        },
                        {
                            "text": "The doctor showed a very <w>human</w> side when he comforted the crying child.",
                            "target_lang_translations": {
                                "ru": "Доктор проявил свою <w>человеческую</w> сторону, когда утешал плачущего ребенка."
                            }
                        }
                    ],
                    "antonyms": [
                        "inhuman"
                    ],
                    "common_phrases": [
                        "human error",
                        "only human"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий типичными качествами, чувствами или слабостями людей, в отличие от божественного или совершенного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человеческий",
                                "target_lang_sense_clarification": "свойственный человеку"
                            },
                            {
                                "target_lang_word": "свойственный человеку",
                                "target_lang_sense_clarification": "фраза: присущий людям"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    "semantic_group_id": "punishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old story, the villain threatened to <w>hide</w> the boy if he stole again.",
                            "target_lang_translations": {
                                "ru": "В старой сказке злодей пригрозил <w>выпороть</w> мальчика, если тот снова украдёт."
                            }
                        },
                        {
                            "text": "He was <w>hided</w> for his disobedience.",
                            "target_lang_translations": {
                                "ru": "Его <w>выпороли</w> за непослушание."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить кого-либо плетью или ремнём (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пороть",

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8f5e4f9-4d29-3b7b-81a7-5d66c0f8fd1c",
                    "sense_definition": "Państwo, w którym władzę sprawują osoby wybrane przez obywateli na określony czas, a głową państwa nie jest monarcha.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska jest <w>republiką</w> położoną w Europie Środkowej.",
                            "target_lang_translations": {
                                "ru": "Польша — это <w>республика</w>, расположенная в Центральной Европе."
                            }
                        },
                        {
                            "text": "Prezydent tej <w>republiki</w> spotkał się z zagranicznymi dyplomatami.",
                            "target_lang_translations": {
                                "ru": "Президент этой <w>республики</w> встретился с иностранными дипломатами."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzeczpospolita"
                    ],
                    "antonyms": [
                        "monarchia",
                        "królestwo"
                    ],
                    "common_phrases": [
                        "niepodległa republika",
                        "prezydent republiki",
                        "konstytucja republiki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство, в котором власть принадлежит представителям, избранным народом на определенный срок, а главой государства не является монарх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "республика",
                                "target_lang_sense_clarification": "государство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d0cd367-8350-312f-be99-131f7cdd0430",
                    "sense_definition": "Ustrój polityczny, w którym najwyższe organy władzy są wyłaniane w drodze wyborów, a nie dziedziczone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starożytnym Rzymie wprowadzono <w>republikę</w> po obaleniu króla.",
                            "target_lang_translations": {
                                "ru": "В Древнем Риме ввели <w>республику</w> после свержения царя."
                            }
                        },
                        {
                            "text": "Współczesna <w>republika</w> opiera się na zasadach demokracji i trójpodziału władzy.",
                            "target_lang_translations": {
                                "ru": "Современная <w>республика</w> основывается на принципах демократии и разделения властей."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzeczpospolita"
                    ],
                    "antonyms": [
                        "monarchia"
                    ],
                    "common_phrases": [
                        "republika demokratyczna",
                        "republika parlamentarna",
                        "republika federalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма государственного правления, при которой высшие органы власти избираются населением, а не передаются по наследству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "республика",
                                "target_lang_sense_clarification": "форма правления"
                            }
                        ]
                    }
                }
            ]
        }

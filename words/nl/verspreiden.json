                        "bijeenbrengen"
                    ],
                    "common_phrases": [
                        "nieuws verspreiden",
                        "folders verspreiden",
                        "een geur verspreiden"
                    ],
                    "target_lang_definitions": {
                        "en": "To put into circulation or distribute over a larger area"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spread",
                                "target_lang_sense_clarification": "To distribute or extend over a given area"
                            },
                            {
                                "target_lang_word": "scatter",
                                "target_lang_sense_clarification": "To throw loosely or distribute in different directions"
                            },
                            {
                                "target_lang_word": "disseminate",
                                "target_lang_sense_clarification": "To spread information, ideas, or news widely"
                            },
                            {
                                "target_lang_word": "disperse",
                                "target_lang_sense_clarification": "To drive or send off in various directions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdc759e5-221c-37c5-8ea0-7b2490ecc850",
                    "sense_definition": "Zich uitbreiden naar andere plaatsen of personen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het vuur <w>verspreidt</w> zich snel door de droge bossen.",
                            "target_lang_translations": {
                                "en": "The fire <w>spreads</w> quickly through the dry forests."
                            }
                        },
                        {
                            "text": "Het gerucht <w>verspreidde</w> zich als een lopend vuurtje door het dorp.",
                            "target_lang_translations": {
                                "en": "The rumor <w>spread</w> like wildfire through the village."
                            }
                        },
                        {
                            "text": "Door de droogte <w>verspreiden</w> de branden zich heel snel.",
                            "target_lang_translations": {
                                "en": "Due to the drought, the fires <w>spread</w> very quickly."
                            }
                        }
                    ],
                    "synonyms": [

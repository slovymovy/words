    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "633188e6-782c-31c6-8c9e-1b88d8b916ac",
                    "sense_definition": "A genre of literature and media featuring magical elements, supernatural creatures, and imaginary worlds.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother loves reading <w>fantasy</w> novels about dragons and wizards.",
                            "target_lang_translations": {
                                "ru": "Мой брат любит читать романы в жанре <w>фэнтези</w> о драконах и волшебниках."
                            }
                        },
                        {
                            "text": "The movie is a mix of science fiction and <w>fantasy</w>.",
                            "target_lang_translations": {
                                "ru": "Этот фильм представляет собой смесь научной фантастики и <w>фэнтези</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "non-fiction",
                        "realism"
                    ],
                    "common_phrases": [
                        "fantasy world",
                        "fantasy novel",
                        "high fantasy",
                        "dark fantasy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр литературы и искусства, включающий магические элементы, сверхъестественных существ и вымышленные миры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фэнтези",
                                "target_lang_sense_clarification": "жанр с магией и мифологией"
                            },
                            {
                                "target_lang_word": "фантастика",
                                "target_lang_sense_clarification": "общее название жанра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a14484c-61b7-3967-80ec-a75916bd7b21",
                    "sense_definition": "An idea, situation, or event that is created in the imagination and is not real.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "imagination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She lived in a world of <w>fantasy</w> to escape the difficulties of her daily life.",
                            "target_lang_translations": {
                                "ru": "Она жила в мире <w>фантазий</w>, чтобы уйти от трудностей повседневной жизни."
                            }
                        },
                        {
                            "text": "His plan to become a millionaire overnight is just a <w>fantasy</w>.",
                            "target_lang_translations": {
                                "ru": "Его план стать миллионером за одну ночь — это просто <w>фантазия</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "daydream",
                        "illusion",
                        "fancy",
                        "imagination"
                    ],
                    "antonyms": [
                        "reality",
                        "fact",
                        "truth"
                    ],
                    "common_phrases": [
                        "pure fantasy",
                        "sexual fantasy",
                        "flight of fantasy",
                        "live in a fantasy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идея, ситуация или событие, созданные в воображении и не существующие в реальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фантазия",
                                "target_lang_sense_clarification": "продукт воображения"
                            },
                            {
                                "target_lang_word": "вымысел",
                                "target_lang_sense_clarification": "нечто нереальное"
                            },
                            {
                                "target_lang_word": "иллюзия",
                                "target_lang_sense_clarification": "самообман"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90d6dff3-3854-36ff-891f-7f49f619a884",
                    "sense_definition": "A creative, whimsical, or highly imaginative artistic design.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "design",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect created a <w>fantasy</w> of glass and steel that amazed everyone.",
                            "target_lang_translations": {
                                "ru": "Архитектор создал <w>фантазию</w> из стекла и стали, которая поразила всех."
                            }
                        },
                        {
                            "text": "Her dress was embroidered with a floral <w>fantasy</w> of gold thread.",
                            "target_lang_translations": {
                                "ru": "Ее платье было вышито цветочной <w>фантазией</w> из золотых нитей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Творческий, причудливый или высокохудожественный дизайн или композиция."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фантазия",
                                "target_lang_sense_clarification": "художественная композиция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3379de8b-25b6-31fd-9b5f-776eef47a33b",
                    "sense_definition": "A slang term for the illegal drug gamma-hydroxybutyric acid (GHB).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer warned the students about the dangers of taking <w>fantasy</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейский предупредил студентов об опасности употребления <w>оксибутирата</w>."
                            }
                        },
                        {
                            "text": "He was hospitalized after consuming a substance known as <w>fantasy</w> at the club.",
                            "target_lang_translations": {
                                "ru": "Он был госпитализирован после употребления вещества, известного как <w>фэнтези</w>, в клубе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "drug culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сленговое название запрещенного наркотика гамма-оксимасляной кислоты (GHB)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оксибутират",
                                "target_lang_sense_clarification": "химическое вещество"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3d94eb2-b3f7-3f70-aec0-80dd0dce4d2c",
                    "sense_definition": "To imagine or conceive something mentally.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "imagination_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In his quiet moments, he <w>fantasied</w> about traveling to distant stars.",
                            "target_lang_translations": {
                                "ru": "В минуты тишины он <w>фантазировал</w> о путешествиях к далеким звездам."
                            }
                        },
                        {
                            "text": "She <w>fantasied</w> a life where she was free from all responsibilities.",
                            "target_lang_translations": {
                                "ru": "Она <w>воображала</w> жизнь, в которой была свободна от всех обязанностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "imagine",
                        "fantasize",
                        "envision"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern English uses 'fantasize'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять или задумывать что-либо мысленно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фантазировать",
                                "target_lang_sense_clarification": "предаваться мечтам"
                            },
                            {
                                "target_lang_word": "воображать",
                                "target_lang_sense_clarification": "представлять в уме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74e67f66-ee4f-3bec-932b-870751f7b547",
                    "sense_definition": "To have a liking for or be pleased with someone or something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "like_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king <w>fantasied</w> the new tapestry more than any other treasure.",
                            "target_lang_translations": {
                                "ru": "Король <w>любил</w> новый гобелен больше, чем любое другое сокровище."
                            }
                        },
                        {
                            "text": "He <w>fantasied</w> her so much that he ignored all his royal duties.",
                            "target_lang_translations": {
                                "ru": "Он так <w>желал</w> её, что игнорировал все свои королевские обязанности."
                            }
                        }
                    ],
                    "synonyms": [
                        "fancy",
                        "like",
                        "desire"
                    ],
                    "antonyms": [
                        "dislike"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "obsolete usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать симпатию или влечение к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "хотеть"
                            },
                            {
                                "target_lang_word": "любить",
                                "target_lang_sense_clarification": "испытывать привязанность"
                            }
                        ]
                    }
                }
            ]
        }
    ]

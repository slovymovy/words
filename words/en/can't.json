    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d0b8c367-2592-3a13-9339-849d97ea37db",
                    "sense_definition": "Contraction of <w>cannot</w>; to not have the ability, skill, or capacity to do something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <can't> hear the music because it is too quiet.",
                            "target_lang_translations": {
                                "ru": "Я <не могу> расслышать музыку, потому что она играет слишком тихо."
                            }
                        },
                        {
                            "text": "She <can't> wait to see her friends again.",
                            "target_lang_translations": {
                                "ru": "Она <не может> дождаться, когда снова увидит своих друзей."
                            }
                        }
                    ],
                    "antonyms": [
                        "can",
                        "be able to"
                    ],
                    "common_phrases": [
                        "can't wait",
                        "can't stand",
                        "can't help",
                        "can't afford"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "contraction of cannot"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от cannot; отсутствие способности, навыка или возможности сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не мочь",
                                "target_lang_sense_clarification": "отсутствие физической возможности"
                            },
                            {
                                "target_lang_word": "не уметь",
                                "target_lang_sense_clarification": "отсутствие навыка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "298aefdb-cd89-3b57-8334-4da692f771b4",
                    "sense_definition": "Contraction of <w>cannot</w>; to not be allowed or permitted to do something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <can't> park your car in front of the exit.",
                            "target_lang_translations": {
                                "ru": "Здесь <нельзя> парковать машину перед выездом."
                            }
                        },
                        {
                            "text": "Students <can't> use their phones during the exam.",
                            "target_lang_translations": {
                                "ru": "Студентам <нельзя> пользоваться телефонами во время экзамена."
                            }
                        }
                    ],
                    "antonyms": [
                        "can"
                    ],
                    "common_phrases": [
                        "can't do that",
                        "can't go"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "contraction of cannot"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от cannot; отсутствие разрешения или права сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нельзя",
                                "target_lang_sense_clarification": "запрет"
                            },
                            {
                                "target_lang_word": "не мочь",
                                "target_lang_sense_clarification": "отсутствие права/разрешения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7d6064f-8d87-3de5-b617-5821202eb7f1",
                    "sense_definition": "Contraction of <w>cannot</w>; used to say that something is logically impossible or very unlikely to be true.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <can't> be true that he missed the train again.",
                            "target_lang_translations": {
                                "ru": "<Не может> быть правдой, что он снова опоздал на поезд."
                            }
                        },
                        {
                            "text": "That <can't> be the right house because the number is wrong.",
                            "target_lang_translations": {
                                "ru": "Это <не может> быть тот дом, потому что номер неправильный."
                            }
                        }
                    ],
                    "antonyms": [
                        "must"
                    ],
                    "common_phrases": [
                        "can't be true",
                        "can't be right",
                        "can't be serious"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "contraction of cannot"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от cannot; используется для выражения логической невозможности или крайней маловероятности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не мочь",
                                "target_lang_sense_clarification": "логическая невозможность"
                            },
                            {
                                "target_lang_word": "не может быть",
                                "target_lang_sense_clarification": "выражение недоверия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
